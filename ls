[32m+[m		[32m// is present, execute the factory and get jQuery.[m
[32m+[m		[32m// For environments that do not have a `window` with a `document`[m
[32m+[m		[32m// (such as Node.js), expose a factory as module.exports.[m
[32m+[m		[32m// This accentuates the need for the creation of a real `window`.[m
[32m+[m		[32m// e.g. var jQuery = require("jquery")(window);[m
[32m+[m		[32m// See ticket #14549 for more info.[m
[32m+[m		[32mmodule.exports = global.document ?[m
[32m+[m			[32mfactory( global, true ) :[m
[32m+[m			[32mfunction( w ) {[m
[32m+[m				[32mif ( !w.document ) {[m
[32m+[m					[32mthrow new Error( "jQuery requires a window with a document" );[m
[32m+[m				[32m}[m
[32m+[m				[32mreturn factory( w );[m
[32m+[m			[32m};[m
[32m+[m	[32m} else {[m
[32m+[m		[32mfactory( global );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m// Pass this if window is not defined yet[m
[32m+[m[32m} )( typeof window !== "undefined" ? window : this, function( window, noGlobal ) {[m
[32m+[m
[32m+[m[32m// Edge <= 12 - 13+, Firefox <=18 - 45+, IE 10 - 11, Safari 5.1 - 9+, iOS 6 - 9.1[m
[32m+[m[32m// throw exceptions when non-strict code (e.g., ASP.NET 4.5) accesses strict mode[m
[32m+[m[32m// arguments.callee.caller (trac-13335). But as of jQuery 3.0 (2016), strict mode should be common[m
[32m+[m[32m// enough that all such attempts are guarded in a try block.[m
[32m+[m[32m"use strict";[m
[32m+[m
[32m+[m[32mvar arr = [];[m
[32m+[m
[32m+[m[32mvar document = window.document;[m
[32m+[m
[32m+[m[32mvar getProto = Object.getPrototypeOf;[m
[32m+[m
[32m+[m[32mvar slice = arr.slice;[m
[32m+[m
[32m+[m[32mvar concat = arr.concat;[m
[32m+[m
[32m+[m[32mvar push = arr.push;[m
[32m+[m
[32m+[m[32mvar indexOf = arr.indexOf;[m
[32m+[m
[32m+[m[32mvar class2type = {};[m
[32m+[m
[32m+[m[32mvar toString = class2type.toString;[m
[32m+[m
[32m+[m[32mvar hasOwn = class2type.hasOwnProperty;[m
[32m+[m
[32m+[m[32mvar fnToString = hasOwn.toString;[m
[32m+[m
[32m+[m[32mvar ObjectFunctionString = fnToString.call( Object );[m
[32m+[m
[32m+[m[32mvar support = {};[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m	[32mfunction DOMEval( code, doc ) {[m
[32m+[m		[32mdoc = doc || document;[m
[32m+[m
[32m+[m		[32mvar script = doc.createElement( "script" );[m
[32m+[m
[32m+[m		[32mscript.text = code;[m
[32m+[m		[32mdoc.head.appendChild( script ).parentNode.removeChild( script );[m
[32m+[m	[32m}[m
[32m+[m[32m/* global Symbol */[m
[32m+[m[32m// Defining this global in .eslintrc would create a danger of using the global[m
[32m+[m[32m// unguarded in another place, it seems safer to define global only for this module[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mvar[m
[32m+[m	[32mversion = "3.1.0",[m
[32m+[m
[32m+[m	[32m// Define a local copy of jQuery[m
[32m+[m	[32mjQuery = function( selector, context ) {[m
[32m+[m
[32m+[m		[32m// The jQuery object is actually just the init constructor 'enhanced'[m
[32m+[m		[32m// Need init if jQuery is called (just allow error to be thrown if not included)[m
[32m+[m		[32mreturn new jQuery.fn.init( selector, context );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Support: Android <=4.0 only[m
[32m+[m	[32m// Make sure we trim BOM and NBSP[m
[32m+[m	[32mrtrim = /^[\s\uFEFF\xA0]+|[\s\uFEFF\xA0]+$/g,[m
[32m+[m
[32m+[m	[32m// Matches dashed string for camelizing[m
[32m+[m	[32mrmsPrefix = /^-ms-/,[m
[32m+[m	[32mrdashAlpha = /-([a-z])/g,[m
[32m+[m
[32m+[m	[32m// Used by jQuery.camelCase as callback to replace()[m
[32m+[m	[32mfcamelCase = function( all, letter ) {[m
[32m+[m		[32mreturn letter.toUpperCase();[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m[32mjQuery.fn = jQuery.prototype = {[m
[32m+[m
[32m+[m	[32m// The current version of jQuery being used[m
[32m+[m	[32mjquery: version,[m
[32m+[m
[32m+[m	[32mconstructor: jQuery,[m
[32m+[m
[32m+[m	[32m// The default length of a jQuery object is 0[m
[32m+[m	[32mlength: 0,[m
[32m+[m
[32m+[m	[32mtoArray: function() {[m
[32m+[m		[32mreturn slice.call( this );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Get the Nth element in the matched element set OR[m
[32m+[m	[32m// Get the whole matched element set as a clean array[m
[32m+[m	[32mget: function( num ) {[m
[32m+[m		[32mreturn num != null ?[m
[32m+[m
[32m+[m			[32m// Return just the one element from the set[m
[32m+[m			[32m( num < 0 ? this[ num + this.length ] : this[ num ] ) :[m
[32m+[m
[32m+[m			[32m// Return all the elements in a clean array[m
[32m+[m			[32mslice.call( this );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Take an array of elements and push it onto the stack[m
[32m+[m	[32m// (returning the new matched element set)[m
[32m+[m	[32mpushStack: function( elems ) {[m
[32m+[m
[32m+[m		[32m// Build a new jQuery matched element set[m
[32m+[m		[32mvar ret = jQuery.merge( this.constructor(), elems );[m
[32m+[m
[32m+[m		[32m// Add the old object onto the stack (as a reference)[m
[32m+[m		[32mret.prevObject = this;[m
[32m+[m
[32m+[m		[32m// Return the newly-formed element set[m
[32m+[m		[32mreturn ret;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Execute a callback for every element in the matched set.[m
[32m+[m	[32meach: function( callback ) {[m
[32m+[m		[32mreturn jQuery.each( this, callback );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mmap: function( callback ) {[m
[32m+[m		[32mreturn this.pushStack( jQuery.map( this, function( elem, i ) {[m
[32m+[m			[32mreturn callback.call( elem, i, elem );[m
[32m+[m		[32m} ) );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mslice: function() {[m
[32m+[m		[32mreturn this.pushStack( slice.apply( this, arguments ) );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mfirst: function() {[m
[32m+[m		[32mreturn this.eq( 0 );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mlast: function() {[m
[32m+[m		[32mreturn this.eq( -1 );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32meq: function( i ) {[m
[32m+[m		[32mvar len = this.length,[m
[32m+[m			[32mj = +i + ( i < 0 ? len : 0 );[m
[32m+[m		[32mreturn this.pushStack( j >= 0 && j < len ? [ this[ j ] ] : [] );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mend: function() {[m
[32m+[m		[32mreturn this.prevObject || this.constructor();[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// For internal use only.[m
[32m+[m	[32m// Behaves like an Array's method, not like a jQuery method.[m
[32m+[m	[32mpush: push,[m
[32m+[m	[32msort: arr.sort,[m
[32m+[m	[32msplice: arr.splice[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.extend = jQuery.fn.extend = function() {[m
[32m+[m	[32mvar options, name, src, copy, copyIsArray, clone,[m
[32m+[m		[32mtarget = arguments[ 0 ] || {},[m
[32m+[m		[32mi = 1,[m
[32m+[m		[32mlength = arguments.length,[m
[32m+[m		[32mdeep = false;[m
[32m+[m
[32m+[m	[32m// Handle a deep copy situation[m
[32m+[m	[32mif ( typeof target === "boolean" ) {[m
[32m+[m		[32mdeep = target;[m
[32m+[m
[32m+[m		[32m// Skip the boolean and the target[m
[32m+[m		[32mtarget = arguments[ i ] || {};[m
[32m+[m		[32mi++;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Handle case when target is a string or something (possible in deep copy)[m
[32m+[m	[32mif ( typeof target !== "object" && !jQuery.isFunction( target ) ) {[m
[32m+[m		[32mtarget = {};[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Extend jQuery itself if only one argument is passed[m
[32m+[m	[32mif ( i === length ) {[m
[32m+[m		[32mtarget = this;[m
[32m+[m		[32mi--;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mfor ( ; i < length; i++ ) {[m
[32m+[m
[32m+[m		[32m// Only deal with non-null/undefined values[m
[32m+[m		[32mif ( ( options = arguments[ i ] ) != null ) {[m
[32m+[m
[32m+[m			[32m// Extend the base object[m
[32m+[m			[32mfor ( name in options ) {[m
[32m+[m				[32msrc = target[ name ];[m
[32m+[m				[32mcopy = options[ name ];[m
[32m+[m
[32m+[m				[32m// Prevent never-ending loop[m
[32m+[m				[32mif ( target === copy ) {[m
[32m+[m					[32mcontinue;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Recurse if we're merging plain objects or arrays[m
[32m+[m				[32mif ( deep && copy && ( jQuery.isPlainObject( copy ) ||[m
[32m+[m					[32m( copyIsArray = jQuery.isArray( copy ) ) ) ) {[m
[32m+[m
[32m+[m					[32mif ( copyIsArray ) {[m
[32m+[m						[32mcopyIsArray = false;[m
[32m+[m						[32mclone = src && jQuery.isArray( src ) ? src : [];[m
[32m+[m
[32m+[m					[32m} else {[m
[32m+[m						[32mclone = src && jQuery.isPlainObject( src ) ? src : {};[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// Never move original objects, clone them[m
[32m+[m					[32mtarget[ name ] = jQuery.extend( deep, clone, copy );[m
[32m+[m
[32m+[m				[32m// Don't bring in undefined values[m
[32m+[m				[32m} else if ( copy !== undefined ) {[m
[32m+[m					[32mtarget[ name ] = copy;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Return the modified object[m
[32m+[m	[32mreturn target;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.extend( {[m
[32m+[m
[32m+[m	[32m// Unique for each copy of jQuery on the page[m
[32m+[m	[32mexpando: "jQuery" + ( version + Math.random() ).replace( /\D/g, "" ),[m
[32m+[m
[32m+[m	[32m// Assume jQuery is ready without the ready module[m
[32m+[m	[32misReady: true,[m
[32m+[m
[32m+[m	[32merror: function( msg ) {[m
[32m+[m		[32mthrow new Error( msg );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mnoop: function() {},[m
[32m+[m
[32m+[m	[32misFunction: function( obj ) {[m
[32m+[m		[32mreturn jQuery.type( obj ) === "function";[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32misArray: Array.isArray,[m
[32m+[m
[32m+[m	[32misWindow: function( obj ) {[m
[32m+[m		[32mreturn obj != null && obj === obj.window;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32misNumeric: function( obj ) {[m
[32m+[m
[32m+[m		[32m// As of jQuery 3.0, isNumeric is limited to[m
[32m+[m		[32m// strings and numbers (primitives or objects)[m
[32m+[m		[32m// that can be coerced to finite numbers (gh-2662)[m
[32m+[m		[32mvar type = jQuery.type( obj );[m
[32m+[m		[32mreturn ( type === "number" || type === "string" ) &&[m
[32m+[m
[32m+[m			[32m// parseFloat NaNs numeric-cast false positives ("")[m
[32m+[m			[32m// ...but misinterprets leading-number strings, particularly hex literals ("0x...")[m
[32m+[m			[32m// subtraction forces infinities to NaN[m
[32m+[m			[32m!isNaN( obj - parseFloat( obj ) );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32misPlainObject: function( obj ) {[m
[32m+[m		[32mvar proto, Ctor;[m
[32m+[m
[32m+[m		[32m// Detect obvious negatives[m
[32m+[m		[32m// Use toString instead of jQuery.type to catch host objects[m
[32m+[m		[32mif ( !obj || toString.call( obj ) !== "[object Object]" ) {[m
[32m+[m			[32mreturn false;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mproto = getProto( obj );[m
[32m+[m
[32m+[m		[32m// Objects with no prototype (e.g., `Object.create( null )`) are plain[m
[32m+[m		[32mif ( !proto ) {[m
[32m+[m			[32mreturn true;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Objects with prototype are plain iff they were constructed by a global Object function[m
[32m+[m		[32mCtor = hasOwn.call( proto, "constructor" ) && proto.constructor;[m
[32m+[m		[32mreturn typeof Ctor === "function" && fnToString.call( Ctor ) === ObjectFunctionString;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32misEmptyObject: function( obj ) {[m
[32m+[m
[32m+[m		[32m/* eslint-disable no-unused-vars */[m
[32m+[m		[32m// See https://github.com/eslint/eslint/issues/6125[m
[32m+[m		[32mvar name;[m
[32m+[m
[32m+[m		[32mfor ( name in obj ) {[m
[32m+[m			[32mreturn false;[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn true;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mtype: function( obj ) {[m
[32m+[m		[32mif ( obj == null ) {[m
[32m+[m			[32mreturn obj + "";[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Support: Android <=2.3 only (functionish RegExp)[m
[32m+[m		[32mreturn typeof obj === "object" || typeof obj === "function" ?[m
[32m+[m			[32mclass2type[ toString.call( obj ) ] || "object" :[m
[32m+[m			[32mtypeof obj;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Evaluates a script in a global context[m
[32m+[m	[32mglobalEval: function( code ) {[m
[32m+[m		[32mDOMEval( code );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Convert dashed to camelCase; used by the css and data modules[m
[32m+[m	[32m// Support: IE <=9 - 11, Edge 12 - 13[m
[32m+[m	[32m// Microsoft forgot to hump their vendor prefix (#9572)[m
[32m+[m	[32mcamelCase: function( string ) {[m
[32m+[m		[32mreturn string.replace( rmsPrefix, "ms-" ).replace( rdashAlpha, fcamelCase );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mnodeName: function( elem, name ) {[m
[32m+[m		[32mreturn elem.nodeName && elem.nodeName.toLowerCase() === name.toLowerCase();[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32meach: function( obj, callback ) {[m
[32m+[m		[32mvar length, i = 0;[m
[32m+[m
[32m+[m		[32mif ( isArrayLike( obj ) ) {[m
[32m+[m			[32mlength = obj.length;[m
[32m+[m			[32mfor ( ; i < length; i++ ) {[m
[32m+[m				[32mif ( callback.call( obj[ i ], i, obj[ i ] ) === false ) {[m
[32m+[m					[32mbreak;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m} else {[m
[32m+[m			[32mfor ( i in obj ) {[m
[32m+[m				[32mif ( callback.call( obj[ i ], i, obj[ i ] ) === false ) {[m
[32m+[m					[32mbreak;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn obj;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Support: Android <=4.0 only[m
[32m+[m	[32mtrim: function( text ) {[m
[32m+[m		[32mreturn text == null ?[m
[32m+[m			[32m"" :[m
[32m+[m			[32m( text + "" ).replace( rtrim, "" );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// results is for internal usage only[m
[32m+[m	[32mmakeArray: function( arr, results ) {[m
[32m+[m		[32mvar ret = results || [];[m
[32m+[m
[32m+[m		[32mif ( arr != null ) {[m
[32m+[m			[32mif ( isArrayLike( Object( arr ) ) ) {[m
[32m+[m				[32mjQuery.merge( ret,[m
[32m+[m					[32mtypeof arr === "string" ?[m
[32m+[m					[32m[ arr ] : arr[m
[32m+[m				[32m);[m
[32m+[m			[32m} else {[m
[32m+[m				[32mpush.call( ret, arr );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn ret;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32minArray: function( elem, arr, i ) {[m
[32m+[m		[32mreturn arr == null ? -1 : indexOf.call( arr, elem, i );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Support: Android <=4.0 only, PhantomJS 1 only[m
[32m+[m	[32m// push.apply(_, arraylike) throws on ancient WebKit[m
[32m+[m	[32mmerge: function( first, second ) {[m
[32m+[m		[32mvar len = +second.length,[m
[32m+[m			[32mj = 0,[m
[32m+[m			[32mi = first.length;[m
[32m+[m
[32m+[m		[32mfor ( ; j < len; j++ ) {[m
[32m+[m			[32mfirst[ i++ ] = second[ j ];[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mfirst.length = i;[m
[32m+[m
[32m+[m		[32mreturn first;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mgrep: function( elems, callback, invert ) {[m
[32m+[m		[32mvar callbackInverse,[m
[32m+[m			[32mmatches = [],[m
[32m+[m			[32mi = 0,[m
[32m+[m			[32mlength = elems.length,[m
[32m+[m			[32mcallbackExpect = !invert;[m
[32m+[m
[32m+[m		[32m// Go through the array, only saving the items[m
[32m+[m		[32m// that pass the validator function[m
[32m+[m		[32mfor ( ; i < length; i++ ) {[m
[32m+[m			[32mcallbackInverse = !callback( elems[ i ], i );[m
[32m+[m			[32mif ( callbackInverse !== callbackExpect ) {[m
[32m+[m				[32mmatches.push( elems[ i ] );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn matches;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// arg is for internal usage only[m
[32m+[m	[32mmap: function( elems, callback, arg ) {[m
[32m+[m		[32mvar length, value,[m
[32m+[m			[32mi = 0,[m
[32m+[m			[32mret = [];[m
[32m+[m
[32m+[m		[32m// Go through the array, translating each of the items to their new values[m
[32m+[m		[32mif ( isArrayLike( elems ) ) {[m
[32m+[m			[32mlength = elems.length;[m
[32m+[m			[32mfor ( ; i < length; i++ ) {[m
[32m+[m				[32mvalue = callback( elems[ i ], i, arg );[m
[32m+[m
[32m+[m				[32mif ( value != null ) {[m
[32m+[m					[32mret.push( value );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m// Go through every key on the object,[m
[32m+[m		[32m} else {[m
[32m+[m			[32mfor ( i in elems ) {[m
[32m+[m				[32mvalue = callback( elems[ i ], i, arg );[m
[32m+[m
[32m+[m				[32mif ( value != null ) {[m
[32m+[m					[32mret.push( value );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Flatten any nested arrays[m
[32m+[m		[32mreturn concat.apply( [], ret );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// A global GUID counter for objects[m
[32m+[m	[32mguid: 1,[m
[32m+[m
[32m+[m	[32m// Bind a function to a context, optionally partially applying any[m
[32m+[m	[32m// arguments.[m
[32m+[m	[32mproxy: function( fn, context ) {[m
[32m+[m		[32mvar tmp, args, proxy;[m
[32m+[m
[32m+[m		[32mif ( typeof context === "string" ) {[m
[32m+[m			[32mtmp = fn[ context ];[m
[32m+[m			[32mcontext = fn;[m
[32m+[m			[32mfn = tmp;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Quick check to determine if target is callable, in the spec[m
[32m+[m		[32m// this throws a TypeError, but we will just return undefined.[m
[32m+[m		[32mif ( !jQuery.isFunction( fn ) ) {[m
[32m+[m			[32mreturn undefined;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Simulated bind[m
[32m+[m		[32margs = slice.call( arguments, 2 );[m
[32m+[m		[32mproxy = function() {[m
[32m+[m			[32mreturn fn.apply( context || this, args.concat( slice.call( arguments ) ) );[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m		[32m// Set the guid of unique handler to the same of original handler, so it can be removed[m
[32m+[m		[32mproxy.guid = fn.guid = fn.guid || jQuery.guid++;[m
[32m+[m
[32m+[m		[32mreturn proxy;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mnow: Date.now,[m
[32m+[m
[32m+[m	[32m// jQuery.support is not used in Core but other projects attach their[m
[32m+[m	[32m// properties to it so it needs to exist.[m
[32m+[m	[32msupport: support[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mif ( typeof Symbol === "function" ) {[m
[32m+[m	[32mjQuery.fn[ Symbol.iterator ] = arr[ Symbol.iterator ];[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Populate the class2type map[m
[32m+[m[32mjQuery.each( "Boolean Number String Function Array Date RegExp Object Error Symbol".split( " " ),[m
[32m+[m[32mfunction( i, name ) {[m
[32m+[m	[32mclass2type[ "[object " + name + "]" ] = name.toLowerCase();[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mfunction isArrayLike( obj ) {[m
[32m+[m
[32m+[m	[32m// Support: real iOS 8.2 only (not reproducible in simulator)[m
[32m+[m	[32m// `in` check used to prevent JIT error (gh-2145)[m
[32m+[m	[32m// hasOwn isn't used here due to false negatives[m
[32m+[m	[32m// regarding Nodelist length in IE[m
[32m+[m	[32mvar length = !!obj && "length" in obj && obj.length,[m
[32m+[m		[32mtype = jQuery.type( obj );[m
[32m+[m
[32m+[m	[32mif ( type === "function" || jQuery.isWindow( obj ) ) {[m
[32m+[m		[32mreturn false;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn type === "array" || length === 0 ||[m
[32m+[m		[32mtypeof length === "number" && length > 0 && ( length - 1 ) in obj;[m
[32m+[m[32m}[m
[32m+[m[32mvar Sizzle =[m
[32m+[m[32m/*![m
[32m+[m[32m * Sizzle CSS Selector Engine v2.3.0[m
[32m+[m[32m * https://sizzlejs.com/[m
[32m+[m[32m *[m
[32m+[m[32m * Copyright jQuery Foundation and other contributors[m
[32m+[m[32m * Released under the MIT license[m
[32m+[m[32m * http://jquery.org/license[m
[32m+[m[32m *[m
[32m+[m[32m * Date: 2016-01-04[m
[32m+[m[32m */[m
[32m+[m[32m(function( window ) {[m
[32m+[m
[32m+[m[32mvar i,[m
[32m+[m	[32msupport,[m
[32m+[m	[32mExpr,[m
[32m+[m	[32mgetText,[m
[32m+[m	[32misXML,[m
[32m+[m	[32mtokenize,[m
[32m+[m	[32mcompile,[m
[32m+[m	[32mselect,[m
[32m+[m	[32moutermostContext,[m
[32m+[m	[32msortInput,[m
[32m+[m	[32mhasDuplicate,[m
[32m+[m
[32m+[m	[32m// Local document vars[m
[32m+[m	[32msetDocument,[m
[32m+[m	[32mdocument,[m
[32m+[m	[32mdocElem,[m
[32m+[m	[32mdocumentIsHTML,[m
[32m+[m	[32mrbuggyQSA,[m
[32m+[m	[32mrbuggyMatches,[m
[32m+[m	[32mmatches,[m
[32m+[m	[32mcontains,[m
[32m+[m
[32m+[m	[32m// Instance-specific data[m
[32m+[m	[32mexpando = "sizzle" + 1 * new Date(),[m
[32m+[m	[32mpreferredDoc = window.document,[m
[32m+[m	[32mdirruns = 0,[m
[32m+[m	[32mdone = 0,[m
[32m+[m	[32mclassCache = createCache(),[m
[32m+[m	[32mtokenCache = createCache(),[m
[32m+[m	[32mcompilerCache = createCache(),[m
[32m+[m	[32msortOrder = function( a, b ) {[m
[32m+[m		[32mif ( a === b ) {[m
[32m+[m			[32mhasDuplicate = true;[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn 0;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Instance methods[m
[32m+[m	[32mhasOwn = ({}).hasOwnProperty,[m
[32m+[m	[32marr = [],[m
[32m+[m	[32mpop = arr.pop,[m
[32m+[m	[32mpush_native = arr.push,[m
[32m+[m	[32mpush = arr.push,[m
[32m+[m	[32mslice = arr.slice,[m
[32m+[m	[32m// Use a stripped-down indexOf as it's faster than native[m
[32m+[m	[32m// https://jsperf.com/thor-indexof-vs-for/5[m
[32m+[m	[32mindexOf = function( list, elem ) {[m
[32m+[m		[32mvar i = 0,[m
[32m+[m			[32mlen = list.length;[m
[32m+[m		[32mfor ( ; i < len; i++ ) {[m
[32m+[m			[32mif ( list[i] === elem ) {[m
[32m+[m				[32mreturn i;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn -1;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mbooleans = "checked|selected|async|autofocus|autoplay|controls|defer|disabled|hidden|ismap|loop|multiple|open|readonly|required|scoped",[m
[32m+[m
[32m+[m	[32m// Regular expressions[m
[32m+[m
[32m+[m	[32m// http://www.w3.org/TR/css3-selectors/#whitespace[m
[32m+[m	[32mwhitespace = "[\\x20\\t\\r\\n\\f]",[m
[32m+[m
[32m+[m	[32m// http://www.w3.org/TR/CSS21/syndata.html#value-def-identifier[m
[32m+[m	[32midentifier = "(?:\\\\.|[\\w-]|[^\0-\\xa0])+",[m
[32m+[m
[32m+[m	[32m// Attribute selectors: http://www.w3.org/TR/selectors/#attribute-selectors[m
[32m+[m	[32mattributes = "\\[" + whitespace + "*(" + identifier + ")(?:" + whitespace +[m
[32m+[m		[32m// Operator (capture 2)[m
[32m+[m		[32m"*([*^$|!~]?=)" + whitespace +[m
[32m+[m		[32m// "Attribute values must be CSS identifiers [capture 5] or strings [capture 3 or capture 4]"[m
[32m+[m		[32m"*(?:'((?:\\\\.|[^\\\\'])*)'|\"((?:\\\\.|[^\\\\\"])*)\"|(" + identifier + "))|)" + whitespace +[m
[32m+[m		[32m"*\\]",[m
[32m+[m
[32m+[m	[32mpseudos = ":(" + identifier + ")(?:\\((" +[m
[32m+[m		[32m// To reduce the number of selectors needing tokenize in the preFilter, prefer arguments:[m
[32m+[m		[32m// 1. quoted (capture 3; capture 4 or capture 5)[m
[32m+[m		[32m"('((?:\\\\.|[^\\\\'])*)'|\"((?:\\\\.|[^\\\\\"])*)\")|" +[m
[32m+[m		[32m// 2. simple (capture 6)[m
[32m+[m		[32m"((?:\\\\.|[^\\\\()[\\]]|" + attributes + ")*)|" +[m
[32m+[m		[32m// 3. anything else (capture 2)[m
[32m+[m		[32m".*" +[m
[32m+[m		[32m")\\)|)",[m
[32m+[m
[32m+[m	[32m// Leading and non-escaped trailing whitespace, capturing some non-whitespace characters preceding the latter[m
[32m+[m	[32mrwhitespace = new RegExp( whitespace + "+", "g" ),[m
[32m+[m	[32mrtrim = new RegExp( "^" + whitespace + "+|((?:^|[^\\\\])(?:\\\\.)*)" + whitespace + "+$", "g" ),[m
[32m+[m
[32m+[m	[32mrcomma = new RegExp( "^" + whitespace + "*," + whitespace + "*" ),[m
[32m+[m	[32mrcombinators = new RegExp( "^" + whitespace + "*([>+~]|" + whitespace + ")" + whitespace + "*" ),[m
[32m+[m
[32m+[m	[32mrattributeQuotes = new RegExp( "=" + whitespace + "*([^\\]'\"]*?)" + whitespace + "*\\]", "g" ),[m
[32m+[m
[32m+[m	[32mrpseudo = new RegExp( pseudos ),[m
[32m+[m	[32mridentifier = new RegExp( "^" + identifier + "$" ),[m
[32m+[m
[32m+[m	[32mmatchExpr = {[m
[32m+[m		[32m"ID": new RegExp( "^#(" + identifier + ")" ),[m
[32m+[m		[32m"CLASS": new RegExp( "^\\.(" + identifier + ")" ),[m
[32m+[m		[32m"TAG": new RegExp( "^(" + identifier + "|[*])" ),[m
[32m+[m		[32m"ATTR": new RegExp( "^" + attributes ),[m
[32m+[m		[32m"PSEUDO": new RegExp( "^" + pseudos ),[m
[32m+[m		[32m"CHILD": new RegExp( "^:(only|first|last|nth|nth-last)-(child|of-type)(?:\\(" + whitespace +[m
[32m+[m			[32m"*(even|odd|(([+-]|)(\\d*)n|)" + whitespace + "*(?:([+-]|)" + whitespace +[m
[32m+[m			[32m"*(\\d+)|))" + whitespace + "*\\)|)", "i" ),[m
[32m+[m		[32m"bool": new RegExp( "^(?:" + booleans + ")$", "i" ),[m
[32m+[m		[32m// For use in libraries implementing .is()[m
[32m+[m		[32m// We use this for POS matching in `select`[m
[32m+[m		[32m"needsContext": new RegExp( "^" + whitespace + "*[>+~]|:(even|odd|eq|gt|lt|nth|first|last)(?:\\(" +[m
[32m+[m			[32mwhitespace + "*((?:-\\d)?\\d*)" + whitespace + "*\\)|)(?=[^-]|$)", "i" )[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mrinputs = /^(?:input|select|textarea|button)$/i,[m
[32m+[m	[32mrheader = /^h\d$/i,[m
[32m+[m
[32m+[m	[32mrnative = /^[^{]+\{\s*\[native \w/,[m
[32m+[m
[32m+[m	[32m// Easily-parseable/retrievable ID or TAG or CLASS selectors[m
[32m+[m	[32mrquickExpr = /^(?:#([\w-]+)|(\w+)|\.([\w-]+))$/,[m
[32m+[m
[32m+[m	[32mrsibling = /[+~]/,[m
[32m+[m
[32m+[m	[32m// CSS escapes[m
[32m+[m	[32m// http://www.w3.org/TR/CSS21/syndata.html#escaped-characters[m
[32m+[m	[32mrunescape = new RegExp( "\\\\([\\da-f]{1,6}" + whitespace + "?|(" + whitespace + ")|.)", "ig" ),[m
[32m+[m	[32mfunescape = function( _, escaped, escapedWhitespace ) {[m
[32m+[m		[32mvar high = "0x" + escaped - 0x10000;[m
[32m+[m		[32m// NaN means non-codepoint[m
[32m+[m		[32m// Support: Firefox<24[m
[32m+[m		[32m// Workaround erroneous numeric interpretation of +"0x"[m
[32m+[m		[32mreturn high !== high || escapedWhitespace ?[m
[32m+[m			[32mescaped :[m
[32m+[m			[32mhigh < 0 ?[m
[32m+[m				[32m// BMP codepoint[m
[32m+[m				[32mString.fromCharCode( high + 0x10000 ) :[m
[32m+[m				[32m// Supplemental Plane codepoint (surrogate pair)[m
[32m+[m				[32mString.fromCharCode( high >> 10 | 0xD800, high & 0x3FF | 0xDC00 );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// CSS string/identifier serialization[m
[32m+[m	[32m// https://drafts.csswg.org/cssom/#common-serializing-idioms[m
[32m+[m	[32mrcssescape = /([\0-\x1f\x7f]|^-?\d)|^-$|[^\x80-\uFFFF\w-]/g,[m
[32m+[m	[32mfcssescape = function( ch, asCodePoint ) {[m
[32m+[m		[32mif ( asCodePoint ) {[m
[32m+[m
[32m+[m			[32m// U+0000 NULL becomes U+FFFD REPLACEMENT CHARACTER[m
[32m+[m			[32mif ( ch === "\0" ) {[m
[32m+[m				[32mreturn "\uFFFD";[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Control characters and (dependent upon position) numbers get escaped as code points[m
[32m+[m			[32mreturn ch.slice( 0, -1 ) + "\\" + ch.charCodeAt( ch.length - 1 ).toString( 16 ) + " ";[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Other potentially-special ASCII characters get backslash-escaped[m
[32m+[m		[32mreturn "\\" + ch;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Used for iframes[m
[32m+[m	[32m// See setDocument()[m
[32m+[m	[32m// Removing the function wrapper causes a "Permission Denied"[m
[32m+[m	[32m// error in IE[m
[32m+[m	[32munloadHandler = function() {[m
[32m+[m		[32msetDocument();[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mdisabledAncestor = addCombinator([m
[32m+[m		[32mfunction( elem ) {[m
[32m+[m			[32mreturn elem.disabled === true;[m
[32m+[m		[32m},[m
[32m+[m		[32m{ dir: "parentNode", next: "legend" }[m
[32m+[m	[32m);[m
[32m+[m
[32m+[m[32m// Optimize for push.apply( _, NodeList )[m
[32m+[m[32mtry {[m
[32m+[m	[32mpush.apply([m
[32m+[m		[32m(arr = slice.call( preferredDoc.childNodes )),[m
[32m+[m		[32mpreferredDoc.childNodes[m
[32m+[m	[32m);[m
[32m+[m	[32m// Support: Android<4.0[m
[32m+[m	[32m// Detect silently failing push.apply[m
[32m+[m	[32marr[ preferredDoc.childNodes.length ].nodeType;[m
[32m+[m[32m} catch ( e ) {[m
[32m+[m	[32mpush = { apply: arr.length ?[m
[32m+[m
[32m+[m		[32m// Leverage slice if possible[m
[32m+[m		[32mfunction( target, els ) {[m
[32m+[m			[32mpush_native.apply( target, slice.call(els) );[m
[32m+[m		[32m} :[m
[32m+[m
[32m+[m		[32m// Support: IE<9[m
[32m+[m		[32m// Otherwise append directly[m
[32m+[m		[32mfunction( target, els ) {[m
[32m+[m			[32mvar j = target.length,[m
[32m+[m				[32mi = 0;[m
[32m+[m			[32m// Can't trust NodeList.length[m
[32m+[m			[32mwhile ( (target[j++] = els[i++]) ) {}[m
[32m+[m			[32mtarget.length = j - 1;[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction Sizzle( selector, context, results, seed ) {[m
[32m+[m	[32mvar m, i, elem, nid, match, groups, newSelector,[m
[32m+[m		[32mnewContext = context && context.ownerDocument,[m
[32m+[m
[32m+[m		[32m// nodeType defaults to 9, since context defaults to document[m
[32m+[m		[32mnodeType = context ? context.nodeType : 9;[m
[32m+[m
[32m+[m	[32mresults = results || [];[m
[32m+[m
[32m+[m	[32m// Return early from calls with invalid selector or context[m
[32m+[m	[32mif ( typeof selector !== "string" || !selector ||[m
[32m+[m		[32mnodeType !== 1 && nodeType !== 9 && nodeType !== 11 ) {[m
[32m+[m
[32m+[m		[32mreturn results;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Try to shortcut find operations (as opposed to filters) in HTML documents[m
[32m+[m	[32mif ( !seed ) {[m
[32m+[m
[32m+[m		[32mif ( ( context ? context.ownerDocument || context : preferredDoc ) !== document ) {[m
[32m+[m			[32msetDocument( context );[m
[32m+[m		[32m}[m
[32m+[m		[32mcontext = context || document;[m
[32m+[m
[32m+[m		[32mif ( documentIsHTML ) {[m
[32m+[m
[32m+[m			[32m// If the selector is sufficiently simple, try using a "get*By*" DOM method[m
[32m+[m			[32m// (excepting DocumentFragment context, where the methods don't exist)[m
[32m+[m			[32mif ( nodeType !== 11 && (match = rquickExpr.exec( selector )) ) {[m
[32m+[m
[32m+[m				[32m// ID selector[m
[32m+[m				[32mif ( (m = match[1]) ) {[m
[32m+[m
[32m+[m					[32m// Document context[m
[32m+[m					[32mif ( nodeType === 9 ) {[m
[32m+[m						[32mif ( (elem = context.getElementById( m )) ) {[m
[32m+[m
[32m+[m							[32m// Support: IE, Opera, Webkit[m
[32m+[m							[32m// TODO: identify versions[m
[32m+[m							[32m// getElementById can match elements by name instead of ID[m
[32m+[m							[32mif ( elem.id === m ) {[m
[32m+[m								[32mresults.push( elem );[m
[32m+[m								[32mreturn results;[m
[32m+[m							[32m}[m
[32m+[m						[32m} else {[m
[32m+[m							[32mreturn results;[m
[32m+[m						[32m}[m
[32m+[m
[32m+[m					[32m// Element context[m
[32m+[m					[32m} else {[m
[32m+[m
[32m+[m						[32m// Support: IE, Opera, Webkit[m
[32m+[m						[32m// TODO: identify versions[m
[32m+[m						[32m// getElementById can match elements by name instead of ID[m
[32m+[m						[32mif ( newContext && (elem = newContext.getElementById( m )) &&[m
[32m+[m							[32mcontains( context, elem ) &&[m
[32m+[m							[32melem.id === m ) {[m
[32m+[m
[32m+[m							[32mresults.push( elem );[m
[32m+[m							[32mreturn results;[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m				[32m// Type selector[m
[32m+[m				[32m} else if ( match[2] ) {[m
[32m+[m					[32mpush.apply( results, context.getElementsByTagName( selector ) );[m
[32m+[m					[32mreturn results;[m
[32m+[m
[32m+[m				[32m// Class selector[m
[32m+[m				[32m} else if ( (m = match[3]) && support.getElementsByClassName &&[m
[32m+[m					[32mcontext.getElementsByClassName ) {[m
[32m+[m
[32m+[m					[32mpush.apply( results, context.getElementsByClassName( m ) );[m
[32m+[m					[32mreturn results;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Take advantage of querySelectorAll[m
[32m+[m			[32mif ( support.qsa &&[m
[32m+[m				[32m!compilerCache[ selector + " " ] &&[m
[32m+[m				[32m(!rbuggyQSA || !rbuggyQSA.test( selector )) ) {[m
[32m+[m
[32m+[m				[32mif ( nodeType !== 1 ) {[m
[32m+[m					[32mnewContext = context;[m
[32m+[m					[32mnewSelector = selector;[m
[32m+[m
[32m+[m				[32m// qSA looks outside Element context, which is not what we want[m
[32m+[m				[32m// Thanks to Andrew Dupont for this workaround technique[m
[32m+[m				[32m// Support: IE <=8[m
[32m+[m				[32m// Exclude object elements[m
[32m+[m				[32m} else if ( context.nodeName.toLowerCase() !== "object" ) {[m
[32m+[m
[32m+[m					[32m// Capture the context ID, setting it first if necessary[m
[32m+[m					[32mif ( (nid = context.getAttribute( "id" )) ) {[m
[32m+[m						[32mnid = nid.replace( rcssescape, fcssescape );[m
[32m+[m					[32m} else {[m
[32m+[m						[32mcontext.setAttribute( "id", (nid = expando) );[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// Prefix every selector in the list[m
[32m+[m					[32mgroups = tokenize( selector );[m
[32m+[m					[32mi = groups.length;[m
[32m+[m					[32mwhile ( i-- ) {[m
[32m+[m						[32mgroups[i] = "#" + nid + " " + toSelector( groups[i] );[m
[32m+[m					[32m}[m
[32m+[m					[32mnewSelector = groups.join( "," );[m
[32m+[m
[32m+[m					[32m// Expand context for sibling selectors[m
[32m+[m					[32mnewContext = rsibling.test( selector ) && testContext( context.parentNode ) ||[m
[32m+[m						[32mcontext;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif ( newSelector ) {[m
[32m+[m					[32mtry {[m
[32m+[m						[32mpush.apply( results,[m
[32m+[m							[32mnewContext.querySelectorAll( newSelector )[m
[32m+[m						[32m);[m
[32m+[m						[32mreturn results;[m
[32m+[m					[32m} catch ( qsaError ) {[m
[32m+[m					[32m} finally {[m
[32m+[m						[32mif ( nid === expando ) {[m
[32m+[m							[32mcontext.removeAttribute( "id" );[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// All others[m
[32m+[m	[32mreturn select( selector.replace( rtrim, "$1" ), context, results, seed );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Create key-value caches of limited size[m
[32m+[m[32m * @returns {function(string, object)} Returns the Object data after storing it on itself with[m
[32m+[m[32m *	property name the (space-suffixed) string and (if the cache is larger than Expr.cacheLength)[m
[32m+[m[32m *	deleting the oldest entry[m
[32m+[m[32m */[m
[32m+[m[32mfunction createCache() {[m
[32m+[m	[32mvar keys = [];[m
[32m+[m
[32m+[m	[32mfunction cache( key, value ) {[m
[32m+[m		[32m// Use (key + " ") to avoid collision with native prototype properties (see Issue #157)[m
[32m+[m		[32mif ( keys.push( key + " " ) > Expr.cacheLength ) {[m
[32m+[m			[32m// Only keep the most recent entries[m
[32m+[m			[32mdelete cache[ keys.shift() ];[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn (cache[ key + " " ] = value);[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn cache;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Mark a function for special use by Sizzle[m
[32m+[m[32m * @param {Function} fn The function to mark[m
[32m+[m[32m */[m
[32m+[m[32mfunction markFunction( fn ) {[m
[32m+[m	[32mfn[ expando ] = true;[m
[32m+[m	[32mreturn fn;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Support testing using an element[m
[32m+[m[32m * @param {Function} fn Passed the created element and returns a boolean result[m
[32m+[m[32m */[m
[32m+[m[32mfunction assert( fn ) {[m
[32m+[m	[32mvar el = document.createElement("fieldset");[m
[32m+[m
[32m+[m	[32mtry {[m
[32m+[m		[32mreturn !!fn( el );[m
[32m+[m	[32m} catch (e) {[m
[32m+[m		[32mreturn false;[m
[32m+[m	[32m} finally {[m
[32m+[m		[32m// Remove from its parent by default[m
[32m+[m		[32mif ( el.parentNode ) {[m
[32m+[m			[32mel.parentNode.removeChild( el );[m
[32m+[m		[32m}[m
[32m+[m		[32m// release memory in IE[m
[32m+[m		[32mel = null;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Adds the same handler for all of the specified attrs[m
[32m+[m[32m * @param {String} attrs Pipe-separated list of attributes[m
[32m+[m[32m * @param {Function} handler The method that will be applied[m
[32m+[m[32m */[m
[32m+[m[32mfunction addHandle( attrs, handler ) {[m
[32m+[m	[32mvar arr = attrs.split("|"),[m
[32m+[m		[32mi = arr.length;[m
[32m+[m
[32m+[m	[32mwhile ( i-- ) {[m
[32m+[m		[32mExpr.attrHandle[ arr[i] ] = handler;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Checks document order of two siblings[m
[32m+[m[32m * @param {Element} a[m
[32m+[m[32m * @param {Element} b[m
[32m+[m[32m * @returns {Number} Returns less than 0 if a precedes b, greater than 0 if a follows b[m
[32m+[m[32m */[m
[32m+[m[32mfunction siblingCheck( a, b ) {[m
[32m+[m	[32mvar cur = b && a,[m
[32m+[m		[32mdiff = cur && a.nodeType === 1 && b.nodeType === 1 &&[m
[32m+[m			[32ma.sourceIndex - b.sourceIndex;[m
[32m+[m
[32m+[m	[32m// Use IE sourceIndex if available on both nodes[m
[32m+[m	[32mif ( diff ) {[m
[32m+[m		[32mreturn diff;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Check if b follows a[m
[32m+[m	[32mif ( cur ) {[m
[32m+[m		[32mwhile ( (cur = cur.nextSibling) ) {[m
[32m+[m			[32mif ( cur === b ) {[m
[32m+[m				[32mreturn -1;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn a ? 1 : -1;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Returns a function to use in pseudos for input types[m
[32m+[m[32m * @param {String} type[m
[32m+[m[32m */[m
[32m+[m[32mfunction createInputPseudo( type ) {[m
[32m+[m	[32mreturn function( elem ) {[m
[32m+[m		[32mvar name = elem.nodeName.toLowerCase();[m
[32m+[m		[32mreturn name === "input" && elem.type === type;[m
[32m+[m	[32m};[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Returns a function to use in pseudos for buttons[m
[32m+[m[32m * @param {String} type[m
[32m+[m[32m */[m
[32m+[m[32mfunction createButtonPseudo( type ) {[m
[32m+[m	[32mreturn function( elem ) {[m
[32m+[m		[32mvar name = elem.nodeName.toLowerCase();[m
[32m+[m		[32mreturn (name === "input" || name === "button") && elem.type === type;[m
[32m+[m	[32m};[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Returns a function to use in pseudos for :enabled/:disabled[m
[32m+[m[32m * @param {Boolean} disabled true for :disabled; false for :enabled[m
[32m+[m[32m */[m
[32m+[m[32mfunction createDisabledPseudo( disabled ) {[m
[32m+[m	[32m// Known :disabled false positives:[m
[32m+[m	[32m// IE: *[disabled]:not(button, input, select, textarea, optgroup, option, menuitem, fieldset)[m
[32m+[m	[32m// not IE: fieldset[disabled] > legend:nth-of-type(n+2) :can-disable[m
[32m+[m	[32mreturn function( elem ) {[m
[32m+[m
[32m+[m		[32m// Check form elements and option elements for explicit disabling[m
[32m+[m		[32mreturn "label" in elem && elem.disabled === disabled ||[m
[32m+[m			[32m"form" in elem && elem.disabled === disabled ||[m
[32m+[m
[32m+[m			[32m// Check non-disabled form elements for fieldset[disabled] ancestors[m
[32m+[m			[32m"form" in elem && elem.disabled === false && ([m
[32m+[m				[32m// Support: IE6-11+[m
[32m+[m				[32m// Ancestry is covered for us[m
[32m+[m				[32melem.isDisabled === disabled ||[m
[32m+[m
[32m+[m				[32m// Otherwise, assume any non-<option> under fieldset[disabled] is disabled[m
[32m+[m				[32m/* jshint -W018 */[m
[32m+[m				[32melem.isDisabled !== !disabled &&[m
[32m+[m					[32m("label" in elem || !disabledAncestor( elem )) !== disabled[m
[32m+[m			[32m);[m
[32m+[m	[32m};[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Returns a function to use in pseudos for positionals[m
[32m+[m[32m * @param {Function} fn[m
[32m+[m[32m */[m
[32m+[m[32mfunction createPositionalPseudo( fn ) {[m
[32m+[m	[32mreturn markFunction(function( argument ) {[m
[32m+[m		[32margument = +argument;[m
[32m+[m		[32mreturn markFunction(function( seed, matches ) {[m
[32m+[m			[32mvar j,[m
[32m+[m				[32mmatchIndexes = fn( [], seed.length, argument ),[m
[32m+[m				[32mi = matchIndexes.length;[m
[32m+[m
[32m+[m			[32m// Match elements found at the specified indexes[m
[32m+[m			[32mwhile ( i-- ) {[m
[32m+[m				[32mif ( seed[ (j = matchIndexes[i]) ] ) {[m
[32m+[m					[32mseed[j] = !(matches[j] = seed[j]);[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m});[m
[32m+[m	[32m});[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Checks a node for validity as a Sizzle context[m
[32m+[m[32m * @param {Element|Object=} context[m
[32m+[m[32m * @returns {Element|Object|Boolean} The input node if acceptable, otherwise a falsy value[m
[32m+[m[32m */[m
[32m+[m[32mfunction testContext( context ) {[m
[32m+[m	[32mreturn context && typeof context.getElementsByTagName !== "undefined" && context;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Expose support vars for convenience[m
[32m+[m[32msupport = Sizzle.support = {};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Detects XML nodes[m
[32m+[m[32m * @param {Element|Object} elem An element or a document[m
[32m+[m[32m * @returns {Boolean} True iff elem is a non-HTML XML node[m
[32m+[m[32m */[m
[32m+[m[32misXML = Sizzle.isXML = function( elem ) {[m
[32m+[m	[32m// documentElement is verified for cases where it doesn't yet exist[m
[32m+[m	[32m// (such as loading iframes in IE - #4833)[m
[32m+[m	[32mvar documentElement = elem && (elem.ownerDocument || elem).documentElement;[m
[32m+[m	[32mreturn documentElement ? documentElement.nodeName !== "HTML" : false;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Sets document-related variables once based on the current document[m
[32m+[m[32m * @param {Element|Object} [doc] An element or document object to use to set the document[m
[32m+[m[32m * @returns {Object} Returns the current document[m
[32m+[m[32m */[m
[32m+[m[32msetDocument = Sizzle.setDocument = function( node ) {[m
[32m+[m	[32mvar hasCompare, subWindow,[m
[32m+[m		[32mdoc = node ? node.ownerDocument || node : preferredDoc;[m
[32m+[m
[32m+[m	[32m// Return early if doc is invalid or already selected[m
[32m+[m	[32mif ( doc === document || doc.nodeType !== 9 || !doc.documentElement ) {[m
[32m+[m		[32mreturn document;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Update global variables[m
[32m+[m	[32mdocument = doc;[m
[32m+[m	[32mdocElem = document.documentElement;[m
[32m+[m	[32mdocumentIsHTML = !isXML( document );[m
[32m+[m
[32m+[m	[32m// Support: IE 9-11, Edge[m
[32m+[m	[32m// Accessing iframe documents after unload throws "permission denied" errors (jQuery #13936)[m
[32m+[m	[32mif ( preferredDoc !== document &&[m
[32m+[m		[32m(subWindow = document.defaultView) && subWindow.top !== subWindow ) {[m
[32m+[m
[32m+[m		[32m// Support: IE 11, Edge[m
[32m+[m		[32mif ( subWindow.addEventListener ) {[m
[32m+[m			[32msubWindow.addEventListener( "unload", unloadHandler, false );[m
[32m+[m
[32m+[m		[32m// Support: IE 9 - 10 only[m
[32m+[m		[32m} else if ( subWindow.attachEvent ) {[m
[32m+[m			[32msubWindow.attachEvent( "onunload", unloadHandler );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m/* Attributes[m
[32m+[m	[32m---------------------------------------------------------------------- */[m
[32m+[m
[32m+[m	[32m// Support: IE<8[m
[32m+[m	[32m// Verify that getAttribute really returns attributes and not properties[m
[32m+[m	[32m// (excepting IE8 booleans)[m
[32m+[m	[32msupport.attributes = assert(function( el ) {[m
[32m+[m		[32mel.className = "i";[m
[32m+[m		[32mreturn !el.getAttribute("className");[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32m/* getElement(s)By*[m
[32m+[m	[32m---------------------------------------------------------------------- */[m
[32m+[m
[32m+[m	[32m// Check if getElementsByTagName("*") returns only elements[m
[32m+[m	[32msupport.getElementsByTagName = assert(function( el ) {[m
[32m+[m		[32mel.appendChild( document.createComment("") );[m
[32m+[m		[32mreturn !el.getElementsByTagName("*").length;[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32m// Support: IE<9[m
[32m+[m	[32msupport.getElementsByClassName = rnative.test( document.getElementsByClassName );[m
[32m+[m
[32m+[m	[32m// Support: IE<10[m
[32m+[m	[32m// Check if getElementById returns elements by name[m
[32m+[m	[32m// The broken getElementById methods don't pick up programmatically-set names,[m
[32m+[m	[32m// so use a roundabout getElementsByName test[m
[32m+[m	[32msupport.getById = assert(function( el ) {[m
[32m+[m		[32mdocElem.appendChild( el ).id = expando;[m
[32m+[m		[32mreturn !document.getElementsByName || !document.getElementsByName( expando ).length;[m
[32m+[m	[32m});[m
[32m+[m
[32m+[m	[32m// ID find and filter[m
[32m+[m	[32mif ( support.getById ) {[m
[32m+[m		[32mExpr.find["ID"] = function( id, context ) {[m
[32m+[m			[32mif ( typeof context.getElementById !== "undefined" && documentIsHTML ) {[m
[32m+[m				[32mvar m = context.getElementById( id );[m
[32m+[m				[32mreturn m ? [ m ] : [];[m
[32m+[m			[32m}[m
[32m+[m		[32m};[m
[32m+[m		[32mExpr.filter["ID"] = function( id ) {[m
[32m+[m			[32mvar attrId = id.replace( runescape, funescape );[m
[32m+[m			[32mreturn function( elem ) {[m
[32m+[m				[32mreturn elem.getAttribute("id") === attrId;[m
[32m+[m			[32m};[m
[32m+[m		[32m};[m
[32m+[m	[32m} else {[m
[32m+[m		[32m// Support: IE6/7[m
[32m+[m		[32m// getElementById is not reliable as a find shortcut[m
[32m+[m		[32mdelete Expr.find["ID"];[m
[32m+[m
[32m+[m		[32mExpr.filter["ID"] =  function( id ) {[m
[32m+[m			[32mvar attrId = id.replace( runescape, funescape );[m
[32m+[m			[32mreturn function( elem ) {[m
[32m+[m				[32mvar node = typeof elem.getAttributeNode !== "undefined" &&[m
[32m+[m					[32melem.getAttributeNode("id");[m
[32m+[m				[32mreturn node && node.value === attrId;[m
[32m+[m			[32m};[m
[32m+[m		[32m};[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Tag[m
[32m+[m	[32mExpr.find["TAG"] = support.getElementsByTagName ?[m
[32m+[m		[32mfunction( tag, context ) {[m
[32m+[m			[32mif ( typeof context.getElementsByTagName !== "undefined" ) {[m
[32m+[m				[32mreturn context.getElementsByTagName( tag );[m
[32m+[m
[32m+[m			[32m// DocumentFragment nodes don't have gEBTN[m
[32m+[m			[32m} else if ( support.qsa ) {[m
[32m+[m				[32mreturn context.querySelectorAll( tag );[m
[32m+[m			[32m}[m
[32m+[m		[32m} :[m
[32m+[m
[32m+[m		[32mfunction( tag, context ) {[m
[32m+[m			[32mvar elem,[m
[32m+[m				[32mtmp = [],[m
[32m+[m				[32mi = 0,[m
[32m+[m				[32m// By happy coincidence, a (broken) gEBTN appears on DocumentFragment nodes too[m
[32m+[m				[32mresults = context.getElementsByTagName( tag );[m
[32m+[m
[32m+[m			[32m// Filter out possible comments[m
[32m+[m			[32mif ( tag === "*" ) {[m
[32m+[m				[32mwhile ( (elem = results[i++]) ) {[m
[32m+[m					[32mif ( elem.nodeType === 1 ) {[m
[32m+[m						[32mtmp.push( elem );[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mreturn tmp;[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn results;[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m	[32m// Class[m
[32m+[m	[32mExpr.find["CLASS"] = support.getElementsByClassName && function( className, context ) {[m
[32m+[m		[32mif ( typeof context.getElementsByClassName !== "undefined" && documentIsHTML ) {[m
[32m+[m			[32mreturn context.getElementsByClassName( className );[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m	[32m/* QSA/matchesSelector[m
[32m+[m	[32m---------------------------------------------------------------------- */[m
[32m+[m
[32m+[m	[32m// QSA and matchesSelector support[m
[32m+[m
[32m+[m	[32m// matchesSelector(:active) reports false when true (IE9/Opera 11.5)[m
[32m+[m	[32mrbuggyMatches = [];[m
[32m+[m
[32m+[m	[32m// qSa(:focus) reports false when true (Chrome 21)[m
[32m+[m	[32m// We allow this because of a bug in IE8/9 that throws an error[m
[32m+[m	[32m// whenever `document.activeElement` is accessed on an iframe[m
[32m+[m	[32m// So, we allow :focus to pass through QSA all the time to avoid the IE error[m
[32m+[m	[32m// See https://bugs.jquery.com/ticket/13378[m
[32m+[m	[32mrbuggyQSA = [];[m
[32m+[m
[32m+[m	[32mif ( (support.qsa = rnative.test( document.querySelectorAll )) ) {[m
[32m+[m		[32m// Build QSA regex[m
[32m+[m		[32m// Regex strategy adopted from Diego Perini[m
[32m+[m		[32massert(function( el ) {[m
[32m+[m			[32m// Select is set to empty string on purpose[m
[32m+[m			[32m// This is to test IE's treatment of not explicitly[m
[32m+[m			[32m// setting a boolean content attribute,[m
[32m+[m			[32m// since its presence should be enough[m
[32m+[m			[32m// https://bugs.jquery.com/ticket/12359[m
[32m+[m			[32mdocElem.appendChild( el ).innerHTML = "<a id='" + expando + "'></a>" +[m
[32m+[m				[32m"<select id='" + expando + "-\r\\' msallowcapture=''>" +[m
[32m+[m				[32m"<option selected=''></option></select>";[m
[32m+[m
[32m+[m			[32m// Support: IE8, Opera 11-12.16[m
[32m+[m			[32m// Nothing should be selected when empty strings follow ^= or $= or *=[m
[32m+[m			[32m// The test attribute must be unknown in Opera but "safe" for WinRT[m
[32m+[m			[32m// https://msdn.microsoft.com/en-us/library/ie/hh465388.aspx#attribute_section[m
[32m+[m			[32mif ( el.querySelectorAll("[msallowcapture^='']").length ) {[m
[32m+[m				[32mrbuggyQSA.push( "[*^$]=" + whitespace + "*(?:''|\"\")" );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Support: IE8[m
[32m+[m			[32m// Boolean attributes and "value" are not treated correctly[m
[32m+[m			[32mif ( !el.querySelectorAll("[selected]").length ) {[m
[32m+[m				[32mrbuggyQSA.push( "\\[" + whitespace + "*(?:value|" + booleans + ")" );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Support: Chrome<29, Android<4.4, Safari<7.0+, iOS<7.0+, PhantomJS<1.9.8+[m
[32m+[m			[32mif ( !el.querySelectorAll( "[id~=" + expando + "-]" ).length ) {[m
[32m+[m				[32mrbuggyQSA.push("~=");[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Webkit/Opera - :checked should return selected option elements[m
[32m+[m			[32m// http://www.w3.org/TR/2011/REC-css3-selectors-20110929/#checked[m
[32m+[m			[32m// IE8 throws error here and will not see later tests[m
[32m+[m			[32mif ( !el.querySelectorAll(":checked").length ) {[m
[32m+[m				[32mrbuggyQSA.push(":checked");[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Support: Safari 8+, iOS 8+[m
[32m+[m			[32m// https://bugs.webkit.org/show_bug.cgi?id=136851[m
[32m+[m			[32m// In-page `selector#id sibling-combinator selector` fails[m
[32m+[m			[32mif ( !el.querySelectorAll( "a#" + expando + "+*" ).length ) {[m
[32m+[m				[32mrbuggyQSA.push(".#.+[+~]");[m
[32m+[m			[32m}[m
[32m+[m		[32m});[m
[32m+[m
[32m+[m		[32massert(function( el ) {[m
[32m+[m			[32mel.innerHTML = "<a href='' disabled='disabled'></a>" +[m
[32m+[m				[32m"<select disabled='disabled'><option/></select>";[m
[32m+[m
[32m+[m			[32m// Support: Windows 8 Native Apps[m
[32m+[m			[32m// The type and name attributes are restricted during .innerHTML assignment[m
[32m+[m			[32mvar input = document.createElement("input");[m
[32m+[m			[32minput.setAttribute( "type", "hidden" );[m
[32m+[m			[32mel.appendChild( input ).setAttribute( "name", "D" );[m
[32m+[m
[32m+[m			[32m// Support: IE8[m
[32m+[m			[32m// Enforce case-sensitivity of name attribute[m
[32m+[m			[32mif ( el.querySelectorAll("[name=d]").length ) {[m
[32m+[m				[32mrbuggyQSA.push( "name" + whitespace + "*[*^$|!~]?=" );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// FF 3.5 - :enabled/:disabled and hidden elements (hidden elements are still enabled)[m
[32m+[m			[32m// IE8 throws error here and will not see later tests[m
[32m+[m			[32mif ( el.querySelectorAll(":enabled").length !== 2 ) {[m
[32m+[m				[32mrbuggyQSA.push( ":enabled", ":disabled" );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Support: IE9-11+[m
[32m+[m			[32m// IE's :disabled selector does not pick up the children of disabled fieldsets[m
[32m+[m			[32mdocElem.appendChild( el ).disabled = true;[m
[32m+[m			[32mif ( el.querySelectorAll(":disabled").length !== 2 ) {[m
[32m+[m				[32mrbuggyQSA.push( ":enabled", ":disabled" );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Opera 10-11 does not throw on post-comma invalid pseudos[m
[32m+[m			[32mel.querySelectorAll("*,:x");[m
[32m+[m			[32mrbuggyQSA.push(",.*:");[m
[32m+[m		[32m});[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( (support.matchesSelector = rnative.test( (matches = docElem.matches ||[m
[32m+[m		[32mdocElem.webkitMatchesSelector ||[m
[32m+[m		[32mdocElem.mozMatchesSelector ||[m
[32m+[m		[32mdocElem.oMatchesSelector ||[m
[32m+[m		[32mdocElem.msMatchesSelector) )) ) {[m
[32m+[m
[32m+[m		[32massert(function( el ) {[m
[32m+[m			[32m// Check to see if it's possible to do matchesSelector[m
[32m+[m			[32m// on a disconnected node (IE 9)[m
[32m+[m			[32msupport.disconnectedMatch = matches.call( el, "*" );[m
[32m+[m
[32m+[m			[32m// This should fail with an exception[m
[32m+[m			[32m// Gecko does not error, returns false instead[m
[32m+[m			[32mmatches.call( el, "[s!='']:x" );[m
[32m+[m			[32mrbuggyMatches.push( "!=", pseudos );[m
[32m+[m		[32m});[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mrbuggyQSA = rbuggyQSA.length && new RegExp( rbuggyQSA.join("|") );[m
[32m+[m	[32mrbuggyMatches = rbuggyMatches.length && new RegExp( rbuggyMatches.join("|") );[m
[32m+[m
[32m+[m	[32m/* Contains[m
[32m+[m	[32m---------------------------------------------------------------------- */[m
[32m+[m	[32mhasCompare = rnative.test( docElem.compareDocumentPosition );[m
[32m+[m
[32m+[m	[32m// Element contains another[m
[32m+[m	[32m// Purposefully self-exclusive[m
[32m+[m	[32m// As in, an element does not contain itself[m
[32m+[m	[32mcontains = hasCompare || rnative.test( docElem.contains ) ?[m
[32m+[m		[32mfunction( a, b ) {[m
[32m+[m			[32mvar adown = a.nodeType === 9 ? a.documentElement : a,[m
[32m+[m				[32mbup = b && b.parentNode;[m
[32m+[m			[32mreturn a === bup || !!( bup && bup.nodeType === 1 && ([m
[32m+[m				[32madown.contains ?[m
[32m+[m					[32madown.contains( bup ) :[m
[32m+[m					[32ma.compareDocumentPosition && a.compareDocumentPosition( bup ) & 16[m
[32m+[m			[32m));[m
[32m+[m		[32m} :[m
[32m+[m		[32mfunction( a, b ) {[m
[32m+[m			[32mif ( b ) {[m
[32m+[m				[32mwhile ( (b = b.parentNode) ) {[m
[32m+[m					[32mif ( b === a ) {[m
[32m+[m						[32mreturn true;[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn false;[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m	[32m/* Sorting[m
[32m+[m	[32m---------------------------------------------------------------------- */[m
[32m+[m
[32m+[m	[32m// Document order sorting[m
[32m+[m	[32msortOrder = hasCompare ?[m
[32m+[m	[32mfunction( a, b ) {[m
[32m+[m
[32m+[m		[32m// Flag for duplicate removal[m
[32m+[m		[32mif ( a === b ) {[m
[32m+[m			[32mhasDuplicate = true;[m
[32m+[m			[32mreturn 0;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Sort on method existence if only one input has compareDocumentPosition[m
[32m+[m		[32mvar compare = !a.compareDocumentPosition - !b.compareDocumentPosition;[m
[32m+[m		[32mif ( compare ) {[m
[32m+[m			[32mreturn compare;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Calculate position if both inputs belong to the same document[m
[32m+[m		[32mcompare = ( a.ownerDocument || a ) === ( b.ownerDocument || b ) ?[m
[32m+[m			[32ma.compareDocumentPosition( b ) :[m
[32m+[m
[32m+[m			[32m// Otherwise we know they are disconnected[m
[32m+[m			[32m1;[m
[32m+[m
[32m+[m		[32m// Disconnected nodes[m
[32m+[m		[32mif ( compare & 1 ||[m
[32m+[m			[32m(!support.sortDetached && b.compareDocumentPosition( a ) === compare) ) {[m
[32m+[m
[32m+[m			[32m// Choose the first element that is related to our preferred document[m
[32m+[m			[32mif ( a === document || a.ownerDocument === preferredDoc && contains(preferredDoc, a) ) {[m
[32m+[m				[32mreturn -1;[m
[32m+[m			[32m}[m
[32m+[m			[32mif ( b === document || b.ownerDocument === preferredDoc && contains(preferredDoc, b) ) {[m
[32m+[m				[32mreturn 1;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Maintain original order[m
[32m+[m			[32mreturn sortInput ?[m
[32m+[m				[32m( indexOf( sortInput, a ) - indexOf( sortInput, b ) ) :[m
[32m+[m				[32m0;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn compare & 4 ? -1 : 1;[m
[32m+[m	[32m} :[m
[32m+[m	[32mfunction( a, b ) {[m
[32m+[m		[32m// Exit early if the nodes are identical[m
[32m+[m		[32mif ( a === b ) {[m
[32m+[m			[32mhasDuplicate = true;[m
[32m+[m			[32mreturn 0;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mvar cur,[m
[32m+[m			[32mi = 0,[m
[32m+[m			[32maup = a.parentNode,[m
[32m+[m			[32mbup = b.parentNode,[m
[32m+[m			[32map = [ a ],[m
[32m+[m			[32mbp = [ b ];[m
[32m+[m
[32m+[m		[32m// Parentless nodes are either documents or disconnected[m
[32m+[m		[32mif ( !aup || !bup ) {[m
[32m+[m			[32mreturn a === document ? -1 :[m
[32m+[m				[32mb === document ? 1 :[m
[32m+[m				[32maup ? -1 :[m
[32m+[m				[32mbup ? 1 :[m
[32m+[m				[32msortInput ?[m
[32m+[m				[32m( indexOf( sortInput, a ) - indexOf( sortInput, b ) ) :[m
[32m+[m				[32m0;[m
[32m+[m
[32m+[m		[32m// If the nodes are siblings, we can do a quick check[m
[32m+[m		[32m} else if ( aup === bup ) {[m
[32m+[m			[32mreturn siblingCheck( a, b );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Otherwise we need full lists of their ancestors for comparison[m
[32m+[m		[32mcur = a;[m
[32m+[m		[32mwhile ( (cur = cur.parentNode) ) {[m
[32m+[m			[32map.unshift( cur );[m
[32m+[m		[32m}[m
[32m+[m		[32mcur = b;[m
[32m+[m		[32mwhile ( (cur = cur.parentNode) ) {[m
[32m+[m			[32mbp.unshift( cur );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Walk down the tree looking for a discrepancy[m
[32m+[m		[32mwhile ( ap[i] === bp[i] ) {[m
[32m+[m			[32mi++;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn i ?[m
[32m+[m			[32m// Do a sibling check if the nodes have a common ancestor[m
[32m+[m			[32msiblingCheck( ap[i], bp[i] ) :[m
[32m+[m
[32m+[m			[32m// Otherwise nodes in our document sort first[m
[32m+[m			[32map[i] === preferredDoc ? -1 :[m
[32m+[m			[32mbp[i] === preferredDoc ? 1 :[m
[32m+[m			[32m0;[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m	[32mreturn document;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mSizzle.matches = function( expr, elements ) {[m
[32m+[m	[32mreturn Sizzle( expr, null, null, elements );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mSizzle.matchesSelector = function( elem, expr ) {[m
[32m+[m	[32m// Set document vars if needed[m
[32m+[m	[32mif ( ( elem.ownerDocument || elem ) !== document ) {[m
[32m+[m		[32msetDocument( elem );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Make sure that attribute selectors are quoted[m
[32m+[m	[32mexpr = expr.replace( rattributeQuotes, "='$1']" );[m
[32m+[m
[32m+[m	[32mif ( support.matchesSelector && documentIsHTML &&[m
[32m+[m		[32m!compilerCache[ expr + " " ] &&[m
[32m+[m		[32m( !rbuggyMatches || !rbuggyMatches.test( expr ) ) &&[m
[32m+[m		[32m( !rbuggyQSA     || !rbuggyQSA.test( expr ) ) ) {[m
[32m+[m
[32m+[m		[32mtry {[m
[32m+[m			[32mvar ret = matches.call( elem, expr );[m
[32m+[m
[32m+[m			[32m// IE 9's matchesSelector returns false on disconnected nodes[m
[32m+[m			[32mif ( ret || support.disconnectedMatch ||[m
[32m+[m					[32m// As well, disconnected nodes are said to be in a document[m
[32m+[m					[32m// fragment in IE 9[m
[32m+[m					[32melem.document && elem.document.nodeType !== 11 ) {[m
[32m+[m				[32mreturn ret;[m
[32m+[m			[32m}[m
[32m+[m		[32m} catch (e) {}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn Sizzle( expr, document, null, [ elem ] ).length > 0;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mSizzle.contains = function( context, elem ) {[m
[32m+[m	[32m// Set document vars if needed[m
[32m+[m	[32mif ( ( context.ownerDocument || context ) !== document ) {[m
[32m+[m		[32msetDocument( context );[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn contains( context, elem );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mSizzle.attr = function( elem, name ) {[m
[32m+[m	[32m// Set document vars if needed[m
[32m+[m	[32mif ( ( elem.ownerDocument || elem ) !== document ) {[m
[32m+[m		[32msetDocument( elem );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mvar fn = Expr.attrHandle[ name.toLowerCase() ],[m
[32m+[m		[32m// Don't get fooled by Object.prototype properties (jQuery #13807)[m
[32m+[m		[32mval = fn && hasOwn.call( Expr.attrHandle, name.toLowerCase() ) ?[m
[32m+[m			[32mfn( elem, name, !documentIsHTML ) :[m
[32m+[m			[32mundefined;[m
[32m+[m
[32m+[m	[32mreturn val !== undefined ?[m
[32m+[m		[32mval :[m
[32m+[m		[32msupport.attributes || !documentIsHTML ?[m
[32m+[m			[32melem.getAttribute( name ) :[m
[32m+[m			[32m(val = elem.getAttributeNode(name)) && val.specified ?[m
[32m+[m				[32mval.value :[m
[32m+[m				[32mnull;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mSizzle.escape = function( sel ) {[m
[32m+[m	[32mreturn (sel + "").replace( rcssescape, fcssescape );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mSizzle.error = function( msg ) {[m
[32m+[m	[32mthrow new Error( "Syntax error, unrecognized expression: " + msg );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Document sorting and removing duplicates[m
[32m+[m[32m * @param {ArrayLike} results[m
[32m+[m[32m */[m
[32m+[m[32mSizzle.uniqueSort = function( results ) {[m
[32m+[m	[32mvar elem,[m
[32m+[m		[32mduplicates = [],[m
[32m+[m		[32mj = 0,[m
[32m+[m		[32mi = 0;[m
[32m+[m
[32m+[m	[32m// Unless we *know* we can detect duplicates, assume their presence[m
[32m+[m	[32mhasDuplicate = !support.detectDuplicates;[m
[32m+[m	[32msortInput = !support.sortStable && results.slice( 0 );[m
[32m+[m	[32mresults.sort( sortOrder );[m
[32m+[m
[32m+[m	[32mif ( hasDuplicate ) {[m
[32m+[m		[32mwhile ( (elem = results[i++]) ) {[m
[32m+[m			[32mif ( elem === results[ i ] ) {[m
[32m+[m				[32mj = duplicates.push( i );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32mwhile ( j-- ) {[m
[32m+[m			[32mresults.splice( duplicates[ j ], 1 );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Clear input after sorting to release objects[m
[32m+[m	[32m// See https://github.com/jquery/sizzle/pull/225[m
[32m+[m	[32msortInput = null;[m
[32m+[m
[32m+[m	[32mreturn results;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Utility function for retrieving the text value of an array of DOM nodes[m
[32m+[m[32m * @param {Array|Element} elem[m
[32m+[m[32m */[m
[32m+[m[32mgetText = Sizzle.getText = function( elem ) {[m
[32m+[m	[32mvar node,[m
[32m+[m		[32mret = "",[m
[32m+[m		[32mi = 0,[m
[32m+[m		[32mnodeType = elem.nodeType;[m
[32m+[m
[32m+[m	[32mif ( !nodeType ) {[m
[32m+[m		[32m// If no nodeType, this is expected to be an array[m
[32m+[m		[32mwhile ( (node = elem[i++]) ) {[m
[32m+[m			[32m// Do not traverse comment nodes[m
[32m+[m			[32mret += getText( node );[m
[32m+[m		[32m}[m
[32m+[m	[32m} else if ( nodeType === 1 || nodeType === 9 || nodeType === 11 ) {[m
[32m+[m		[32m// Use textContent for elements[m
[32m+[m		[32m// innerText usage removed for consistency of new lines (jQuery #11153)[m
[32m+[m		[32mif ( typeof elem.textContent === "string" ) {[m
[32m+[m			[32mreturn elem.textContent;[m
[32m+[m		[32m} else {[m
[32m+[m			[32m// Traverse its children[m
[32m+[m			[32mfor ( elem = elem.firstChild; elem; elem = elem.nextSibling ) {[m
[32m+[m				[32mret += getText( elem );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m} else if ( nodeType === 3 || nodeType === 4 ) {[m
[32m+[m		[32mreturn elem.nodeValue;[m
[32m+[m	[32m}[m
[32m+[m	[32m// Do not include comment or processing instruction nodes[m
[32m+[m
[32m+[m	[32mreturn ret;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mExpr = Sizzle.selectors = {[m
[32m+[m
[32m+[m	[32m// Can be adjusted by the user[m
[32m+[m	[32mcacheLength: 50,[m
[32m+[m
[32m+[m	[32mcreatePseudo: markFunction,[m
[32m+[m
[32m+[m	[32mmatch: matchExpr,[m
[32m+[m
[32m+[m	[32mattrHandle: {},[m
[32m+[m
[32m+[m	[32mfind: {},[m
[32m+[m
[32m+[m	[32mrelative: {[m
[32m+[m		[32m">": { dir: "parentNode", first: true },[m
[32m+[m		[32m" ": { dir: "parentNode" },[m
[32m+[m		[32m"+": { dir: "previousSibling", first: true },[m
[32m+[m		[32m"~": { dir: "previousSibling" }[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mpreFilter: {[m
[32m+[m		[32m"ATTR": function( match ) {[m
[32m+[m			[32mmatch[1] = match[1].replace( runescape, funescape );[m
[32m+[m
[32m+[m			[32m// Move the given value to match[3] whether quoted or unquoted[m
[32m+[m			[32mmatch[3] = ( match[3] || match[4] || match[5] || "" ).replace( runescape, funescape );[m
[32m+[m
[32m+[m			[32mif ( match[2] === "~=" ) {[m
[32m+[m				[32mmatch[3] = " " + match[3] + " ";[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn match.slice( 0, 4 );[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m"CHILD": function( match ) {[m
[32m+[m			[32m/* matches from matchExpr["CHILD"][m
[32m+[m				[32m1 type (only|nth|...)[m
[32m+[m				[32m2 what (child|of-type)[m
[32m+[m				[32m3 argument (even|odd|\d*|\d*n([+-]\d+)?|...)[m
[32m+[m				[32m4 xn-component of xn+y argument ([+-]?\d*n|)[m
[32m+[m				[32m5 sign of xn-component[m
[32m+[m				[32m6 x of xn-component[m
[32m+[m				[32m7 sign of y-component[m
[32m+[m				[32m8 y of y-component[m
[32m+[m			[32m*/[m
[32m+[m			[32mmatch[1] = match[1].toLowerCase();[m
[32m+[m
[32m+[m			[32mif ( match[1].slice( 0, 3 ) === "nth" ) {[m
[32m+[m				[32m// nth-* requires argument[m
[32m+[m				[32mif ( !match[3] ) {[m
[32m+[m					[32mSizzle.error( match[0] );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// numeric x and y parameters for Expr.filter.CHILD[m
[32m+[m				[32m// remember that false/true cast respectively to 0/1[m
[32m+[m				[32mmatch[4] = +( match[4] ? match[5] + (match[6] || 1) : 2 * ( match[3] === "even" || match[3] === "odd" ) );[m
[32m+[m				[32mmatch[5] = +( ( match[7] + match[8] ) || match[3] === "odd" );[m
[32m+[m
[32m+[m			[32m// other types prohibit arguments[m
[32m+[m			[32m} else if ( match[3] ) {[m
[32m+[m				[32mSizzle.error( match[0] );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn match;[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m"PSEUDO": function( match ) {[m
[32m+[m			[32mvar excess,[m
[32m+[m				[32munquoted = !match[6] && match[2];[m
[32m+[m
[32m+[m			[32mif ( matchExpr["CHILD"].test( match[0] ) ) {[m
[32m+[m				[32mreturn null;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Accept quoted arguments as-is[m
[32m+[m			[32mif ( match[3] ) {[m
[32m+[m				[32mmatch[2] = match[4] || match[5] || "";[m
[32m+[m
[32m+[m			[32m// Strip excess characters from unquoted arguments[m
[32m+[m			[32m} else if ( unquoted && rpseudo.test( unquoted ) &&[m
[32m+[m				[32m// Get excess from tokenize (recursively)[m
[32m+[m				[32m(excess = tokenize( unquoted, true )) &&[m
[32m+[m				[32m// advance to the next closing parenthesis[m
[32m+[m				[32m(excess = unquoted.indexOf( ")", unquoted.length - excess ) - unquoted.length) ) {[m
[32m+[m
[32m+[m				[32m// excess is a negative index[m
[32m+[m				[32mmatch[0] = match[0].slice( 0, excess );[m
[32m+[m				[32mmatch[2] = unquoted.slice( 0, excess );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Return only captures needed by the pseudo filter method (type and argument)[m
[32m+[m			[32mreturn match.slice( 0, 3 );[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mfilter: {[m
[32m+[m
[32m+[m		[32m"TAG": function( nodeNameSelector ) {[m
[32m+[m			[32mvar nodeName = nodeNameSelector.replace( runescape, funescape ).toLowerCase();[m
[32m+[m			[32mreturn nodeNameSelector === "*" ?[m
[32m+[m				[32mfunction() { return true; } :[m
[32m+[m				[32mfunction( elem ) {[m
[32m+[m					[32mreturn elem.nodeName && elem.nodeName.toLowerCase() === nodeName;[m
[32m+[m				[32m};[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m"CLASS": function( className ) {[m
[32m+[m			[32mvar pattern = classCache[ className + " " ];[m
[32m+[m
[32m+[m			[32mreturn pattern ||[m
[32m+[m				[32m(pattern = new RegExp( "(^|" + whitespace + ")" + className + "(" + whitespace + "|$)" )) &&[m
[32m+[m				[32mclassCache( className, function( elem ) {[m
[32m+[m					[32mreturn pattern.test( typeof elem.className === "string" && elem.className || typeof elem.getAttribute !== "undefined" && elem.getAttribute("class") || "" );[m
[32m+[m				[32m});[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m"ATTR": function( name, operator, check ) {[m
[32m+[m			[32mreturn function( elem ) {[m
[32m+[m				[32mvar result = Sizzle.attr( elem, name );[m
[32m+[m
[32m+[m				[32mif ( result == null ) {[m
[32m+[m					[32mreturn operator === "!=";[m
[32m+[m				[32m}[m
[32m+[m				[32mif ( !operator ) {[m
[32m+[m					[32mreturn true;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mresult += "";[m
[32m+[m
[32m+[m				[32mreturn operator === "=" ? result === check :[m
[32m+[m					[32moperator === "!=" ? result !== check :[m
[32m+[m					[32moperator === "^=" ? check && result.indexOf( check ) === 0 :[m
[32m+[m					[32moperator === "*=" ? check && result.indexOf( check ) > -1 :[m
[32m+[m					[32moperator === "$=" ? check && result.slice( -check.length ) === check :[m
[32m+[m					[32moperator === "~=" ? ( " " + result.replace( rwhitespace, " " ) + " " ).indexOf( check ) > -1 :[m
[32m+[m					[32moperator === "|=" ? result === check || result.slice( 0, check.length + 1 ) === check + "-" :[m
[32m+[m					[32mfalse;[m
[32m+[m			[32m};[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m"CHILD": function( type, what, argument, first, last ) {[m
[32m+[m			[32mvar simple = type.slice( 0, 3 ) !== "nth",[m
[32m+[m				[32mforward = type.slice( -4 ) !== "last",[m
[32m+[m				[32mofType = what === "of-type";[m
[32m+[m
[32m+[m			[32mreturn first === 1 && last === 0 ?[m
[32m+[m
[32m+[m				[32m// Shortcut for :nth-*(n)[m
[32m+[m				[32mfunction( elem ) {[m
[32m+[m					[32mreturn !!elem.parentNode;[m
[32m+[m				[32m} :[m
[32m+[m
[32m+[m				[32mfunction( elem, context, xml ) {[m
[32m+[m					[32mvar cache, uniqueCache, outerCache, node, nodeIndex, start,[m
[32m+[m						[32mdir = simple !== forward ? "nextSibling" : "previousSibling",[m
[32m+[m						[32mparent = elem.parentNode,[m
[32m+[m						[32mname = ofType && elem.nodeName.toLowerCase(),[m
[32m+[m						[32museCache = !xml && !ofType,[m
[32m+[m						[32mdiff = false;[m
[32m+[m
[32m+[m					[32mif ( parent ) {[m
[32m+[m
[32m+[m						[32m// :(first|last|only)-(child|of-type)[m
[32m+[m						[32mif ( simple ) {[m
[32m+[m							[32mwhile ( dir ) {[m
[32m+[m								[32mnode = elem;[m
[32m+[m								[32mwhile ( (node = node[ dir ]) ) {[m
[32m+[m									[32mif ( ofType ?[m
[32m+[m										[32mnode.nodeName.toLowerCase() === name :[m
[32m+[m										[32mnode.nodeType === 1 ) {[m
[32m+[m
[32m+[m										[32mreturn false;[m
[32m+[m									[32m}[m
[32m+[m								[32m}[m
[32m+[m								[32m// Reverse direction for :only-* (if we haven't yet done so)[m
[32m+[m								[32mstart = dir = type === "only" && !start && "nextSibling";[m
[32m+[m							[32m}[m
[32m+[m							[32mreturn true;[m
[32m+[m						[32m}[m
[32m+[m
[32m+[m						[32mstart = [ forward ? parent.firstChild : parent.lastChild ];[m
[32m+[m
[32m+[m						[32m// non-xml :nth-child(...) stores cache data on `parent`[m
[32m+[m						[32mif ( forward && useCache ) {[m
[32m+[m
[32m+[m							[32m// Seek `elem` from a previously-cached index[m
[32m+[m
[32m+[m							[32m// ...in a gzip-friendly way[m
[32m+[m							[32mnode = parent;[m
[32m+[m							[32mouterCache = node[ expando ] || (node[ expando ] = {});[m
[32m+[m
[32m+[m							[32m// Support: IE <9 only[m
[32m+[m							[32m// Defend against cloned attroperties (jQuery gh-1709)[m
[32m+[m							[32muniqueCache = outerCache[ node.uniqueID ] ||[m
[32m+[m								[32m(outerCache[ node.uniqueID ] = {});[m
[32m+[m
[32m+[m							[32mcache = uniqueCache[ type ] || [];[m
[32m+[m							[32mnodeIndex = cache[ 0 ] === dirruns && cache[ 1 ];[m
[32m+[m							[32mdiff = nodeIndex && cache[ 2 ];[m
[32m+[m							[32mnode = nodeIndex && parent.childNodes[ nodeIndex ];[m
[32m+[m
[32m+[m							[32mwhile ( (node = ++nodeIndex && node && node[ dir ] ||[m
[32m+[m
[32m+[m								[32m// Fallback to seeking `elem` from the start[m
[32m+[m								[32m(diff = nodeIndex = 0) || start.pop()) ) {[m
[32m+[m
[32m+[m								[32m// When found, cache indexes on `parent` and break[m
[32m+[m								[32mif ( node.nodeType === 1 && ++diff && node === elem ) {[m
[32m+[m									[32muniqueCache[ type ] = [ dirruns, nodeIndex, diff ];[m
[32m+[m									[32mbreak;[m
[32m+[m								[32m}[m
[32m+[m							[32m}[m
[32m+[m
[32m+[m						[32m} else {[m
[32m+[m							[32m// Use previously-cached element index if available[m
[32m+[m							[32mif ( useCache ) {[m
[32m+[m								[32m// ...in a gzip-friendly way[m
[32m+[m								[32mnode = elem;[m
[32m+[m								[32mouterCache = node[ expando ] || (node[ expando ] = {});[m
[32m+[m
[32m+[m								[32m// Support: IE <9 only[m
[32m+[m								[32m// Defend against cloned attroperties (jQuery gh-1709)[m
[32m+[m								[32muniqueCache = outerCache[ node.uniqueID ] ||[m
[32m+[m									[32m(outerCache[ node.uniqueID ] = {});[m
[32m+[m
[32m+[m								[32mcache = uniqueCache[ type ] || [];[m
[32m+[m								[32mnodeIndex = cache[ 0 ] === dirruns && cache[ 1 ];[m
[32m+[m								[32mdiff = nodeIndex;[m
[32m+[m							[32m}[m
[32m+[m
[32m+[m							[32m// xml :nth-child(...)[m
[32m+[m							[32m// or :nth-last-child(...) or :nth(-last)?-of-type(...)[m
[32m+[m							[32mif ( diff === false ) {[m
[32m+[m								[32m// Use the same loop as above to seek `elem` from the start[m
[32m+[m								[32mwhile ( (node = ++nodeIndex && node && node[ dir ] ||[m
[32m+[m									[32m(diff = nodeIndex = 0) || start.pop()) ) {[m
[32m+[m
[32m+[m									[32mif ( ( ofType ?[m
[32m+[m										[32mnode.nodeName.toLowerCase() === name :[m
[32m+[m										[32mnode.nodeType === 1 ) &&[m
[32m+[m										[32m++diff ) {[m
[32m+[m
[32m+[m										[32m// Cache the index of each encountered element[m
[32m+[m										[32mif ( useCache ) {[m
[32m+[m											[32mouterCache = node[ expando ] || (node[ expando ] = {});[m
[32m+[m
[32m+[m											[32m// Support: IE <9 only[m
[32m+[m											[32m// Defend against cloned attroperties (jQuery gh-1709)[m
[32m+[m											[32muniqueCache = outerCache[ node.uniqueID ] ||[m
[32m+[m												[32m(outerCache[ node.uniqueID ] = {});[m
[32m+[m
[32m+[m											[32muniqueCache[ type ] = [ dirruns, diff ];[m
[32m+[m										[32m}[m
[32m+[m
[32m+[m										[32mif ( node === elem ) {[m
[32m+[m											[32mbreak;[m
[32m+[m										[32m}[m
[32m+[m									[32m}[m
[32m+[m								[32m}[m
[32m+[m							[32m}[m
[32m+[m						[32m}[m
[32m+[m
[32m+[m						[32m// Incorporate the offset, then check against cycle size[m
[32m+[m						[32mdiff -= last;[m
[32m+[m						[32mreturn diff === first || ( diff % first === 0 && diff / first >= 0 );[m
[32m+[m					[32m}[m
[32m+[m				[32m};[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m"PSEUDO": function( pseudo, argument ) {[m
[32m+[m			[32m// pseudo-class names are case-insensitive[m
[32m+[m			[32m// http://www.w3.org/TR/selectors/#pseudo-classes[m
[32m+[m			[32m// Prioritize by case sensitivity in case custom pseudos are added with uppercase letters[m
[32m+[m			[32m// Remember that setFilters inherits from pseudos[m
[32m+[m			[32mvar args,[m
[32m+[m				[32mfn = Expr.pseudos[ pseudo ] || Expr.setFilters[ pseudo.toLowerCase() ] ||[m
[32m+[m					[32mSizzle.error( "unsupported pseudo: " + pseudo );[m
[32m+[m
[32m+[m			[32m// The user may use createPseudo to indicate that[m
[32m+[m			[32m// arguments are needed to create the filter function[m
[32m+[m			[32m// just as Sizzle does[m
[32m+[m			[32mif ( fn[ expando ] ) {[m
[32m+[m				[32mreturn fn( argument );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// But maintain support for old signatures[m
[32m+[m			[32mif ( fn.length > 1 ) {[m
[32m+[m				[32margs = [ pseudo, pseudo, "", argument ];[m
[32m+[m				[32mreturn Expr.setFilters.hasOwnProperty( pseudo.toLowerCase() ) ?[m
[32m+[m					[32mmarkFunction(function( seed, matches ) {[m
[32m+[m						[32mvar idx,[m
[32m+[m							[32mmatched = fn( seed, argument ),[m
[32m+[m							[32mi = matched.length;[m
[32m+[m						[32mwhile ( i-- ) {[m
[32m+[m							[32midx = indexOf( seed, matched[i] );[m
[32m+[m							[32mseed[ idx ] = !( matches[ idx ] = matched[i] );[m
[32m+[m						[32m}[m
[32m+[m					[32m}) :[m
[32m+[m					[32mfunction( elem ) {[m
[32m+[m						[32mreturn fn( elem, 0, args );[m
[32m+[m					[32m};[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn fn;[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mpseudos: {[m
[32m+[m		[32m// Potentially complex pseudos[m
[32m+[m		[32m"not": markFunction(function( selector ) {[m
[32m+[m			[32m// Trim the selector passed to compile[m
[32m+[m			[32m// to avoid treating leading and trailing[m
[32m+[m			[32m// spaces as combinators[m
[32m+[m			[32mvar input = [],[m
[32m+[m				[32mresults = [],[m
[32m+[m				[32mmatcher = compile( selector.replace( rtrim, "$1" ) );[m
[32m+[m
[32m+[m			[32mreturn matcher[ expando ] ?[m
[32m+[m				[32mmarkFunction(function( seed, matches, context, xml ) {[m
[32m+[m					[32mvar elem,[m
[32m+[m						[32munmatched = matcher( seed, null, xml, [] ),[m
[32m+[m						[32mi = seed.length;[m
[32m+[m
[32m+[m					[32m// Match elements unmatched by `matcher`[m
[32m+[m					[32mwhile ( i-- ) {[m
[32m+[m						[32mif ( (elem = unmatched[i]) ) {[m
[32m+[m							[32mseed[i] = !(matches[i] = elem);[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m}) :[m
[32m+[m				[32mfunction( elem, context, xml ) {[m
[32m+[m					[32minput[0] = elem;[m
[32m+[m					[32mmatcher( input, null, xml, results );[m
[32m+[m					[32m// Don't keep the element (issue #299)[m
[32m+[m					[32minput[0] = null;[m
[32m+[m					[32mreturn !results.pop();[m
[32m+[m				[32m};[m
[32m+[m		[32m}),[m
[32m+[m
[32m+[m		[32m"has": markFunction(function( selector ) {[m
[32m+[m			[32mreturn function( elem ) {[m
[32m+[m				[32mreturn Sizzle( selector, elem ).length > 0;[m
[32m+[m			[32m};[m
[32m+[m		[32m}),[m
[32m+[m
[32m+[m		[32m"contains": markFunction(function( text ) {[m
[32m+[m			[32mtext = text.replace( runescape, funescape );[m
[32m+[m			[32mreturn function( elem ) {[m
[32m+[m				[32mreturn ( elem.textContent || elem.innerText || getText( elem ) ).indexOf( text ) > -1;[m
[32m+[m			[32m};[m
[32m+[m		[32m}),[m
[32m+[m
[32m+[m		[32m// "Whether an element is represented by a :lang() selector[m
[32m+[m		[32m// is based solely on the element's language value[m
[32m+[m		[32m// being equal to the identifier C,[m
[32m+[m		[32m// or beginning with the identifier C immediately followed by "-".[m
[32m+[m		[32m// The matching of C against the element's language value is performed case-insensitively.[m
[32m+[m		[32m// The identifier C does not have to be a valid language name."[m
[32m+[m		[32m// http://www.w3.org/TR/selectors/#lang-pseudo[m
[32m+[m		[32m"lang": markFunction( function( lang ) {[m
[32m+[m			[32m// lang value must be a valid identifier[m
[32m+[m			[32mif ( !ridentifier.test(lang || "") ) {[m
[32m+[m				[32mSizzle.error( "unsupported lang: " + lang );[m
[32m+[m			[32m}[m
[32m+[m			[32mlang = lang.replace( runescape, funescape ).toLowerCase();[m
[32m+[m			[32mreturn function( elem ) {[m
[32m+[m				[32mvar elemLang;[m
[32m+[m				[32mdo {[m
[32m+[m					[32mif ( (elemLang = documentIsHTML ?[m
[32m+[m						[32melem.lang :[m
[32m+[m						[32melem.getAttribute("xml:lang") || elem.getAttribute("lang")) ) {[m
[32m+[m
[32m+[m						[32melemLang = elemLang.toLowerCase();[m
[32m+[m						[32mreturn elemLang === lang || elemLang.indexOf( lang + "-" ) === 0;[m
[32m+[m					[32m}[m
[32m+[m				[32m} while ( (elem = elem.parentNode) && elem.nodeType === 1 );[m
[32m+[m				[32mreturn false;[m
[32m+[m			[32m};[m
[32m+[m		[32m}),[m
[32m+[m
[32m+[m		[32m// Miscellaneous[m
[32m+[m		[32m"target": function( elem ) {[m
[32m+[m			[32mvar hash = window.location && window.location.hash;[m
[32m+[m			[32mreturn hash && hash.slice( 1 ) === elem.id;[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m"root": function( elem ) {[m
[32m+[m			[32mreturn elem === docElem;[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m"focus": function( elem ) {[m
[32m+[m			[32mreturn elem === document.activeElement && (!document.hasFocus || document.hasFocus()) && !!(elem.type || elem.href || ~elem.tabIndex);[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Boolean properties[m
[32m+[m		[32m"enabled": createDisabledPseudo( false ),[m
[32m+[m		[32m"disabled": createDisabledPseudo( true ),[m
[32m+[m
[32m+[m		[32m"checked": function( elem ) {[m
[32m+[m			[32m// In CSS3, :checked should return both checked and selected elements[m
[32m+[m			[32m// http://www.w3.org/TR/2011/REC-css3-selectors-20110929/#checked[m
[32m+[m			[32mvar nodeName = elem.nodeName.toLowerCase();[m
[32m+[m			[32mreturn (nodeName === "input" && !!elem.checked) || (nodeName === "option" && !!elem.selected);[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m"selected": function( elem ) {[m
[32m+[m			[32m// Accessing this property makes selected-by-default[m
[32m+[m			[32m// options in Safari work properly[m
[32m+[m			[32mif ( elem.parentNode ) {[m
[32m+[m				[32melem.parentNode.selectedIndex;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn elem.selected === true;[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Contents[m
[32m+[m		[32m"empty": function( elem ) {[m
[32m+[m			[32m// http://www.w3.org/TR/selectors/#empty-pseudo[m
[32m+[m			[32m// :empty is negated by element (1) or content nodes (text: 3; cdata: 4; entity ref: 5),[m
[32m+[m			[32m//   but not by others (comment: 8; processing instruction: 7; etc.)[m
[32m+[m			[32m// nodeType < 6 works because attributes (2) do not appear as children[m
[32m+[m			[32mfor ( elem = elem.firstChild; elem; elem = elem.nextSibling ) {[m
[32m+[m				[32mif ( elem.nodeType < 6 ) {[m
[32m+[m					[32mreturn false;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn true;[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m"parent": function( elem ) {[m
[32m+[m			[32mreturn !Expr.pseudos["empty"]( elem );[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Element/input types[m
[32m+[m		[32m"header": function( elem ) {[m
[32m+[m			[32mreturn rheader.test( elem.nodeName );[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m"input": function( elem ) {[m
[32m+[m			[32mreturn rinputs.test( elem.nodeName );[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m"button": function( elem ) {[m
[32m+[m			[32mvar name = elem.nodeName.toLowerCase();[m
[32m+[m			[32mreturn name === "input" && elem.type === "button" || name === "button";[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m"text": function( elem ) {[m
[32m+[m			[32mvar attr;[m
[32m+[m			[32mreturn elem.nodeName.toLowerCase() === "input" &&[m
[32m+[m				[32melem.type === "text" &&[m
[32m+[m
[32m+[m				[32m// Support: IE<8[m
[32m+[m				[32m// New HTML5 attribute values (e.g., "search") appear with elem.type === "text"[m
[32m+[m				[32m( (attr = elem.getAttribute("type")) == null || attr.toLowerCase() === "text" );[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Position-in-collection[m
[32m+[m		[32m"first": createPositionalPseudo(function() {[m
[32m+[m			[32mreturn [ 0 ];[m
[32m+[m		[32m}),[m
[32m+[m
[32m+[m		[32m"last": createPositionalPseudo(function( matchIndexes, length ) {[m
[32m+[m			[32mreturn [ length - 1 ];[m
[32m+[m		[32m}),[m
[32m+[m
[32m+[m		[32m"eq": createPositionalPseudo(function( matchIndexes, length, argument ) {[m
[32m+[m			[32mreturn [ argument < 0 ? argument + length : argument ];[m
[32m+[m		[32m}),[m
[32m+[m
[32m+[m		[32m"even": createPositionalPseudo(function( matchIndexes, length ) {[m
[32m+[m			[32mvar i = 0;[m
[32m+[m			[32mfor ( ; i < length; i += 2 ) {[m
[32m+[m				[32mmatchIndexes.push( i );[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn matchIndexes;[m
[32m+[m		[32m}),[m
[32m+[m
[32m+[m		[32m"odd": createPositionalPseudo(function( matchIndexes, length ) {[m
[32m+[m			[32mvar i = 1;[m
[32m+[m			[32mfor ( ; i < length; i += 2 ) {[m
[32m+[m				[32mmatchIndexes.push( i );[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn matchIndexes;[m
[32m+[m		[32m}),[m
[32m+[m
[32m+[m		[32m"lt": createPositionalPseudo(function( matchIndexes, length, argument ) {[m
[32m+[m			[32mvar i = argument < 0 ? argument + length : argument;[m
[32m+[m			[32mfor ( ; --i >= 0; ) {[m
[32m+[m				[32mmatchIndexes.push( i );[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn matchIndexes;[m
[32m+[m		[32m}),[m
[32m+[m
[32m+[m		[32m"gt": createPositionalPseudo(function( matchIndexes, length, argument ) {[m
[32m+[m			[32mvar i = argument < 0 ? argument + length : argument;[m
[32m+[m			[32mfor ( ; ++i < length; ) {[m
[32m+[m				[32mmatchIndexes.push( i );[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn matchIndexes;[m
[32m+[m		[32m})[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mExpr.pseudos["nth"] = Expr.pseudos["eq"];[m
[32m+[m
[32m+[m[32m// Add button/input type pseudos[m
[32m+[m[32mfor ( i in { radio: true, checkbox: true, file: true, password: true, image: true } ) {[m
[32m+[m	[32mExpr.pseudos[ i ] = createInputPseudo( i );[m
[32m+[m[32m}[m
[32m+[m[32mfor ( i in { submit: true, reset: true } ) {[m
[32m+[m	[32mExpr.pseudos[ i ] = createButtonPseudo( i );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Easy API for creating new setFilters[m
[32m+[m[32mfunction setFilters() {}[m
[32m+[m[32msetFilters.prototype = Expr.filters = Expr.pseudos;[m
[32m+[m[32mExpr.setFilters = new setFilters();[m
[32m+[m
[32m+[m[32mtokenize = Sizzle.tokenize = function( selector, parseOnly ) {[m
[32m+[m	[32mvar matched, match, tokens, type,[m
[32m+[m		[32msoFar, groups, preFilters,[m
[32m+[m		[32mcached = tokenCache[ selector + " " ];[m
[32m+[m
[32m+[m	[32mif ( cached ) {[m
[32m+[m		[32mreturn parseOnly ? 0 : cached.slice( 0 );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32msoFar = selector;[m
[32m+[m	[32mgroups = [];[m
[32m+[m	[32mpreFilters = Expr.preFilter;[m
[32m+[m
[32m+[m	[32mwhile ( soFar ) {[m
[32m+[m
[32m+[m		[32m// Comma and first run[m
[32m+[m		[32mif ( !matched || (match = rcomma.exec( soFar )) ) {[m
[32m+[m			[32mif ( match ) {[m
[32m+[m				[32m// Don't consume trailing commas as valid[m
[32m+[m				[32msoFar = soFar.slice( match[0].length ) || soFar;[m
[32m+[m			[32m}[m
[32m+[m			[32mgroups.push( (tokens = []) );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mmatched = false;[m
[32m+[m
[32m+[m		[32m// Combinators[m
[32m+[m		[32mif ( (match = rcombinators.exec( soFar )) ) {[m
[32m+[m			[32mmatched = match.shift();[m
[32m+[m			[32mtokens.push({[m
[32m+[m				[32mvalue: matched,[m
[32m+[m				[32m// Cast descendant combinators to space[m
[32m+[m				[32mtype: match[0].replace( rtrim, " " )[m
[32m+[m			[32m});[m
[32m+[m			[32msoFar = soFar.slice( matched.length );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Filters[m
[32m+[m		[32mfor ( type in Expr.filter ) {[m
[32m+[m			[32mif ( (match = matchExpr[ type ].exec( soFar )) && (!preFilters[ type ] ||[m
[32m+[m				[32m(match = preFilters[ type ]( match ))) ) {[m
[32m+[m				[32mmatched = match.shift();[m
[32m+[m				[32mtokens.push({[m
[32m+[m					[32mvalue: matched,[m
[32m+[m					[32mtype: type,[m
[32m+[m					[32mmatches: match[m
[32m+[m				[32m});[m
[32m+[m				[32msoFar = soFar.slice( matched.length );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( !matched ) {[m
[32m+[m			[32mbreak;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Return the length of the invalid excess[m
[32m+[m	[32m// if we're just parsing[m
[32m+[m	[32m// Otherwise, throw an error or return tokens[m
[32m+[m	[32mreturn parseOnly ?[m
[32m+[m		[32msoFar.length :[m
[32m+[m		[32msoFar ?[m
[32m+[m			[32mSizzle.error( selector ) :[m
[32m+[m			[32m// Cache the tokens[m
[32m+[m			[32mtokenCache( selector, groups ).slice( 0 );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mfunction toSelector( tokens ) {[m
[32m+[m	[32mvar i = 0,[m
[32m+[m		[32mlen = tokens.length,[m
[32m+[m		[32mselector = "";[m
[32m+[m	[32mfor ( ; i < len; i++ ) {[m
[32m+[m		[32mselector += tokens[i].value;[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn selector;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction addCombinator( matcher, combinator, base ) {[m
[32m+[m	[32mvar dir = combinator.dir,[m
[32m+[m		[32mskip = combinator.next,[m
[32m+[m		[32mkey = skip || dir,[m
[32m+[m		[32mcheckNonElements = base && key === "parentNode",[m
[32m+[m		[32mdoneName = done++;[m
[32m+[m
[32m+[m	[32mreturn combinator.first ?[m
[32m+[m		[32m// Check against closest ancestor/preceding element[m
[32m+[m		[32mfunction( elem, context, xml ) {[m
[32m+[m			[32mwhile ( (elem = elem[ dir ]) ) {[m
[32m+[m				[32mif ( elem.nodeType === 1 || checkNonElements ) {[m
[32m+[m					[32mreturn matcher( elem, context, xml );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m} :[m
[32m+[m
[32m+[m		[32m// Check against all ancestor/preceding elements[m
[32m+[m		[32mfunction( elem, context, xml ) {[m
[32m+[m			[32mvar oldCache, uniqueCache, outerCache,[m
[32m+[m				[32mnewCache = [ dirruns, doneName ];[m
[32m+[m
[32m+[m			[32m// We can't set arbitrary data on XML nodes, so they don't benefit from combinator caching[m
[32m+[m			[32mif ( xml ) {[m
[32m+[m				[32mwhile ( (elem = elem[ dir ]) ) {[m
[32m+[m					[32mif ( elem.nodeType === 1 || checkNonElements ) {[m
[32m+[m						[32mif ( matcher( elem, context, xml ) ) {[m
[32m+[m							[32mreturn true;[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m} else {[m
[32m+[m				[32mwhile ( (elem = elem[ dir ]) ) {[m
[32m+[m					[32mif ( elem.nodeType === 1 || checkNonElements ) {[m
[32m+[m						[32mouterCache = elem[ expando ] || (elem[ expando ] = {});[m
[32m+[m
[32m+[m						[32m// Support: IE <9 only[m
[32m+[m						[32m// Defend against cloned attroperties (jQuery gh-1709)[m
[32m+[m						[32muniqueCache = outerCache[ elem.uniqueID ] || (outerCache[ elem.uniqueID ] = {});[m
[32m+[m
[32m+[m						[32mif ( skip && skip === elem.nodeName.toLowerCase() ) {[m
[32m+[m							[32melem = elem[ dir ] || elem;[m
[32m+[m						[32m} else if ( (oldCache = uniqueCache[ key ]) &&[m
[32m+[m							[32moldCache[ 0 ] === dirruns && oldCache[ 1 ] === doneName ) {[m
[32m+[m
[32m+[m							[32m// Assign to newCache so results back-propagate to previous elements[m
[32m+[m							[32mreturn (newCache[ 2 ] = oldCache[ 2 ]);[m
[32m+[m						[32m} else {[m
[32m+[m							[32m// Reuse newcache so results back-propagate to previous elements[m
[32m+[m							[32muniqueCache[ key ] = newCache;[m
[32m+[m
[32m+[m							[32m// A match means we're done; a fail means we have to keep checking[m
[32m+[m							[32mif ( (newCache[ 2 ] = matcher( elem, context, xml )) ) {[m
[32m+[m								[32mreturn true;[m
[32m+[m							[32m}[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m};[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction elementMatcher( matchers ) {[m
[32m+[m	[32mreturn matchers.length > 1 ?[m
[32m+[m		[32mfunction( elem, context, xml ) {[m
[32m+[m			[32mvar i = matchers.length;[m
[32m+[m			[32mwhile ( i-- ) {[m
[32m+[m				[32mif ( !matchers[i]( elem, context, xml ) ) {[m
[32m+[m					[32mreturn false;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn true;[m
[32m+[m		[32m} :[m
[32m+[m		[32mmatchers[0];[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction multipleContexts( selector, contexts, results ) {[m
[32m+[m	[32mvar i = 0,[m
[32m+[m		[32mlen = contexts.length;[m
[32m+[m	[32mfor ( ; i < len; i++ ) {[m
[32m+[m		[32mSizzle( selector, contexts[i], results );[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn results;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction condense( unmatched, map, filter, context, xml ) {[m
[32m+[m	[32mvar elem,[m
[32m+[m		[32mnewUnmatched = [],[m
[32m+[m		[32mi = 0,[m
[32m+[m		[32mlen = unmatched.length,[m
[32m+[m		[32mmapped = map != null;[m
[32m+[m
[32m+[m	[32mfor ( ; i < len; i++ ) {[m
[32m+[m		[32mif ( (elem = unmatched[i]) ) {[m
[32m+[m			[32mif ( !filter || filter( elem, context, xml ) ) {[m
[32m+[m				[32mnewUnmatched.push( elem );[m
[32m+[m				[32mif ( mapped ) {[m
[32m+[m					[32mmap.push( i );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn newUnmatched;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction setMatcher( preFilter, selector, matcher, postFilter, postFinder, postSelector ) {[m
[32m+[m	[32mif ( postFilter && !postFilter[ expando ] ) {[m
[32m+[m		[32mpostFilter = setMatcher( postFilter );[m
[32m+[m	[32m}[m
[32m+[m	[32mif ( postFinder && !postFinder[ expando ] ) {[m
[32m+[m		[32mpostFinder = setMatcher( postFinder, postSelector );[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn markFunction(function( seed, results, context, xml ) {[m
[32m+[m		[32mvar temp, i, elem,[m
[32m+[m			[32mpreMap = [],[m
[32m+[m			[32mpostMap = [],[m
[32m+[m			[32mpreexisting = results.length,[m
[32m+[m
[32m+[m			[32m// Get initial elements from seed or context[m
[32m+[m			[32melems = seed || multipleContexts( selector || "*", context.nodeType ? [ context ] : context, [] ),[m
[32m+[m
[32m+[m			[32m// Prefilter to get matcher input, preserving a map for seed-results synchronization[m
[32m+[m			[32mmatcherIn = preFilter && ( seed || !selector ) ?[m
[32m+[m				[32mcondense( elems, preMap, preFilter, context, xml ) :[m
[32m+[m				[32melems,[m
[32m+[m
[32m+[m			[32mmatcherOut = matcher ?[m
[32m+[m				[32m// If we have a postFinder, or filtered seed, or non-seed postFilter or preexisting results,[m
[32m+[m				[32mpostFinder || ( seed ? preFilter : preexisting || postFilter ) ?[m
[32m+[m
[32m+[m					[32m// ...intermediate processing is necessary[m
[32m+[m					[32m[] :[m
[32m+[m
[32m+[m					[32m// ...otherwise use results directly[m
[32m+[m					[32mresults :[m
[32m+[m				[32mmatcherIn;[m
[32m+[m
[32m+[m		[32m// Find primary matches[m
[32m+[m		[32mif ( matcher ) {[m
[32m+[m			[32mmatcher( matcherIn, matcherOut, context, xml );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Apply postFilter[m
[32m+[m		[32mif ( postFilter ) {[m
[32m+[m			[32mtemp = condense( matcherOut, postMap );[m
[32m+[m			[32mpostFilter( temp, [], context, xml );[m
[32m+[m
[32m+[m			[32m// Un-match failing elements by moving them back to matcherIn[m
[32m+[m			[32mi = temp.length;[m
[32m+[m			[32mwhile ( i-- ) {[m
[32m+[m				[32mif ( (elem = temp[i]) ) {[m
[32m+[m					[32mmatcherOut[ postMap[i] ] = !(matcherIn[ postMap[i] ] = elem);[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( seed ) {[m
[32m+[m			[32mif ( postFinder || preFilter ) {[m
[32m+[m				[32mif ( postFinder ) {[m
[32m+[m					[32m// Get the final matcherOut by condensing this intermediate into postFinder contexts[m
[32m+[m					[32mtemp = [];[m
[32m+[m					[32mi = matcherOut.length;[m
[32m+[m					[32mwhile ( i-- ) {[m
[32m+[m						[32mif ( (elem = matcherOut[i]) ) {[m
[32m+[m							[32m// Restore matcherIn since elem is not yet a final match[m
[32m+[m							[32mtemp.push( (matcherIn[i] = elem) );[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m					[32mpostFinder( null, (matcherOut = []), temp, xml );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Move matched elements from seed to results to keep them synchronized[m
[32m+[m				[32mi = matcherOut.length;[m
[32m+[m				[32mwhile ( i-- ) {[m
[32m+[m					[32mif ( (elem = matcherOut[i]) &&[m
[32m+[m						[32m(temp = postFinder ? indexOf( seed, elem ) : preMap[i]) > -1 ) {[m
[32m+[m
[32m+[m						[32mseed[temp] = !(results[temp] = elem);[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m// Add elements to results, through postFinder if defined[m
[32m+[m		[32m} else {[m
[32m+[m			[32mmatcherOut = condense([m
[32m+[m				[32mmatcherOut === results ?[m
[32m+[m					[32mmatcherOut.splice( preexisting, matcherOut.length ) :[m
[32m+[m					[32mmatcherOut[m
[32m+[m			[32m);[m
[32m+[m			[32mif ( postFinder ) {[m
[32m+[m				[32mpostFinder( null, results, matcherOut, xml );[m
[32m+[m			[32m} else {[m
[32m+[m				[32mpush.apply( results, matcherOut );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m});[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction matcherFromTokens( tokens ) {[m
[32m+[m	[32mvar checkContext, matcher, j,[m
[32m+[m		[32mlen = tokens.length,[m
[32m+[m		[32mleadingRelative = Expr.relative[ tokens[0].type ],[m
[32m+[m		[32mimplicitRelative = leadingRelative || Expr.relative[" "],[m
[32m+[m		[32mi = leadingRelative ? 1 : 0,[m
[32m+[m
[32m+[m		[32m// The foundational matcher ensures that elements are reachable from top-level context(s)[m
[32m+[m		[32mmatchContext = addCombinator( function( elem ) {[m
[32m+[m			[32mreturn elem === checkContext;[m
[32m+[m		[32m}, implicitRelative, true ),[m
[32m+[m		[32mmatchAnyContext = addCombinator( function( elem ) {[m
[32m+[m			[32mreturn indexOf( checkContext, elem ) > -1;[m
[32m+[m		[32m}, implicitRelative, true ),[m
[32m+[m		[32mmatchers = [ function( elem, context, xml ) {[m
[32m+[m			[32mvar ret = ( !leadingRelative && ( xml || context !== outermostContext ) ) || ([m
[32m+[m				[32m(checkContext = context).nodeType ?[m
[32m+[m					[32mmatchContext( elem, context, xml ) :[m
[32m+[m					[32mmatchAnyContext( elem, context, xml ) );[m
[32m+[m			[32m// Avoid hanging onto element (issue #299)[m
[32m+[m			[32mcheckContext = null;[m
[32m+[m			[32mreturn ret;[m
[32m+[m		[32m} ];[m
[32m+[m
[32m+[m	[32mfor ( ; i < len; i++ ) {[m
[32m+[m		[32mif ( (matcher = Expr.relative[ tokens[i].type ]) ) {[m
[32m+[m			[32mmatchers = [ addCombinator(elementMatcher( matchers ), matcher) ];[m
[32m+[m		[32m} else {[m
[32m+[m			[32mmatcher = Expr.filter[ tokens[i].type ].apply( null, tokens[i].matches );[m
[32m+[m
[32m+[m			[32m// Return special upon seeing a positional matcher[m
[32m+[m			[32mif ( matcher[ expando ] ) {[m
[32m+[m				[32m// Find the next relative operator (if any) for proper handling[m
[32m+[m				[32mj = ++i;[m
[32m+[m				[32mfor ( ; j < len; j++ ) {[m
[32m+[m					[32mif ( Expr.relative[ tokens[j].type ] ) {[m
[32m+[m						[32mbreak;[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m				[32mreturn setMatcher([m
[32m+[m					[32mi > 1 && elementMatcher( matchers ),[m
[32m+[m					[32mi > 1 && toSelector([m
[32m+[m						[32m// If the preceding token was a descendant combinator, insert an implicit any-element `*`[m
[32m+[m						[32mtokens.slice( 0, i - 1 ).concat({ value: tokens[ i - 2 ].type === " " ? "*" : "" })[m
[32m+[m					[32m).replace( rtrim, "$1" ),[m
[32m+[m					[32mmatcher,[m
[32m+[m					[32mi < j && matcherFromTokens( tokens.slice( i, j ) ),[m
[32m+[m					[32mj < len && matcherFromTokens( (tokens = tokens.slice( j )) ),[m
[32m+[m					[32mj < len && toSelector( tokens )[m
[32m+[m				[32m);[m
[32m+[m			[32m}[m
[32m+[m			[32mmatchers.push( matcher );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn elementMatcher( matchers );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction matcherFromGroupMatchers( elementMatchers, setMatchers ) {[m
[32m+[m	[32mvar bySet = setMatchers.length > 0,[m
[32m+[m		[32mbyElement = elementMatchers.length > 0,[m
[32m+[m		[32msuperMatcher = function( seed, context, xml, results, outermost ) {[m
[32m+[m			[32mvar elem, j, matcher,[m
[32m+[m				[32mmatchedCount = 0,[m
[32m+[m				[32mi = "0",[m
[32m+[m				[32munmatched = seed && [],[m
[32m+[m				[32msetMatched = [],[m
[32m+[m				[32mcontextBackup = outermostContext,[m
[32m+[m				[32m// We must always have either seed elements or outermost context[m
[32m+[m				[32melems = seed || byElement && Expr.find["TAG"]( "*", outermost ),[m
[32m+[m				[32m// Use integer dirruns iff this is the outermost matcher[m
[32m+[m				[32mdirrunsUnique = (dirruns += contextBackup == null ? 1 : Math.random() || 0.1),[m
[32m+[m				[32mlen = elems.length;[m
[32m+[m
[32m+[m			[32mif ( outermost ) {[m
[32m+[m				[32moutermostContext = context === document || context || outermost;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Add elements passing elementMatchers directly to results[m
[32m+[m			[32m// Support: IE<9, Safari[m
[32m+[m			[32m// Tolerate NodeList properties (IE: "length"; Safari: <number>) matching elements by id[m
[32m+[m			[32mfor ( ; i !== len && (elem = elems[i]) != null; i++ ) {[m
[32m+[m				[32mif ( byElement && elem ) {[m
[32m+[m					[32mj = 0;[m
[32m+[m					[32mif ( !context && elem.ownerDocument !== document ) {[m
[32m+[m						[32msetDocument( elem );[m
[32m+[m						[32mxml = !documentIsHTML;[m
[32m+[m					[32m}[m
[32m+[m					[32mwhile ( (matcher = elementMatchers[j++]) ) {[m
[32m+[m						[32mif ( matcher( elem, context || document, xml) ) {[m
[32m+[m							[32mresults.push( elem );[m
[32m+[m							[32mbreak;[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m					[32mif ( outermost ) {[m
[32m+[m						[32mdirruns = dirrunsUnique;[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Track unmatched elements for set filters[m
[32m+[m				[32mif ( bySet ) {[m
[32m+[m					[32m// They will have gone through all possible matchers[m
[32m+[m					[32mif ( (elem = !matcher && elem) ) {[m
[32m+[m						[32mmatchedCount--;[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// Lengthen the array for every element, matched or not[m
[32m+[m					[32mif ( seed ) {[m
[32m+[m						[32munmatched.push( elem );[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// `i` is now the count of elements visited above, and adding it to `matchedCount`[m
[32m+[m			[32m// makes the latter nonnegative.[m
[32m+[m			[32mmatchedCount += i;[m
[32m+[m
[32m+[m			[32m// Apply set filters to unmatched elements[m
[32m+[m			[32m// NOTE: This can be skipped if there are no unmatched elements (i.e., `matchedCount`[m
[32m+[m			[32m// equals `i`), unless we didn't visit _any_ elements in the above loop because we have[m
[32m+[m			[32m// no element matchers and no seed.[m
[32m+[m			[32m// Incrementing an initially-string "0" `i` allows `i` to remain a string only in that[m
[32m+[m			[32m// case, which will result in a "00" `matchedCount` that differs from `i` but is also[m
[32m+[m			[32m// numerically zero.[m
[32m+[m			[32mif ( bySet && i !== matchedCount ) {[m
[32m+[m				[32mj = 0;[m
[32m+[m				[32mwhile ( (matcher = setMatchers[j++]) ) {[m
[32m+[m					[32mmatcher( unmatched, setMatched, context, xml );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mif ( seed ) {[m
[32m+[m					[32m// Reintegrate element matches to eliminate the need for sorting[m
[32m+[m					[32mif ( matchedCount > 0 ) {[m
[32m+[m						[32mwhile ( i-- ) {[m
[32m+[m							[32mif ( !(unmatched[i] || setMatched[i]) ) {[m
[32m+[m								[32msetMatched[i] = pop.call( results );[m
[32m+[m							[32m}[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// Discard index placeholder values to get only actual matches[m
[32m+[m					[32msetMatched = condense( setMatched );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Add matches to results[m
[32m+[m				[32mpush.apply( results, setMatched );[m
[32m+[m
[32m+[m				[32m// Seedless set matches succeeding multiple successful matchers stipulate sorting[m
[32m+[m				[32mif ( outermost && !seed && setMatched.length > 0 &&[m
[32m+[m					[32m( matchedCount + setMatchers.length ) > 1 ) {[m
[32m+[m
[32m+[m					[32mSizzle.uniqueSort( results );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Override manipulation of globals by nested matchers[m
[32m+[m			[32mif ( outermost ) {[m
[32m+[m				[32mdirruns = dirrunsUnique;[m
[32m+[m				[32moutermostContext = contextBackup;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn unmatched;[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m	[32mreturn bySet ?[m
[32m+[m		[32mmarkFunction( superMatcher ) :[m
[32m+[m		[32msuperMatcher;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mcompile = Sizzle.compile = function( selector, match /* Internal Use Only */ ) {[m
[32m+[m	[32mvar i,[m
[32m+[m		[32msetMatchers = [],[m
[32m+[m		[32melementMatchers = [],[m
[32m+[m		[32mcached = compilerCache[ selector + " " ];[m
[32m+[m
[32m+[m	[32mif ( !cached ) {[m
[32m+[m		[32m// Generate a function of recursive functions that can be used to check each element[m
[32m+[m		[32mif ( !match ) {[m
[32m+[m			[32mmatch = tokenize( selector );[m
[32m+[m		[32m}[m
[32m+[m		[32mi = match.length;[m
[32m+[m		[32mwhile ( i-- ) {[m
[32m+[m			[32mcached = matcherFromTokens( match[i] );[m
[32m+[m			[32mif ( cached[ expando ] ) {[m
[32m+[m				[32msetMatchers.push( cached );[m
[32m+[m			[32m} else {[m
[32m+[m				[32melementMatchers.push( cached );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Cache the compiled function[m
[32m+[m		[32mcached = compilerCache( selector, matcherFromGroupMatchers( elementMatchers, setMatchers ) );[m
[32m+[m
[32m+[m		[32m// Save selector and tokenization[m
[32m+[m		[32mcached.selector = selector;[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn cached;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * A low-level selection function that works with Sizzle's compiled[m
[32m+[m[32m *  selector functions[m
[32m+[m[32m * @param {String|Function} selector A selector or a pre-compiled[m
[32m+[m[32m *  selector function built with Sizzle.compile[m
[32m+[m[32m * @param {Element} context[m
[32m+[m[32m * @param {Array} [results][m
[32m+[m[32m * @param {Array} [seed] A set of elements to match against[m
[32m+[m[32m */[m
[32m+[m[32mselect = Sizzle.select = function( selector, context, results, seed ) {[m
[32m+[m	[32mvar i, tokens, token, type, find,[m
[32m+[m		[32mcompiled = typeof selector === "function" && selector,[m
[32m+[m		[32mmatch = !seed && tokenize( (selector = compiled.selector || selector) );[m
[32m+[m
[32m+[m	[32mresults = results || [];[m
[32m+[m
[32m+[m	[32m// Try to minimize operations if there is only one selector in the list and no seed[m
[32m+[m	[32m// (the latter of which guarantees us context)[m
[32m+[m	[32mif ( match.length === 1 ) {[m
[32m+[m
[32m+[m		[32m// Reduce context if the leading compound selector is an ID[m
[32m+[m		[32mtokens = match[0] = match[0].slice( 0 );[m
[32m+[m		[32mif ( tokens.length > 2 && (token = tokens[0]).type === "ID" &&[m
[32m+[m				[32msupport.getById && context.nodeType === 9 && documentIsHTML &&[m
[32m+[m				[32mExpr.relative[ tokens[1].type ] ) {[m
[32m+[m
[32m+[m			[32mcontext = ( Expr.find["ID"]( token.matches[0].replace(runescape, funescape), context ) || [] )[0];[m
[32m+[m			[32mif ( !context ) {[m
[32m+[m				[32mreturn results;[m
[32m+[m
[32m+[m			[32m// Precompiled matchers will still verify ancestry, so step up a level[m
[32m+[m			[32m} else if ( compiled ) {[m
[32m+[m				[32mcontext = context.parentNode;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mselector = selector.slice( tokens.shift().value.length );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Fetch a seed set for right-to-left matching[m
[32m+[m		[32mi = matchExpr["needsContext"].test( selector ) ? 0 : tokens.length;[m
[32m+[m		[32mwhile ( i-- ) {[m
[32m+[m			[32mtoken = tokens[i];[m
[32m+[m
[32m+[m			[32m// Abort if we hit a combinator[m
[32m+[m			[32mif ( Expr.relative[ (type = token.type) ] ) {[m
[32m+[m				[32mbreak;[m
[32m+[m			[32m}[m
[32m+[m			[32mif ( (find = Expr.find[ type ]) ) {[m
[32m+[m				[32m// Search, expanding context for leading sibling combinators[m
[32m+[m				[32mif ( (seed = find([m
[32m+[m					[32mtoken.matches[0].replace( runescape, funescape ),[m
[32m+[m					[32mrsibling.test( tokens[0].type ) && testContext( context.parentNode ) || context[m
[32m+[m				[32m)) ) {[m
[32m+[m
[32m+[m					[32m// If seed is empty or no tokens remain, we can return early[m
[32m+[m					[32mtokens.splice( i, 1 );[m
[32m+[m					[32mselector = seed.length && toSelector( tokens );[m
[32m+[m					[32mif ( !selector ) {[m
[32m+[m						[32mpush.apply( results, seed );[m
[32m+[m						[32mreturn results;[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32mbreak;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Compile and execute a filtering function if one is not provided[m
[32m+[m	[32m// Provide `match` to avoid retokenization if we modified the selector above[m
[32m+[m	[32m( compiled || compile( selector, match ) )([m
[32m+[m		[32mseed,[m
[32m+[m		[32mcontext,[m
[32m+[m		[32m!documentIsHTML,[m
[32m+[m		[32mresults,[m
[32m+[m		[32m!context || rsibling.test( selector ) && testContext( context.parentNode ) || context[m
[32m+[m	[32m);[m
[32m+[m	[32mreturn results;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// One-time assignments[m
[32m+[m
[32m+[m[32m// Sort stability[m
[32m+[m[32msupport.sortStable = expando.split("").sort( sortOrder ).join("") === expando;[m
[32m+[m
[32m+[m[32m// Support: Chrome 14-35+[m
[32m+[m[32m// Always assume duplicates if they aren't passed to the comparison function[m
[32m+[m[32msupport.detectDuplicates = !!hasDuplicate;[m
[32m+[m
[32m+[m[32m// Initialize against the default document[m
[32m+[m[32msetDocument();[m
[32m+[m
[32m+[m[32m// Support: Webkit<537.32 - Safari 6.0.3/Chrome 25 (fixed in Chrome 27)[m
[32m+[m[32m// Detached nodes confoundingly follow *each other*[m
[32m+[m[32msupport.sortDetached = assert(function( el ) {[m
[32m+[m	[32m// Should return 1, but returns 4 (following)[m
[32m+[m	[32mreturn el.compareDocumentPosition( document.createElement("fieldset") ) & 1;[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m// Support: IE<8[m
[32m+[m[32m// Prevent attribute/property "interpolation"[m
[32m+[m[32m// https://msdn.microsoft.com/en-us/library/ms536429%28VS.85%29.aspx[m
[32m+[m[32mif ( !assert(function( el ) {[m
[32m+[m	[32mel.innerHTML = "<a href='#'></a>";[m
[32m+[m	[32mreturn el.firstChild.getAttribute("href") === "#" ;[m
[32m+[m[32m}) ) {[m
[32m+[m	[32maddHandle( "type|href|height|width", function( elem, name, isXML ) {[m
[32m+[m		[32mif ( !isXML ) {[m
[32m+[m			[32mreturn elem.getAttribute( name, name.toLowerCase() === "type" ? 1 : 2 );[m
[32m+[m		[32m}[m
[32m+[m	[32m});[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Support: IE<9[m
[32m+[m[32m// Use defaultValue in place of getAttribute("value")[m
[32m+[m[32mif ( !support.attributes || !assert(function( el ) {[m
[32m+[m	[32mel.innerHTML = "<input/>";[m
[32m+[m	[32mel.firstChild.setAttribute( "value", "" );[m
[32m+[m	[32mreturn el.firstChild.getAttribute( "value" ) === "";[m
[32m+[m[32m}) ) {[m
[32m+[m	[32maddHandle( "value", function( elem, name, isXML ) {[m
[32m+[m		[32mif ( !isXML && elem.nodeName.toLowerCase() === "input" ) {[m
[32m+[m			[32mreturn elem.defaultValue;[m
[32m+[m		[32m}[m
[32m+[m	[32m});[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Support: IE<9[m
[32m+[m[32m// Use getAttributeNode to fetch booleans when getAttribute lies[m
[32m+[m[32mif ( !assert(function( el ) {[m
[32m+[m	[32mreturn el.getAttribute("disabled") == null;[m
[32m+[m[32m}) ) {[m
[32m+[m	[32maddHandle( booleans, function( elem, name, isXML ) {[m
[32m+[m		[32mvar val;[m
[32m+[m		[32mif ( !isXML ) {[m
[32m+[m			[32mreturn elem[ name ] === true ? name.toLowerCase() :[m
[32m+[m					[32m(val = elem.getAttributeNode( name )) && val.specified ?[m
[32m+[m					[32mval.value :[m
[32m+[m				[32mnull;[m
[32m+[m		[32m}[m
[32m+[m	[32m});[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mreturn Sizzle;[m
[32m+[m
[32m+[m[32m})( window );[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mjQuery.find = Sizzle;[m
[32m+[m[32mjQuery.expr = Sizzle.selectors;[m
[32m+[m
[32m+[m[32m// Deprecated[m
[32m+[m[32mjQuery.expr[ ":" ] = jQuery.expr.pseudos;[m
[32m+[m[32mjQuery.uniqueSort = jQuery.unique = Sizzle.uniqueSort;[m
[32m+[m[32mjQuery.text = Sizzle.getText;[m
[32m+[m[32mjQuery.isXMLDoc = Sizzle.isXML;[m
[32m+[m[32mjQuery.contains = Sizzle.contains;[m
[32m+[m[32mjQuery.escapeSelector = Sizzle.escape;[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mvar dir = function( elem, dir, until ) {[m
[32m+[m	[32mvar matched = [],[m
[32m+[m		[32mtruncate = until !== undefined;[m
[32m+[m
[32m+[m	[32mwhile ( ( elem = elem[ dir ] ) && elem.nodeType !== 9 ) {[m
[32m+[m		[32mif ( elem.nodeType === 1 ) {[m
[32m+[m			[32mif ( truncate && jQuery( elem ).is( until ) ) {[m
[32m+[m				[32mbreak;[m
[32m+[m			[32m}[m
[32m+[m			[32mmatched.push( elem );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn matched;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m[32mvar siblings = function( n, elem ) {[m
[32m+[m	[32mvar matched = [];[m
[32m+[m
[32m+[m	[32mfor ( ; n; n = n.nextSibling ) {[m
[32m+[m		[32mif ( n.nodeType === 1 && n !== elem ) {[m
[32m+[m			[32mmatched.push( n );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn matched;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m[32mvar rneedsContext = jQuery.expr.match.needsContext;[m
[32m+[m
[32m+[m[32mvar rsingleTag = ( /^<([a-z][^\/\0>:\x20\t\r\n\f]*)[\x20\t\r\n\f]*\/?>(?:<\/\1>|)$/i );[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mvar risSimple = /^.[^:#\[\.,]*$/;[m
[32m+[m
[32m+[m[32m// Implement the identical functionality for filter and not[m
[32m+[m[32mfunction winnow( elements, qualifier, not ) {[m
[32m+[m	[32mif ( jQuery.isFunction( qualifier ) ) {[m
[32m+[m		[32mreturn jQuery.grep( elements, function( elem, i ) {[m
[32m+[m			[32mreturn !!qualifier.call( elem, i, elem ) !== not;[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( qualifier.nodeType ) {[m
[32m+[m		[32mreturn jQuery.grep( elements, function( elem ) {[m
[32m+[m			[32mreturn ( elem === qualifier ) !== not;[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( typeof qualifier === "string" ) {[m
[32m+[m		[32mif ( risSimple.test( qualifier ) ) {[m
[32m+[m			[32mreturn jQuery.filter( qualifier, elements, not );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mqualifier = jQuery.filter( qualifier, elements );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn jQuery.grep( elements, function( elem ) {[m
[32m+[m		[32mreturn ( indexOf.call( qualifier, elem ) > -1 ) !== not && elem.nodeType === 1;[m
[32m+[m	[32m} );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mjQuery.filter = function( expr, elems, not ) {[m
[32m+[m	[32mvar elem = elems[ 0 ];[m
[32m+[m
[32m+[m	[32mif ( not ) {[m
[32m+[m		[32mexpr = ":not(" + expr + ")";[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn elems.length === 1 && elem.nodeType === 1 ?[m
[32m+[m		[32mjQuery.find.matchesSelector( elem, expr ) ? [ elem ] : [] :[m
[32m+[m		[32mjQuery.find.matches( expr, jQuery.grep( elems, function( elem ) {[m
[32m+[m			[32mreturn elem.nodeType === 1;[m
[32m+[m		[32m} ) );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m	[32mfind: function( selector ) {[m
[32m+[m		[32mvar i, ret,[m
[32m+[m			[32mlen = this.length,[m
[32m+[m			[32mself = this;[m
[32m+[m
[32m+[m		[32mif ( typeof selector !== "string" ) {[m
[32m+[m			[32mreturn this.pushStack( jQuery( selector ).filter( function() {[m
[32m+[m				[32mfor ( i = 0; i < len; i++ ) {[m
[32m+[m					[32mif ( jQuery.contains( self[ i ], this ) ) {[m
[32m+[m						[32mreturn true;[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m} ) );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mret = this.pushStack( [] );[m
[32m+[m
[32m+[m		[32mfor ( i = 0; i < len; i++ ) {[m
[32m+[m			[32mjQuery.find( selector, self[ i ], ret );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn len > 1 ? jQuery.uniqueSort( ret ) : ret;[m
[32m+[m	[32m},[m
[32m+[m	[32mfilter: function( selector ) {[m
[32m+[m		[32mreturn this.pushStack( winnow( this, selector || [], false ) );[m
[32m+[m	[32m},[m
[32m+[m	[32mnot: function( selector ) {[m
[32m+[m		[32mreturn this.pushStack( winnow( this, selector || [], true ) );[m
[32m+[m	[32m},[m
[32m+[m	[32mis: function( selector ) {[m
[32m+[m		[32mreturn !!winnow([m
[32m+[m			[32mthis,[m
[32m+[m
[32m+[m			[32m// If this is a positional/relative selector, check membership in the returned set[m
[32m+[m			[32m// so $("p:first").is("p:last") won't return true for a doc with two "p".[m
[32m+[m			[32mtypeof selector === "string" && rneedsContext.test( selector ) ?[m
[32m+[m				[32mjQuery( selector ) :[m
[32m+[m				[32mselector || [],[m
[32m+[m			[32mfalse[m
[32m+[m		[32m).length;[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m[32m// Initialize a jQuery object[m
[32m+[m
[32m+[m
[32m+[m[32m// A central reference to the root jQuery(document)[m
[32m+[m[32mvar rootjQuery,[m
[32m+[m
[32m+[m	[32m// A simple way to check for HTML strings[m
[32m+[m	[32m// Prioritize #id over <tag> to avoid XSS via location.hash (#9521)[m
[32m+[m	[32m// Strict HTML recognition (#11290: must start with <)[m
[32m+[m	[32m// Shortcut simple #id case for speed[m
[32m+[m	[32mrquickExpr = /^(?:\s*(<[\w\W]+>)[^>]*|#([\w-]+))$/,[m
[32m+[m
[32m+[m	[32minit = jQuery.fn.init = function( selector, context, root ) {[m
[32m+[m		[32mvar match, elem;[m
[32m+[m
[32m+[m		[32m// HANDLE: $(""), $(null), $(undefined), $(false)[m
[32m+[m		[32mif ( !selector ) {[m
[32m+[m			[32mreturn this;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Method init() accepts an alternate rootjQuery[m
[32m+[m		[32m// so migrate can support jQuery.sub (gh-2101)[m
[32m+[m		[32mroot = root || rootjQuery;[m
[32m+[m
[32m+[m		[32m// Handle HTML strings[m
[32m+[m		[32mif ( typeof selector === "string" ) {[m
[32m+[m			[32mif ( selector[ 0 ] === "<" &&[m
[32m+[m				[32mselector[ selector.length - 1 ] === ">" &&[m
[32m+[m				[32mselector.length >= 3 ) {[m
[32m+[m
[32m+[m				[32m// Assume that strings that start and end with <> are HTML and skip the regex check[m
[32m+[m				[32mmatch = [ null, selector, null ];[m
[32m+[m
[32m+[m			[32m} else {[m
[32m+[m				[32mmatch = rquickExpr.exec( selector );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Match html or make sure no context is specified for #id[m
[32m+[m			[32mif ( match && ( match[ 1 ] || !context ) ) {[m
[32m+[m
[32m+[m				[32m// HANDLE: $(html) -> $(array)[m
[32m+[m				[32mif ( match[ 1 ] ) {[m
[32m+[m					[32mcontext = context instanceof jQuery ? context[ 0 ] : context;[m
[32m+[m
[32m+[m					[32m// Option to run scripts is true for back-compat[m
[32m+[m					[32m// Intentionally let the error be thrown if parseHTML is not present[m
[32m+[m					[32mjQuery.merge( this, jQuery.parseHTML([m
[32m+[m						[32mmatch[ 1 ],[m
[32m+[m						[32mcontext && context.nodeType ? context.ownerDocument || context : document,[m
[32m+[m						[32mtrue[m
[32m+[m					[32m) );[m
[32m+[m
[32m+[m					[32m// HANDLE: $(html, props)[m
[32m+[m					[32mif ( rsingleTag.test( match[ 1 ] ) && jQuery.isPlainObject( context ) ) {[m
[32m+[m						[32mfor ( match in context ) {[m
[32m+[m
[32m+[m							[32m// Properties of context are called as methods if possible[m
[32m+[m							[32mif ( jQuery.isFunction( this[ match ] ) ) {[m
[32m+[m								[32mthis[ match ]( context[ match ] );[m
[32m+[m
[32m+[m							[32m// ...and otherwise set as attributes[m
[32m+[m							[32m} else {[m
[32m+[m								[32mthis.attr( match, context[ match ] );[m
[32m+[m							[32m}[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32mreturn this;[m
[32m+[m
[32m+[m				[32m// HANDLE: $(#id)[m
[32m+[m				[32m} else {[m
[32m+[m					[32melem = document.getElementById( match[ 2 ] );[m
[32m+[m
[32m+[m					[32mif ( elem ) {[m
[32m+[m
[32m+[m						[32m// Inject the element directly into the jQuery object[m
[32m+[m						[32mthis[ 0 ] = elem;[m
[32m+[m						[32mthis.length = 1;[m
[32m+[m					[32m}[m
[32m+[m					[32mreturn this;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m// HANDLE: $(expr, $(...))[m
[32m+[m			[32m} else if ( !context || context.jquery ) {[m
[32m+[m				[32mreturn ( context || root ).find( selector );[m
[32m+[m
[32m+[m			[32m// HANDLE: $(expr, context)[m
[32m+[m			[32m// (which is just equivalent to: $(context).find(expr)[m
[32m+[m			[32m} else {[m
[32m+[m				[32mreturn this.constructor( context ).find( selector );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m// HANDLE: $(DOMElement)[m
[32m+[m		[32m} else if ( selector.nodeType ) {[m
[32m+[m			[32mthis[ 0 ] = selector;[m
[32m+[m			[32mthis.length = 1;[m
[32m+[m			[32mreturn this;[m
[32m+[m
[32m+[m		[32m// HANDLE: $(function)[m
[32m+[m		[32m// Shortcut for document ready[m
[32m+[m		[32m} else if ( jQuery.isFunction( selector ) ) {[m
[32m+[m			[32mreturn root.ready !== undefined ?[m
[32m+[m				[32mroot.ready( selector ) :[m
[32m+[m
[32m+[m				[32m// Execute immediately if ready is not present[m
[32m+[m				[32mselector( jQuery );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn jQuery.makeArray( selector, this );[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m[32m// Give the init function the jQuery prototype for later instantiation[m
[32m+[m[32minit.prototype = jQuery.fn;[m
[32m+[m
[32m+[m[32m// Initialize central reference[m
[32m+[m[32mrootjQuery = jQuery( document );[m
[32m+[m
[32m+[m
[32m+[m[32mvar rparentsprev = /^(?:parents|prev(?:Until|All))/,[m
[32m+[m
[32m+[m	[32m// Methods guaranteed to produce a unique set when starting from a unique set[m
[32m+[m	[32mguaranteedUnique = {[m
[32m+[m		[32mchildren: true,[m
[32m+[m		[32mcontents: true,[m
[32m+[m		[32mnext: true,[m
[32m+[m		[32mprev: true[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m	[32mhas: function( target ) {[m
[32m+[m		[32mvar targets = jQuery( target, this ),[m
[32m+[m			[32ml = targets.length;[m
[32m+[m
[32m+[m		[32mreturn this.filter( function() {[m
[32m+[m			[32mvar i = 0;[m
[32m+[m			[32mfor ( ; i < l; i++ ) {[m
[32m+[m				[32mif ( jQuery.contains( this, targets[ i ] ) ) {[m
[32m+[m					[32mreturn true;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mclosest: function( selectors, context ) {[m
[32m+[m		[32mvar cur,[m
[32m+[m			[32mi = 0,[m
[32m+[m			[32ml = this.length,[m
[32m+[m			[32mmatched = [],[m
[32m+[m			[32mtargets = typeof selectors !== "string" && jQuery( selectors );[m
[32m+[m
[32m+[m		[32m// Positional selectors never match, since there's no _selection_ context[m
[32m+[m		[32mif ( !rneedsContext.test( selectors ) ) {[m
[32m+[m			[32mfor ( ; i < l; i++ ) {[m
[32m+[m				[32mfor ( cur = this[ i ]; cur && cur !== context; cur = cur.parentNode ) {[m
[32m+[m
[32m+[m					[32m// Always skip document fragments[m
[32m+[m					[32mif ( cur.nodeType < 11 && ( targets ?[m
[32m+[m						[32mtargets.index( cur ) > -1 :[m
[32m+[m
[32m+[m						[32m// Don't pass non-elements to Sizzle[m
[32m+[m						[32mcur.nodeType === 1 &&[m
[32m+[m							[32mjQuery.find.matchesSelector( cur, selectors ) ) ) {[m
[32m+[m
[32m+[m						[32mmatched.push( cur );[m
[32m+[m						[32mbreak;[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn this.pushStack( matched.length > 1 ? jQuery.uniqueSort( matched ) : matched );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Determine the position of an element within the set[m
[32m+[m	[32mindex: function( elem ) {[m
[32m+[m
[32m+[m		[32m// No argument, return index in parent[m
[32m+[m		[32mif ( !elem ) {[m
[32m+[m			[32mreturn ( this[ 0 ] && this[ 0 ].parentNode ) ? this.first().prevAll().length : -1;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Index in selector[m
[32m+[m		[32mif ( typeof elem === "string" ) {[m
[32m+[m			[32mreturn indexOf.call( jQuery( elem ), this[ 0 ] );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Locate the position of the desired element[m
[32m+[m		[32mreturn indexOf.call( this,[m
[32m+[m
[32m+[m			[32m// If it receives a jQuery object, the first element is used[m
[32m+[m			[32melem.jquery ? elem[ 0 ] : elem[m
[32m+[m		[32m);[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32madd: function( selector, context ) {[m
[32m+[m		[32mreturn this.pushStack([m
[32m+[m			[32mjQuery.uniqueSort([m
[32m+[m				[32mjQuery.merge( this.get(), jQuery( selector, context ) )[m
[32m+[m			[32m)[m
[32m+[m		[32m);[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32maddBack: function( selector ) {[m
[32m+[m		[32mreturn this.add( selector == null ?[m
[32m+[m			[32mthis.prevObject : this.prevObject.filter( selector )[m
[32m+[m		[32m);[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mfunction sibling( cur, dir ) {[m
[32m+[m	[32mwhile ( ( cur = cur[ dir ] ) && cur.nodeType !== 1 ) {}[m
[32m+[m	[32mreturn cur;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mjQuery.each( {[m
[32m+[m	[32mparent: function( elem ) {[m
[32m+[m		[32mvar parent = elem.parentNode;[m
[32m+[m		[32mreturn parent && parent.nodeType !== 11 ? parent : null;[m
[32m+[m	[32m},[m
[32m+[m	[32mparents: function( elem ) {[m
[32m+[m		[32mreturn dir( elem, "parentNode" );[m
[32m+[m	[32m},[m
[32m+[m	[32mparentsUntil: function( elem, i, until ) {[m
[32m+[m		[32mreturn dir( elem, "parentNode", until );[m
[32m+[m	[32m},[m
[32m+[m	[32mnext: function( elem ) {[m
[32m+[m		[32mreturn sibling( elem, "nextSibling" );[m
[32m+[m	[32m},[m
[32m+[m	[32mprev: function( elem ) {[m
[32m+[m		[32mreturn sibling( elem, "previousSibling" );[m
[32m+[m	[32m},[m
[32m+[m	[32mnextAll: function( elem ) {[m
[32m+[m		[32mreturn dir( elem, "nextSibling" );[m
[32m+[m	[32m},[m
[32m+[m	[32mprevAll: function( elem ) {[m
[32m+[m		[32mreturn dir( elem, "previousSibling" );[m
[32m+[m	[32m},[m
[32m+[m	[32mnextUntil: function( elem, i, until ) {[m
[32m+[m		[32mreturn dir( elem, "nextSibling", until );[m
[32m+[m	[32m},[m
[32m+[m	[32mprevUntil: function( elem, i, until ) {[m
[32m+[m		[32mreturn dir( elem, "previousSibling", until );[m
[32m+[m	[32m},[m
[32m+[m	[32msiblings: function( elem ) {[m
[32m+[m		[32mreturn siblings( ( elem.parentNode || {} ).firstChild, elem );[m
[32m+[m	[32m},[m
[32m+[m	[32mchildren: function( elem ) {[m
[32m+[m		[32mreturn siblings( elem.firstChild );[m
[32m+[m	[32m},[m
[32m+[m	[32mcontents: function( elem ) {[m
[32m+[m		[32mreturn elem.contentDocument || jQuery.merge( [], elem.childNodes );[m
[32m+[m	[32m}[m
[32m+[m[32m}, function( name, fn ) {[m
[32m+[m	[32mjQuery.fn[ name ] = function( until, selector ) {[m
[32m+[m		[32mvar matched = jQuery.map( this, fn, until );[m
[32m+[m
[32m+[m		[32mif ( name.slice( -5 ) !== "Until" ) {[m
[32m+[m			[32mselector = until;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( selector && typeof selector === "string" ) {[m
[32m+[m			[32mmatched = jQuery.filter( selector, matched );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( this.length > 1 ) {[m
[32m+[m
[32m+[m			[32m// Remove duplicates[m
[32m+[m			[32mif ( !guaranteedUnique[ name ] ) {[m
[32m+[m				[32mjQuery.uniqueSort( matched );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Reverse order for parents* and prev-derivatives[m
[32m+[m			[32mif ( rparentsprev.test( name ) ) {[m
[32m+[m				[32mmatched.reverse();[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn this.pushStack( matched );[m
[32m+[m	[32m};[m
[32m+[m[32m} );[m
[32m+[m[32mvar rnotwhite = ( /\S+/g );[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m// Convert String-formatted options into Object-formatted ones[m
[32m+[m[32mfunction createOptions( options ) {[m
[32m+[m	[32mvar object = {};[m
[32m+[m	[32mjQuery.each( options.match( rnotwhite ) || [], function( _, flag ) {[m
[32m+[m		[32mobject[ flag ] = true;[m
[32m+[m	[32m} );[m
[32m+[m	[32mreturn object;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m * Create a callback list using the following parameters:[m
[32m+[m[32m *[m
[32m+[m[32m *	options: an optional list of space-separated options that will change how[m
[32m+[m[32m *			the callback list behaves or a more traditional option object[m
[32m+[m[32m *[m
[32m+[m[32m * By default a callback list will act like an event callback list and can be[m
[32m+[m[32m * "fired" multiple times.[m
[32m+[m[32m *[m
[32m+[m[32m * Possible options:[m
[32m+[m[32m *[m
[32m+[m[32m *	once:			will ensure the callback list can only be fired once (like a Deferred)[m
[32m+[m[32m *[m
[32m+[m[32m *	memory:			will keep track of previous values and will call any callback added[m
[32m+[m[32m *					after the list has been fired right away with the latest "memorized"[m
[32m+[m[32m *					values (like a Deferred)[m
[32m+[m[32m *[m
[32m+[m[32m *	unique:			will ensure a callback can only be added once (no duplicate in the list)[m
[32m+[m[32m *[m
[32m+[m[32m *	stopOnFalse:	interrupt callings when a callback returns false[m
[32m+[m[32m *[m
[32m+[m[32m */[m
[32m+[m[32mjQuery.Callbacks = function( options ) {[m
[32m+[m
[32m+[m	[32m// Convert options from String-formatted to Object-formatted if needed[m
[32m+[m	[32m// (we check in cache first)[m
[32m+[m	[32moptions = typeof options === "string" ?[m
[32m+[m		[32mcreateOptions( options ) :[m
[32m+[m		[32mjQuery.extend( {}, options );[m
[32m+[m
[32m+[m	[32mvar // Flag to know if list is currently firing[m
[32m+[m		[32mfiring,[m
[32m+[m
[32m+[m		[32m// Last fire value for non-forgettable lists[m
[32m+[m		[32mmemory,[m
[32m+[m
[32m+[m		[32m// Flag to know if list was already fired[m
[32m+[m		[32mfired,[m
[32m+[m
[32m+[m		[32m// Flag to prevent firing[m
[32m+[m		[32mlocked,[m
[32m+[m
[32m+[m		[32m// Actual callback list[m
[32m+[m		[32mlist = [],[m
[32m+[m
[32m+[m		[32m// Queue of execution data for repeatable lists[m
[32m+[m		[32mqueue = [],[m
[32m+[m
[32m+[m		[32m// Index of currently firing callback (modified by add/remove as needed)[m
[32m+[m		[32mfiringIndex = -1,[m
[32m+[m
[32m+[m		[32m// Fire callbacks[m
[32m+[m		[32mfire = function() {[m
[32m+[m
[32m+[m			[32m// Enforce single-firing[m
[32m+[m			[32mlocked = options.once;[m
[32m+[m
[32m+[m			[32m// Execute callbacks for all pending executions,[m
[32m+[m			[32m// respecting firingIndex overrides and runtime changes[m
[32m+[m			[32mfired = firing = true;[m
[32m+[m			[32mfor ( ; queue.length; firingIndex = -1 ) {[m
[32m+[m				[32mmemory = queue.shift();[m
[32m+[m				[32mwhile ( ++firingIndex < list.length ) {[m
[32m+[m
[32m+[m					[32m// Run callback and check for early termination[m
[32m+[m					[32mif ( list[ firingIndex ].apply( memory[ 0 ], memory[ 1 ] ) === false &&[m
[32m+[m						[32moptions.stopOnFalse ) {[m
[32m+[m
[32m+[m						[32m// Jump to end and forget the data so .add doesn't re-fire[m
[32m+[m						[32mfiringIndex = list.length;[m
[32m+[m						[32mmemory = false;[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Forget the data if we're done with it[m
[32m+[m			[32mif ( !options.memory ) {[m
[32m+[m				[32mmemory = false;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mfiring = false;[m
[32m+[m
[32m+[m			[32m// Clean up if we're done firing for good[m
[32m+[m			[32mif ( locked ) {[m
[32m+[m
[32m+[m				[32m// Keep an empty list if we have data for future add calls[m
[32m+[m				[32mif ( memory ) {[m
[32m+[m					[32mlist = [];[m
[32m+[m
[32m+[m				[32m// Otherwise, this object is spent[m
[32m+[m				[32m} else {[m
[32m+[m					[32mlist = "";[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Actual Callbacks object[m
[32m+[m		[32mself = {[m
[32m+[m
[32m+[m			[32m// Add a callback or a collection of callbacks to the list[m
[32m+[m			[32madd: function() {[m
[32m+[m				[32mif ( list ) {[m
[32m+[m
[32m+[m					[32m// If we have memory from a past run, we should fire after adding[m
[32m+[m					[32mif ( memory && !firing ) {[m
[32m+[m						[32mfiringIndex = list.length - 1;[m
[32m+[m						[32mqueue.push( memory );[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m( function add( args ) {[m
[32m+[m						[32mjQuery.each( args, function( _, arg ) {[m
[32m+[m							[32mif ( jQuery.isFunction( arg ) ) {[m
[32m+[m								[32mif ( !options.unique || !self.has( arg ) ) {[m
[32m+[m									[32mlist.push( arg );[m
[32m+[m								[32m}[m
[32m+[m							[32m} else if ( arg && arg.length && jQuery.type( arg ) !== "string" ) {[m
[32m+[m
[32m+[m								[32m// Inspect recursively[m
[32m+[m								[32madd( arg );[m
[32m+[m							[32m}[m
[32m+[m						[32m} );[m
[32m+[m					[32m} )( arguments );[m
[32m+[m
[32m+[m					[32mif ( memory && !firing ) {[m
[32m+[m						[32mfire();[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m				[32mreturn this;[m
[32m+[m			[32m},[m
[32m+[m
[32m+[m			[32m// Remove a callback from the list[m
[32m+[m			[32mremove: function() {[m
[32m+[m				[32mjQuery.each( arguments, function( _, arg ) {[m
[32m+[m					[32mvar index;[m
[32m+[m					[32mwhile ( ( index = jQuery.inArray( arg, list, index ) ) > -1 ) {[m
[32m+[m						[32mlist.splice( index, 1 );[m
[32m+[m
[32m+[m						[32m// Handle firing indexes[m
[32m+[m						[32mif ( index <= firingIndex ) {[m
[32m+[m							[32mfiringIndex--;[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m} );[m
[32m+[m				[32mreturn this;[m
[32m+[m			[32m},[m
[32m+[m
[32m+[m			[32m// Check if a given callback is in the list.[m
[32m+[m			[32m// If no argument is given, return whether or not list has callbacks attached.[m
[32m+[m			[32mhas: function( fn ) {[m
[32m+[m				[32mreturn fn ?[m
[32m+[m					[32mjQuery.inArray( fn, list ) > -1 :[m
[32m+[m					[32mlist.length > 0;[m
[32m+[m			[32m},[m
[32m+[m
[32m+[m			[32m// Remove all callbacks from the list[m
[32m+[m			[32mempty: function() {[m
[32m+[m				[32mif ( list ) {[m
[32m+[m					[32mlist = [];[m
[32m+[m				[32m}[m
[32m+[m				[32mreturn this;[m
[32m+[m			[32m},[m
[32m+[m
[32m+[m			[32m// Disable .fire and .add[m
[32m+[m			[32m// Abort any current/pending executions[m
[32m+[m			[32m// Clear all callbacks and values[m
[32m+[m			[32mdisable: function() {[m
[32m+[m				[32mlocked = queue = [];[m
[32m+[m				[32mlist = memory = "";[m
[32m+[m				[32mreturn this;[m
[32m+[m			[32m},[m
[32m+[m			[32mdisabled: function() {[m
[32m+[m				[32mreturn !list;[m
[32m+[m			[32m},[m
[32m+[m
[32m+[m			[32m// Disable .fire[m
[32m+[m			[32m// Also disable .add unless we have memory (since it would have no effect)[m
[32m+[m			[32m// Abort any pending executions[m
[32m+[m			[32mlock: function() {[m
[32m+[m				[32mlocked = queue = [];[m
[32m+[m				[32mif ( !memory && !firing ) {[m
[32m+[m					[32mlist = memory = "";[m
[32m+[m				[32m}[m
[32m+[m				[32mreturn this;[m
[32m+[m			[32m},[m
[32m+[m			[32mlocked: function() {[m
[32m+[m				[32mreturn !!locked;[m
[32m+[m			[32m},[m
[32m+[m
[32m+[m			[32m// Call all callbacks with the given context and arguments[m
[32m+[m			[32mfireWith: function( context, args ) {[m
[32m+[m				[32mif ( !locked ) {[m
[32m+[m					[32margs = args || [];[m
[32m+[m					[32margs = [ context, args.slice ? args.slice() : args ];[m
[32m+[m					[32mqueue.push( args );[m
[32m+[m					[32mif ( !firing ) {[m
[32m+[m						[32mfire();[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m				[32mreturn this;[m
[32m+[m			[32m},[m
[32m+[m
[32m+[m			[32m// Call all the callbacks with the given arguments[m
[32m+[m			[32mfire: function() {[m
[32m+[m				[32mself.fireWith( this, arguments );[m
[32m+[m				[32mreturn this;[m
[32m+[m			[32m},[m
[32m+[m
[32m+[m			[32m// To know if the callbacks have already been called at least once[m
[32m+[m			[32mfired: function() {[m
[32m+[m				[32mreturn !!fired;[m
[32m+[m			[32m}[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m	[32mreturn self;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m[32mfunction Identity( v ) {[m
[32m+[m	[32mreturn v;[m
[32m+[m[32m}[m
[32m+[m[32mfunction Thrower( ex ) {[m
[32m+[m	[32mthrow ex;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction adoptValue( value, resolve, reject ) {[m
[32m+[m	[32mvar method;[m
[32m+[m
[32m+[m	[32mtry {[m
[32m+[m
[32m+[m		[32m// Check for promise aspect first to privilege synchronous behavior[m
[32m+[m		[32mif ( value && jQuery.isFunction( ( method = value.promise ) ) ) {[m
[32m+[m			[32mmethod.call( value ).done( resolve ).fail( reject );[m
[32m+[m
[32m+[m		[32m// Other thenables[m
[32m+[m		[32m} else if ( value && jQuery.isFunction( ( method = value.then ) ) ) {[m
[32m+[m			[32mmethod.call( value, resolve, reject );[m
[32m+[m
[32m+[m		[32m// Other non-thenables[m
[32m+[m		[32m} else {[m
[32m+[m
[32m+[m			[32m// Support: Android 4.0 only[m
[32m+[m			[32m// Strict mode functions invoked without .call/.apply get global-object context[m
[32m+[m			[32mresolve.call( undefined, value );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m// For Promises/A+, convert exceptions into rejections[m
[32m+[m	[32m// Since jQuery.when doesn't unwrap thenables, we can skip the extra checks appearing in[m
[32m+[m	[32m// Deferred#then to conditionally suppress rejection.[m
[32m+[m	[32m} catch ( value ) {[m
[32m+[m
[32m+[m		[32m// Support: Android 4.0 only[m
[32m+[m		[32m// Strict mode functions invoked without .call/.apply get global-object context[m
[32m+[m		[32mreject.call( undefined, value );[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mjQuery.extend( {[m
[32m+[m
[32m+[m	[32mDeferred: function( func ) {[m
[32m+[m		[32mvar tuples = [[m
[32m+[m
[32m+[m				[32m// action, add listener, callbacks,[m
[32m+[m				[32m// ... .then handlers, argument index, [final state][m
[32m+[m				[32m[ "notify", "progress", jQuery.Callbacks( "memory" ),[m
[32m+[m					[32mjQuery.Callbacks( "memory" ), 2 ],[m
[32m+[m				[32m[ "resolve", "done", jQuery.Callbacks( "once memory" ),[m
[32m+[m					[32mjQuery.Callbacks( "once memory" ), 0, "resolved" ],[m
[32m+[m				[32m[ "reject", "fail", jQuery.Callbacks( "once memory" ),[m
[32m+[m					[32mjQuery.Callbacks( "once memory" ), 1, "rejected" ][m
[32m+[m			[32m],[m
[32m+[m			[32mstate = "pending",[m
[32m+[m			[32mpromise = {[m
[32m+[m				[32mstate: function() {[m
[32m+[m					[32mreturn state;[m
[32m+[m				[32m},[m
[32m+[m				[32malways: function() {[m
[32m+[m					[32mdeferred.done( arguments ).fail( arguments );[m
[32m+[m					[32mreturn this;[m
[32m+[m				[32m},[m
[32m+[m				[32m"catch": function( fn ) {[m
[32m+[m					[32mreturn promise.then( null, fn );[m
[32m+[m				[32m},[m
[32m+[m
[32m+[m				[32m// Keep pipe for back-compat[m
[32m+[m				[32mpipe: function( /* fnDone, fnFail, fnProgress */ ) {[m
[32m+[m					[32mvar fns = arguments;[m
[32m+[m
[32m+[m					[32mreturn jQuery.Deferred( function( newDefer ) {[m
[32m+[m						[32mjQuery.each( tuples, function( i, tuple ) {[m
[32m+[m
[32m+[m							[32m// Map tuples (progress, done, fail) to arguments (done, fail, progress)[m
[32m+[m							[32mvar fn = jQuery.isFunction( fns[ tuple[ 4 ] ] ) && fns[ tuple[ 4 ] ];[m
[32m+[m
[32m+[m							[32m// deferred.progress(function() { bind to newDefer or newDefer.notify })[m
[32m+[m							[32m// deferred.done(function() { bind to newDefer or newDefer.resolve })[m
[32m+[m							[32m// deferred.fail(function() { bind to newDefer or newDefer.reject })[m
[32m+[m							[32mdeferred[ tuple[ 1 ] ]( function() {[m
[32m+[m								[32mvar returned = fn && fn.apply( this, arguments );[m
[32m+[m								[32mif ( returned && jQuery.isFunction( returned.promise ) ) {[m
[32m+[m									[32mreturned.promise()[m
[32m+[m										[32m.progress( newDefer.notify )[m
[32m+[m										[32m.done( newDefer.resolve )[m
[32m+[m										[32m.fail( newDefer.reject );[m
[32m+[m								[32m} else {[m
[32m+[m									[32mnewDefer[ tuple[ 0 ] + "With" ]([m
[32m+[m										[32mthis,[m
[32m+[m										[32mfn ? [ returned ] : arguments[m
[32m+[m									[32m);[m
[32m+[m								[32m}[m
[32m+[m							[32m} );[m
[32m+[m						[32m} );[m
[32m+[m						[32mfns = null;[m
[32m+[m					[32m} ).promise();[m
[32m+[m				[32m},[m
[32m+[m				[32mthen: function( onFulfilled, onRejected, onProgress ) {[m
[32m+[m					[32mvar maxDepth = 0;[m
[32m+[m					[32mfunction resolve( depth, deferred, handler, special ) {[m
[32m+[m						[32mreturn function() {[m
[32m+[m							[32mvar that = this,[m
[32m+[m								[32margs = arguments,[m
[32m+[m								[32mmightThrow = function() {[m
[32m+[m									[32mvar returned, then;[m
[32m+[m
[32m+[m									[32m// Support: Promises/A+ section 2.3.3.3.3[m
[32m+[m									[32m// https://promisesaplus.com/#point-59[m
[32m+[m									[32m// Ignore double-resolution attempts[m
[32m+[m									[32mif ( depth < maxDepth ) {[m
[32m+[m										[32mreturn;[m
[32m+[m									[32m}[m
[32m+[m
[32m+[m									[32mreturned = handler.apply( that, args );[m
[32m+[m
[32m+[m									[32m// Support: Promises/A+ section 2.3.1[m
[32m+[m									[32m// https://promisesaplus.com/#point-48[m
[32m+[m									[32mif ( returned === deferred.promise() ) {[m
[32m+[m										[32mthrow new TypeError( "Thenable self-resolution" );[m
[32m+[m									[32m}[m
[32m+[m
[32m+[m									[32m// Support: Promises/A+ sections 2.3.3.1, 3.5[m
[32m+[m									[32m// https://promisesaplus.com/#point-54[m
[32m+[m									[32m// https://promisesaplus.com/#point-75[m
[32m+[m									[32m// Retrieve `then` only once[m
[32m+[m									[32mthen = returned &&[m
[32m+[m
[32m+[m										[32m// Support: Promises/A+ section 2.3.4[m
[32m+[m										[32m// https://promisesaplus.com/#point-64[m
[32m+[m										[32m// Only check objects and functions for thenability[m
[32m+[m										[32m( typeof returned === "object" ||[m
[32m+[m											[32mtypeof returned === "function" ) &&[m
[32m+[m										[32mreturned.then;[m
[32m+[m
[32m+[m									[32m// Handle a returned thenable[m
[32m+[m									[32mif ( jQuery.isFunction( then ) ) {[m
[32m+[m
[32m+[m										[32m// Special processors (notify) just wait for resolution[m
[32m+[m										[32mif ( special ) {[m
[32m+[m											[32mthen.call([m
[32m+[m												[32mreturned,[m
[32m+[m												[32mresolve( maxDepth, deferred, Identity, special ),[m
[32m+[m												[32mresolve( maxDepth, deferred, Thrower, special )[m
[32m+[m											[32m);[m
[32m+[m
[32m+[m										[32m// Normal processors (resolve) also hook into progress[m
[32m+[m										[32m} else {[m
[32m+[m
[32m+[m											[32m// ...and disregard older resolution values[m
[32m+[m											[32mmaxDepth++;[m
[32m+[m
[32m+[m											[32mthen.call([m
[32m+[m												[32mreturned,[m
[32m+[m												[32mresolve( maxDepth, deferred, Identity, special ),[m
[32m+[m												[32mresolve( maxDepth, deferred, Thrower, special ),[m
[32m+[m												[32mresolve( maxDepth, deferred, Identity,[m
[32m+[m													[32mdeferred.notifyWith )[m
[32m+[m											[32m);[m
[32m+[m										[32m}[m
[32m+[m
[32m+[m									[32m// Handle all other returned values[m
[32m+[m									[32m} else {[m
[32m+[m
[32m+[m										[32m// Only substitute handlers pass on context[m
[32m+[m										[32m// and multiple values (non-spec behavior)[m
[32m+[m										[32mif ( handler !== Identity ) {[m
[32m+[m											[32mthat = undefined;[m
[32m+[m											[32margs = [ returned ];[m
[32m+[m										[32m}[m
[32m+[m
[32m+[m										[32m// Process the value(s)[m
[32m+[m										[32m// Default process is resolve[m
[32m+[m										[32m( special || deferred.resolveWith )( that, args );[m
[32m+[m									[32m}[m
[32m+[m								[32m},[m
[32m+[m
[32m+[m								[32m// Only normal processors (resolve) catch and reject exceptions[m
[32m+[m								[32mprocess = special ?[m
[32m+[m									[32mmightThrow :[m
[32m+[m									[32mfunction() {[m
[32m+[m										[32mtry {[m
[32m+[m											[32mmightThrow();[m
[32m+[m										[32m} catch ( e ) {[m
[32m+[m
[32m+[m											[32mif ( jQuery.Deferred.exceptionHook ) {[m
[32m+[m												[32mjQuery.Deferred.exceptionHook( e,[m
[32m+[m													[32mprocess.stackTrace );[m
[32m+[m											[32m}[m
[32m+[m
[32m+[m											[32m// Support: Promises/A+ section 2.3.3.3.4.1[m
[32m+[m											[32m// https://promisesaplus.com/#point-61[m
[32m+[m											[32m// Ignore post-resolution exceptions[m
[32m+[m											[32mif ( depth + 1 >= maxDepth ) {[m
[32m+[m
[32m+[m												[32m// Only substitute handlers pass on context[m
[32m+[m												[32m// and multiple values (non-spec behavior)[m
[32m+[m												[32mif ( handler !== Thrower ) {[m
[32m+[m													[32mthat = undefined;[m
[32m+[m													[32margs = [ e ];[m
[32m+[m												[32m}[m
[32m+[m
[32m+[m												[32mdeferred.rejectWith( that, args );[m
[32m+[m											[32m}[m
[32m+[m										[32m}[m
[32m+[m									[32m};[m
[32m+[m
[32m+[m							[32m// Support: Promises/A+ section 2.3.3.3.1[m
[32m+[m							[32m// https://promisesaplus.com/#point-57[m
[32m+[m							[32m// Re-resolve promises immediately to dodge false rejection from[m
[32m+[m							[32m// subsequent errors[m
[32m+[m							[32mif ( depth ) {[m
[32m+[m								[32mprocess();[m
[32m+[m							[32m} else {[m
[32m+[m
[32m+[m								[32m// Call an optional hook to record the stack, in case of exception[m
[32m+[m								[32m// since it's otherwise lost when execution goes async[m
[32m+[m								[32mif ( jQuery.Deferred.getStackHook ) {[m
[32m+[m									[32mprocess.stackTrace = jQuery.Deferred.getStackHook();[m
[32m+[m								[32m}[m
[32m+[m								[32mwindow.setTimeout( process );[m
[32m+[m							[32m}[m
[32m+[m						[32m};[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32mreturn jQuery.Deferred( function( newDefer ) {[m
[32m+[m
[32m+[m						[32m// progress_handlers.add( ... )[m
[32m+[m						[32mtuples[ 0 ][ 3 ].add([m
[32m+[m							[32mresolve([m
[32m+[m								[32m0,[m
[32m+[m								[32mnewDefer,[m
[32m+[m								[32mjQuery.isFunction( onProgress ) ?[m
[32m+[m									[32monProgress :[m
[32m+[m									[32mIdentity,[m
[32m+[m								[32mnewDefer.notifyWith[m
[32m+[m							[32m)[m
[32m+[m						[32m);[m
[32m+[m
[32m+[m						[32m// fulfilled_handlers.add( ... )[m
[32m+[m						[32mtuples[ 1 ][ 3 ].add([m
[32m+[m							[32mresolve([m
[32m+[m								[32m0,[m
[32m+[m								[32mnewDefer,[m
[32m+[m								[32mjQuery.isFunction( onFulfilled ) ?[m
[32m+[m									[32monFulfilled :[m
[32m+[m									[32mIdentity[m
[32m+[m							[32m)[m
[32m+[m						[32m);[m
[32m+[m
[32m+[m						[32m// rejected_handlers.add( ... )[m
[32m+[m						[32mtuples[ 2 ][ 3 ].add([m
[32m+[m							[32mresolve([m
[32m+[m								[32m0,[m
[32m+[m								[32mnewDefer,[m
[32m+[m								[32mjQuery.isFunction( onRejected ) ?[m
[32m+[m									[32monRejected :[m
[32m+[m									[32mThrower[m
[32m+[m							[32m)[m
[32m+[m						[32m);[m
[32m+[m					[32m} ).promise();[m
[32m+[m				[32m},[m
[32m+[m
[32m+[m				[32m// Get a promise for this deferred[m
[32m+[m				[32m// If obj is provided, the promise aspect is added to the object[m
[32m+[m				[32mpromise: function( obj ) {[m
[32m+[m					[32mreturn obj != null ? jQuery.extend( obj, promise ) : promise;[m
[32m+[m				[32m}[m
[32m+[m			[32m},[m
[32m+[m			[32mdeferred = {};[m
[32m+[m
[32m+[m		[32m// Add list-specific methods[m
[32m+[m		[32mjQuery.each( tuples, function( i, tuple ) {[m
[32m+[m			[32mvar list = tuple[ 2 ],[m
[32m+[m				[32mstateString = tuple[ 5 ];[m
[32m+[m
[32m+[m			[32m// promise.progress = list.add[m
[32m+[m			[32m// promise.done = list.add[m
[32m+[m			[32m// promise.fail = list.add[m
[32m+[m			[32mpromise[ tuple[ 1 ] ] = list.add;[m
[32m+[m
[32m+[m			[32m// Handle state[m
[32m+[m			[32mif ( stateString ) {[m
[32m+[m				[32mlist.add([m
[32m+[m					[32mfunction() {[m
[32m+[m
[32m+[m						[32m// state = "resolved" (i.e., fulfilled)[m
[32m+[m						[32m// state = "rejected"[m
[32m+[m						[32mstate = stateString;[m
[32m+[m					[32m},[m
[32m+[m
[32m+[m					[32m// rejected_callbacks.disable[m
[32m+[m					[32m// fulfilled_callbacks.disable[m
[32m+[m					[32mtuples[ 3 - i ][ 2 ].disable,[m
[32m+[m
[32m+[m					[32m// progress_callbacks.lock[m
[32m+[m					[32mtuples[ 0 ][ 2 ].lock[m
[32m+[m				[32m);[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// progress_handlers.fire[m
[32m+[m			[32m// fulfilled_handlers.fire[m
[32m+[m			[32m// rejected_handlers.fire[m
[32m+[m			[32mlist.add( tuple[ 3 ].fire );[m
[32m+[m
[32m+[m			[32m// deferred.notify = function() { deferred.notifyWith(...) }[m
[32m+[m			[32m// deferred.resolve = function() { deferred.resolveWith(...) }[m
[32m+[m			[32m// deferred.reject = function() { deferred.rejectWith(...) }[m
[32m+[m			[32mdeferred[ tuple[ 0 ] ] = function() {[m
[32m+[m				[32mdeferred[ tuple[ 0 ] + "With" ]( this === deferred ? undefined : this, arguments );[m
[32m+[m				[32mreturn this;[m
[32m+[m			[32m};[m
[32m+[m
[32m+[m			[32m// deferred.notifyWith = list.fireWith[m
[32m+[m			[32m// deferred.resolveWith = list.fireWith[m
[32m+[m			[32m// deferred.rejectWith = list.fireWith[m
[32m+[m			[32mdeferred[ tuple[ 0 ] + "With" ] = list.fireWith;[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m		[32m// Make the deferred a promise[m
[32m+[m		[32mpromise.promise( deferred );[m
[32m+[m
[32m+[m		[32m// Call given func if any[m
[32m+[m		[32mif ( func ) {[m
[32m+[m			[32mfunc.call( deferred, deferred );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// All done![m
[32m+[m		[32mreturn deferred;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Deferred helper[m
[32m+[m	[32mwhen: function( singleValue ) {[m
[32m+[m		[32mvar[m
[32m+[m
[32m+[m			[32m// count of uncompleted subordinates[m
[32m+[m			[32mremaining = arguments.length,[m
[32m+[m
[32m+[m			[32m// count of unprocessed arguments[m
[32m+[m			[32mi = remaining,[m
[32m+[m
[32m+[m			[32m// subordinate fulfillment data[m
[32m+[m			[32mresolveContexts = Array( i ),[m
[32m+[m			[32mresolveValues = slice.call( arguments ),[m
[32m+[m
[32m+[m			[32m// the master Deferred[m
[32m+[m			[32mmaster = jQuery.Deferred(),[m
[32m+[m
[32m+[m			[32m// subordinate callback factory[m
[32m+[m			[32mupdateFunc = function( i ) {[m
[32m+[m				[32mreturn function( value ) {[m
[32m+[m					[32mresolveContexts[ i ] = this;[m
[32m+[m					[32mresolveValues[ i ] = arguments.length > 1 ? slice.call( arguments ) : value;[m
[32m+[m					[32mif ( !( --remaining ) ) {[m
[32m+[m						[32mmaster.resolveWith( resolveContexts, resolveValues );[m
[32m+[m					[32m}[m
[32m+[m				[32m};[m
[32m+[m			[32m};[m
[32m+[m
[32m+[m		[32m// Single- and empty arguments are adopted like Promise.resolve[m
[32m+[m		[32mif ( remaining <= 1 ) {[m
[32m+[m			[32madoptValue( singleValue, master.done( updateFunc( i ) ).resolve, master.reject );[m
[32m+[m
[32m+[m			[32m// Use .then() to unwrap secondary thenables (cf. gh-3000)[m
[32m+[m			[32mif ( master.state() === "pending" ||[m
[32m+[m				[32mjQuery.isFunction( resolveValues[ i ] && resolveValues[ i ].then ) ) {[m
[32m+[m
[32m+[m				[32mreturn master.then();[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Multiple arguments are aggregated like Promise.all array elements[m
[32m+[m		[32mwhile ( i-- ) {[m
[32m+[m			[32madoptValue( resolveValues[ i ], updateFunc( i ), master.reject );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn master.promise();[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m[32m// These usually indicate a programmer mistake during development,[m
[32m+[m[32m// warn about them ASAP rather than swallowing them by default.[m
[32m+[m[32mvar rerrorNames = /^(Eval|Internal|Range|Reference|Syntax|Type|URI)Error$/;[m
[32m+[m
[32m+[m[32mjQuery.Deferred.exceptionHook = function( error, stack ) {[m
[32m+[m
[32m+[m	[32m// Support: IE 8 - 9 only[m
[32m+[m	[32m// Console exists when dev tools are open, which can happen at any time[m
[32m+[m	[32mif ( window.console && window.console.warn && error && rerrorNames.test( error.name ) ) {[m
[32m+[m		[32mwindow.console.warn( "jQuery.Deferred exception: " + error.message, error.stack, stack );[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mjQuery.readyException = function( error ) {[m
[32m+[m	[32mwindow.setTimeout( function() {[m
[32m+[m		[32mthrow error;[m
[32m+[m	[32m} );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m// The deferred used on DOM ready[m
[32m+[m[32mvar readyList = jQuery.Deferred();[m
[32m+[m
[32m+[m[32mjQuery.fn.ready = function( fn ) {[m
[32m+[m
[32m+[m	[32mreadyList[m
[32m+[m		[32m.then( fn )[m
[32m+[m
[32m+[m		[32m// Wrap jQuery.readyException in a function so that the lookup[m
[32m+[m		[32m// happens at the time of error handling instead of callback[m
[32m+[m		[32m// registration.[m
[32m+[m		[32m.catch( function( error ) {[m
[32m+[m			[32mjQuery.readyException( error );[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m	[32mreturn this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.extend( {[m
[32m+[m
[32m+[m	[32m// Is the DOM ready to be used? Set to true once it occurs.[m
[32m+[m	[32misReady: false,[m
[32m+[m
[32m+[m	[32m// A counter to track how many items to wait for before[m
[32m+[m	[32m// the ready event fires. See #6781[m
[32m+[m	[32mreadyWait: 1,[m
[32m+[m
[32m+[m	[32m// Hold (or release) the ready event[m
[32m+[m	[32mholdReady: function( hold ) {[m
[32m+[m		[32mif ( hold ) {[m
[32m+[m			[32mjQuery.readyWait++;[m
[32m+[m		[32m} else {[m
[32m+[m			[32mjQuery.ready( true );[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Handle when the DOM is ready[m
[32m+[m	[32mready: function( wait ) {[m
[32m+[m
[32m+[m		[32m// Abort if there are pending holds or we're already ready[m
[32m+[m		[32mif ( wait === true ? --jQuery.readyWait : jQuery.isReady ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Remember that the DOM is ready[m
[32m+[m		[32mjQuery.isReady = true;[m
[32m+[m
[32m+[m		[32m// If a normal DOM Ready event fired, decrement, and wait if need be[m
[32m+[m		[32mif ( wait !== true && --jQuery.readyWait > 0 ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// If there are functions bound, to execute[m
[32m+[m		[32mreadyList.resolveWith( document, [ jQuery ] );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.ready.then = readyList.then;[m
[32m+[m
[32m+[m[32m// The ready event handler and self cleanup method[m
[32m+[m[32mfunction completed() {[m
[32m+[m	[32mdocument.removeEventListener( "DOMContentLoaded", completed );[m
[32m+[m	[32mwindow.removeEventListener( "load", completed );[m
[32m+[m	[32mjQuery.ready();[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Catch cases where $(document).ready() is called[m
[32m+[m[32m// after the browser event has already occurred.[m
[32m+[m[32m// Support: IE <=9 - 10 only[m
[32m+[m[32m// Older IE sometimes signals "interactive" too soon[m
[32m+[m[32mif ( document.readyState === "complete" ||[m
[32m+[m	[32m( document.readyState !== "loading" && !document.documentElement.doScroll ) ) {[m
[32m+[m
[32m+[m	[32m// Handle it asynchronously to allow scripts the opportunity to delay ready[m
[32m+[m	[32mwindow.setTimeout( jQuery.ready );[m
[32m+[m
[32m+[m[32m} else {[m
[32m+[m
[32m+[m	[32m// Use the handy event callback[m
[32m+[m	[32mdocument.addEventListener( "DOMContentLoaded", completed );[m
[32m+[m
[32m+[m	[32m// A fallback to window.onload, that will always work[m
[32m+[m	[32mwindow.addEventListener( "load", completed );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m// Multifunctional method to get and set values of a collection[m
[32m+[m[32m// The value/s can optionally be executed if it's a function[m
[32m+[m[32mvar access = function( elems, fn, key, value, chainable, emptyGet, raw ) {[m
[32m+[m	[32mvar i = 0,[m
[32m+[m		[32mlen = elems.length,[m
[32m+[m		[32mbulk = key == null;[m
[32m+[m
[32m+[m	[32m// Sets many values[m
[32m+[m	[32mif ( jQuery.type( key ) === "object" ) {[m
[32m+[m		[32mchainable = true;[m
[32m+[m		[32mfor ( i in key ) {[m
[32m+[m			[32maccess( elems, fn, i, key[ i ], true, emptyGet, raw );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m// Sets one value[m
[32m+[m	[32m} else if ( value !== undefined ) {[m
[32m+[m		[32mchainable = true;[m
[32m+[m
[32m+[m		[32mif ( !jQuery.isFunction( value ) ) {[m
[32m+[m			[32mraw = true;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( bulk ) {[m
[32m+[m
[32m+[m			[32m// Bulk operations run against the entire set[m
[32m+[m			[32mif ( raw ) {[m
[32m+[m				[32mfn.call( elems, value );[m
[32m+[m				[32mfn = null;[m
[32m+[m
[32m+[m			[32m// ...except when executing function values[m
[32m+[m			[32m} else {[m
[32m+[m				[32mbulk = fn;[m
[32m+[m				[32mfn = function( elem, key, value ) {[m
[32m+[m					[32mreturn bulk.call( jQuery( elem ), value );[m
[32m+[m				[32m};[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( fn ) {[m
[32m+[m			[32mfor ( ; i < len; i++ ) {[m
[32m+[m				[32mfn([m
[32m+[m					[32melems[ i ], key, raw ?[m
[32m+[m					[32mvalue :[m
[32m+[m					[32mvalue.call( elems[ i ], i, fn( elems[ i ], key ) )[m
[32m+[m				[32m);[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn chainable ?[m
[32m+[m		[32melems :[m
[32m+[m
[32m+[m		[32m// Gets[m
[32m+[m		[32mbulk ?[m
[32m+[m			[32mfn.call( elems ) :[m
[32m+[m			[32mlen ? fn( elems[ 0 ], key ) : emptyGet;[m
[32m+[m[32m};[m
[32m+[m[32mvar acceptData = function( owner ) {[m
[32m+[m
[32m+[m	[32m// Accepts only:[m
[32m+[m	[32m//  - Node[m
[32m+[m	[32m//    - Node.ELEMENT_NODE[m
[32m+[m	[32m//    - Node.DOCUMENT_NODE[m
[32m+[m	[32m//  - Object[m
[32m+[m	[32m//    - Any[m
[32m+[m	[32mreturn owner.nodeType === 1 || owner.nodeType === 9 || !( +owner.nodeType );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mfunction Data() {[m
[32m+[m	[32mthis.expando = jQuery.expando + Data.uid++;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mData.uid = 1;[m
[32m+[m
[32m+[m[32mData.prototype = {[m
[32m+[m
[32m+[m	[32mcache: function( owner ) {[m
[32m+[m
[32m+[m		[32m// Check if the owner object already has a cache[m
[32m+[m		[32mvar value = owner[ this.expando ];[m
[32m+[m
[32m+[m		[32m// If not, create one[m
[32m+[m		[32mif ( !value ) {[m
[32m+[m			[32mvalue = {};[m
[32m+[m
[32m+[m			[32m// We can accept data for non-element nodes in modern browsers,[m
[32m+[m			[32m// but we should not, see #8335.[m
[32m+[m			[32m// Always return an empty object.[m
[32m+[m			[32mif ( acceptData( owner ) ) {[m
[32m+[m
[32m+[m				[32m// If it is a node unlikely to be stringify-ed or looped over[m
[32m+[m				[32m// use plain assignment[m
[32m+[m				[32mif ( owner.nodeType ) {[m
[32m+[m					[32mowner[ this.expando ] = value;[m
[32m+[m
[32m+[m				[32m// Otherwise secure it in a non-enumerable property[m
[32m+[m				[32m// configurable must be true to allow the property to be[m
[32m+[m				[32m// deleted when data is removed[m
[32m+[m				[32m} else {[m
[32m+[m					[32mObject.defineProperty( owner, this.expando, {[m
[32m+[m						[32mvalue: value,[m
[32m+[m						[32mconfigurable: true[m
[32m+[m					[32m} );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn value;[m
[32m+[m	[32m},[m
[32m+[m	[32mset: function( owner, data, value ) {[m
[32m+[m		[32mvar prop,[m
[32m+[m			[32mcache = this.cache( owner );[m
[32m+[m
[32m+[m		[32m// Handle: [ owner, key, value ] args[m
[32m+[m		[32m// Always use camelCase key (gh-2257)[m
[32m+[m		[32mif ( typeof data === "string" ) {[m
[32m+[m			[32mcache[ jQuery.camelCase( data ) ] = value;[m
[32m+[m
[32m+[m		[32m// Handle: [ owner, { properties } ] args[m
[32m+[m		[32m} else {[m
[32m+[m
[32m+[m			[32m// Copy the properties one-by-one to the cache object[m
[32m+[m			[32mfor ( prop in data ) {[m
[32m+[m				[32mcache[ jQuery.camelCase( prop ) ] = data[ prop ];[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn cache;[m
[32m+[m	[32m},[m
[32m+[m	[32mget: function( owner, key ) {[m
[32m+[m		[32mreturn key === undefined ?[m
[32m+[m			[32mthis.cache( owner ) :[m
[32m+[m
[32m+[m			[32m// Always use camelCase key (gh-2257)[m
[32m+[m			[32mowner[ this.expando ] && owner[ this.expando ][ jQuery.camelCase( key ) ];[m
[32m+[m	[32m},[m
[32m+[m	[32maccess: function( owner, key, value ) {[m
[32m+[m
[32m+[m		[32m// In cases where either:[m
[32m+[m		[32m//[m
[32m+[m		[32m//   1. No key was specified[m
[32m+[m		[32m//   2. A string key was specified, but no value provided[m
[32m+[m		[32m//[m
[32m+[m		[32m// Take the "read" path and allow the get method to determine[m
[32m+[m		[32m// which value to return, respectively either:[m
[32m+[m		[32m//[m
[32m+[m		[32m//   1. The entire cache object[m
[32m+[m		[32m//   2. The data stored at the key[m
[32m+[m		[32m//[m
[32m+[m		[32mif ( key === undefined ||[m
[32m+[m				[32m( ( key && typeof key === "string" ) && value === undefined ) ) {[m
[32m+[m
[32m+[m			[32mreturn this.get( owner, key );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// When the key is not a string, or both a key and value[m
[32m+[m		[32m// are specified, set or extend (existing objects) with either:[m
[32m+[m		[32m//[m
[32m+[m		[32m//   1. An object of properties[m
[32m+[m		[32m//   2. A key and value[m
[32m+[m		[32m//[m
[32m+[m		[32mthis.set( owner, key, value );[m
[32m+[m
[32m+[m		[32m// Since the "set" path can have two possible entry points[m
[32m+[m		[32m// return the expected data based on which path was taken[*][m
[32m+[m		[32mreturn value !== undefined ? value : key;[m
[32m+[m	[32m},[m
[32m+[m	[32mremove: function( owner, key ) {[m
[32m+[m		[32mvar i,[m
[32m+[m			[32mcache = owner[ this.expando ];[m
[32m+[m
[32m+[m		[32mif ( cache === undefined ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( key !== undefined ) {[m
[32m+[m
[32m+[m			[32m// Support array or space separated string of keys[m
[32m+[m			[32mif ( jQuery.isArray( key ) ) {[m
[32m+[m
[32m+[m				[32m// If key is an array of keys...[m
[32m+[m				[32m// We always set camelCase keys, so remove that.[m
[32m+[m				[32mkey = key.map( jQuery.camelCase );[m
[32m+[m			[32m} else {[m
[32m+[m				[32mkey = jQuery.camelCase( key );[m
[32m+[m
[32m+[m				[32m// If a key with the spaces exists, use it.[m
[32m+[m				[32m// Otherwise, create an array by matching non-whitespace[m
[32m+[m				[32mkey = key in cache ?[m
[32m+[m					[32m[ key ] :[m
[32m+[m					[32m( key.match( rnotwhite ) || [] );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mi = key.length;[m
[32m+[m
[32m+[m			[32mwhile ( i-- ) {[m
[32m+[m				[32mdelete cache[ key[ i ] ];[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Remove the expando if there's no more data[m
[32m+[m		[32mif ( key === undefined || jQuery.isEmptyObject( cache ) ) {[m
[32m+[m
[32m+[m			[32m// Support: Chrome <=35 - 45[m
[32m+[m			[32m// Webkit & Blink performance suffers when deleting properties[m
[32m+[m			[32m// from DOM nodes, so set to undefined instead[m
[32m+[m			[32m// https://bugs.chromium.org/p/chromium/issues/detail?id=378607 (bug restricted)[m
[32m+[m			[32mif ( owner.nodeType ) {[m
[32m+[m				[32mowner[ this.expando ] = undefined;[m
[32m+[m			[32m} else {[m
[32m+[m				[32mdelete owner[ this.expando ];[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m	[32mhasData: function( owner ) {[m
[32m+[m		[32mvar cache = owner[ this.expando ];[m
[32m+[m		[32mreturn cache !== undefined && !jQuery.isEmptyObject( cache );[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m[32mvar dataPriv = new Data();[m
[32m+[m
[32m+[m[32mvar dataUser = new Data();[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m//	Implementation Summary[m
[32m+[m[32m//[m
[32m+[m[32m//	1. Enforce API surface and semantic compatibility with 1.9.x branch[m
[32m+[m[32m//	2. Improve the module's maintainability by reducing the storage[m
[32m+[m[32m//		paths to a single mechanism.[m
[32m+[m[32m//	3. Use the same single mechanism to support "private" and "user" data.[m
[32m+[m[32m//	4. _Never_ expose "private" data to user code (TODO: Drop _data, _removeData)[m
[32m+[m[32m//	5. Avoid exposing implementation details on user objects (eg. expando properties)[m
[32m+[m[32m//	6. Provide a clear path for implementation upgrade to WeakMap in 2014[m
[32m+[m
[32m+[m[32mvar rbrace = /^(?:\{[\w\W]*\}|\[[\w\W]*\])$/,[m
[32m+[m	[32mrmultiDash = /[A-Z]/g;[m
[32m+[m
[32m+[m[32mfunction dataAttr( elem, key, data ) {[m
[32m+[m	[32mvar name;[m
[32m+[m
[32m+[m	[32m// If nothing was found internally, try to fetch any[m
[32m+[m	[32m// data from the HTML5 data-* attribute[m
[32m+[m	[32mif ( data === undefined && elem.nodeType === 1 ) {[m
[32m+[m		[32mname = "data-" + key.replace( rmultiDash, "-$&" ).toLowerCase();[m
[32m+[m		[32mdata = elem.getAttribute( name );[m
[32m+[m
[32m+[m		[32mif ( typeof data === "string" ) {[m
[32m+[m			[32mtry {[m
[32m+[m				[32mdata = data === "true" ? true :[m
[32m+[m					[32mdata === "false" ? false :[m
[32m+[m					[32mdata === "null" ? null :[m
[32m+[m
[32m+[m					[32m// Only convert to a number if it doesn't change the string[m
[32m+[m					[32m+data + "" === data ? +data :[m
[32m+[m					[32mrbrace.test( data ) ? JSON.parse( data ) :[m
[32m+[m					[32mdata;[m
[32m+[m			[32m} catch ( e ) {}[m
[32m+[m
[32m+[m			[32m// Make sure we set the data so it isn't changed later[m
[32m+[m			[32mdataUser.set( elem, key, data );[m
[32m+[m		[32m} else {[m
[32m+[m			[32mdata = undefined;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn data;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mjQuery.extend( {[m
[32m+[m	[32mhasData: function( elem ) {[m
[32m+[m		[32mreturn dataUser.hasData( elem ) || dataPriv.hasData( elem );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mdata: function( elem, name, data ) {[m
[32m+[m		[32mreturn dataUser.access( elem, name, data );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mremoveData: function( elem, name ) {[m
[32m+[m		[32mdataUser.remove( elem, name );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// TODO: Now that all calls to _data and _removeData have been replaced[m
[32m+[m	[32m// with direct calls to dataPriv methods, these can be deprecated.[m
[32m+[m	[32m_data: function( elem, name, data ) {[m
[32m+[m		[32mreturn dataPriv.access( elem, name, data );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m_removeData: function( elem, name ) {[m
[32m+[m		[32mdataPriv.remove( elem, name );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m	[32mdata: function( key, value ) {[m
[32m+[m		[32mvar i, name, data,[m
[32m+[m			[32melem = this[ 0 ],[m
[32m+[m			[32mattrs = elem && elem.attributes;[m
[32m+[m
[32m+[m		[32m// Gets all values[m
[32m+[m		[32mif ( key === undefined ) {[m
[32m+[m			[32mif ( this.length ) {[m
[32m+[m				[32mdata = dataUser.get( elem );[m
[32m+[m
[32m+[m				[32mif ( elem.nodeType === 1 && !dataPriv.get( elem, "hasDataAttrs" ) ) {[m
[32m+[m					[32mi = attrs.length;[m
[32m+[m					[32mwhile ( i-- ) {[m
[32m+[m
[32m+[m						[32m// Support: IE 11 only[m
[32m+[m						[32m// The attrs elements can be null (#14894)[m
[32m+[m						[32mif ( attrs[ i ] ) {[m
[32m+[m							[32mname = attrs[ i ].name;[m
[32m+[m							[32mif ( name.indexOf( "data-" ) === 0 ) {[m
[32m+[m								[32mname = jQuery.camelCase( name.slice( 5 ) );[m
[32m+[m								[32mdataAttr( elem, name, data[ name ] );[m
[32m+[m							[32m}[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m					[32mdataPriv.set( elem, "hasDataAttrs", true );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn data;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Sets multiple values[m
[32m+[m		[32mif ( typeof key === "object" ) {[m
[32m+[m			[32mreturn this.each( function() {[m
[32m+[m				[32mdataUser.set( this, key );[m
[32m+[m			[32m} );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn access( this, function( value ) {[m
[32m+[m			[32mvar data;[m
[32m+[m
[32m+[m			[32m// The calling jQuery object (element matches) is not empty[m
[32m+[m			[32m// (and therefore has an element appears at this[ 0 ]) and the[m
[32m+[m			[32m// `value` parameter was not undefined. An empty jQuery object[m
[32m+[m			[32m// will result in `undefined` for elem = this[ 0 ] which will[m
[32m+[m			[32m// throw an exception if an attempt to read a data cache is made.[m
[32m+[m			[32mif ( elem && value === undefined ) {[m
[32m+[m
[32m+[m				[32m// Attempt to get data from the cache[m
[32m+[m				[32m// The key will always be camelCased in Data[m
[32m+[m				[32mdata = dataUser.get( elem, key );[m
[32m+[m				[32mif ( data !== undefined ) {[m
[32m+[m					[32mreturn data;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Attempt to "discover" the data in[m
[32m+[m				[32m// HTML5 custom data-* attrs[m
[32m+[m				[32mdata = dataAttr( elem, key );[m
[32m+[m				[32mif ( data !== undefined ) {[m
[32m+[m					[32mreturn data;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// We tried really hard, but the data doesn't exist.[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Set the data...[m
[32m+[m			[32mthis.each( function() {[m
[32m+[m
[32m+[m				[32m// We always store the camelCased key[m
[32m+[m				[32mdataUser.set( this, key, value );[m
[32m+[m			[32m} );[m
[32m+[m		[32m}, null, value, arguments.length > 1, null, true );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mremoveData: function( key ) {[m
[32m+[m		[32mreturn this.each( function() {[m
[32m+[m			[32mdataUser.remove( this, key );[m
[32m+[m		[32m} );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m[32mjQuery.extend( {[m
[32m+[m	[32mqueue: function( elem, type, data ) {[m
[32m+[m		[32mvar queue;[m
[32m+[m
[32m+[m		[32mif ( elem ) {[m
[32m+[m			[32mtype = ( type || "fx" ) + "queue";[m
[32m+[m			[32mqueue = dataPriv.get( elem, type );[m
[32m+[m
[32m+[m			[32m// Speed up dequeue by getting out quickly if this is just a lookup[m
[32m+[m			[32mif ( data ) {[m
[32m+[m				[32mif ( !queue || jQuery.isArray( data ) ) {[m
[32m+[m					[32mqueue = dataPriv.access( elem, type, jQuery.makeArray( data ) );[m
[32m+[m				[32m} else {[m
[32m+[m					[32mqueue.push( data );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn queue || [];[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mdequeue: function( elem, type ) {[m
[32m+[m		[32mtype = type || "fx";[m
[32m+[m
[32m+[m		[32mvar queue = jQuery.queue( elem, type ),[m
[32m+[m			[32mstartLength = queue.length,[m
[32m+[m			[32mfn = queue.shift(),[m
[32m+[m			[32mhooks = jQuery._queueHooks( elem, type ),[m
[32m+[m			[32mnext = function() {[m
[32m+[m				[32mjQuery.dequeue( elem, type );[m
[32m+[m			[32m};[m
[32m+[m
[32m+[m		[32m// If the fx queue is dequeued, always remove the progress sentinel[m
[32m+[m		[32mif ( fn === "inprogress" ) {[m
[32m+[m			[32mfn = queue.shift();[m
[32m+[m			[32mstartLength--;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( fn ) {[m
[32m+[m
[32m+[m			[32m// Add a progress sentinel to prevent the fx queue from being[m
[32m+[m			[32m// automatically dequeued[m
[32m+[m			[32mif ( type === "fx" ) {[m
[32m+[m				[32mqueue.unshift( "inprogress" );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Clear up the last queue stop function[m
[32m+[m			[32mdelete hooks.stop;[m
[32m+[m			[32mfn.call( elem, next, hooks );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( !startLength && hooks ) {[m
[32m+[m			[32mhooks.empty.fire();[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Not public - generate a queueHooks object, or return the current one[m
[32m+[m	[32m_queueHooks: function( elem, type ) {[m
[32m+[m		[32mvar key = type + "queueHooks";[m
[32m+[m		[32mreturn dataPriv.get( elem, key ) || dataPriv.access( elem, key, {[m
[32m+[m			[32mempty: jQuery.Callbacks( "once memory" ).add( function() {[m
[32m+[m				[32mdataPriv.remove( elem, [ type + "queue", key ] );[m
[32m+[m			[32m} )[m
[32m+[m		[32m} );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m	[32mqueue: function( type, data ) {[m
[32m+[m		[32mvar setter = 2;[m
[32m+[m
[32m+[m		[32mif ( typeof type !== "string" ) {[m
[32m+[m			[32mdata = type;[m
[32m+[m			[32mtype = "fx";[m
[32m+[m			[32msetter--;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( arguments.length < setter ) {[m
[32m+[m			[32mreturn jQuery.queue( this[ 0 ], type );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn data === undefined ?[m
[32m+[m			[32mthis :[m
[32m+[m			[32mthis.each( function() {[m
[32m+[m				[32mvar queue = jQuery.queue( this, type, data );[m
[32m+[m
[32m+[m				[32m// Ensure a hooks for this queue[m
[32m+[m				[32mjQuery._queueHooks( this, type );[m
[32m+[m
[32m+[m				[32mif ( type === "fx" && queue[ 0 ] !== "inprogress" ) {[m
[32m+[m					[32mjQuery.dequeue( this, type );[m
[32m+[m				[32m}[m
[32m+[m			[32m} );[m
[32m+[m	[32m},[m
[32m+[m	[32mdequeue: function( type ) {[m
[32m+[m		[32mreturn this.each( function() {[m
[32m+[m			[32mjQuery.dequeue( this, type );[m
[32m+[m		[32m} );[m
[32m+[m	[32m},[m
[32m+[m	[32mclearQueue: function( type ) {[m
[32m+[m		[32mreturn this.queue( type || "fx", [] );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Get a promise resolved when queues of a certain type[m
[32m+[m	[32m// are emptied (fx is the type by default)[m
[32m+[m	[32mpromise: function( type, obj ) {[m
[32m+[m		[32mvar tmp,[m
[32m+[m			[32mcount = 1,[m
[32m+[m			[32mdefer = jQuery.Deferred(),[m
[32m+[m			[32melements = this,[m
[32m+[m			[32mi = this.length,[m
[32m+[m			[32mresolve = function() {[m
[32m+[m				[32mif ( !( --count ) ) {[m
[32m+[m					[32mdefer.resolveWith( elements, [ elements ] );[m
[32m+[m				[32m}[m
[32m+[m			[32m};[m
[32m+[m
[32m+[m		[32mif ( typeof type !== "string" ) {[m
[32m+[m			[32mobj = type;[m
[32m+[m			[32mtype = undefined;[m
[32m+[m		[32m}[m
[32m+[m		[32mtype = type || "fx";[m
[32m+[m
[32m+[m		[32mwhile ( i-- ) {[m
[32m+[m			[32mtmp = dataPriv.get( elements[ i ], type + "queueHooks" );[m
[32m+[m			[32mif ( tmp && tmp.empty ) {[m
[32m+[m				[32mcount++;[m
[32m+[m				[32mtmp.empty.add( resolve );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32mresolve();[m
[32m+[m		[32mreturn defer.promise( obj );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m[32mvar pnum = ( /[+-]?(?:\d*\.|)\d+(?:[eE][+-]?\d+|)/ ).source;[m
[32m+[m
[32m+[m[32mvar rcssNum = new RegExp( "^(?:([+-])=|)(" + pnum + ")([a-z%]*)$", "i" );[m
[32m+[m
[32m+[m
[32m+[m[32mvar cssExpand = [ "Top", "Right", "Bottom", "Left" ];[m
[32m+[m
[32m+[m[32mvar isHiddenWithinTree = function( elem, el ) {[m
[32m+[m
[32m+[m		[32m// isHiddenWithinTree might be called from jQuery#filter function;[m
[32m+[m		[32m// in that case, element will be second argument[m
[32m+[m		[32melem = el || elem;[m
[32m+[m
[32m+[m		[32m// Inline style trumps all[m
[32m+[m		[32mreturn elem.style.display === "none" ||[m
[32m+[m			[32melem.style.display === "" &&[m
[32m+[m
[32m+[m			[32m// Otherwise, check computed style[m
[32m+[m			[32m// Support: Firefox <=43 - 45[m
[32m+[m			[32m// Disconnected elements can have computed display: none, so first confirm that elem is[m
[32m+[m			[32m// in the document.[m
[32m+[m			[32mjQuery.contains( elem.ownerDocument, elem ) &&[m
[32m+[m
[32m+[m			[32mjQuery.css( elem, "display" ) === "none";[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m[32mvar swap = function( elem, options, callback, args ) {[m
[32m+[m	[32mvar ret, name,[m
[32m+[m		[32mold = {};[m
[32m+[m
[32m+[m	[32m// Remember the old values, and insert the new ones[m
[32m+[m	[32mfor ( name in options ) {[m
[32m+[m		[32mold[ name ] = elem.style[ name ];[m
[32m+[m		[32melem.style[ name ] = options[ name ];[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mret = callback.apply( elem, args || [] );[m
[32m+[m
[32m+[m	[32m// Revert the old values[m
[32m+[m	[32mfor ( name in options ) {[m
[32m+[m		[32melem.style[ name ] = old[ name ];[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn ret;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mfunction adjustCSS( elem, prop, valueParts, tween ) {[m
[32m+[m	[32mvar adjusted,[m
[32m+[m		[32mscale = 1,[m
[32m+[m		[32mmaxIterations = 20,[m
[32m+[m		[32mcurrentValue = tween ?[m
[32m+[m			[32mfunction() {[m
[32m+[m				[32mreturn tween.cur();[m
[32m+[m			[32m} :[m
[32m+[m			[32mfunction() {[m
[32m+[m				[32mreturn jQuery.css( elem, prop, "" );[m
[32m+[m			[32m},[m
[32m+[m		[32minitial = currentValue(),[m
[32m+[m		[32munit = valueParts && valueParts[ 3 ] || ( jQuery.cssNumber[ prop ] ? "" : "px" ),[m
[32m+[m
[32m+[m		[32m// Starting value computation is required for potential unit mismatches[m
[32m+[m		[32minitialInUnit = ( jQuery.cssNumber[ prop ] || unit !== "px" && +initial ) &&[m
[32m+[m			[32mrcssNum.exec( jQuery.css( elem, prop ) );[m
[32m+[m
[32m+[m	[32mif ( initialInUnit && initialInUnit[ 3 ] !== unit ) {[m
[32m+[m
[32m+[m		[32m// Trust units reported by jQuery.css[m
[32m+[m		[32munit = unit || initialInUnit[ 3 ];[m
[32m+[m
[32m+[m		[32m// Make sure we update the tween properties later on[m
[32m+[m		[32mvalueParts = valueParts || [];[m
[32m+[m
[32m+[m		[32m// Iteratively approximate from a nonzero starting point[m
[32m+[m		[32minitialInUnit = +initial || 1;[m
[32m+[m
[32m+[m		[32mdo {[m
[32m+[m
[32m+[m			[32m// If previous iteration zeroed out, double until we get *something*.[m
[32m+[m			[32m// Use string for doubling so we don't accidentally see scale as unchanged below[m
[32m+[m			[32mscale = scale || ".5";[m
[32m+[m
[32m+[m			[32m// Adjust and apply[m
[32m+[m			[32minitialInUnit = initialInUnit / scale;[m
[32m+[m			[32mjQuery.style( elem, prop, initialInUnit + unit );[m
[32m+[m
[32m+[m		[32m// Update scale, tolerating zero or NaN from tween.cur()[m
[32m+[m		[32m// Break the loop if scale is unchanged or perfect, or if we've just had enough.[m
[32m+[m		[32m} while ([m
[32m+[m			[32mscale !== ( scale = currentValue() / initial ) && scale !== 1 && --maxIterations[m
[32m+[m		[32m);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( valueParts ) {[m
[32m+[m		[32minitialInUnit = +initialInUnit || +initial || 0;[m
[32m+[m
[32m+[m		[32m// Apply relative offset (+=/-=) if specified[m
[32m+[m		[32madjusted = valueParts[ 1 ] ?[m
[32m+[m			[32minitialInUnit + ( valueParts[ 1 ] + 1 ) * valueParts[ 2 ] :[m
[32m+[m			[32m+valueParts[ 2 ];[m
[32m+[m		[32mif ( tween ) {[m
[32m+[m			[32mtween.unit = unit;[m
[32m+[m			[32mtween.start = initialInUnit;[m
[32m+[m			[32mtween.end = adjusted;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn adjusted;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mvar defaultDisplayMap = {};[m
[32m+[m
[32m+[m[32mfunction getDefaultDisplay( elem ) {[m
[32m+[m	[32mvar temp,[m
[32m+[m		[32mdoc = elem.ownerDocument,[m
[32m+[m		[32mnodeName = elem.nodeName,[m
[32m+[m		[32mdisplay = defaultDisplayMap[ nodeName ];[m
[32m+[m
[32m+[m	[32mif ( display ) {[m
[32m+[m		[32mreturn display;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mtemp = doc.body.appendChild( doc.createElement( nodeName ) ),[m
[32m+[m	[32mdisplay = jQuery.css( temp, "display" );[m
[32m+[m
[32m+[m	[32mtemp.parentNode.removeChild( temp );[m
[32m+[m
[32m+[m	[32mif ( display === "none" ) {[m
[32m+[m		[32mdisplay = "block";[m
[32m+[m	[32m}[m
[32m+[m	[32mdefaultDisplayMap[ nodeName ] = display;[m
[32m+[m
[32m+[m	[32mreturn display;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction showHide( elements, show ) {[m
[32m+[m	[32mvar display, elem,[m
[32m+[m		[32mvalues = [],[m
[32m+[m		[32mindex = 0,[m
[32m+[m		[32mlength = elements.length;[m
[32m+[m
[32m+[m	[32m// Determine new display value for elements that need to change[m
[32m+[m	[32mfor ( ; index < length; index++ ) {[m
[32m+[m		[32melem = elements[ index ];[m
[32m+[m		[32mif ( !elem.style ) {[m
[32m+[m			[32mcontinue;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mdisplay = elem.style.display;[m
[32m+[m		[32mif ( show ) {[m
[32m+[m
[32m+[m			[32m// Since we force visibility upon cascade-hidden elements, an immediate (and slow)[m
[32m+[m			[32m// check is required in this first loop unless we have a nonempty display value (either[m
[32m+[m			[32m// inline or about-to-be-restored)[m
[32m+[m			[32mif ( display === "none" ) {[m
[32m+[m				[32mvalues[ index ] = dataPriv.get( elem, "display" ) || null;[m
[32m+[m				[32mif ( !values[ index ] ) {[m
[32m+[m					[32melem.style.display = "";[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m			[32mif ( elem.style.display === "" && isHiddenWithinTree( elem ) ) {[m
[32m+[m				[32mvalues[ index ] = getDefaultDisplay( elem );[m
[32m+[m			[32m}[m
[32m+[m		[32m} else {[m
[32m+[m			[32mif ( display !== "none" ) {[m
[32m+[m				[32mvalues[ index ] = "none";[m
[32m+[m
[32m+[m				[32m// Remember what we're overwriting[m
[32m+[m				[32mdataPriv.set( elem, "display", display );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Set the display of the elements in a second loop to avoid constant reflow[m
[32m+[m	[32mfor ( index = 0; index < length; index++ ) {[m
[32m+[m		[32mif ( values[ index ] != null ) {[m
[32m+[m			[32melements[ index ].style.display = values[ index ];[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn elements;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m	[32mshow: function() {[m
[32m+[m		[32mreturn showHide( this, true );[m
[32m+[m	[32m},[m
[32m+[m	[32mhide: function() {[m
[32m+[m		[32mreturn showHide( this );[m
[32m+[m	[32m},[m
[32m+[m	[32mtoggle: function( state ) {[m
[32m+[m		[32mif ( typeof state === "boolean" ) {[m
[32m+[m			[32mreturn state ? this.show() : this.hide();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn this.each( function() {[m
[32m+[m			[32mif ( isHiddenWithinTree( this ) ) {[m
[32m+[m				[32mjQuery( this ).show();[m
[32m+[m			[32m} else {[m
[32m+[m				[32mjQuery( this ).hide();[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m[32mvar rcheckableType = ( /^(?:checkbox|radio)$/i );[m
[32m+[m
[32m+[m[32mvar rtagName = ( /<([a-z][^\/\0>\x20\t\r\n\f]+)/i );[m
[32m+[m
[32m+[m[32mvar rscriptType = ( /^$|\/(?:java|ecma)script/i );[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m// We have to close these tags to support XHTML (#13200)[m
[32m+[m[32mvar wrapMap = {[m
[32m+[m
[32m+[m	[32m// Support: IE <=9 only[m
[32m+[m	[32moption: [ 1, "<select multiple='multiple'>", "</select>" ],[m
[32m+[m
[32m+[m	[32m// XHTML parsers do not magically insert elements in the[m
[32m+[m	[32m// same way that tag soup parsers do. So we cannot shorten[m
[32m+[m	[32m// this by omitting <tbody> or other required elements.[m
[32m+[m	[32mthead: [ 1, "<table>", "</table>" ],[m
[32m+[m	[32mcol: [ 2, "<table><colgroup>", "</colgroup></table>" ],[m
[32m+[m	[32mtr: [ 2, "<table><tbody>", "</tbody></table>" ],[m
[32m+[m	[32mtd: [ 3, "<table><tbody><tr>", "</tr></tbody></table>" ],[m
[32m+[m
[32m+[m	[32m_default: [ 0, "", "" ][m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Support: IE <=9 only[m
[32m+[m[32mwrapMap.optgroup = wrapMap.option;[m
[32m+[m
[32m+[m[32mwrapMap.tbody = wrapMap.tfoot = wrapMap.colgroup = wrapMap.caption = wrapMap.thead;[m
[32m+[m[32mwrapMap.th = wrapMap.td;[m
[32m+[m
[32m+[m
[32m+[m[32mfunction getAll( context, tag ) {[m
[32m+[m
[32m+[m	[32m// Support: IE <=9 - 11 only[m
[32m+[m	[32m// Use typeof to avoid zero-argument method invocation on host objects (#15151)[m
[32m+[m	[32mvar ret = typeof context.getElementsByTagName !== "undefined" ?[m
[32m+[m			[32mcontext.getElementsByTagName( tag || "*" ) :[m
[32m+[m			[32mtypeof context.querySelectorAll !== "undefined" ?[m
[32m+[m				[32mcontext.querySelectorAll( tag || "*" ) :[m
[32m+[m			[32m[];[m
[32m+[m
[32m+[m	[32mreturn tag === undefined || tag && jQuery.nodeName( context, tag ) ?[m
[32m+[m		[32mjQuery.merge( [ context ], ret ) :[m
[32m+[m		[32mret;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m// Mark scripts as having already been evaluated[m
[32m+[m[32mfunction setGlobalEval( elems, refElements ) {[m
[32m+[m	[32mvar i = 0,[m
[32m+[m		[32ml = elems.length;[m
[32m+[m
[32m+[m	[32mfor ( ; i < l; i++ ) {[m
[32m+[m		[32mdataPriv.set([m
[32m+[m			[32melems[ i ],[m
[32m+[m			[32m"globalEval",[m
[32m+[m			[32m!refElements || dataPriv.get( refElements[ i ], "globalEval" )[m
[32m+[m		[32m);[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mvar rhtml = /<|&#?\w+;/;[m
[32m+[m
[32m+[m[32mfunction buildFragment( elems, context, scripts, selection, ignored ) {[m
[32m+[m	[32mvar elem, tmp, tag, wrap, contains, j,[m
[32m+[m		[32mfragment = context.createDocumentFragment(),[m
[32m+[m		[32mnodes = [],[m
[32m+[m		[32mi = 0,[m
[32m+[m		[32ml = elems.length;[m
[32m+[m
[32m+[m	[32mfor ( ; i < l; i++ ) {[m
[32m+[m		[32melem = elems[ i ];[m
[32m+[m
[32m+[m		[32mif ( elem || elem === 0 ) {[m
[32m+[m
[32m+[m			[32m// Add nodes directly[m
[32m+[m			[32mif ( jQuery.type( elem ) === "object" ) {[m
[32m+[m
[32m+[m				[32m// Support: Android <=4.0 only, PhantomJS 1 only[m
[32m+[m				[32m// push.apply(_, arraylike) throws on ancient WebKit[m
[32m+[m				[32mjQuery.merge( nodes, elem.nodeType ? [ elem ] : elem );[m
[32m+[m
[32m+[m			[32m// Convert non-html into a text node[m
[32m+[m			[32m} else if ( !rhtml.test( elem ) ) {[m
[32m+[m				[32mnodes.push( context.createTextNode( elem ) );[m
[32m+[m
[32m+[m			[32m// Convert html into DOM nodes[m
[32m+[m			[32m} else {[m
[32m+[m				[32mtmp = tmp || fragment.appendChild( context.createElement( "div" ) );[m
[32m+[m
[32m+[m				[32m// Deserialize a standard representation[m
[32m+[m				[32mtag = ( rtagName.exec( elem ) || [ "", "" ] )[ 1 ].toLowerCase();[m
[32m+[m				[32mwrap = wrapMap[ tag ] || wrapMap._default;[m
[32m+[m				[32mtmp.innerHTML = wrap[ 1 ] + jQuery.htmlPrefilter( elem ) + wrap[ 2 ];[m
[32m+[m
[32m+[m				[32m// Descend through wrappers to the right content[m
[32m+[m				[32mj = wrap[ 0 ];[m
[32m+[m				[32mwhile ( j-- ) {[m
[32m+[m					[32mtmp = tmp.lastChild;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Support: Android <=4.0 only, PhantomJS 1 only[m
[32m+[m				[32m// push.apply(_, arraylike) throws on ancient WebKit[m
[32m+[m				[32mjQuery.merge( nodes, tmp.childNodes );[m
[32m+[m
[32m+[m				[32m// Remember the top-level container[m
[32m+[m				[32mtmp = fragment.firstChild;[m
[32m+[m
[32m+[m				[32m// Ensure the created nodes are orphaned (#12392)[m
[32m+[m				[32mtmp.textContent = "";[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Remove wrapper from fragment[m
[32m+[m	[32mfragment.textContent = "";[m
[32m+[m
[32m+[m	[32mi = 0;[m
[32m+[m	[32mwhile ( ( elem = nodes[ i++ ] ) ) {[m
[32m+[m
[32m+[m		[32m// Skip elements already in the context collection (trac-4087)[m
[32m+[m		[32mif ( selection && jQuery.inArray( elem, selection ) > -1 ) {[m
[32m+[m			[32mif ( ignored ) {[m
[32m+[m				[32mignored.push( elem );[m
[32m+[m			[32m}[m
[32m+[m			[32mcontinue;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mcontains = jQuery.contains( elem.ownerDocument, elem );[m
[32m+[m
[32m+[m		[32m// Append to fragment[m
[32m+[m		[32mtmp = getAll( fragment.appendChild( elem ), "script" );[m
[32m+[m
[32m+[m		[32m// Preserve script evaluation history[m
[32m+[m		[32mif ( contains ) {[m
[32m+[m			[32msetGlobalEval( tmp );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Capture executables[m
[32m+[m		[32mif ( scripts ) {[m
[32m+[m			[32mj = 0;[m
[32m+[m			[32mwhile ( ( elem = tmp[ j++ ] ) ) {[m
[32m+[m				[32mif ( rscriptType.test( elem.type || "" ) ) {[m
[32m+[m					[32mscripts.push( elem );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn fragment;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m( function() {[m
[32m+[m	[32mvar fragment = document.createDocumentFragment(),[m
[32m+[m		[32mdiv = fragment.appendChild( document.createElement( "div" ) ),[m
[32m+[m		[32minput = document.createElement( "input" );[m
[32m+[m
[32m+[m	[32m// Support: Android 4.0 - 4.3 only[m
[32m+[m	[32m// Check state lost if the name is set (#11217)[m
[32m+[m	[32m// Support: Windows Web Apps (WWA)[m
[32m+[m	[32m// `name` and `type` must use .setAttribute for WWA (#14901)[m
[32m+[m	[32minput.setAttribute( "type", "radio" );[m
[32m+[m	[32minput.setAttribute( "checked", "checked" );[m
[32m+[m	[32minput.setAttribute( "name", "t" );[m
[32m+[m
[32m+[m	[32mdiv.appendChild( input );[m
[32m+[m
[32m+[m	[32m// Support: Android <=4.1 only[m
[32m+[m	[32m// Older WebKit doesn't clone checked state correctly in fragments[m
[32m+[m	[32msupport.checkClone = div.cloneNode( true ).cloneNode( true ).lastChild.checked;[m
[32m+[m
[32m+[m	[32m// Support: IE <=11 only[m
[32m+[m	[32m// Make sure textarea (and checkbox) defaultValue is properly cloned[m
[32m+[m	[32mdiv.innerHTML = "<textarea>x</textarea>";[m
[32m+[m	[32msupport.noCloneChecked = !!div.cloneNode( true ).lastChild.defaultValue;[m
[32m+[m[32m} )();[m
[32m+[m[32mvar documentElement = document.documentElement;[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mvar[m
[32m+[m	[32mrkeyEvent = /^key/,[m
[32m+[m	[32mrmouseEvent = /^(?:mouse|pointer|contextmenu|drag|drop)|click/,[m
[32m+[m	[32mrtypenamespace = /^([^.]*)(?:\.(.+)|)/;[m
[32m+[m
[32m+[m[32mfunction returnTrue() {[m
[32m+[m	[32mreturn true;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction returnFalse() {[m
[32m+[m	[32mreturn false;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Support: IE <=9 only[m
[32m+[m[32m// See #13393 for more info[m
[32m+[m[32mfunction safeActiveElement() {[m
[32m+[m	[32mtry {[m
[32m+[m		[32mreturn document.activeElement;[m
[32m+[m	[32m} catch ( err ) { }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction on( elem, types, selector, data, fn, one ) {[m
[32m+[m	[32mvar origFn, type;[m
[32m+[m
[32m+[m	[32m// Types can be a map of types/handlers[m
[32m+[m	[32mif ( typeof types === "object" ) {[m
[32m+[m
[32m+[m		[32m// ( types-Object, selector, data )[m
[32m+[m		[32mif ( typeof selector !== "string" ) {[m
[32m+[m
[32m+[m			[32m// ( types-Object, data )[m
[32m+[m			[32mdata = data || selector;[m
[32m+[m			[32mselector = undefined;[m
[32m+[m		[32m}[m
[32m+[m		[32mfor ( type in types ) {[m
[32m+[m			[32mon( elem, type, selector, data, types[ type ], one );[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn elem;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( data == null && fn == null ) {[m
[32m+[m
[32m+[m		[32m// ( types, fn )[m
[32m+[m		[32mfn = selector;[m
[32m+[m		[32mdata = selector = undefined;[m
[32m+[m	[32m} else if ( fn == null ) {[m
[32m+[m		[32mif ( typeof selector === "string" ) {[m
[32m+[m
[32m+[m			[32m// ( types, selector, fn )[m
[32m+[m			[32mfn = data;[m
[32m+[m			[32mdata = undefined;[m
[32m+[m		[32m} else {[m
[32m+[m
[32m+[m			[32m// ( types, data, fn )[m
[32m+[m			[32mfn = data;[m
[32m+[m			[32mdata = selector;[m
[32m+[m			[32mselector = undefined;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mif ( fn === false ) {[m
[32m+[m		[32mfn = returnFalse;[m
[32m+[m	[32m} else if ( !fn ) {[m
[32m+[m		[32mreturn elem;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( one === 1 ) {[m
[32m+[m		[32morigFn = fn;[m
[32m+[m		[32mfn = function( event ) {[m
[32m+[m
[32m+[m			[32m// Can use an empty set, since event contains the info[m
[32m+[m			[32mjQuery().off( event );[m
[32m+[m			[32mreturn origFn.apply( this, arguments );[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m		[32m// Use same guid so caller can remove using origFn[m
[32m+[m		[32mfn.guid = origFn.guid || ( origFn.guid = jQuery.guid++ );[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn elem.each( function() {[m
[32m+[m		[32mjQuery.event.add( this, types, fn, data, selector );[m
[32m+[m	[32m} );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m * Helper functions for managing events -- not part of the public interface.[m
[32m+[m[32m * Props to Dean Edwards' addEvent library for many of the ideas.[m
[32m+[m[32m */[m
[32m+[m[32mjQuery.event = {[m
[32m+[m
[32m+[m	[32mglobal: {},[m
[32m+[m
[32m+[m	[32madd: function( elem, types, handler, data, selector ) {[m
[32m+[m
[32m+[m		[32mvar handleObjIn, eventHandle, tmp,[m
[32m+[m			[32mevents, t, handleObj,[m
[32m+[m			[32mspecial, handlers, type, namespaces, origType,[m
[32m+[m			[32melemData = dataPriv.get( elem );[m
[32m+[m
[32m+[m		[32m// Don't attach events to noData or text/comment nodes (but allow plain objects)[m
[32m+[m		[32mif ( !elemData ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Caller can pass in an object of custom data in lieu of the handler[m
[32m+[m		[32mif ( handler.handler ) {[m
[32m+[m			[32mhandleObjIn = handler;[m
[32m+[m			[32mhandler = handleObjIn.handler;[m
[32m+[m			[32mselector = handleObjIn.selector;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Ensure that invalid selectors throw exceptions at attach time[m
[32m+[m		[32m// Evaluate against documentElement in case elem is a non-element node (e.g., document)[m
[32m+[m		[32mif ( selector ) {[m
[32m+[m			[32mjQuery.find.matchesSelector( documentElement, selector );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Make sure that the handler has a unique ID, used to find/remove it later[m
[32m+[m		[32mif ( !handler.guid ) {[m
[32m+[m			[32mhandler.guid = jQuery.guid++;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Init the element's event structure and main handler, if this is the first[m
[32m+[m		[32mif ( !( events = elemData.events ) ) {[m
[32m+[m			[32mevents = elemData.events = {};[m
[32m+[m		[32m}[m
[32m+[m		[32mif ( !( eventHandle = elemData.handle ) ) {[m
[32m+[m			[32meventHandle = elemData.handle = function( e ) {[m
[32m+[m
[32m+[m				[32m// Discard the second event of a jQuery.event.trigger() and[m
[32m+[m				[32m// when an event is called after a page has unloaded[m
[32m+[m				[32mreturn typeof jQuery !== "undefined" && jQuery.event.triggered !== e.type ?[m
[32m+[m					[32mjQuery.event.dispatch.apply( elem, arguments ) : undefined;[m
[32m+[m			[32m};[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Handle multiple events separated by a space[m
[32m+[m		[32mtypes = ( types || "" ).match( rnotwhite ) || [ "" ];[m
[32m+[m		[32mt = types.length;[m
[32m+[m		[32mwhile ( t-- ) {[m
[32m+[m			[32mtmp = rtypenamespace.exec( types[ t ] ) || [];[m
[32m+[m			[32mtype = origType = tmp[ 1 ];[m
[32m+[m			[32mnamespaces = ( tmp[ 2 ] || "" ).split( "." ).sort();[m
[32m+[m
[32m+[m			[32m// There *must* be a type, no attaching namespace-only handlers[m
[32m+[m			[32mif ( !type ) {[m
[32m+[m				[32mcontinue;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// If event changes its type, use the special event handlers for the changed type[m
[32m+[m			[32mspecial = jQuery.event.special[ type ] || {};[m
[32m+[m
[32m+[m			[32m// If selector defined, determine special event api type, otherwise given type[m
[32m+[m			[32mtype = ( selector ? special.delegateType : special.bindType ) || type;[m
[32m+[m
[32m+[m			[32m// Update special based on newly reset type[m
[32m+[m			[32mspecial = jQuery.event.special[ type ] || {};[m
[32m+[m
[32m+[m			[32m// handleObj is passed to all event handlers[m
[32m+[m			[32mhandleObj = jQuery.extend( {[m
[32m+[m				[32mtype: type,[m
[32m+[m				[32morigType: origType,[m
[32m+[m				[32mdata: data,[m
[32m+[m				[32mhandler: handler,[m
[32m+[m				[32mguid: handler.guid,[m
[32m+[m				[32mselector: selector,[m
[32m+[m				[32mneedsContext: selector && jQuery.expr.match.needsContext.test( selector ),[m
[32m+[m				[32mnamespace: namespaces.join( "." )[m
[32m+[m			[32m}, handleObjIn );[m
[32m+[m
[32m+[m			[32m// Init the event handler queue if we're the first[m
[32m+[m			[32mif ( !( handlers = events[ type ] ) ) {[m
[32m+[m				[32mhandlers = events[ type ] = [];[m
[32m+[m				[32mhandlers.delegateCount = 0;[m
[32m+[m
[32m+[m				[32m// Only use addEventListener if the special events handler returns false[m
[32m+[m				[32mif ( !special.setup ||[m
[32m+[m					[32mspecial.setup.call( elem, data, namespaces, eventHandle ) === false ) {[m
[32m+[m
[32m+[m					[32mif ( elem.addEventListener ) {[m
[32m+[m						[32melem.addEventListener( type, eventHandle );[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif ( special.add ) {[m
[32m+[m				[32mspecial.add.call( elem, handleObj );[m
[32m+[m
[32m+[m				[32mif ( !handleObj.handler.guid ) {[m
[32m+[m					[32mhandleObj.handler.guid = handler.guid;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Add to the element's handler list, delegates in front[m
[32m+[m			[32mif ( selector ) {[m
[32m+[m				[32mhandlers.splice( handlers.delegateCount++, 0, handleObj );[m
[32m+[m			[32m} else {[m
[32m+[m				[32mhandlers.push( handleObj );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Keep track of which events have ever been used, for event optimization[m
[32m+[m			[32mjQuery.event.global[ type ] = true;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Detach an event or set of events from an element[m
[32m+[m	[32mremove: function( elem, types, handler, selector, mappedTypes ) {[m
[32m+[m
[32m+[m		[32mvar j, origCount, tmp,[m
[32m+[m			[32mevents, t, handleObj,[m
[32m+[m			[32mspecial, handlers, type, namespaces, origType,[m
[32m+[m			[32melemData = dataPriv.hasData( elem ) && dataPriv.get( elem );[m
[32m+[m
[32m+[m		[32mif ( !elemData || !( events = elemData.events ) ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Once for each type.namespace in types; type may be omitted[m
[32m+[m		[32mtypes = ( types || "" ).match( rnotwhite ) || [ "" ];[m
[32m+[m		[32mt = types.length;[m
[32m+[m		[32mwhile ( t-- ) {[m
[32m+[m			[32mtmp = rtypenamespace.exec( types[ t ] ) || [];[m
[32m+[m			[32mtype = origType = tmp[ 1 ];[m
[32m+[m			[32mnamespaces = ( tmp[ 2 ] || "" ).split( "." ).sort();[m
[32m+[m
[32m+[m			[32m// Unbind all events (on this namespace, if provided) for the element[m
[32m+[m			[32mif ( !type ) {[m
[32m+[m				[32mfor ( type in events ) {[m
[32m+[m					[32mjQuery.event.remove( elem, type + types[ t ], handler, selector, true );[m
[32m+[m				[32m}[m
[32m+[m				[32mcontinue;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mspecial = jQuery.event.special[ type ] || {};[m
[32m+[m			[32mtype = ( selector ? special.delegateType : special.bindType ) || type;[m
[32m+[m			[32mhandlers = events[ type ] || [];[m
[32m+[m			[32mtmp = tmp[ 2 ] &&[m
[32m+[m				[32mnew RegExp( "(^|\\.)" + namespaces.join( "\\.(?:.*\\.|)" ) + "(\\.|$)" );[m
[32m+[m
[32m+[m			[32m// Remove matching events[m
[32m+[m			[32morigCount = j = handlers.length;[m
[32m+[m			[32mwhile ( j-- ) {[m
[32m+[m				[32mhandleObj = handlers[ j ];[m
[32m+[m
[32m+[m				[32mif ( ( mappedTypes || origType === handleObj.origType ) &&[m
[32m+[m					[32m( !handler || handler.guid === handleObj.guid ) &&[m
[32m+[m					[32m( !tmp || tmp.test( handleObj.namespace ) ) &&[m
[32m+[m					[32m( !selector || selector === handleObj.selector ||[m
[32m+[m						[32mselector === "**" && handleObj.selector ) ) {[m
[32m+[m					[32mhandlers.splice( j, 1 );[m
[32m+[m
[32m+[m					[32mif ( handleObj.selector ) {[m
[32m+[m						[32mhandlers.delegateCount--;[m
[32m+[m					[32m}[m
[32m+[m					[32mif ( special.remove ) {[m
[32m+[m						[32mspecial.remove.call( elem, handleObj );[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Remove generic event handler if we removed something and no more handlers exist[m
[32m+[m			[32m// (avoids potential for endless recursion during removal of special event handlers)[m
[32m+[m			[32mif ( origCount && !handlers.length ) {[m
[32m+[m				[32mif ( !special.teardown ||[m
[32m+[m					[32mspecial.teardown.call( elem, namespaces, elemData.handle ) === false ) {[m
[32m+[m
[32m+[m					[32mjQuery.removeEvent( elem, type, elemData.handle );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mdelete events[ type ];[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Remove data and the expando if it's no longer used[m
[32m+[m		[32mif ( jQuery.isEmptyObject( events ) ) {[m
[32m+[m			[32mdataPriv.remove( elem, "handle events" );[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mdispatch: function( nativeEvent ) {[m
[32m+[m
[32m+[m		[32m// Make a writable jQuery.Event from the native event object[m
[32m+[m		[32mvar event = jQuery.event.fix( nativeEvent );[m
[32m+[m
[32m+[m		[32mvar i, j, ret, matched, handleObj, handlerQueue,[m
[32m+[m			[32margs = new Array( arguments.length ),[m
[32m+[m			[32mhandlers = ( dataPriv.get( this, "events" ) || {} )[ event.type ] || [],[m
[32m+[m			[32mspecial = jQuery.event.special[ event.type ] || {};[m
[32m+[m
[32m+[m		[32m// Use the fix-ed jQuery.Event rather than the (read-only) native event[m
[32m+[m		[32margs[ 0 ] = event;[m
[32m+[m
[32m+[m		[32mfor ( i = 1; i < arguments.length; i++ ) {[m
[32m+[m			[32margs[ i ] = arguments[ i ];[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mevent.delegateTarget = this;[m
[32m+[m
[32m+[m		[32m// Call the preDispatch hook for the mapped type, and let it bail if desired[m
[32m+[m		[32mif ( special.preDispatch && special.preDispatch.call( this, event ) === false ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Determine handlers[m
[32m+[m		[32mhandlerQueue = jQuery.event.handlers.call( this, event, handlers );[m
[32m+[m
[32m+[m		[32m// Run delegates first; they may want to stop propagation beneath us[m
[32m+[m		[32mi = 0;[m
[32m+[m		[32mwhile ( ( matched = handlerQueue[ i++ ] ) && !event.isPropagationStopped() ) {[m
[32m+[m			[32mevent.currentTarget = matched.elem;[m
[32m+[m
[32m+[m			[32mj = 0;[m
[32m+[m			[32mwhile ( ( handleObj = matched.handlers[ j++ ] ) &&[m
[32m+[m				[32m!event.isImmediatePropagationStopped() ) {[m
[32m+[m
[32m+[m				[32m// Triggered event must either 1) have no namespace, or 2) have namespace(s)[m
[32m+[m				[32m// a subset or equal to those in the bound event (both can have no namespace).[m
[32m+[m				[32mif ( !event.rnamespace || event.rnamespace.test( handleObj.namespace ) ) {[m
[32m+[m
[32m+[m					[32mevent.handleObj = handleObj;[m
[32m+[m					[32mevent.data = handleObj.data;[m
[32m+[m
[32m+[m					[32mret = ( ( jQuery.event.special[ handleObj.origType ] || {} ).handle ||[m
[32m+[m						[32mhandleObj.handler ).apply( matched.elem, args );[m
[32m+[m
[32m+[m					[32mif ( ret !== undefined ) {[m
[32m+[m						[32mif ( ( event.result = ret ) === false ) {[m
[32m+[m							[32mevent.preventDefault();[m
[32m+[m							[32mevent.stopPropagation();[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Call the postDispatch hook for the mapped type[m
[32m+[m		[32mif ( special.postDispatch ) {[m
[32m+[m			[32mspecial.postDispatch.call( this, event );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn event.result;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mhandlers: function( event, handlers ) {[m
[32m+[m		[32mvar i, matches, sel, handleObj,[m
[32m+[m			[32mhandlerQueue = [],[m
[32m+[m			[32mdelegateCount = handlers.delegateCount,[m
[32m+[m			[32mcur = event.target;[m
[32m+[m
[32m+[m		[32m// Support: IE <=9[m
[32m+[m		[32m// Find delegate handlers[m
[32m+[m		[32m// Black-hole SVG <use> instance trees (#13180)[m
[32m+[m		[32m//[m
[32m+[m		[32m// Support: Firefox <=42[m
[32m+[m		[32m// Avoid non-left-click in FF but don't block IE radio events (#3861, gh-2343)[m
[32m+[m		[32mif ( delegateCount && cur.nodeType &&[m
[32m+[m			[32m( event.type !== "click" || isNaN( event.button ) || event.button < 1 ) ) {[m
[32m+[m
[32m+[m			[32mfor ( ; cur !== this; cur = cur.parentNode || this ) {[m
[32m+[m
[32m+[m				[32m// Don't check non-elements (#13208)[m
[32m+[m				[32m// Don't process clicks on disabled elements (#6911, #8165, #11382, #11764)[m
[32m+[m				[32mif ( cur.nodeType === 1 && ( cur.disabled !== true || event.type !== "click" ) ) {[m
[32m+[m					[32mmatches = [];[m
[32m+[m					[32mfor ( i = 0; i < delegateCount; i++ ) {[m
[32m+[m						[32mhandleObj = handlers[ i ];[m
[32m+[m
[32m+[m						[32m// Don't conflict with Object.prototype properties (#13203)[m
[32m+[m						[32msel = handleObj.selector + " ";[m
[32m+[m
[32m+[m						[32mif ( matches[ sel ] === undefined ) {[m
[32m+[m							[32mmatches[ sel ] = handleObj.needsContext ?[m
[32m+[m								[32mjQuery( sel, this ).index( cur ) > -1 :[m
[32m+[m								[32mjQuery.find( sel, this, null, [ cur ] ).length;[m
[32m+[m						[32m}[m
[32m+[m						[32mif ( matches[ sel ] ) {[m
[32m+[m							[32mmatches.push( handleObj );[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m					[32mif ( matches.length ) {[m
[32m+[m						[32mhandlerQueue.push( { elem: cur, handlers: matches } );[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Add the remaining (directly-bound) handlers[m
[32m+[m		[32mif ( delegateCount < handlers.length ) {[m
[32m+[m			[32mhandlerQueue.push( { elem: this, handlers: handlers.slice( delegateCount ) } );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn handlerQueue;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32maddProp: function( name, hook ) {[m
[32m+[m		[32mObject.defineProperty( jQuery.Event.prototype, name, {[m
[32m+[m			[32menumerable: true,[m
[32m+[m			[32mconfigurable: true,[m
[32m+[m
[32m+[m			[32mget: jQuery.isFunction( hook ) ?[m
[32m+[m				[32mfunction() {[m
[32m+[m					[32mif ( this.originalEvent ) {[m
[32m+[m							[32mreturn hook( this.originalEvent );[m
[32m+[m					[32m}[m
[32m+[m				[32m} :[m
[32m+[m				[32mfunction() {[m
[32m+[m					[32mif ( this.originalEvent ) {[m
[32m+[m							[32mreturn this.originalEvent[ name ];[m
[32m+[m					[32m}[m
[32m+[m				[32m},[m
[32m+[m
[32m+[m			[32mset: function( value ) {[m
[32m+[m				[32mObject.defineProperty( this, name, {[m
[32m+[m					[32menumerable: true,[m
[32m+[m					[32mconfigurable: true,[m
[32m+[m					[32mwritable: true,[m
[32m+[m					[32mvalue: value[m
[32m+[m				[32m} );[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mfix: function( originalEvent ) {[m
[32m+[m		[32mreturn originalEvent[ jQuery.expando ] ?[m
[32m+[m			[32moriginalEvent :[m
[32m+[m			[32mnew jQuery.Event( originalEvent );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mspecial: {[m
[32m+[m		[32mload: {[m
[32m+[m
[32m+[m			[32m// Prevent triggered image.load events from bubbling to window.load[m
[32m+[m			[32mnoBubble: true[m
[32m+[m		[32m},[m
[32m+[m		[32mfocus: {[m
[32m+[m
[32m+[m			[32m// Fire native event if possible so blur/focus sequence is correct[m
[32m+[m			[32mtrigger: function() {[m
[32m+[m				[32mif ( this !== safeActiveElement() && this.focus ) {[m
[32m+[m					[32mthis.focus();[m
[32m+[m					[32mreturn false;[m
[32m+[m				[32m}[m
[32m+[m			[32m},[m
[32m+[m			[32mdelegateType: "focusin"[m
[32m+[m		[32m},[m
[32m+[m		[32mblur: {[m
[32m+[m			[32mtrigger: function() {[m
[32m+[m				[32mif ( this === safeActiveElement() && this.blur ) {[m
[32m+[m					[32mthis.blur();[m
[32m+[m					[32mreturn false;[m
[32m+[m				[32m}[m
[32m+[m			[32m},[m
[32m+[m			[32mdelegateType: "focusout"[m
[32m+[m		[32m},[m
[32m+[m		[32mclick: {[m
[32m+[m
[32m+[m			[32m// For checkbox, fire native event so checked state will be right[m
[32m+[m			[32mtrigger: function() {[m
[32m+[m				[32mif ( this.type === "checkbox" && this.click && jQuery.nodeName( this, "input" ) ) {[m
[32m+[m					[32mthis.click();[m
[32m+[m					[32mreturn false;[m
[32m+[m				[32m}[m
[32m+[m			[32m},[m
[32m+[m
[32m+[m			[32m// For cross-browser consistency, don't fire native .click() on links[m
[32m+[m			[32m_default: function( event ) {[m
[32m+[m				[32mreturn jQuery.nodeName( event.target, "a" );[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mbeforeunload: {[m
[32m+[m			[32mpostDispatch: function( event ) {[m
[32m+[m
[32m+[m				[32m// Support: Firefox 20+[m
[32m+[m				[32m// Firefox doesn't alert if the returnValue field is not set.[m
[32m+[m				[32mif ( event.result !== undefined && event.originalEvent ) {[m
[32m+[m					[32mevent.originalEvent.returnValue = event.result;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.removeEvent = function( elem, type, handle ) {[m
[32m+[m
[32m+[m	[32m// This "if" is needed for plain objects[m
[32m+[m	[32mif ( elem.removeEventListener ) {[m
[32m+[m		[32melem.removeEventListener( type, handle );[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.Event = function( src, props ) {[m
[32m+[m
[32m+[m	[32m// Allow instantiation without the 'new' keyword[m
[32m+[m	[32mif ( !( this instanceof jQuery.Event ) ) {[m
[32m+[m		[32mreturn new jQuery.Event( src, props );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Event object[m
[32m+[m	[32mif ( src && src.type ) {[m
[32m+[m		[32mthis.originalEvent = src;[m
[32m+[m		[32mthis.type = src.type;[m
[32m+[m
[32m+[m		[32m// Events bubbling up the document may have been marked as prevented[m
[32m+[m		[32m// by a handler lower down the tree; reflect the correct value.[m
[32m+[m		[32mthis.isDefaultPrevented = src.defaultPrevented ||[m
[32m+[m				[32msrc.defaultPrevented === undefined &&[m
[32m+[m
[32m+[m				[32m// Support: Android <=2.3 only[m
[32m+[m				[32msrc.returnValue === false ?[m
[32m+[m			[32mreturnTrue :[m
[32m+[m			[32mreturnFalse;[m
[32m+[m
[32m+[m		[32m// Create target properties[m
[32m+[m		[32m// Support: Safari <=6 - 7 only[m
[32m+[m		[32m// Target should not be a text node (#504, #13143)[m
[32m+[m		[32mthis.target = ( src.target && src.target.nodeType === 3 ) ?[m
[32m+[m			[32msrc.target.parentNode :[m
[32m+[m			[32msrc.target;[m
[32m+[m
[32m+[m		[32mthis.currentTarget = src.currentTarget;[m
[32m+[m		[32mthis.relatedTarget = src.relatedTarget;[m
[32m+[m
[32m+[m	[32m// Event type[m
[32m+[m	[32m} else {[m
[32m+[m		[32mthis.type = src;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Put explicitly provided properties onto the event object[m
[32m+[m	[32mif ( props ) {[m
[32m+[m		[32mjQuery.extend( this, props );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Create a timestamp if incoming event doesn't have one[m
[32m+[m	[32mthis.timeStamp = src && src.timeStamp || jQuery.now();[m
[32m+[m
[32m+[m	[32m// Mark it as fixed[m
[32m+[m	[32mthis[ jQuery.expando ] = true;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// jQuery.Event is based on DOM3 Events as specified by the ECMAScript Language Binding[m
[32m+[m[32m// https://www.w3.org/TR/2003/WD-DOM-Level-3-Events-20030331/ecma-script-binding.html[m
[32m+[m[32mjQuery.Event.prototype = {[m
[32m+[m	[32mconstructor: jQuery.Event,[m
[32m+[m	[32misDefaultPrevented: returnFalse,[m
[32m+[m	[32misPropagationStopped: returnFalse,[m
[32m+[m	[32misImmediatePropagationStopped: returnFalse,[m
[32m+[m	[32misSimulated: false,[m
[32m+[m
[32m+[m	[32mpreventDefault: function() {[m
[32m+[m		[32mvar e = this.originalEvent;[m
[32m+[m
[32m+[m		[32mthis.isDefaultPrevented = returnTrue;[m
[32m+[m
[32m+[m		[32mif ( e && !this.isSimulated ) {[m
[32m+[m			[32me.preventDefault();[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m	[32mstopPropagation: function() {[m
[32m+[m		[32mvar e = this.originalEvent;[m
[32m+[m
[32m+[m		[32mthis.isPropagationStopped = returnTrue;[m
[32m+[m
[32m+[m		[32mif ( e && !this.isSimulated ) {[m
[32m+[m			[32me.stopPropagation();[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m	[32mstopImmediatePropagation: function() {[m
[32m+[m		[32mvar e = this.originalEvent;[m
[32m+[m
[32m+[m		[32mthis.isImmediatePropagationStopped = returnTrue;[m
[32m+[m
[32m+[m		[32mif ( e && !this.isSimulated ) {[m
[32m+[m			[32me.stopImmediatePropagation();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mthis.stopPropagation();[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Includes all common event props including KeyEvent and MouseEvent specific props[m
[32m+[m[32mjQuery.each( {[m
[32m+[m	[32maltKey: true,[m
[32m+[m	[32mbubbles: true,[m
[32m+[m	[32mcancelable: true,[m
[32m+[m	[32mchangedTouches: true,[m
[32m+[m	[32mctrlKey: true,[m
[32m+[m	[32mdetail: true,[m
[32m+[m	[32meventPhase: true,[m
[32m+[m	[32mmetaKey: true,[m
[32m+[m	[32mpageX: true,[m
[32m+[m	[32mpageY: true,[m
[32m+[m	[32mshiftKey: true,[m
[32m+[m	[32mview: true,[m
[32m+[m	[32m"char": true,[m
[32m+[m	[32mcharCode: true,[m
[32m+[m	[32mkey: true,[m
[32m+[m	[32mkeyCode: true,[m
[32m+[m	[32mbutton: true,[m
[32m+[m	[32mbuttons: true,[m
[32m+[m	[32mclientX: true,[m
[32m+[m	[32mclientY: true,[m
[32m+[m	[32moffsetX: true,[m
[32m+[m	[32moffsetY: true,[m
[32m+[m	[32mpointerId: true,[m
[32m+[m	[32mpointerType: true,[m
[32m+[m	[32mscreenX: true,[m
[32m+[m	[32mscreenY: true,[m
[32m+[m	[32mtargetTouches: true,[m
[32m+[m	[32mtoElement: true,[m
[32m+[m	[32mtouches: true,[m
[32m+[m
[32m+[m	[32mwhich: function( event ) {[m
[32m+[m		[32mvar button = event.button;[m
[32m+[m
[32m+[m		[32m// Add which for key events[m
[32m+[m		[32mif ( event.which == null && rkeyEvent.test( event.type ) ) {[m
[32m+[m			[32mreturn event.charCode != null ? event.charCode : event.keyCode;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Add which for click: 1 === left; 2 === middle; 3 === right[m
[32m+[m		[32mif ( !event.which && button !== undefined && rmouseEvent.test( event.type ) ) {[m
[32m+[m			[32mreturn ( button & 1 ? 1 : ( button & 2 ? 3 : ( button & 4 ? 2 : 0 ) ) );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn event.which;[m
[32m+[m	[32m}[m
[32m+[m[32m}, jQuery.event.addProp );[m
[32m+[m
[32m+[m[32m// Create mouseenter/leave events using mouseover/out and event-time checks[m
[32m+[m[32m// so that event delegation works in jQuery.[m
[32m+[m[32m// Do the same for pointerenter/pointerleave and pointerover/pointerout[m
[32m+[m[32m//[m
[32m+[m[32m// Support: Safari 7 only[m
[32m+[m[32m// Safari sends mouseenter too often; see:[m
[32m+[m[32m// https://bugs.chromium.org/p/chromium/issues/detail?id=470258[m
[32m+[m[32m// for the description of the bug (it existed in older Chrome versions as well).[m
[32m+[m[32mjQuery.each( {[m
[32m+[m	[32mmouseenter: "mouseover",[m
[32m+[m	[32mmouseleave: "mouseout",[m
[32m+[m	[32mpointerenter: "pointerover",[m
[32m+[m	[32mpointerleave: "pointerout"[m
[32m+[m[32m}, function( orig, fix ) {[m
[32m+[m	[32mjQuery.event.special[ orig ] = {[m
[32m+[m		[32mdelegateType: fix,[m
[32m+[m		[32mbindType: fix,[m
[32m+[m
[32m+[m		[32mhandle: function( event ) {[m
[32m+[m			[32mvar ret,[m
[32m+[m				[32mtarget = this,[m
[32m+[m				[32mrelated = event.relatedTarget,[m
[32m+[m				[32mhandleObj = event.handleObj;[m
[32m+[m
[32m+[m			[32m// For mouseenter/leave call the handler if related is outside the target.[m
[32m+[m			[32m// NB: No relatedTarget if the mouse left/entered the browser window[m
[32m+[m			[32mif ( !related || ( related !== target && !jQuery.contains( target, related ) ) ) {[m
[32m+[m				[32mevent.type = handleObj.origType;[m
[32m+[m				[32mret = handleObj.handler.apply( this, arguments );[m
[32m+[m				[32mevent.type = fix;[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn ret;[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m
[32m+[m	[32mon: function( types, selector, data, fn ) {[m
[32m+[m		[32mreturn on( this, types, selector, data, fn );[m
[32m+[m	[32m},[m
[32m+[m	[32mone: function( types, selector, data, fn ) {[m
[32m+[m		[32mreturn on( this, types, selector, data, fn, 1 );[m
[32m+[m	[32m},[m
[32m+[m	[32moff: function( types, selector, fn ) {[m
[32m+[m		[32mvar handleObj, type;[m
[32m+[m		[32mif ( types && types.preventDefault && types.handleObj ) {[m
[32m+[m
[32m+[m			[32m// ( event )  dispatched jQuery.Event[m
[32m+[m			[32mhandleObj = types.handleObj;[m
[32m+[m			[32mjQuery( types.delegateTarget ).off([m
[32m+[m				[32mhandleObj.namespace ?[m
[32m+[m					[32mhandleObj.origType + "." + handleObj.namespace :[m
[32m+[m					[32mhandleObj.origType,[m
[32m+[m				[32mhandleObj.selector,[m
[32m+[m				[32mhandleObj.handler[m
[32m+[m			[32m);[m
[32m+[m			[32mreturn this;[m
[32m+[m		[32m}[m
[32m+[m		[32mif ( typeof types === "object" ) {[m
[32m+[m
[32m+[m			[32m// ( types-object [, selector] )[m
[32m+[m			[32mfor ( type in types ) {[m
[32m+[m				[32mthis.off( type, selector, types[ type ] );[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn this;[m
[32m+[m		[32m}[m
[32m+[m		[32mif ( selector === false || typeof selector === "function" ) {[m
[32m+[m
[32m+[m			[32m// ( types [, fn] )[m
[32m+[m			[32mfn = selector;[m
[32m+[m			[32mselector = undefined;[m
[32m+[m		[32m}[m
[32m+[m		[32mif ( fn === false ) {[m
[32m+[m			[32mfn = returnFalse;[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn this.each( function() {[m
[32m+[m			[32mjQuery.event.remove( this, types, fn, selector );[m
[32m+[m		[32m} );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m[32mvar[m
[32m+[m
[32m+[m	[32m/* eslint-disable max-len */[m
[32m+[m
[32m+[m	[32m// See https://github.com/eslint/eslint/issues/3229[m
[32m+[m	[32mrxhtmlTag = /<(?!area|br|col|embed|hr|img|input|link|meta|param)(([a-z][^\/\0>\x20\t\r\n\f]*)[^>]*)\/>/gi,[m
[32m+[m
[32m+[m	[32m/* eslint-enable */[m
[32m+[m
[32m+[m	[32m// Support: IE <=10 - 11, Edge 12 - 13[m
[32m+[m	[32m// In IE/Edge using regex groups here causes severe slowdowns.[m
[32m+[m	[32m// See https://connect.microsoft.com/IE/feedback/details/1736512/[m
[32m+[m	[32mrnoInnerhtml = /<script|<style|<link/i,[m
[32m+[m
[32m+[m	[32m// checked="checked" or checked[m
[32m+[m	[32mrchecked = /checked\s*(?:[^=]|=\s*.checked.)/i,[m
[32m+[m	[32mrscriptTypeMasked = /^true\/(.*)/,[m
[32m+[m	[32mrcleanScript = /^\s*<!(?:\[CDATA\[|--)|(?:\]\]|--)>\s*$/g;[m
[32m+[m
[32m+[m[32mfunction manipulationTarget( elem, content ) {[m
[32m+[m	[32mif ( jQuery.nodeName( elem, "table" ) &&[m
[32m+[m		[32mjQuery.nodeName( content.nodeType !== 11 ? content : content.firstChild, "tr" ) ) {[m
[32m+[m
[32m+[m		[32mreturn elem.getElementsByTagName( "tbody" )[ 0 ] || elem;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn elem;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Replace/restore the type attribute of script elements for safe DOM manipulation[m
[32m+[m[32mfunction disableScript( elem ) {[m
[32m+[m	[32melem.type = ( elem.getAttribute( "type" ) !== null ) + "/" + elem.type;[m
[32m+[m	[32mreturn elem;[m
[32m+[m[32m}[m
[32m+[m[32mfunction restoreScript( elem ) {[m
[32m+[m	[32mvar match = rscriptTypeMasked.exec( elem.type );[m
[32m+[m
[32m+[m	[32mif ( match ) {[m
[32m+[m		[32melem.type = match[ 1 ];[m
[32m+[m	[32m} else {[m
[32m+[m		[32melem.removeAttribute( "type" );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn elem;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction cloneCopyEvent( src, dest ) {[m
[32m+[m	[32mvar i, l, type, pdataOld, pdataCur, udataOld, udataCur, events;[m
[32m+[m
[32m+[m	[32mif ( dest.nodeType !== 1 ) {[m
[32m+[m		[32mreturn;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// 1. Copy private data: events, handlers, etc.[m
[32m+[m	[32mif ( dataPriv.hasData( src ) ) {[m
[32m+[m		[32mpdataOld = dataPriv.access( src );[m
[32m+[m		[32mpdataCur = dataPriv.set( dest, pdataOld );[m
[32m+[m		[32mevents = pdataOld.events;[m
[32m+[m
[32m+[m		[32mif ( events ) {[m
[32m+[m			[32mdelete pdataCur.handle;[m
[32m+[m			[32mpdataCur.events = {};[m
[32m+[m
[32m+[m			[32mfor ( type in events ) {[m
[32m+[m				[32mfor ( i = 0, l = events[ type ].length; i < l; i++ ) {[m
[32m+[m					[32mjQuery.event.add( dest, type, events[ type ][ i ] );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// 2. Copy user data[m
[32m+[m	[32mif ( dataUser.hasData( src ) ) {[m
[32m+[m		[32mudataOld = dataUser.access( src );[m
[32m+[m		[32mudataCur = jQuery.extend( {}, udataOld );[m
[32m+[m
[32m+[m		[32mdataUser.set( dest, udataCur );[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Fix IE bugs, see support tests[m
[32m+[m[32mfunction fixInput( src, dest ) {[m
[32m+[m	[32mvar nodeName = dest.nodeName.toLowerCase();[m
[32m+[m
[32m+[m	[32m// Fails to persist the checked state of a cloned checkbox or radio button.[m
[32m+[m	[32mif ( nodeName === "input" && rcheckableType.test( src.type ) ) {[m
[32m+[m		[32mdest.checked = src.checked;[m
[32m+[m
[32m+[m	[32m// Fails to return the selected option to the default selected state when cloning options[m
[32m+[m	[32m} else if ( nodeName === "input" || nodeName === "textarea" ) {[m
[32m+[m		[32mdest.defaultValue = src.defaultValue;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction domManip( collection, args, callback, ignored ) {[m
[32m+[m
[32m+[m	[32m// Flatten any nested arrays[m
[32m+[m	[32margs = concat.apply( [], args );[m
[32m+[m
[32m+[m	[32mvar fragment, first, scripts, hasScripts, node, doc,[m
[32m+[m		[32mi = 0,[m
[32m+[m		[32ml = collection.length,[m
[32m+[m		[32miNoClone = l - 1,[m
[32m+[m		[32mvalue = args[ 0 ],[m
[32m+[m		[32misFunction = jQuery.isFunction( value );[m
[32m+[m
[32m+[m	[32m// We can't cloneNode fragments that contain checked, in WebKit[m
[32m+[m	[32mif ( isFunction ||[m
[32m+[m			[32m( l > 1 && typeof value === "string" &&[m
[32m+[m				[32m!support.checkClone && rchecked.test( value ) ) ) {[m
[32m+[m		[32mreturn collection.each( function( index ) {[m
[32m+[m			[32mvar self = collection.eq( index );[m
[32m+[m			[32mif ( isFunction ) {[m
[32m+[m				[32margs[ 0 ] = value.call( this, index, self.html() );[m
[32m+[m			[32m}[m
[32m+[m			[32mdomManip( self, args, callback, ignored );[m
[32m+[m		[32m} );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( l ) {[m
[32m+[m		[32mfragment = buildFragment( args, collection[ 0 ].ownerDocument, false, collection, ignored );[m
[32m+[m		[32mfirst = fragment.firstChild;[m
[32m+[m
[32m+[m		[32mif ( fragment.childNodes.length === 1 ) {[m
[32m+[m			[32mfragment = first;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Require either new content or an interest in ignored elements to invoke the callback[m
[32m+[m		[32mif ( first || ignored ) {[m
[32m+[m			[32mscripts = jQuery.map( getAll( fragment, "script" ), disableScript );[m
[32m+[m			[32mhasScripts = scripts.length;[m
[32m+[m
[32m+[m			[32m// Use the original fragment for the last item[m
[32m+[m			[32m// instead of the first because it can end up[m
[32m+[m			[32m// being emptied incorrectly in certain situations (#8070).[m
[32m+[m			[32mfor ( ; i < l; i++ ) {[m
[32m+[m				[32mnode = fragment;[m
[32m+[m
[32m+[m				[32mif ( i !== iNoClone ) {[m
[32m+[m					[32mnode = jQuery.clone( node, true, true );[m
[32m+[m
[32m+[m					[32m// Keep references to cloned scripts for later restoration[m
[32m+[m					[32mif ( hasScripts ) {[m
[32m+[m
[32m+[m						[32m// Support: Android <=4.0 only, PhantomJS 1 only[m
[32m+[m						[32m// push.apply(_, arraylike) throws on ancient WebKit[m
[32m+[m						[32mjQuery.merge( scripts, getAll( node, "script" ) );[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mcallback.call( collection[ i ], node, i );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif ( hasScripts ) {[m
[32m+[m				[32mdoc = scripts[ scripts.length - 1 ].ownerDocument;[m
[32m+[m
[32m+[m				[32m// Reenable scripts[m
[32m+[m				[32mjQuery.map( scripts, restoreScript );[m
[32m+[m
[32m+[m				[32m// Evaluate executable scripts on first document insertion[m
[32m+[m				[32mfor ( i = 0; i < hasScripts; i++ ) {[m
[32m+[m					[32mnode = scripts[ i ];[m
[32m+[m					[32mif ( rscriptType.test( node.type || "" ) &&[m
[32m+[m						[32m!dataPriv.access( node, "globalEval" ) &&[m
[32m+[m						[32mjQuery.contains( doc, node ) ) {[m
[32m+[m
[32m+[m						[32mif ( node.src ) {[m
[32m+[m
[32m+[m							[32m// Optional AJAX dependency, but won't run scripts if not present[m
[32m+[m							[32mif ( jQuery._evalUrl ) {[m
[32m+[m								[32mjQuery._evalUrl( node.src );[m
[32m+[m							[32m}[m
[32m+[m						[32m} else {[m
[32m+[m							[32mDOMEval( node.textContent.replace( rcleanScript, "" ), doc );[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn collection;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction remove( elem, selector, keepData ) {[m
[32m+[m	[32mvar node,[m
[32m+[m		[32mnodes = selector ? jQuery.filter( selector, elem ) : elem,[m
[32m+[m		[32mi = 0;[m
[32m+[m
[32m+[m	[32mfor ( ; ( node = nodes[ i ] ) != null; i++ ) {[m
[32m+[m		[32mif ( !keepData && node.nodeType === 1 ) {[m
[32m+[m			[32mjQuery.cleanData( getAll( node ) );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( node.parentNode ) {[m
[32m+[m			[32mif ( keepData && jQuery.contains( node.ownerDocument, node ) ) {[m
[32m+[m				[32msetGlobalEval( getAll( node, "script" ) );[m
[32m+[m			[32m}[m
[32m+[m			[32mnode.parentNode.removeChild( node );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn elem;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mjQuery.extend( {[m
[32m+[m	[32mhtmlPrefilter: function( html ) {[m
[32m+[m		[32mreturn html.replace( rxhtmlTag, "<$1></$2>" );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mclone: function( elem, dataAndEvents, deepDataAndEvents ) {[m
[32m+[m		[32mvar i, l, srcElements, destElements,[m
[32m+[m			[32mclone = elem.cloneNode( true ),[m
[32m+[m			[32minPage = jQuery.contains( elem.ownerDocument, elem );[m
[32m+[m
[32m+[m		[32m// Fix IE cloning issues[m
[32m+[m		[32mif ( !support.noCloneChecked && ( elem.nodeType === 1 || elem.nodeType === 11 ) &&[m
[32m+[m				[32m!jQuery.isXMLDoc( elem ) ) {[m
[32m+[m
[32m+[m			[32m// We eschew Sizzle here for performance reasons: https://jsperf.com/getall-vs-sizzle/2[m
[32m+[m			[32mdestElements = getAll( clone );[m
[32m+[m			[32msrcElements = getAll( elem );[m
[32m+[m
[32m+[m			[32mfor ( i = 0, l = srcElements.length; i < l; i++ ) {[m
[32m+[m				[32mfixInput( srcElements[ i ], destElements[ i ] );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Copy the events from the original to the clone[m
[32m+[m		[32mif ( dataAndEvents ) {[m
[32m+[m			[32mif ( deepDataAndEvents ) {[m
[32m+[m				[32msrcElements = srcElements || getAll( elem );[m
[32m+[m				[32mdestElements = destElements || getAll( clone );[m
[32m+[m
[32m+[m				[32mfor ( i = 0, l = srcElements.length; i < l; i++ ) {[m
[32m+[m					[32mcloneCopyEvent( srcElements[ i ], destElements[ i ] );[m
[32m+[m				[32m}[m
[32m+[m			[32m} else {[m
[32m+[m				[32mcloneCopyEvent( elem, clone );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Preserve script evaluation history[m
[32m+[m		[32mdestElements = getAll( clone, "script" );[m
[32m+[m		[32mif ( destElements.length > 0 ) {[m
[32m+[m			[32msetGlobalEval( destElements, !inPage && getAll( elem, "script" ) );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Return the cloned set[m
[32m+[m		[32mreturn clone;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mcleanData: function( elems ) {[m
[32m+[m		[32mvar data, elem, type,[m
[32m+[m			[32mspecial = jQuery.event.special,[m
[32m+[m			[32mi = 0;[m
[32m+[m
[32m+[m		[32mfor ( ; ( elem = elems[ i ] ) !== undefined; i++ ) {[m
[32m+[m			[32mif ( acceptData( elem ) ) {[m
[32m+[m				[32mif ( ( data = elem[ dataPriv.expando ] ) ) {[m
[32m+[m					[32mif ( data.events ) {[m
[32m+[m						[32mfor ( type in data.events ) {[m
[32m+[m							[32mif ( special[ type ] ) {[m
[32m+[m								[32mjQuery.event.remove( elem, type );[m
[32m+[m
[32m+[m							[32m// This is a shortcut to avoid jQuery.event.remove's overhead[m
[32m+[m							[32m} else {[m
[32m+[m								[32mjQuery.removeEvent( elem, type, data.handle );[m
[32m+[m							[32m}[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// Support: Chrome <=35 - 45+[m
[32m+[m					[32m// Assign undefined instead of using delete, see Data#remove[m
[32m+[m					[32melem[ dataPriv.expando ] = undefined;[m
[32m+[m				[32m}[m
[32m+[m				[32mif ( elem[ dataUser.expando ] ) {[m
[32m+[m
[32m+[m					[32m// Support: Chrome <=35 - 45+[m
[32m+[m					[32m// Assign undefined instead of using delete, see Data#remove[m
[32m+[m					[32melem[ dataUser.expando ] = undefined;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m	[32mdetach: function( selector ) {[m
[32m+[m		[32mreturn remove( this, selector, true );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mremove: function( selector ) {[m
[32m+[m		[32mreturn remove( this, selector );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mtext: function( value ) {[m
[32m+[m		[32mreturn access( this, function( value ) {[m
[32m+[m			[32mreturn value === undefined ?[m
[32m+[m				[32mjQuery.text( this ) :[m
[32m+[m				[32mthis.empty().each( function() {[m
[32m+[m					[32mif ( this.nodeType === 1 || this.nodeType === 11 || this.nodeType === 9 ) {[m
[32m+[m						[32mthis.textContent = value;[m
[32m+[m					[32m}[m
[32m+[m				[32m} );[m
[32m+[m		[32m}, null, value, arguments.length );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mappend: function() {[m
[32m+[m		[32mreturn domManip( this, arguments, function( elem ) {[m
[32m+[m			[32mif ( this.nodeType === 1 || this.nodeType === 11 || this.nodeType === 9 ) {[m
[32m+[m				[32mvar target = manipulationTarget( this, elem );[m
[32m+[m				[32mtarget.appendChild( elem );[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mprepend: function() {[m
[32m+[m		[32mreturn domManip( this, arguments, function( elem ) {[m
[32m+[m			[32mif ( this.nodeType === 1 || this.nodeType === 11 || this.nodeType === 9 ) {[m
[32m+[m				[32mvar target = manipulationTarget( this, elem );[m
[32m+[m				[32mtarget.insertBefore( elem, target.firstChild );[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mbefore: function() {[m
[32m+[m		[32mreturn domManip( this, arguments, function( elem ) {[m
[32m+[m			[32mif ( this.parentNode ) {[m
[32m+[m				[32mthis.parentNode.insertBefore( elem, this );[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mafter: function() {[m
[32m+[m		[32mreturn domManip( this, arguments, function( elem ) {[m
[32m+[m			[32mif ( this.parentNode ) {[m
[32m+[m				[32mthis.parentNode.insertBefore( elem, this.nextSibling );[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mempty: function() {[m
[32m+[m		[32mvar elem,[m
[32m+[m			[32mi = 0;[m
[32m+[m
[32m+[m		[32mfor ( ; ( elem = this[ i ] ) != null; i++ ) {[m
[32m+[m			[32mif ( elem.nodeType === 1 ) {[m
[32m+[m
[32m+[m				[32m// Prevent memory leaks[m
[32m+[m				[32mjQuery.cleanData( getAll( elem, false ) );[m
[32m+[m
[32m+[m				[32m// Remove any remaining nodes[m
[32m+[m				[32melem.textContent = "";[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn this;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mclone: function( dataAndEvents, deepDataAndEvents ) {[m
[32m+[m		[32mdataAndEvents = dataAndEvents == null ? false : dataAndEvents;[m
[32m+[m		[32mdeepDataAndEvents = deepDataAndEvents == null ? dataAndEvents : deepDataAndEvents;[m
[32m+[m
[32m+[m		[32mreturn this.map( function() {[m
[32m+[m			[32mreturn jQuery.clone( this, dataAndEvents, deepDataAndEvents );[m
[32m+[m		[32m} );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mhtml: function( value ) {[m
[32m+[m		[32mreturn access( this, function( value ) {[m
[32m+[m			[32mvar elem = this[ 0 ] || {},[m
[32m+[m				[32mi = 0,[m
[32m+[m				[32ml = this.length;[m
[32m+[m
[32m+[m			[32mif ( value === undefined && elem.nodeType === 1 ) {[m
[32m+[m				[32mreturn elem.innerHTML;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// See if we can take a shortcut and just use innerHTML[m
[32m+[m			[32mif ( typeof value === "string" && !rnoInnerhtml.test( value ) &&[m
[32m+[m				[32m!wrapMap[ ( rtagName.exec( value ) || [ "", "" ] )[ 1 ].toLowerCase() ] ) {[m
[32m+[m
[32m+[m				[32mvalue = jQuery.htmlPrefilter( value );[m
[32m+[m
[32m+[m				[32mtry {[m
[32m+[m					[32mfor ( ; i < l; i++ ) {[m
[32m+[m						[32melem = this[ i ] || {};[m
[32m+[m
[32m+[m						[32m// Remove element nodes and prevent memory leaks[m
[32m+[m						[32mif ( elem.nodeType === 1 ) {[m
[32m+[m							[32mjQuery.cleanData( getAll( elem, false ) );[m
[32m+[m							[32melem.innerHTML = value;[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32melem = 0;[m
[32m+[m
[32m+[m				[32m// If using innerHTML throws an exception, use the fallback method[m
[32m+[m				[32m} catch ( e ) {}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif ( elem ) {[m
[32m+[m				[32mthis.empty().append( value );[m
[32m+[m			[32m}[m
[32m+[m		[32m}, null, value, arguments.length );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mreplaceWith: function() {[m
[32m+[m		[32mvar ignored = [];[m
[32m+[m
[32m+[m		[32m// Make the changes, replacing each non-ignored context element with the new content[m
[32m+[m		[32mreturn domManip( this, arguments, function( elem ) {[m
[32m+[m			[32mvar parent = this.parentNode;[m
[32m+[m
[32m+[m			[32mif ( jQuery.inArray( this, ignored ) < 0 ) {[m
[32m+[m				[32mjQuery.cleanData( getAll( this ) );[m
[32m+[m				[32mif ( parent ) {[m
[32m+[m					[32mparent.replaceChild( elem, this );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m// Force callback invocation[m
[32m+[m		[32m}, ignored );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.each( {[m
[32m+[m	[32mappendTo: "append",[m
[32m+[m	[32mprependTo: "prepend",[m
[32m+[m	[32minsertBefore: "before",[m
[32m+[m	[32minsertAfter: "after",[m
[32m+[m	[32mreplaceAll: "replaceWith"[m
[32m+[m[32m}, function( name, original ) {[m
[32m+[m	[32mjQuery.fn[ name ] = function( selector ) {[m
[32m+[m		[32mvar elems,[m
[32m+[m			[32mret = [],[m
[32m+[m			[32minsert = jQuery( selector ),[m
[32m+[m			[32mlast = insert.length - 1,[m
[32m+[m			[32mi = 0;[m
[32m+[m
[32m+[m		[32mfor ( ; i <= last; i++ ) {[m
[32m+[m			[32melems = i === last ? this : this.clone( true );[m
[32m+[m			[32mjQuery( insert[ i ] )[ original ]( elems );[m
[32m+[m
[32m+[m			[32m// Support: Android <=4.0 only, PhantomJS 1 only[m
[32m+[m			[32m// .get() because push.apply(_, arraylike) throws on ancient WebKit[m
[32m+[m			[32mpush.apply( ret, elems.get() );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn this.pushStack( ret );[m
[32m+[m	[32m};[m
[32m+[m[32m} );[m
[32m+[m[32mvar rmargin = ( /^margin/ );[m
[32m+[m
[32m+[m[32mvar rnumnonpx = new RegExp( "^(" + pnum + ")(?!px)[a-z%]+$", "i" );[m
[32m+[m
[32m+[m[32mvar getStyles = function( elem ) {[m
[32m+[m
[32m+[m		[32m// Support: IE <=11 only, Firefox <=30 (#15098, #14150)[m
[32m+[m		[32m// IE throws on elements created in popups[m
[32m+[m		[32m// FF meanwhile throws on frame elements through "defaultView.getComputedStyle"[m
[32m+[m		[32mvar view = elem.ownerDocument.defaultView;[m
[32m+[m
[32m+[m		[32mif ( !view || !view.opener ) {[m
[32m+[m			[32mview = window;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn view.getComputedStyle( elem );[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m( function() {[m
[32m+[m
[32m+[m	[32m// Executing both pixelPosition & boxSizingReliable tests require only one layout[m
[32m+[m	[32m// so they're executed at the same time to save the second computation.[m
[32m+[m	[32mfunction computeStyleTests() {[m
[32m+[m
[32m+[m		[32m// This is a singleton, we need to execute it only once[m
[32m+[m		[32mif ( !div ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mdiv.style.cssText =[m
[32m+[m			[32m"box-sizing:border-box;" +[m
[32m+[m			[32m"position:relative;display:block;" +[m
[32m+[m			[32m"margin:auto;border:1px;padding:1px;" +[m
[32m+[m			[32m"top:1%;width:50%";[m
[32m+[m		[32mdiv.innerHTML = "";[m
[32m+[m		[32mdocumentElement.appendChild( container );[m
[32m+[m
[32m+[m		[32mvar divStyle = window.getComputedStyle( div );[m
[32m+[m		[32mpixelPositionVal = divStyle.top !== "1%";[m
[32m+[m
[32m+[m		[32m// Support: Android 4.0 - 4.3 only, Firefox <=3 - 44[m
[32m+[m		[32mreliableMarginLeftVal = divStyle.marginLeft === "2px";[m
[32m+[m		[32mboxSizingReliableVal = divStyle.width === "4px";[m
[32m+[m
[32m+[m		[32m// Support: Android 4.0 - 4.3 only[m
[32m+[m		[32m// Some styles come back with percentage values, even though they shouldn't[m
[32m+[m		[32mdiv.style.marginRight = "50%";[m
[32m+[m		[32mpixelMarginRightVal = divStyle.marginRight === "4px";[m
[32m+[m
[32m+[m		[32mdocumentElement.removeChild( container );[m
[32m+[m
[32m+[m		[32m// Nullify the div so it wouldn't be stored in the memory and[m
[32m+[m		[32m// it will also be a sign that checks already performed[m
[32m+[m		[32mdiv = null;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mvar pixelPositionVal, boxSizingReliableVal, pixelMarginRightVal, reliableMarginLeftVal,[m
[32m+[m		[32mcontainer = document.createElement( "div" ),[m
[32m+[m		[32mdiv = document.createElement( "div" );[m
[32m+[m
[32m+[m	[32m// Finish early in limited (non-browser) environments[m
[32m+[m	[32mif ( !div.style ) {[m
[32m+[m		[32mreturn;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Support: IE <=9 - 11 only[m
[32m+[m	[32m// Style of cloned element affects source element cloned (#8908)[m
[32m+[m	[32mdiv.style.backgroundClip = "content-box";[m
[32m+[m	[32mdiv.cloneNode( true ).style.backgroundClip = "";[m
[32m+[m	[32msupport.clearCloneStyle = div.style.backgroundClip === "content-box";[m
[32m+[m
[32m+[m	[32mcontainer.style.cssText = "border:0;width:8px;height:0;top:0;left:-9999px;" +[m
[32m+[m		[32m"padding:0;margin-top:1px;position:absolute";[m
[32m+[m	[32mcontainer.appendChild( div );[m
[32m+[m
[32m+[m	[32mjQuery.extend( support, {[m
[32m+[m		[32mpixelPosition: function() {[m
[32m+[m			[32mcomputeStyleTests();[m
[32m+[m			[32mreturn pixelPositionVal;[m
[32m+[m		[32m},[m
[32m+[m		[32mboxSizingReliable: function() {[m
[32m+[m			[32mcomputeStyleTests();[m
[32m+[m			[32mreturn boxSizingReliableVal;[m
[32m+[m		[32m},[m
[32m+[m		[32mpixelMarginRight: function() {[m
[32m+[m			[32mcomputeStyleTests();[m
[32m+[m			[32mreturn pixelMarginRightVal;[m
[32m+[m		[32m},[m
[32m+[m		[32mreliableMarginLeft: function() {[m
[32m+[m			[32mcomputeStyleTests();[m
[32m+[m			[32mreturn reliableMarginLeftVal;[m
[32m+[m		[32m}[m
[32m+[m	[32m} );[m
[32m+[m[32m} )();[m
[32m+[m
[32m+[m
[32m+[m[32mfunction curCSS( elem, name, computed ) {[m
[32m+[m	[32mvar width, minWidth, maxWidth, ret,[m
[32m+[m		[32mstyle = elem.style;[m
[32m+[m
[32m+[m	[32mcomputed = computed || getStyles( elem );[m
[32m+[m
[32m+[m	[32m// Support: IE <=9 only[m
[32m+[m	[32m// getPropertyValue is only needed for .css('filter') (#12537)[m
[32m+[m	[32mif ( computed ) {[m
[32m+[m		[32mret = computed.getPropertyValue( name ) || computed[ name ];[m
[32m+[m
[32m+[m		[32mif ( ret === "" && !jQuery.contains( elem.ownerDocument, elem ) ) {[m
[32m+[m			[32mret = jQuery.style( elem, name );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// A tribute to the "awesome hack by Dean Edwards"[m
[32m+[m		[32m// Android Browser returns percentage for some values,[m
[32m+[m		[32m// but width seems to be reliably pixels.[m
[32m+[m		[32m// This is against the CSSOM draft spec:[m
[32m+[m		[32m// https://drafts.csswg.org/cssom/#resolved-values[m
[32m+[m		[32mif ( !support.pixelMarginRight() && rnumnonpx.test( ret ) && rmargin.test( name ) ) {[m
[32m+[m
[32m+[m			[32m// Remember the original values[m
[32m+[m			[32mwidth = style.width;[m
[32m+[m			[32mminWidth = style.minWidth;[m
[32m+[m			[32mmaxWidth = style.maxWidth;[m
[32m+[m
[32m+[m			[32m// Put in the new values to get a computed value out[m
[32m+[m			[32mstyle.minWidth = style.maxWidth = style.width = ret;[m
[32m+[m			[32mret = computed.width;[m
[32m+[m
[32m+[m			[32m// Revert the changed values[m
[32m+[m			[32mstyle.width = width;[m
[32m+[m			[32mstyle.minWidth = minWidth;[m
[32m+[m			[32mstyle.maxWidth = maxWidth;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn ret !== undefined ?[m
[32m+[m
[32m+[m		[32m// Support: IE <=9 - 11 only[m
[32m+[m		[32m// IE returns zIndex value as an integer.[m
[32m+[m		[32mret + "" :[m
[32m+[m		[32mret;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mfunction addGetHookIf( conditionFn, hookFn ) {[m
[32m+[m
[32m+[m	[32m// Define the hook, we'll check on the first run if it's really needed.[m
[32m+[m	[32mreturn {[m
[32m+[m		[32mget: function() {[m
[32m+[m			[32mif ( conditionFn() ) {[m
[32m+[m
[32m+[m				[32m// Hook not needed (or it's not possible to use it due[m
[32m+[m				[32m// to missing dependency), remove it.[m
[32m+[m				[32mdelete this.get;[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Hook needed; redefine it so that the support test is not executed again.[m
[32m+[m			[32mreturn ( this.get = hookFn ).apply( this, arguments );[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mvar[m
[32m+[m
[32m+[m	[32m// Swappable if display is none or starts with table[m
[32m+[m	[32m// except "table", "table-cell", or "table-caption"[m
[32m+[m	[32m// See here for display values: https://developer.mozilla.org/en-US/docs/CSS/display[m
[32m+[m	[32mrdisplayswap = /^(none|table(?!-c[ea]).+)/,[m
[32m+[m	[32mcssShow = { position: "absolute", visibility: "hidden", display: "block" },[m
[32m+[m	[32mcssNormalTransform = {[m
[32m+[m		[32mletterSpacing: "0",[m
[32m+[m		[32mfontWeight: "400"[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mcssPrefixes = [ "Webkit", "Moz", "ms" ],[m
[32m+[m	[32memptyStyle = document.createElement( "div" ).style;[m
[32m+[m
[32m+[m[32m// Return a css property mapped to a potentially vendor prefixed property[m
[32m+[m[32mfunction vendorPropName( name ) {[m
[32m+[m
[32m+[m	[32m// Shortcut for names that are not vendor prefixed[m
[32m+[m	[32mif ( name in emptyStyle ) {[m
[32m+[m		[32mreturn name;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Check for vendor prefixed names[m
[32m+[m	[32mvar capName = name[ 0 ].toUpperCase() + name.slice( 1 ),[m
[32m+[m		[32mi = cssPrefixes.length;[m
[32m+[m
[32m+[m	[32mwhile ( i-- ) {[m
[32m+[m		[32mname = cssPrefixes[ i ] + capName;[m
[32m+[m		[32mif ( name in emptyStyle ) {[m
[32m+[m			[32mreturn name;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction setPositiveNumber( elem, value, subtract ) {[m
[32m+[m
[32m+[m	[32m// Any relative (+/-) values have already been[m
[32m+[m	[32m// normalized at this point[m
[32m+[m	[32mvar matches = rcssNum.exec( value );[m
[32m+[m	[32mreturn matches ?[m
[32m+[m
[32m+[m		[32m// Guard against undefined "subtract", e.g., when used as in cssHooks[m
[32m+[m		[32mMath.max( 0, matches[ 2 ] - ( subtract || 0 ) ) + ( matches[ 3 ] || "px" ) :[m
[32m+[m		[32mvalue;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction augmentWidthOrHeight( elem, name, extra, isBorderBox, styles ) {[m
[32m+[m	[32mvar i = extra === ( isBorderBox ? "border" : "content" ) ?[m
[32m+[m
[32m+[m		[32m// If we already have the right measurement, avoid augmentation[m
[32m+[m		[32m4 :[m
[32m+[m
[32m+[m		[32m// Otherwise initialize for horizontal or vertical properties[m
[32m+[m		[32mname === "width" ? 1 : 0,[m
[32m+[m
[32m+[m		[32mval = 0;[m
[32m+[m
[32m+[m	[32mfor ( ; i < 4; i += 2 ) {[m
[32m+[m
[32m+[m		[32m// Both box models exclude margin, so add it if we want it[m
[32m+[m		[32mif ( extra === "margin" ) {[m
[32m+[m			[32mval += jQuery.css( elem, extra + cssExpand[ i ], true, styles );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( isBorderBox ) {[m
[32m+[m
[32m+[m			[32m// border-box includes padding, so remove it if we want content[m
[32m+[m			[32mif ( extra === "content" ) {[m
[32m+[m				[32mval -= jQuery.css( elem, "padding" + cssExpand[ i ], true, styles );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// At this point, extra isn't border nor margin, so remove border[m
[32m+[m			[32mif ( extra !== "margin" ) {[m
[32m+[m				[32mval -= jQuery.css( elem, "border" + cssExpand[ i ] + "Width", true, styles );[m
[32m+[m			[32m}[m
[32m+[m		[32m} else {[m
[32m+[m
[32m+[m			[32m// At this point, extra isn't content, so add padding[m
[32m+[m			[32mval += jQuery.css( elem, "padding" + cssExpand[ i ], true, styles );[m
[32m+[m
[32m+[m			[32m// At this point, extra isn't content nor padding, so add border[m
[32m+[m			[32mif ( extra !== "padding" ) {[m
[32m+[m				[32mval += jQuery.css( elem, "border" + cssExpand[ i ] + "Width", true, styles );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn val;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction getWidthOrHeight( elem, name, extra ) {[m
[32m+[m
[32m+[m	[32m// Start with offset property, which is equivalent to the border-box value[m
[32m+[m	[32mvar val,[m
[32m+[m		[32mvalueIsBorderBox = true,[m
[32m+[m		[32mstyles = getStyles( elem ),[m
[32m+[m		[32misBorderBox = jQuery.css( elem, "boxSizing", false, styles ) === "border-box";[m
[32m+[m
[32m+[m	[32m// Support: IE <=11 only[m
[32m+[m	[32m// Running getBoundingClientRect on a disconnected node[m
[32m+[m	[32m// in IE throws an error.[m
[32m+[m	[32mif ( elem.getClientRects().length ) {[m
[32m+[m		[32mval = elem.getBoundingClientRect()[ name ];[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Some non-html elements return undefined for offsetWidth, so check for null/undefined[m
[32m+[m	[32m// svg - https://bugzilla.mozilla.org/show_bug.cgi?id=649285[m
[32m+[m	[32m// MathML - https://bugzilla.mozilla.org/show_bug.cgi?id=491668[m
[32m+[m	[32mif ( val <= 0 || val == null ) {[m
[32m+[m
[32m+[m		[32m// Fall back to computed then uncomputed css if necessary[m
[32m+[m		[32mval = curCSS( elem, name, styles );[m
[32m+[m		[32mif ( val < 0 || val == null ) {[m
[32m+[m			[32mval = elem.style[ name ];[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Computed unit is not pixels. Stop here and return.[m
[32m+[m		[32mif ( rnumnonpx.test( val ) ) {[m
[32m+[m			[32mreturn val;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Check for style in case a browser which returns unreliable values[m
[32m+[m		[32m// for getComputedStyle silently falls back to the reliable elem.style[m
[32m+[m		[32mvalueIsBorderBox = isBorderBox &&[m
[32m+[m			[32m( support.boxSizingReliable() || val === elem.style[ name ] );[m
[32m+[m
[32m+[m		[32m// Normalize "", auto, and prepare for extra[m
[32m+[m		[32mval = parseFloat( val ) || 0;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Use the active box-sizing model to add/subtract irrelevant styles[m
[32m+[m	[32mreturn ( val +[m
[32m+[m		[32maugmentWidthOrHeight([m
[32m+[m			[32melem,[m
[32m+[m			[32mname,[m
[32m+[m			[32mextra || ( isBorderBox ? "border" : "content" ),[m
[32m+[m			[32mvalueIsBorderBox,[m
[32m+[m			[32mstyles[m
[32m+[m		[32m)[m
[32m+[m	[32m) + "px";[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mjQuery.extend( {[m
[32m+[m
[32m+[m	[32m// Add in style property hooks for overriding the default[m
[32m+[m	[32m// behavior of getting and setting a style property[m
[32m+[m	[32mcssHooks: {[m
[32m+[m		[32mopacity: {[m
[32m+[m			[32mget: function( elem, computed ) {[m
[32m+[m				[32mif ( computed ) {[m
[32m+[m
[32m+[m					[32m// We should always get a number back from opacity[m
[32m+[m					[32mvar ret = curCSS( elem, "opacity" );[m
[32m+[m					[32mreturn ret === "" ? "1" : ret;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Don't automatically add "px" to these possibly-unitless properties[m
[32m+[m	[32mcssNumber: {[m
[32m+[m		[32m"animationIterationCount": true,[m
[32m+[m		[32m"columnCount": true,[m
[32m+[m		[32m"fillOpacity": true,[m
[32m+[m		[32m"flexGrow": true,[m
[32m+[m		[32m"flexShrink": true,[m
[32m+[m		[32m"fontWeight": true,[m
[32m+[m		[32m"lineHeight": true,[m
[32m+[m		[32m"opacity": true,[m
[32m+[m		[32m"order": true,[m
[32m+[m		[32m"orphans": true,[m
[32m+[m		[32m"widows": true,[m
[32m+[m		[32m"zIndex": true,[m
[32m+[m		[32m"zoom": true[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Add in properties whose names you wish to fix before[m
[32m+[m	[32m// setting or getting the value[m
[32m+[m	[32mcssProps: {[m
[32m+[m		[32m"float": "cssFloat"[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Get and set the style property on a DOM Node[m
[32m+[m	[32mstyle: function( elem, name, value, extra ) {[m
[32m+[m
[32m+[m		[32m// Don't set styles on text and comment nodes[m
[32m+[m		[32mif ( !elem || elem.nodeType === 3 || elem.nodeType === 8 || !elem.style ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Make sure that we're working with the right name[m
[32m+[m		[32mvar ret, type, hooks,[m
[32m+[m			[32morigName = jQuery.camelCase( name ),[m
[32m+[m			[32mstyle = elem.style;[m
[32m+[m
[32m+[m		[32mname = jQuery.cssProps[ origName ] ||[m
[32m+[m			[32m( jQuery.cssProps[ origName ] = vendorPropName( origName ) || origName );[m
[32m+[m
[32m+[m		[32m// Gets hook for the prefixed version, then unprefixed version[m
[32m+[m		[32mhooks = jQuery.cssHooks[ name ] || jQuery.cssHooks[ origName ];[m
[32m+[m
[32m+[m		[32m// Check if we're setting a value[m
[32m+[m		[32mif ( value !== undefined ) {[m
[32m+[m			[32mtype = typeof value;[m
[32m+[m
[32m+[m			[32m// Convert "+=" or "-=" to relative numbers (#7345)[m
[32m+[m			[32mif ( type === "string" && ( ret = rcssNum.exec( value ) ) && ret[ 1 ] ) {[m
[32m+[m				[32mvalue = adjustCSS( elem, name, ret );[m
[32m+[m
[32m+[m				[32m// Fixes bug #9237[m
[32m+[m				[32mtype = "number";[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Make sure that null and NaN values aren't set (#7116)[m
[32m+[m			[32mif ( value == null || value !== value ) {[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// If a number was passed in, add the unit (except for certain CSS properties)[m
[32m+[m			[32mif ( type === "number" ) {[m
[32m+[m				[32mvalue += ret && ret[ 3 ] || ( jQuery.cssNumber[ origName ] ? "" : "px" );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// background-* props affect original clone's values[m
[32m+[m			[32mif ( !support.clearCloneStyle && value === "" && name.indexOf( "background" ) === 0 ) {[m
[32m+[m				[32mstyle[ name ] = "inherit";[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// If a hook was provided, use that value, otherwise just set the specified value[m
[32m+[m			[32mif ( !hooks || !( "set" in hooks ) ||[m
[32m+[m				[32m( value = hooks.set( elem, value, extra ) ) !== undefined ) {[m
[32m+[m
[32m+[m				[32mstyle[ name ] = value;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m		[32m} else {[m
[32m+[m
[32m+[m			[32m// If a hook was provided get the non-computed value from there[m
[32m+[m			[32mif ( hooks && "get" in hooks &&[m
[32m+[m				[32m( ret = hooks.get( elem, false, extra ) ) !== undefined ) {[m
[32m+[m
[32m+[m				[32mreturn ret;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Otherwise just get the value from the style object[m
[32m+[m			[32mreturn style[ name ];[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mcss: function( elem, name, extra, styles ) {[m
[32m+[m		[32mvar val, num, hooks,[m
[32m+[m			[32morigName = jQuery.camelCase( name );[m
[32m+[m
[32m+[m		[32m// Make sure that we're working with the right name[m
[32m+[m		[32mname = jQuery.cssProps[ origName ] ||[m
[32m+[m			[32m( jQuery.cssProps[ origName ] = vendorPropName( origName ) || origName );[m
[32m+[m
[32m+[m		[32m// Try prefixed name followed by the unprefixed name[m
[32m+[m		[32mhooks = jQuery.cssHooks[ name ] || jQuery.cssHooks[ origName ];[m
[32m+[m
[32m+[m		[32m// If a hook was provided get the computed value from there[m
[32m+[m		[32mif ( hooks && "get" in hooks ) {[m
[32m+[m			[32mval = hooks.get( elem, true, extra );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Otherwise, if a way to get the computed value exists, use that[m
[32m+[m		[32mif ( val === undefined ) {[m
[32m+[m			[32mval = curCSS( elem, name, styles );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Convert "normal" to computed value[m
[32m+[m		[32mif ( val === "normal" && name in cssNormalTransform ) {[m
[32m+[m			[32mval = cssNormalTransform[ name ];[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Make numeric if forced or a qualifier was provided and val looks numeric[m
[32m+[m		[32mif ( extra === "" || extra ) {[m
[32m+[m			[32mnum = parseFloat( val );[m
[32m+[m			[32mreturn extra === true || isFinite( num ) ? num || 0 : val;[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn val;[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.each( [ "height", "width" ], function( i, name ) {[m
[32m+[m	[32mjQuery.cssHooks[ name ] = {[m
[32m+[m		[32mget: function( elem, computed, extra ) {[m
[32m+[m			[32mif ( computed ) {[m
[32m+[m
[32m+[m				[32m// Certain elements can have dimension info if we invisibly show them[m
[32m+[m				[32m// but it must have a current display style that would benefit[m
[32m+[m				[32mreturn rdisplayswap.test( jQuery.css( elem, "display" ) ) &&[m
[32m+[m
[32m+[m					[32m// Support: Safari 8+[m
[32m+[m					[32m// Table columns in Safari have non-zero offsetWidth & zero[m
[32m+[m					[32m// getBoundingClientRect().width unless display is changed.[m
[32m+[m					[32m// Support: IE <=11 only[m
[32m+[m					[32m// Running getBoundingClientRect on a disconnected node[m
[32m+[m					[32m// in IE throws an error.[m
[32m+[m					[32m( !elem.getClientRects().length || !elem.getBoundingClientRect().width ) ?[m
[32m+[m						[32mswap( elem, cssShow, function() {[m
[32m+[m							[32mreturn getWidthOrHeight( elem, name, extra );[m
[32m+[m						[32m} ) :[m
[32m+[m						[32mgetWidthOrHeight( elem, name, extra );[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mset: function( elem, value, extra ) {[m
[32m+[m			[32mvar matches,[m
[32m+[m				[32mstyles = extra && getStyles( elem ),[m
[32m+[m				[32msubtract = extra && augmentWidthOrHeight([m
[32m+[m					[32melem,[m
[32m+[m					[32mname,[m
[32m+[m					[32mextra,[m
[32m+[m					[32mjQuery.css( elem, "boxSizing", false, styles ) === "border-box",[m
[32m+[m					[32mstyles[m
[32m+[m				[32m);[m
[32m+[m
[32m+[m			[32m// Convert to pixels if value adjustment is needed[m
[32m+[m			[32mif ( subtract && ( matches = rcssNum.exec( value ) ) &&[m
[32m+[m				[32m( matches[ 3 ] || "px" ) !== "px" ) {[m
[32m+[m
[32m+[m				[32melem.style[ name ] = value;[m
[32m+[m				[32mvalue = jQuery.css( elem, name );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn setPositiveNumber( elem, value, subtract );[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.cssHooks.marginLeft = addGetHookIf( support.reliableMarginLeft,[m
[32m+[m	[32mfunction( elem, computed ) {[m
[32m+[m		[32mif ( computed ) {[m
[32m+[m			[32mreturn ( parseFloat( curCSS( elem, "marginLeft" ) ) ||[m
[32m+[m				[32melem.getBoundingClientRect().left -[m
[32m+[m					[32mswap( elem, { marginLeft: 0 }, function() {[m
[32m+[m						[32mreturn elem.getBoundingClientRect().left;[m
[32m+[m					[32m} )[m
[32m+[m				[32m) + "px";[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m);[m
[32m+[m
[32m+[m[32m// These hooks are used by animate to expand properties[m
[32m+[m[32mjQuery.each( {[m
[32m+[m	[32mmargin: "",[m
[32m+[m	[32mpadding: "",[m
[32m+[m	[32mborder: "Width"[m
[32m+[m[32m}, function( prefix, suffix ) {[m
[32m+[m	[32mjQuery.cssHooks[ prefix + suffix ] = {[m
[32m+[m		[32mexpand: function( value ) {[m
[32m+[m			[32mvar i = 0,[m
[32m+[m				[32mexpanded = {},[m
[32m+[m
[32m+[m				[32m// Assumes a single number if not a string[m
[32m+[m				[32mparts = typeof value === "string" ? value.split( " " ) : [ value ];[m
[32m+[m
[32m+[m			[32mfor ( ; i < 4; i++ ) {[m
[32m+[m				[32mexpanded[ prefix + cssExpand[ i ] + suffix ] =[m
[32m+[m					[32mparts[ i ] || parts[ i - 2 ] || parts[ 0 ];[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn expanded;[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m	[32mif ( !rmargin.test( prefix ) ) {[m
[32m+[m		[32mjQuery.cssHooks[ prefix + suffix ].set = setPositiveNumber;[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m	[32mcss: function( name, value ) {[m
[32m+[m		[32mreturn access( this, function( elem, name, value ) {[m
[32m+[m			[32mvar styles, len,[m
[32m+[m				[32mmap = {},[m
[32m+[m				[32mi = 0;[m
[32m+[m
[32m+[m			[32mif ( jQuery.isArray( name ) ) {[m
[32m+[m				[32mstyles = getStyles( elem );[m
[32m+[m				[32mlen = name.length;[m
[32m+[m
[32m+[m				[32mfor ( ; i < len; i++ ) {[m
[32m+[m					[32mmap[ name[ i ] ] = jQuery.css( elem, name[ i ], false, styles );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mreturn map;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn value !== undefined ?[m
[32m+[m				[32mjQuery.style( elem, name, value ) :[m
[32m+[m				[32mjQuery.css( elem, name );[m
[32m+[m		[32m}, name, value, arguments.length > 1 );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m[32mfunction Tween( elem, options, prop, end, easing ) {[m
[32m+[m	[32mreturn new Tween.prototype.init( elem, options, prop, end, easing );[m
[32m+[m[32m}[m
[32m+[m[32mjQuery.Tween = Tween;[m
[32m+[m
[32m+[m[32mTween.prototype = {[m
[32m+[m	[32mconstructor: Tween,[m
[32m+[m	[32minit: function( elem, options, prop, end, easing, unit ) {[m
[32m+[m		[32mthis.elem = elem;[m
[32m+[m		[32mthis.prop = prop;[m
[32m+[m		[32mthis.easing = easing || jQuery.easing._default;[m
[32m+[m		[32mthis.options = options;[m
[32m+[m		[32mthis.start = this.now = this.cur();[m
[32m+[m		[32mthis.end = end;[m
[32m+[m		[32mthis.unit = unit || ( jQuery.cssNumber[ prop ] ? "" : "px" );[m
[32m+[m	[32m},[m
[32m+[m	[32mcur: function() {[m
[32m+[m		[32mvar hooks = Tween.propHooks[ this.prop ];[m
[32m+[m
[32m+[m		[32mreturn hooks && hooks.get ?[m
[32m+[m			[32mhooks.get( this ) :[m
[32m+[m			[32mTween.propHooks._default.get( this );[m
[32m+[m	[32m},[m
[32m+[m	[32mrun: function( percent ) {[m
[32m+[m		[32mvar eased,[m
[32m+[m			[32mhooks = Tween.propHooks[ this.prop ];[m
[32m+[m
[32m+[m		[32mif ( this.options.duration ) {[m
[32m+[m			[32mthis.pos = eased = jQuery.easing[ this.easing ]([m
[32m+[m				[32mpercent, this.options.duration * percent, 0, 1, this.options.duration[m
[32m+[m			[32m);[m
[32m+[m		[32m} else {[m
[32m+[m			[32mthis.pos = eased = percent;[m
[32m+[m		[32m}[m
[32m+[m		[32mthis.now = ( this.end - this.start ) * eased + this.start;[m
[32m+[m
[32m+[m		[32mif ( this.options.step ) {[m
[32m+[m			[32mthis.options.step.call( this.elem, this.now, this );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( hooks && hooks.set ) {[m
[32m+[m			[32mhooks.set( this );[m
[32m+[m		[32m} else {[m
[32m+[m			[32mTween.propHooks._default.set( this );[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn this;[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mTween.prototype.init.prototype = Tween.prototype;[m
[32m+[m
[32m+[m[32mTween.propHooks = {[m
[32m+[m	[32m_default: {[m
[32m+[m		[32mget: function( tween ) {[m
[32m+[m			[32mvar result;[m
[32m+[m
[32m+[m			[32m// Use a property on the element directly when it is not a DOM element,[m
[32m+[m			[32m// or when there is no matching style property that exists.[m
[32m+[m			[32mif ( tween.elem.nodeType !== 1 ||[m
[32m+[m				[32mtween.elem[ tween.prop ] != null && tween.elem.style[ tween.prop ] == null ) {[m
[32m+[m				[32mreturn tween.elem[ tween.prop ];[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Passing an empty string as a 3rd parameter to .css will automatically[m
[32m+[m			[32m// attempt a parseFloat and fallback to a string if the parse fails.[m
[32m+[m			[32m// Simple values such as "10px" are parsed to Float;[m
[32m+[m			[32m// complex values such as "rotate(1rad)" are returned as-is.[m
[32m+[m			[32mresult = jQuery.css( tween.elem, tween.prop, "" );[m
[32m+[m
[32m+[m			[32m// Empty strings, null, undefined and "auto" are converted to 0.[m
[32m+[m			[32mreturn !result || result === "auto" ? 0 : result;[m
[32m+[m		[32m},[m
[32m+[m		[32mset: function( tween ) {[m
[32m+[m
[32m+[m			[32m// Use step hook for back compat.[m
[32m+[m			[32m// Use cssHook if its there.[m
[32m+[m			[32m// Use .style if available and use plain properties where available.[m
[32m+[m			[32mif ( jQuery.fx.step[ tween.prop ] ) {[m
[32m+[m				[32mjQuery.fx.step[ tween.prop ]( tween );[m
[32m+[m			[32m} else if ( tween.elem.nodeType === 1 &&[m
[32m+[m				[32m( tween.elem.style[ jQuery.cssProps[ tween.prop ] ] != null ||[m
[32m+[m					[32mjQuery.cssHooks[ tween.prop ] ) ) {[m
[32m+[m				[32mjQuery.style( tween.elem, tween.prop, tween.now + tween.unit );[m
[32m+[m			[32m} else {[m
[32m+[m				[32mtween.elem[ tween.prop ] = tween.now;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Support: IE <=9 only[m
[32m+[m[32m// Panic based approach to setting things on disconnected nodes[m
[32m+[m[32mTween.propHooks.scrollTop = Tween.propHooks.scrollLeft = {[m
[32m+[m	[32mset: function( tween ) {[m
[32m+[m		[32mif ( tween.elem.nodeType && tween.elem.parentNode ) {[m
[32m+[m			[32mtween.elem[ tween.prop ] = tween.now;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.easing = {[m
[32m+[m	[32mlinear: function( p ) {[m
[32m+[m		[32mreturn p;[m
[32m+[m	[32m},[m
[32m+[m	[32mswing: function( p ) {[m
[32m+[m		[32mreturn 0.5 - Math.cos( p * Math.PI ) / 2;[m
[32m+[m	[32m},[m
[32m+[m	[32m_default: "swing"[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.fx = Tween.prototype.init;[m
[32m+[m
[32m+[m[32m// Back compat <1.8 extension point[m
[32m+[m[32mjQuery.fx.step = {};[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mvar[m
[32m+[m	[32mfxNow, timerId,[m
[32m+[m	[32mrfxtypes = /^(?:toggle|show|hide)$/,[m
[32m+[m	[32mrrun = /queueHooks$/;[m
[32m+[m
[32m+[m[32mfunction raf() {[m
[32m+[m	[32mif ( timerId ) {[m
[32m+[m		[32mwindow.requestAnimationFrame( raf );[m
[32m+[m		[32mjQuery.fx.tick();[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Animations created synchronously will run synchronously[m
[32m+[m[32mfunction createFxNow() {[m
[32m+[m	[32mwindow.setTimeout( function() {[m
[32m+[m		[32mfxNow = undefined;[m
[32m+[m	[32m} );[m
[32m+[m	[32mreturn ( fxNow = jQuery.now() );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Generate parameters to create a standard animation[m
[32m+[m[32mfunction genFx( type, includeWidth ) {[m
[32m+[m	[32mvar which,[m
[32m+[m		[32mi = 0,[m
[32m+[m		[32mattrs = { height: type };[m
[32m+[m
[32m+[m	[32m// If we include width, step value is 1 to do all cssExpand values,[m
[32m+[m	[32m// otherwise step value is 2 to skip over Left and Right[m
[32m+[m	[32mincludeWidth = includeWidth ? 1 : 0;[m
[32m+[m	[32mfor ( ; i < 4; i += 2 - includeWidth ) {[m
[32m+[m		[32mwhich = cssExpand[ i ];[m
[32m+[m		[32mattrs[ "margin" + which ] = attrs[ "padding" + which ] = type;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( includeWidth ) {[m
[32m+[m		[32mattrs.opacity = attrs.width = type;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn attrs;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction createTween( value, prop, animation ) {[m
[32m+[m	[32mvar tween,[m
[32m+[m		[32mcollection = ( Animation.tweeners[ prop ] || [] ).concat( Animation.tweeners[ "*" ] ),[m
[32m+[m		[32mindex = 0,[m
[32m+[m		[32mlength = collection.length;[m
[32m+[m	[32mfor ( ; index < length; index++ ) {[m
[32m+[m		[32mif ( ( tween = collection[ index ].call( animation, prop, value ) ) ) {[m
[32m+[m
[32m+[m			[32m// We're done with this property[m
[32m+[m			[32mreturn tween;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction defaultPrefilter( elem, props, opts ) {[m
[32m+[m	[32mvar prop, value, toggle, hooks, oldfire, propTween, restoreDisplay, display,[m
[32m+[m		[32misBox = "width" in props || "height" in props,[m
[32m+[m		[32manim = this,[m
[32m+[m		[32morig = {},[m
[32m+[m		[32mstyle = elem.style,[m
[32m+[m		[32mhidden = elem.nodeType && isHiddenWithinTree( elem ),[m
[32m+[m		[32mdataShow = dataPriv.get( elem, "fxshow" );[m
[32m+[m
[32m+[m	[32m// Queue-skipping animations hijack the fx hooks[m
[32m+[m	[32mif ( !opts.queue ) {[m
[32m+[m		[32mhooks = jQuery._queueHooks( elem, "fx" );[m
[32m+[m		[32mif ( hooks.unqueued == null ) {[m
[32m+[m			[32mhooks.unqueued = 0;[m
[32m+[m			[32moldfire = hooks.empty.fire;[m
[32m+[m			[32mhooks.empty.fire = function() {[m
[32m+[m				[32mif ( !hooks.unqueued ) {[m
[32m+[m					[32moldfire();[m
[32m+[m				[32m}[m
[32m+[m			[32m};[m
[32m+[m		[32m}[m
[32m+[m		[32mhooks.unqueued++;[m
[32m+[m
[32m+[m		[32manim.always( function() {[m
[32m+[m
[32m+[m			[32m// Ensure the complete handler is called before this completes[m
[32m+[m			[32manim.always( function() {[m
[32m+[m				[32mhooks.unqueued--;[m
[32m+[m				[32mif ( !jQuery.queue( elem, "fx" ).length ) {[m
[32m+[m					[32mhooks.empty.fire();[m
[32m+[m				[32m}[m
[32m+[m			[32m} );[m
[32m+[m		[32m} );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Detect show/hide animations[m
[32m+[m	[32mfor ( prop in props ) {[m
[32m+[m		[32mvalue = props[ prop ];[m
[32m+[m		[32mif ( rfxtypes.test( value ) ) {[m
[32m+[m			[32mdelete props[ prop ];[m
[32m+[m			[32mtoggle = toggle || value === "toggle";[m
[32m+[m			[32mif ( value === ( hidden ? "hide" : "show" ) ) {[m
[32m+[m
[32m+[m				[32m// Pretend to be hidden if this is a "show" and[m
[32m+[m				[32m// there is still data from a stopped show/hide[m
[32m+[m				[32mif ( value === "show" && dataShow && dataShow[ prop ] !== undefined ) {[m
[32m+[m					[32mhidden = true;[m
[32m+[m
[32m+[m				[32m// Ignore all other no-op show/hide data[m
[32m+[m				[32m} else {[m
[32m+[m					[32mcontinue;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m			[32morig[ prop ] = dataShow && dataShow[ prop ] || jQuery.style( elem, prop );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Bail out if this is a no-op like .hide().hide()[m
[32m+[m	[32mpropTween = !jQuery.isEmptyObject( props );[m
[32m+[m	[32mif ( !propTween && jQuery.isEmptyObject( orig ) ) {[m
[32m+[m		[32mreturn;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Restrict "overflow" and "display" styles during box animations[m
[32m+[m	[32mif ( isBox && elem.nodeType === 1 ) {[m
[32m+[m
[32m+[m		[32m// Support: IE <=9 - 11, Edge 12 - 13[m
[32m+[m		[32m// Record all 3 overflow attributes because IE does not infer the shorthand[m
[32m+[m		[32m// from identically-valued overflowX and overflowY[m
[32m+[m		[32mopts.overflow = [ style.overflow, style.overflowX, style.overflowY ];[m
[32m+[m
[32m+[m		[32m// Identify a display type, preferring old show/hide data over the CSS cascade[m
[32m+[m		[32mrestoreDisplay = dataShow && dataShow.display;[m
[32m+[m		[32mif ( restoreDisplay == null ) {[m
[32m+[m			[32mrestoreDisplay = dataPriv.get( elem, "display" );[m
[32m+[m		[32m}[m
[32m+[m		[32mdisplay = jQuery.css( elem, "display" );[m
[32m+[m		[32mif ( display === "none" ) {[m
[32m+[m			[32mif ( restoreDisplay ) {[m
[32m+[m				[32mdisplay = restoreDisplay;[m
[32m+[m			[32m} else {[m
[32m+[m
[32m+[m				[32m// Get nonempty value(s) by temporarily forcing visibility[m
[32m+[m				[32mshowHide( [ elem ], true );[m
[32m+[m				[32mrestoreDisplay = elem.style.display || restoreDisplay;[m
[32m+[m				[32mdisplay = jQuery.css( elem, "display" );[m
[32m+[m				[32mshowHide( [ elem ] );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Animate inline elements as inline-block[m
[32m+[m		[32mif ( display === "inline" || display === "inline-block" && restoreDisplay != null ) {[m
[32m+[m			[32mif ( jQuery.css( elem, "float" ) === "none" ) {[m
[32m+[m
[32m+[m				[32m// Restore the original display value at the end of pure show/hide animations[m
[32m+[m				[32mif ( !propTween ) {[m
[32m+[m					[32manim.done( function() {[m
[32m+[m						[32mstyle.display = restoreDisplay;[m
[32m+[m					[32m} );[m
[32m+[m					[32mif ( restoreDisplay == null ) {[m
[32m+[m						[32mdisplay = style.display;[m
[32m+[m						[32mrestoreDisplay = display === "none" ? "" : display;[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m				[32mstyle.display = "inline-block";[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( opts.overflow ) {[m
[32m+[m		[32mstyle.overflow = "hidden";[m
[32m+[m		[32manim.always( function() {[m
[32m+[m			[32mstyle.overflow = opts.overflow[ 0 ];[m
[32m+[m			[32mstyle.overflowX = opts.overflow[ 1 ];[m
[32m+[m			[32mstyle.overflowY = opts.overflow[ 2 ];[m
[32m+[m		[32m} );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Implement show/hide animations[m
[32m+[m	[32mpropTween = false;[m
[32m+[m	[32mfor ( prop in orig ) {[m
[32m+[m
[32m+[m		[32m// General show/hide setup for this element animation[m
[32m+[m		[32mif ( !propTween ) {[m
[32m+[m			[32mif ( dataShow ) {[m
[32m+[m				[32mif ( "hidden" in dataShow ) {[m
[32m+[m					[32mhidden = dataShow.hidden;[m
[32m+[m				[32m}[m
[32m+[m			[32m} else {[m
[32m+[m				[32mdataShow = dataPriv.access( elem, "fxshow", { display: restoreDisplay } );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Store hidden/visible for toggle so `.stop().toggle()` "reverses"[m
[32m+[m			[32mif ( toggle ) {[m
[32m+[m				[32mdataShow.hidden = !hidden;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Show elements before animating them[m
[32m+[m			[32mif ( hidden ) {[m
[32m+[m				[32mshowHide( [ elem ], true );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m/* eslint-disable no-loop-func */[m
[32m+[m
[32m+[m			[32manim.done( function() {[m
[32m+[m
[32m+[m			[32m/* eslint-enable no-loop-func */[m
[32m+[m
[32m+[m				[32m// The final step of a "hide" animation is actually hiding the element[m
[32m+[m				[32mif ( !hidden ) {[m
[32m+[m					[32mshowHide( [ elem ] );[m
[32m+[m				[32m}[m
[32m+[m				[32mdataPriv.remove( elem, "fxshow" );[m
[32m+[m				[32mfor ( prop in orig ) {[m
[32m+[m					[32mjQuery.style( elem, prop, orig[ prop ] );[m
[32m+[m				[32m}[m
[32m+[m			[32m} );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Per-property setup[m
[32m+[m		[32mpropTween = createTween( hidden ? dataShow[ prop ] : 0, prop, anim );[m
[32m+[m		[32mif ( !( prop in dataShow ) ) {[m
[32m+[m			[32mdataShow[ prop ] = propTween.start;[m
[32m+[m			[32mif ( hidden ) {[m
[32m+[m				[32mpropTween.end = propTween.start;[m
[32m+[m				[32mpropTween.start = 0;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction propFilter( props, specialEasing ) {[m
[32m+[m	[32mvar index, name, easing, value, hooks;[m
[32m+[m
[32m+[m	[32m// camelCase, specialEasing and expand cssHook pass[m
[32m+[m	[32mfor ( index in props ) {[m
[32m+[m		[32mname = jQuery.camelCase( index );[m
[32m+[m		[32measing = specialEasing[ name ];[m
[32m+[m		[32mvalue = props[ index ];[m
[32m+[m		[32mif ( jQuery.isArray( value ) ) {[m
[32m+[m			[32measing = value[ 1 ];[m
[32m+[m			[32mvalue = props[ index ] = value[ 0 ];[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( index !== name ) {[m
[32m+[m			[32mprops[ name ] = value;[m
[32m+[m			[32mdelete props[ index ];[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mhooks = jQuery.cssHooks[ name ];[m
[32m+[m		[32mif ( hooks && "expand" in hooks ) {[m
[32m+[m			[32mvalue = hooks.expand( value );[m
[32m+[m			[32mdelete props[ name ];[m
[32m+[m
[32m+[m			[32m// Not quite $.extend, this won't overwrite existing keys.[m
[32m+[m			[32m// Reusing 'index' because we have the correct "name"[m
[32m+[m			[32mfor ( index in value ) {[m
[32m+[m				[32mif ( !( index in props ) ) {[m
[32m+[m					[32mprops[ index ] = value[ index ];[m
[32m+[m					[32mspecialEasing[ index ] = easing;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m} else {[m
[32m+[m			[32mspecialEasing[ name ] = easing;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction Animation( elem, properties, options ) {[m
[32m+[m	[32mvar result,[m
[32m+[m		[32mstopped,[m
[32m+[m		[32mindex = 0,[m
[32m+[m		[32mlength = Animation.prefilters.length,[m
[32m+[m		[32mdeferred = jQuery.Deferred().always( function() {[m
[32m+[m
[32m+[m			[32m// Don't match elem in the :animated selector[m
[32m+[m			[32mdelete tick.elem;[m
[32m+[m		[32m} ),[m
[32m+[m		[32mtick = function() {[m
[32m+[m			[32mif ( stopped ) {[m
[32m+[m				[32mreturn false;[m
[32m+[m			[32m}[m
[32m+[m			[32mvar currentTime = fxNow || createFxNow(),[m
[32m+[m				[32mremaining = Math.max( 0, animation.startTime + animation.duration - currentTime ),[m
[32m+[m
[32m+[m				[32m// Support: Android 2.3 only[m
[32m+[m				[32m// Archaic crash bug won't allow us to use `1 - ( 0.5 || 0 )` (#12497)[m
[32m+[m				[32mtemp = remaining / animation.duration || 0,[m
[32m+[m				[32mpercent = 1 - temp,[m
[32m+[m				[32mindex = 0,[m
[32m+[m				[32mlength = animation.tweens.length;[m
[32m+[m
[32m+[m			[32mfor ( ; index < length; index++ ) {[m
[32m+[m				[32manimation.tweens[ index ].run( percent );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mdeferred.notifyWith( elem, [ animation, percent, remaining ] );[m
[32m+[m
[32m+[m			[32mif ( percent < 1 && length ) {[m
[32m+[m				[32mreturn remaining;[m
[32m+[m			[32m} else {[m
[32m+[m				[32mdeferred.resolveWith( elem, [ animation ] );[m
[32m+[m				[32mreturn false;[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m		[32manimation = deferred.promise( {[m
[32m+[m			[32melem: elem,[m
[32m+[m			[32mprops: jQuery.extend( {}, properties ),[m
[32m+[m			[32mopts: jQuery.extend( true, {[m
[32m+[m				[32mspecialEasing: {},[m
[32m+[m				[32measing: jQuery.easing._default[m
[32m+[m			[32m}, options ),[m
[32m+[m			[32moriginalProperties: properties,[m
[32m+[m			[32moriginalOptions: options,[m
[32m+[m			[32mstartTime: fxNow || createFxNow(),[m
[32m+[m			[32mduration: options.duration,[m
[32m+[m			[32mtweens: [],[m
[32m+[m			[32mcreateTween: function( prop, end ) {[m
[32m+[m				[32mvar tween = jQuery.Tween( elem, animation.opts, prop, end,[m
[32m+[m						[32manimation.opts.specialEasing[ prop ] || animation.opts.easing );[m
[32m+[m				[32manimation.tweens.push( tween );[m
[32m+[m				[32mreturn tween;[m
[32m+[m			[32m},[m
[32m+[m			[32mstop: function( gotoEnd ) {[m
[32m+[m				[32mvar index = 0,[m
[32m+[m
[32m+[m					[32m// If we are going to the end, we want to run all the tweens[m
[32m+[m					[32m// otherwise we skip this part[m
[32m+[m					[32mlength = gotoEnd ? animation.tweens.length : 0;[m
[32m+[m				[32mif ( stopped ) {[m
[32m+[m					[32mreturn this;[m
[32m+[m				[32m}[m
[32m+[m				[32mstopped = true;[m
[32m+[m				[32mfor ( ; index < length; index++ ) {[m
[32m+[m					[32manimation.tweens[ index ].run( 1 );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Resolve when we played the last frame; otherwise, reject[m
[32m+[m				[32mif ( gotoEnd ) {[m
[32m+[m					[32mdeferred.notifyWith( elem, [ animation, 1, 0 ] );[m
[32m+[m					[32mdeferred.resolveWith( elem, [ animation, gotoEnd ] );[m
[32m+[m				[32m} else {[m
[32m+[m					[32mdeferred.rejectWith( elem, [ animation, gotoEnd ] );[m
[32m+[m				[32m}[m
[32m+[m				[32mreturn this;[m
[32m+[m			[32m}[m
[32m+[m		[32m} ),[m
[32m+[m		[32mprops = animation.props;[m
[32m+[m
[32m+[m	[32mpropFilter( props, animation.opts.specialEasing );[m
[32m+[m
[32m+[m	[32mfor ( ; index < length; index++ ) {[m
[32m+[m		[32mresult = Animation.prefilters[ index ].call( animation, elem, props, animation.opts );[m
[32m+[m		[32mif ( result ) {[m
[32m+[m			[32mif ( jQuery.isFunction( result.stop ) ) {[m
[32m+[m				[32mjQuery._queueHooks( animation.elem, animation.opts.queue ).stop =[m
[32m+[m					[32mjQuery.proxy( result.stop, result );[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn result;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mjQuery.map( props, createTween, animation );[m
[32m+[m
[32m+[m	[32mif ( jQuery.isFunction( animation.opts.start ) ) {[m
[32m+[m		[32manimation.opts.start.call( elem, animation );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mjQuery.fx.timer([m
[32m+[m		[32mjQuery.extend( tick, {[m
[32m+[m			[32melem: elem,[m
[32m+[m			[32manim: animation,[m
[32m+[m			[32mqueue: animation.opts.queue[m
[32m+[m		[32m} )[m
[32m+[m	[32m);[m
[32m+[m
[32m+[m	[32m// attach callbacks from options[m
[32m+[m	[32mreturn animation.progress( animation.opts.progress )[m
[32m+[m		[32m.done( animation.opts.done, animation.opts.complete )[m
[32m+[m		[32m.fail( animation.opts.fail )[m
[32m+[m		[32m.always( animation.opts.always );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mjQuery.Animation = jQuery.extend( Animation, {[m
[32m+[m
[32m+[m	[32mtweeners: {[m
[32m+[m		[32m"*": [ function( prop, value ) {[m
[32m+[m			[32mvar tween = this.createTween( prop, value );[m
[32m+[m			[32madjustCSS( tween.elem, prop, rcssNum.exec( value ), tween );[m
[32m+[m			[32mreturn tween;[m
[32m+[m		[32m} ][m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mtweener: function( props, callback ) {[m
[32m+[m		[32mif ( jQuery.isFunction( props ) ) {[m
[32m+[m			[32mcallback = props;[m
[32m+[m			[32mprops = [ "*" ];[m
[32m+[m		[32m} else {[m
[32m+[m			[32mprops = props.match( rnotwhite );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mvar prop,[m
[32m+[m			[32mindex = 0,[m
[32m+[m			[32mlength = props.length;[m
[32m+[m
[32m+[m		[32mfor ( ; index < length; index++ ) {[m
[32m+[m			[32mprop = props[ index ];[m
[32m+[m			[32mAnimation.tweeners[ prop ] = Animation.tweeners[ prop ] || [];[m
[32m+[m			[32mAnimation.tweeners[ prop ].unshift( callback );[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mprefilters: [ defaultPrefilter ],[m
[32m+[m
[32m+[m	[32mprefilter: function( callback, prepend ) {[m
[32m+[m		[32mif ( prepend ) {[m
[32m+[m			[32mAnimation.prefilters.unshift( callback );[m
[32m+[m		[32m} else {[m
[32m+[m			[32mAnimation.prefilters.push( callback );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.speed = function( speed, easing, fn ) {[m
[32m+[m	[32mvar opt = speed && typeof speed === "object" ? jQuery.extend( {}, speed ) : {[m
[32m+[m		[32mcomplete: fn || !fn && easing ||[m
[32m+[m			[32mjQuery.isFunction( speed ) && speed,[m
[32m+[m		[32mduration: speed,[m
[32m+[m		[32measing: fn && easing || easing && !jQuery.isFunction( easing ) && easing[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m	[32m// Go to the end state if fx are off or if document is hidden[m
[32m+[m	[32mif ( jQuery.fx.off || document.hidden ) {[m
[32m+[m		[32mopt.duration = 0;[m
[32m+[m
[32m+[m	[32m} else {[m
[32m+[m		[32mopt.duration = typeof opt.duration === "number" ?[m
[32m+[m			[32mopt.duration : opt.duration in jQuery.fx.speeds ?[m
[32m+[m				[32mjQuery.fx.speeds[ opt.duration ] : jQuery.fx.speeds._default;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Normalize opt.queue - true/undefined/null -> "fx"[m
[32m+[m	[32mif ( opt.queue == null || opt.queue === true ) {[m
[32m+[m		[32mopt.queue = "fx";[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Queueing[m
[32m+[m	[32mopt.old = opt.complete;[m
[32m+[m
[32m+[m	[32mopt.complete = function() {[m
[32m+[m		[32mif ( jQuery.isFunction( opt.old ) ) {[m
[32m+[m			[32mopt.old.call( this );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( opt.queue ) {[m
[32m+[m			[32mjQuery.dequeue( this, opt.queue );[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m
[32m+[m
[32m+[m	[32mreturn opt;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m	[32mfadeTo: function( speed, to, easing, callback ) {[m
[32m+[m
[32m+[m		[32m// Show any hidden elements after setting opacity to 0[m
[32m+[m		[32mreturn this.filter( isHiddenWithinTree ).css( "opacity", 0 ).show()[m
[32m+[m
[32m+[m			[32m// Animate to the value specified[m
[32m+[m			[32m.end().animate( { opacity: to }, speed, easing, callback );[m
[32m+[m	[32m},[m
[32m+[m	[32manimate: function( prop, speed, easing, callback ) {[m
[32m+[m		[32mvar empty = jQuery.isEmptyObject( prop ),[m
[32m+[m			[32moptall = jQuery.speed( speed, easing, callback ),[m
[32m+[m			[32mdoAnimation = function() {[m
[32m+[m
[32m+[m				[32m// Operate on a copy of prop so per-property easing won't be lost[m
[32m+[m				[32mvar anim = Animation( this, jQuery.extend( {}, prop ), optall );[m
[32m+[m
[32m+[m				[32m// Empty animations, or finishing resolves immediately[m
[32m+[m				[32mif ( empty || dataPriv.get( this, "finish" ) ) {[m
[32m+[m					[32manim.stop( true );[m
[32m+[m				[32m}[m
[32m+[m			[32m};[m
[32m+[m			[32mdoAnimation.finish = doAnimation;[m
[32m+[m
[32m+[m		[32mreturn empty || optall.queue === false ?[m
[32m+[m			[32mthis.each( doAnimation ) :[m
[32m+[m			[32mthis.queue( optall.queue, doAnimation );[m
[32m+[m	[32m},[m
[32m+[m	[32mstop: function( type, clearQueue, gotoEnd ) {[m
[32m+[m		[32mvar stopQueue = function( hooks ) {[m
[32m+[m			[32mvar stop = hooks.stop;[m
[32m+[m			[32mdelete hooks.stop;[m
[32m+[m			[32mstop( gotoEnd );[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m		[32mif ( typeof type !== "string" ) {[m
[32m+[m			[32mgotoEnd = clearQueue;[m
[32m+[m			[32mclearQueue = type;[m
[32m+[m			[32mtype = undefined;[m
[32m+[m		[32m}[m
[32m+[m		[32mif ( clearQueue && type !== false ) {[m
[32m+[m			[32mthis.queue( type || "fx", [] );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn this.each( function() {[m
[32m+[m			[32mvar dequeue = true,[m
[32m+[m				[32mindex = type != null && type + "queueHooks",[m
[32m+[m				[32mtimers = jQuery.timers,[m
[32m+[m				[32mdata = dataPriv.get( this );[m
[32m+[m
[32m+[m			[32mif ( index ) {[m
[32m+[m				[32mif ( data[ index ] && data[ index ].stop ) {[m
[32m+[m					[32mstopQueue( data[ index ] );[m
[32m+[m				[32m}[m
[32m+[m			[32m} else {[m
[32m+[m				[32mfor ( index in data ) {[m
[32m+[m					[32mif ( data[ index ] && data[ index ].stop && rrun.test( index ) ) {[m
[32m+[m						[32mstopQueue( data[ index ] );[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mfor ( index = timers.length; index--; ) {[m
[32m+[m				[32mif ( timers[ index ].elem === this &&[m
[32m+[m					[32m( type == null || timers[ index ].queue === type ) ) {[m
[32m+[m
[32m+[m					[32mtimers[ index ].anim.stop( gotoEnd );[m
[32m+[m					[32mdequeue = false;[m
[32m+[m					[32mtimers.splice( index, 1 );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Start the next in the queue if the last step wasn't forced.[m
[32m+[m			[32m// Timers currently will call their complete callbacks, which[m
[32m+[m			[32m// will dequeue but only if they were gotoEnd.[m
[32m+[m			[32mif ( dequeue || !gotoEnd ) {[m
[32m+[m				[32mjQuery.dequeue( this, type );[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m	[32m},[m
[32m+[m	[32mfinish: function( type ) {[m
[32m+[m		[32mif ( type !== false ) {[m
[32m+[m			[32mtype = type || "fx";[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn this.each( function() {[m
[32m+[m			[32mvar index,[m
[32m+[m				[32mdata = dataPriv.get( this ),[m
[32m+[m				[32mqueue = data[ type + "queue" ],[m
[32m+[m				[32mhooks = data[ type + "queueHooks" ],[m
[32m+[m				[32mtimers = jQuery.timers,[m
[32m+[m				[32mlength = queue ? queue.length : 0;[m
[32m+[m
[32m+[m			[32m// Enable finishing flag on private data[m
[32m+[m			[32mdata.finish = true;[m
[32m+[m
[32m+[m			[32m// Empty the queue first[m
[32m+[m			[32mjQuery.queue( this, type, [] );[m
[32m+[m
[32m+[m			[32mif ( hooks && hooks.stop ) {[m
[32m+[m				[32mhooks.stop.call( this, true );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Look for any active animations, and finish them[m
[32m+[m			[32mfor ( index = timers.length; index--; ) {[m
[32m+[m				[32mif ( timers[ index ].elem === this && timers[ index ].queue === type ) {[m
[32m+[m					[32mtimers[ index ].anim.stop( true );[m
[32m+[m					[32mtimers.splice( index, 1 );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Look for any animations in the old queue and finish them[m
[32m+[m			[32mfor ( index = 0; index < length; index++ ) {[m
[32m+[m				[32mif ( queue[ index ] && queue[ index ].finish ) {[m
[32m+[m					[32mqueue[ index ].finish.call( this );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Turn off finishing flag[m
[32m+[m			[32mdelete data.finish;[m
[32m+[m		[32m} );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.each( [ "toggle", "show", "hide" ], function( i, name ) {[m
[32m+[m	[32mvar cssFn = jQuery.fn[ name ];[m
[32m+[m	[32mjQuery.fn[ name ] = function( speed, easing, callback ) {[m
[32m+[m		[32mreturn speed == null || typeof speed === "boolean" ?[m
[32m+[m			[32mcssFn.apply( this, arguments ) :[m
[32m+[m			[32mthis.animate( genFx( name, true ), speed, easing, callback );[m
[32m+[m	[32m};[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32m// Generate shortcuts for custom animations[m
[32m+[m[32mjQuery.each( {[m
[32m+[m	[32mslideDown: genFx( "show" ),[m
[32m+[m	[32mslideUp: genFx( "hide" ),[m
[32m+[m	[32mslideToggle: genFx( "toggle" ),[m
[32m+[m	[32mfadeIn: { opacity: "show" },[m
[32m+[m	[32mfadeOut: { opacity: "hide" },[m
[32m+[m	[32mfadeToggle: { opacity: "toggle" }[m
[32m+[m[32m}, function( name, props ) {[m
[32m+[m	[32mjQuery.fn[ name ] = function( speed, easing, callback ) {[m
[32m+[m		[32mreturn this.animate( props, speed, easing, callback );[m
[32m+[m	[32m};[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.timers = [];[m
[32m+[m[32mjQuery.fx.tick = function() {[m
[32m+[m	[32mvar timer,[m
[32m+[m		[32mi = 0,[m
[32m+[m		[32mtimers = jQuery.timers;[m
[32m+[m
[32m+[m	[32mfxNow = jQuery.now();[m
[32m+[m
[32m+[m	[32mfor ( ; i < timers.length; i++ ) {[m
[32m+[m		[32mtimer = timers[ i ];[m
[32m+[m
[32m+[m		[32m// Checks the timer has not already been removed[m
[32m+[m		[32mif ( !timer() && timers[ i ] === timer ) {[m
[32m+[m			[32mtimers.splice( i--, 1 );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( !timers.length ) {[m
[32m+[m		[32mjQuery.fx.stop();[m
[32m+[m	[32m}[m
[32m+[m	[32mfxNow = undefined;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.fx.timer = function( timer ) {[m
[32m+[m	[32mjQuery.timers.push( timer );[m
[32m+[m	[32mif ( timer() ) {[m
[32m+[m		[32mjQuery.fx.start();[m
[32m+[m	[32m} else {[m
[32m+[m		[32mjQuery.timers.pop();[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.fx.interval = 13;[m
[32m+[m[32mjQuery.fx.start = function() {[m
[32m+[m	[32mif ( !timerId ) {[m
[32m+[m		[32mtimerId = window.requestAnimationFrame ?[m
[32m+[m			[32mwindow.requestAnimationFrame( raf ) :[m
[32m+[m			[32mwindow.setInterval( jQuery.fx.tick, jQuery.fx.interval );[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.fx.stop = function() {[m
[32m+[m	[32mif ( window.cancelAnimationFrame ) {[m
[32m+[m		[32mwindow.cancelAnimationFrame( timerId );[m
[32m+[m	[32m} else {[m
[32m+[m		[32mwindow.clearInterval( timerId );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mtimerId = null;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.fx.speeds = {[m
[32m+[m	[32mslow: 600,[m
[32m+[m	[32mfast: 200,[m
[32m+[m
[32m+[m	[32m// Default speed[m
[32m+[m	[32m_default: 400[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m[32m// Based off of the plugin by Clint Helfers, with permission.[m
[32m+[m[32m// https://web.archive.org/web/20100324014747/http://blindsignals.com/index.php/2009/07/jquery-delay/[m
[32m+[m[32mjQuery.fn.delay = function( time, type ) {[m
[32m+[m	[32mtime = jQuery.fx ? jQuery.fx.speeds[ time ] || time : time;[m
[32m+[m	[32mtype = type || "fx";[m
[32m+[m
[32m+[m	[32mreturn this.queue( type, function( next, hooks ) {[m
[32m+[m		[32mvar timeout = window.setTimeout( next, time );[m
[32m+[m		[32mhooks.stop = function() {[m
[32m+[m			[32mwindow.clearTimeout( timeout );[m
[32m+[m		[32m};[m
[32m+[m	[32m} );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m[32m( function() {[m
[32m+[m	[32mvar input = document.createElement( "input" ),[m
[32m+[m		[32mselect = document.createElement( "select" ),[m
[32m+[m		[32mopt = select.appendChild( document.createElement( "option" ) );[m
[32m+[m
[32m+[m	[32minput.type = "checkbox";[m
[32m+[m
[32m+[m	[32m// Support: Android <=4.3 only[m
[32m+[m	[32m// Default value for a checkbox should be "on"[m
[32m+[m	[32msupport.checkOn = input.value !== "";[m
[32m+[m
[32m+[m	[32m// Support: IE <=11 only[m
[32m+[m	[32m// Must access selectedIndex to make default options select[m
[32m+[m	[32msupport.optSelected = opt.selected;[m
[32m+[m
[32m+[m	[32m// Support: IE <=11 only[m
[32m+[m	[32m// An input loses its value after becoming a radio[m
[32m+[m	[32minput = document.createElement( "input" );[m
[32m+[m	[32minput.value = "t";[m
[32m+[m	[32minput.type = "radio";[m
[32m+[m	[32msupport.radioValue = input.value === "t";[m
[32m+[m[32m} )();[m
[32m+[m
[32m+[m
[32m+[m[32mvar boolHook,[m
[32m+[m	[32mattrHandle = jQuery.expr.attrHandle;[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m	[32mattr: function( name, value ) {[m
[32m+[m		[32mreturn access( this, jQuery.attr, name, value, arguments.length > 1 );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mremoveAttr: function( name ) {[m
[32m+[m		[32mreturn this.each( function() {[m
[32m+[m			[32mjQuery.removeAttr( this, name );[m
[32m+[m		[32m} );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.extend( {[m
[32m+[m	[32mattr: function( elem, name, value ) {[m
[32m+[m		[32mvar ret, hooks,[m
[32m+[m			[32mnType = elem.nodeType;[m
[32m+[m
[32m+[m		[32m// Don't get/set attributes on text, comment and attribute nodes[m
[32m+[m		[32mif ( nType === 3 || nType === 8 || nType === 2 ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Fallback to prop when attributes are not supported[m
[32m+[m		[32mif ( typeof elem.getAttribute === "undefined" ) {[m
[32m+[m			[32mreturn jQuery.prop( elem, name, value );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Attribute hooks are determined by the lowercase version[m
[32m+[m		[32m// Grab necessary hook if one is defined[m
[32m+[m		[32mif ( nType !== 1 || !jQuery.isXMLDoc( elem ) ) {[m
[32m+[m			[32mhooks = jQuery.attrHooks[ name.toLowerCase() ] ||[m
[32m+[m				[32m( jQuery.expr.match.bool.test( name ) ? boolHook : undefined );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( value !== undefined ) {[m
[32m+[m			[32mif ( value === null ) {[m
[32m+[m				[32mjQuery.removeAttr( elem, name );[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif ( hooks && "set" in hooks &&[m
[32m+[m				[32m( ret = hooks.set( elem, value, name ) ) !== undefined ) {[m
[32m+[m				[32mreturn ret;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32melem.setAttribute( name, value + "" );[m
[32m+[m			[32mreturn value;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( hooks && "get" in hooks && ( ret = hooks.get( elem, name ) ) !== null ) {[m
[32m+[m			[32mreturn ret;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mret = jQuery.find.attr( elem, name );[m
[32m+[m
[32m+[m		[32m// Non-existent attributes return null, we normalize to undefined[m
[32m+[m		[32mreturn ret == null ? undefined : ret;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mattrHooks: {[m
[32m+[m		[32mtype: {[m
[32m+[m			[32mset: function( elem, value ) {[m
[32m+[m				[32mif ( !support.radioValue && value === "radio" &&[m
[32m+[m					[32mjQuery.nodeName( elem, "input" ) ) {[m
[32m+[m					[32mvar val = elem.value;[m
[32m+[m					[32melem.setAttribute( "type", value );[m
[32m+[m					[32mif ( val ) {[m
[32m+[m						[32melem.value = val;[m
[32m+[m					[32m}[m
[32m+[m					[32mreturn value;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mremoveAttr: function( elem, value ) {[m
[32m+[m		[32mvar name,[m
[32m+[m			[32mi = 0,[m
[32m+[m			[32mattrNames = value && value.match( rnotwhite );[m
[32m+[m
[32m+[m		[32mif ( attrNames && elem.nodeType === 1 ) {[m
[32m+[m			[32mwhile ( ( name = attrNames[ i++ ] ) ) {[m
[32m+[m				[32melem.removeAttribute( name );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32m// Hooks for boolean attributes[m
[32m+[m[32mboolHook = {[m
[32m+[m	[32mset: function( elem, value, name ) {[m
[32m+[m		[32mif ( value === false ) {[m
[32m+[m
[32m+[m			[32m// Remove boolean attributes when set to false[m
[32m+[m			[32mjQuery.removeAttr( elem, name );[m
[32m+[m		[32m} else {[m
[32m+[m			[32melem.setAttribute( name, name );[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn name;[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.each( jQuery.expr.match.bool.source.match( /\w+/g ), function( i, name ) {[m
[32m+[m	[32mvar getter = attrHandle[ name ] || jQuery.find.attr;[m
[32m+[m
[32m+[m	[32mattrHandle[ name ] = function( elem, name, isXML ) {[m
[32m+[m		[32mvar ret, handle,[m
[32m+[m			[32mlowercaseName = name.toLowerCase();[m
[32m+[m
[32m+[m		[32mif ( !isXML ) {[m
[32m+[m
[32m+[m			[32m// Avoid an infinite loop by temporarily removing this function from the getter[m
[32m+[m			[32mhandle = attrHandle[ lowercaseName ];[m
[32m+[m			[32mattrHandle[ lowercaseName ] = ret;[m
[32m+[m			[32mret = getter( elem, name, isXML ) != null ?[m
[32m+[m				[32mlowercaseName :[m
[32m+[m				[32mnull;[m
[32m+[m			[32mattrHandle[ lowercaseName ] = handle;[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn ret;[m
[32m+[m	[32m};[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mvar rfocusable = /^(?:input|select|textarea|button)$/i,[m
[32m+[m	[32mrclickable = /^(?:a|area)$/i;[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m	[32mprop: function( name, value ) {[m
[32m+[m		[32mreturn access( this, jQuery.prop, name, value, arguments.length > 1 );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mremoveProp: function( name ) {[m
[32m+[m		[32mreturn this.each( function() {[m
[32m+[m			[32mdelete this[ jQuery.propFix[ name ] || name ];[m
[32m+[m		[32m} );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.extend( {[m
[32m+[m	[32mprop: function( elem, name, value ) {[m
[32m+[m		[32mvar ret, hooks,[m
[32m+[m			[32mnType = elem.nodeType;[m
[32m+[m
[32m+[m		[32m// Don't get/set properties on text, comment and attribute nodes[m
[32m+[m		[32mif ( nType === 3 || nType === 8 || nType === 2 ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( nType !== 1 || !jQuery.isXMLDoc( elem ) ) {[m
[32m+[m
[32m+[m			[32m// Fix name and attach hooks[m
[32m+[m			[32mname = jQuery.propFix[ name ] || name;[m
[32m+[m			[32mhooks = jQuery.propHooks[ name ];[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( value !== undefined ) {[m
[32m+[m			[32mif ( hooks && "set" in hooks &&[m
[32m+[m				[32m( ret = hooks.set( elem, value, name ) ) !== undefined ) {[m
[32m+[m				[32mreturn ret;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn ( elem[ name ] = value );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( hooks && "get" in hooks && ( ret = hooks.get( elem, name ) ) !== null ) {[m
[32m+[m			[32mreturn ret;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn elem[ name ];[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mpropHooks: {[m
[32m+[m		[32mtabIndex: {[m
[32m+[m			[32mget: function( elem ) {[m
[32m+[m
[32m+[m				[32m// Support: IE <=9 - 11 only[m
[32m+[m				[32m// elem.tabIndex doesn't always return the[m
[32m+[m				[32m// correct value when it hasn't been explicitly set[m
[32m+[m				[32m// https://web.archive.org/web/20141116233347/http://fluidproject.org/blog/2008/01/09/getting-setting-and-removing-tabindex-values-with-javascript/[m
[32m+[m				[32m// Use proper attribute retrieval(#12072)[m
[32m+[m				[32mvar tabindex = jQuery.find.attr( elem, "tabindex" );[m
[32m+[m
[32m+[m				[32mreturn tabindex ?[m
[32m+[m					[32mparseInt( tabindex, 10 ) :[m
[32m+[m					[32mrfocusable.test( elem.nodeName ) ||[m
[32m+[m						[32mrclickable.test( elem.nodeName ) && elem.href ?[m
[32m+[m							[32m0 :[m
[32m+[m							[32m-1;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mpropFix: {[m
[32m+[m		[32m"for": "htmlFor",[m
[32m+[m		[32m"class": "className"[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32m// Support: IE <=11 only[m
[32m+[m[32m// Accessing the selectedIndex property[m
[32m+[m[32m// forces the browser to respect setting selected[m
[32m+[m[32m// on the option[m
[32m+[m[32m// The getter ensures a default option is selected[m
[32m+[m[32m// when in an optgroup[m
[32m+[m[32mif ( !support.optSelected ) {[m
[32m+[m	[32mjQuery.propHooks.selected = {[m
[32m+[m		[32mget: function( elem ) {[m
[32m+[m			[32mvar parent = elem.parentNode;[m
[32m+[m			[32mif ( parent && parent.parentNode ) {[m
[32m+[m				[32mparent.parentNode.selectedIndex;[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn null;[m
[32m+[m		[32m},[m
[32m+[m		[32mset: function( elem ) {[m
[32m+[m			[32mvar parent = elem.parentNode;[m
[32m+[m			[32mif ( parent ) {[m
[32m+[m				[32mparent.selectedIndex;[m
[32m+[m
[32m+[m				[32mif ( parent.parentNode ) {[m
[32m+[m					[32mparent.parentNode.selectedIndex;[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mjQuery.each( [[m
[32m+[m	[32m"tabIndex",[m
[32m+[m	[32m"readOnly",[m
[32m+[m	[32m"maxLength",[m
[32m+[m	[32m"cellSpacing",[m
[32m+[m	[32m"cellPadding",[m
[32m+[m	[32m"rowSpan",[m
[32m+[m	[32m"colSpan",[m
[32m+[m	[32m"useMap",[m
[32m+[m	[32m"frameBorder",[m
[32m+[m	[32m"contentEditable"[m
[32m+[m[32m], function() {[m
[32m+[m	[32mjQuery.propFix[ this.toLowerCase() ] = this;[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mvar rclass = /[\t\r\n\f]/g;[m
[32m+[m
[32m+[m[32mfunction getClass( elem ) {[m
[32m+[m	[32mreturn elem.getAttribute && elem.getAttribute( "class" ) || "";[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m	[32maddClass: function( value ) {[m
[32m+[m		[32mvar classes, elem, cur, curValue, clazz, j, finalValue,[m
[32m+[m			[32mi = 0;[m
[32m+[m
[32m+[m		[32mif ( jQuery.isFunction( value ) ) {[m
[32m+[m			[32mreturn this.each( function( j ) {[m
[32m+[m				[32mjQuery( this ).addClass( value.call( this, j, getClass( this ) ) );[m
[32m+[m			[32m} );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( typeof value === "string" && value ) {[m
[32m+[m			[32mclasses = value.match( rnotwhite ) || [];[m
[32m+[m
[32m+[m			[32mwhile ( ( elem = this[ i++ ] ) ) {[m
[32m+[m				[32mcurValue = getClass( elem );[m
[32m+[m				[32mcur = elem.nodeType === 1 &&[m
[32m+[m					[32m( " " + curValue + " " ).replace( rclass, " " );[m
[32m+[m
[32m+[m				[32mif ( cur ) {[m
[32m+[m					[32mj = 0;[m
[32m+[m					[32mwhile ( ( clazz = classes[ j++ ] ) ) {[m
[32m+[m						[32mif ( cur.indexOf( " " + clazz + " " ) < 0 ) {[m
[32m+[m							[32mcur += clazz + " ";[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// Only assign if different to avoid unneeded rendering.[m
[32m+[m					[32mfinalValue = jQuery.trim( cur );[m
[32m+[m					[32mif ( curValue !== finalValue ) {[m
[32m+[m						[32melem.setAttribute( "class", finalValue );[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn this;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mremoveClass: function( value ) {[m
[32m+[m		[32mvar classes, elem, cur, curValue, clazz, j, finalValue,[m
[32m+[m			[32mi = 0;[m
[32m+[m
[32m+[m		[32mif ( jQuery.isFunction( value ) ) {[m
[32m+[m			[32mreturn this.each( function( j ) {[m
[32m+[m				[32mjQuery( this ).removeClass( value.call( this, j, getClass( this ) ) );[m
[32m+[m			[32m} );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( !arguments.length ) {[m
[32m+[m			[32mreturn this.attr( "class", "" );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( typeof value === "string" && value ) {[m
[32m+[m			[32mclasses = value.match( rnotwhite ) || [];[m
[32m+[m
[32m+[m			[32mwhile ( ( elem = this[ i++ ] ) ) {[m
[32m+[m				[32mcurValue = getClass( elem );[m
[32m+[m
[32m+[m				[32m// This expression is here for better compressibility (see addClass)[m
[32m+[m				[32mcur = elem.nodeType === 1 &&[m
[32m+[m					[32m( " " + curValue + " " ).replace( rclass, " " );[m
[32m+[m
[32m+[m				[32mif ( cur ) {[m
[32m+[m					[32mj = 0;[m
[32m+[m					[32mwhile ( ( clazz = classes[ j++ ] ) ) {[m
[32m+[m
[32m+[m						[32m// Remove *all* instances[m
[32m+[m						[32mwhile ( cur.indexOf( " " + clazz + " " ) > -1 ) {[m
[32m+[m							[32mcur = cur.replace( " " + clazz + " ", " " );[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// Only assign if different to avoid unneeded rendering.[m
[32m+[m					[32mfinalValue = jQuery.trim( cur );[m
[32m+[m					[32mif ( curValue !== finalValue ) {[m
[32m+[m						[32melem.setAttribute( "class", finalValue );[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn this;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mtoggleClass: function( value, stateVal ) {[m
[32m+[m		[32mvar type = typeof value;[m
[32m+[m
[32m+[m		[32mif ( typeof stateVal === "boolean" && type === "string" ) {[m
[32m+[m			[32mreturn stateVal ? this.addClass( value ) : this.removeClass( value );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( jQuery.isFunction( value ) ) {[m
[32m+[m			[32mreturn this.each( function( i ) {[m
[32m+[m				[32mjQuery( this ).toggleClass([m
[32m+[m					[32mvalue.call( this, i, getClass( this ), stateVal ),[m
[32m+[m					[32mstateVal[m
[32m+[m				[32m);[m
[32m+[m			[32m} );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn this.each( function() {[m
[32m+[m			[32mvar className, i, self, classNames;[m
[32m+[m
[32m+[m			[32mif ( type === "string" ) {[m
[32m+[m
[32m+[m				[32m// Toggle individual class names[m
[32m+[m				[32mi = 0;[m
[32m+[m				[32mself = jQuery( this );[m
[32m+[m				[32mclassNames = value.match( rnotwhite ) || [];[m
[32m+[m
[32m+[m				[32mwhile ( ( className = classNames[ i++ ] ) ) {[m
[32m+[m
[32m+[m					[32m// Check each className given, space separated list[m
[32m+[m					[32mif ( self.hasClass( className ) ) {[m
[32m+[m						[32mself.removeClass( className );[m
[32m+[m					[32m} else {[m
[32m+[m						[32mself.addClass( className );[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m			[32m// Toggle whole class name[m
[32m+[m			[32m} else if ( value === undefined || type === "boolean" ) {[m
[32m+[m				[32mclassName = getClass( this );[m
[32m+[m				[32mif ( className ) {[m
[32m+[m
[32m+[m					[32m// Store className if set[m
[32m+[m					[32mdataPriv.set( this, "__className__", className );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// If the element has a class name or if we're passed `false`,[m
[32m+[m				[32m// then remove the whole classname (if there was one, the above saved it).[m
[32m+[m				[32m// Otherwise bring back whatever was previously saved (if anything),[m
[32m+[m				[32m// falling back to the empty string if nothing was stored.[m
[32m+[m				[32mif ( this.setAttribute ) {[m
[32m+[m					[32mthis.setAttribute( "class",[m
[32m+[m						[32mclassName || value === false ?[m
[32m+[m						[32m"" :[m
[32m+[m						[32mdataPriv.get( this, "__className__" ) || ""[m
[32m+[m					[32m);[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mhasClass: function( selector ) {[m
[32m+[m		[32mvar className, elem,[m
[32m+[m			[32mi = 0;[m
[32m+[m
[32m+[m		[32mclassName = " " + selector + " ";[m
[32m+[m		[32mwhile ( ( elem = this[ i++ ] ) ) {[m
[32m+[m			[32mif ( elem.nodeType === 1 &&[m
[32m+[m				[32m( " " + getClass( elem ) + " " ).replace( rclass, " " )[m
[32m+[m					[32m.indexOf( className ) > -1[m
[32m+[m			[32m) {[m
[32m+[m				[32mreturn true;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn false;[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mvar rreturn = /\r/g,[m
[32m+[m	[32mrspaces = /[\x20\t\r\n\f]+/g;[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m	[32mval: function( value ) {[m
[32m+[m		[32mvar hooks, ret, isFunction,[m
[32m+[m			[32melem = this[ 0 ];[m
[32m+[m
[32m+[m		[32mif ( !arguments.length ) {[m
[32m+[m			[32mif ( elem ) {[m
[32m+[m				[32mhooks = jQuery.valHooks[ elem.type ] ||[m
[32m+[m					[32mjQuery.valHooks[ elem.nodeName.toLowerCase() ];[m
[32m+[m
[32m+[m				[32mif ( hooks &&[m
[32m+[m					[32m"get" in hooks &&[m
[32m+[m					[32m( ret = hooks.get( elem, "value" ) ) !== undefined[m
[32m+[m				[32m) {[m
[32m+[m					[32mreturn ret;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mret = elem.value;[m
[32m+[m
[32m+[m				[32mreturn typeof ret === "string" ?[m
[32m+[m
[32m+[m					[32m// Handle most common string cases[m
[32m+[m					[32mret.replace( rreturn, "" ) :[m
[32m+[m
[32m+[m					[32m// Handle cases where value is null/undef or number[m
[32m+[m					[32mret == null ? "" : ret;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32misFunction = jQuery.isFunction( value );[m
[32m+[m
[32m+[m		[32mreturn this.each( function( i ) {[m
[32m+[m			[32mvar val;[m
[32m+[m
[32m+[m			[32mif ( this.nodeType !== 1 ) {[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif ( isFunction ) {[m
[32m+[m				[32mval = value.call( this, i, jQuery( this ).val() );[m
[32m+[m			[32m} else {[m
[32m+[m				[32mval = value;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Treat null/undefined as ""; convert numbers to string[m
[32m+[m			[32mif ( val == null ) {[m
[32m+[m				[32mval = "";[m
[32m+[m
[32m+[m			[32m} else if ( typeof val === "number" ) {[m
[32m+[m				[32mval += "";[m
[32m+[m
[32m+[m			[32m} else if ( jQuery.isArray( val ) ) {[m
[32m+[m				[32mval = jQuery.map( val, function( value ) {[m
[32m+[m					[32mreturn value == null ? "" : value + "";[m
[32m+[m				[32m} );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mhooks = jQuery.valHooks[ this.type ] || jQuery.valHooks[ this.nodeName.toLowerCase() ];[m
[32m+[m
[32m+[m			[32m// If set returns undefined, fall back to normal setting[m
[32m+[m			[32mif ( !hooks || !( "set" in hooks ) || hooks.set( this, val, "value" ) === undefined ) {[m
[32m+[m				[32mthis.value = val;[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.extend( {[m
[32m+[m	[32mvalHooks: {[m
[32m+[m		[32moption: {[m
[32m+[m			[32mget: function( elem ) {[m
[32m+[m
[32m+[m				[32mvar val = jQuery.find.attr( elem, "value" );[m
[32m+[m				[32mreturn val != null ?[m
[32m+[m					[32mval :[m
[32m+[m
[32m+[m					[32m// Support: IE <=10 - 11 only[m
[32m+[m					[32m// option.text throws exceptions (#14686, #14858)[m
[32m+[m					[32m// Strip and collapse whitespace[m
[32m+[m					[32m// https://html.spec.whatwg.org/#strip-and-collapse-whitespace[m
[32m+[m					[32mjQuery.trim( jQuery.text( elem ) ).replace( rspaces, " " );[m
[32m+[m			[32m}[m
[32m+[m		[32m},[m
[32m+[m		[32mselect: {[m
[32m+[m			[32mget: function( elem ) {[m
[32m+[m				[32mvar value, option,[m
[32m+[m					[32moptions = elem.options,[m
[32m+[m					[32mindex = elem.selectedIndex,[m
[32m+[m					[32mone = elem.type === "select-one",[m
[32m+[m					[32mvalues = one ? null : [],[m
[32m+[m					[32mmax = one ? index + 1 : options.length,[m
[32m+[m					[32mi = index < 0 ?[m
[32m+[m						[32mmax :[m
[32m+[m						[32mone ? index : 0;[m
[32m+[m
[32m+[m				[32m// Loop through all the selected options[m
[32m+[m				[32mfor ( ; i < max; i++ ) {[m
[32m+[m					[32moption = options[ i ];[m
[32m+[m
[32m+[m					[32m// Support: IE <=9 only[m
[32m+[m					[32m// IE8-9 doesn't update selected after form reset (#2551)[m
[32m+[m					[32mif ( ( option.selected || i === index ) &&[m
[32m+[m
[32m+[m							[32m// Don't return options that are disabled or in a disabled optgroup[m
[32m+[m							[32m!option.disabled &&[m
[32m+[m							[32m( !option.parentNode.disabled ||[m
[32m+[m								[32m!jQuery.nodeName( option.parentNode, "optgroup" ) ) ) {[m
[32m+[m
[32m+[m						[32m// Get the specific value for the option[m
[32m+[m						[32mvalue = jQuery( option ).val();[m
[32m+[m
[32m+[m						[32m// We don't need an array for one selects[m
[32m+[m						[32mif ( one ) {[m
[32m+[m							[32mreturn value;[m
[32m+[m						[32m}[m
[32m+[m
[32m+[m						[32m// Multi-Selects return an array[m
[32m+[m						[32mvalues.push( value );[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mreturn values;[m
[32m+[m			[32m},[m
[32m+[m
[32m+[m			[32mset: function( elem, value ) {[m
[32m+[m				[32mvar optionSet, option,[m
[32m+[m					[32moptions = elem.options,[m
[32m+[m					[32mvalues = jQuery.makeArray( value ),[m
[32m+[m					[32mi = options.length;[m
[32m+[m
[32m+[m				[32mwhile ( i-- ) {[m
[32m+[m					[32moption = options[ i ];[m
[32m+[m
[32m+[m					[32m/* eslint-disable no-cond-assign */[m
[32m+[m
[32m+[m					[32mif ( option.selected =[m
[32m+[m						[32mjQuery.inArray( jQuery.valHooks.option.get( option ), values ) > -1[m
[32m+[m					[32m) {[m
[32m+[m						[32moptionSet = true;[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m/* eslint-enable no-cond-assign */[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Force browsers to behave consistently when non-matching value is set[m
[32m+[m				[32mif ( !optionSet ) {[m
[32m+[m					[32melem.selectedIndex = -1;[m
[32m+[m				[32m}[m
[32m+[m				[32mreturn values;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32m// Radios and checkboxes getter/setter[m
[32m+[m[32mjQuery.each( [ "radio", "checkbox" ], function() {[m
[32m+[m	[32mjQuery.valHooks[ this ] = {[m
[32m+[m		[32mset: function( elem, value ) {[m
[32m+[m			[32mif ( jQuery.isArray( value ) ) {[m
[32m+[m				[32mreturn ( elem.checked = jQuery.inArray( jQuery( elem ).val(), value ) > -1 );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m
[32m+[m	[32mif ( !support.checkOn ) {[m
[32m+[m		[32mjQuery.valHooks[ this ].get = function( elem ) {[m
[32m+[m			[32mreturn elem.getAttribute( "value" ) === null ? "on" : elem.value;[m
[32m+[m		[32m};[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m// Return jQuery for attributes-only inclusion[m
[32m+[m
[32m+[m
[32m+[m[32mvar rfocusMorph = /^(?:focusinfocus|focusoutblur)$/;[m
[32m+[m
[32m+[m[32mjQuery.extend( jQuery.event, {[m
[32m+[m
[32m+[m	[32mtrigger: function( event, data, elem, onlyHandlers ) {[m
[32m+[m
[32m+[m		[32mvar i, cur, tmp, bubbleType, ontype, handle, special,[m
[32m+[m			[32meventPath = [ elem || document ],[m
[32m+[m			[32mtype = hasOwn.call( event, "type" ) ? event.type : event,[m
[32m+[m			[32mnamespaces = hasOwn.call( event, "namespace" ) ? event.namespace.split( "." ) : [];[m
[32m+[m
[32m+[m		[32mcur = tmp = elem = elem || document;[m
[32m+[m
[32m+[m		[32m// Don't do events on text and comment nodes[m
[32m+[m		[32mif ( elem.nodeType === 3 || elem.nodeType === 8 ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// focus/blur morphs to focusin/out; ensure we're not firing them right now[m
[32m+[m		[32mif ( rfocusMorph.test( type + jQuery.event.triggered ) ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( type.indexOf( "." ) > -1 ) {[m
[32m+[m
[32m+[m			[32m// Namespaced trigger; create a regexp to match event type in handle()[m
[32m+[m			[32mnamespaces = type.split( "." );[m
[32m+[m			[32mtype = namespaces.shift();[m
[32m+[m			[32mnamespaces.sort();[m
[32m+[m		[32m}[m
[32m+[m		[32montype = type.indexOf( ":" ) < 0 && "on" + type;[m
[32m+[m
[32m+[m		[32m// Caller can pass in a jQuery.Event object, Object, or just an event type string[m
[32m+[m		[32mevent = event[ jQuery.expando ] ?[m
[32m+[m			[32mevent :[m
[32m+[m			[32mnew jQuery.Event( type, typeof event === "object" && event );[m
[32m+[m
[32m+[m		[32m// Trigger bitmask: & 1 for native handlers; & 2 for jQuery (always true)[m
[32m+[m		[32mevent.isTrigger = onlyHandlers ? 2 : 3;[m
[32m+[m		[32mevent.namespace = namespaces.join( "." );[m
[32m+[m		[32mevent.rnamespace = event.namespace ?[m
[32m+[m			[32mnew RegExp( "(^|\\.)" + namespaces.join( "\\.(?:.*\\.|)" ) + "(\\.|$)" ) :[m
[32m+[m			[32mnull;[m
[32m+[m
[32m+[m		[32m// Clean up the event in case it is being reused[m
[32m+[m		[32mevent.result = undefined;[m
[32m+[m		[32mif ( !event.target ) {[m
[32m+[m			[32mevent.target = elem;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Clone any incoming data and prepend the event, creating the handler arg list[m
[32m+[m		[32mdata = data == null ?[m
[32m+[m			[32m[ event ] :[m
[32m+[m			[32mjQuery.makeArray( data, [ event ] );[m
[32m+[m
[32m+[m		[32m// Allow special events to draw outside the lines[m
[32m+[m		[32mspecial = jQuery.event.special[ type ] || {};[m
[32m+[m		[32mif ( !onlyHandlers && special.trigger && special.trigger.apply( elem, data ) === false ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Determine event propagation path in advance, per W3C events spec (#9951)[m
[32m+[m		[32m// Bubble up to document, then to window; watch for a global ownerDocument var (#9724)[m
[32m+[m		[32mif ( !onlyHandlers && !special.noBubble && !jQuery.isWindow( elem ) ) {[m
[32m+[m
[32m+[m			[32mbubbleType = special.delegateType || type;[m
[32m+[m			[32mif ( !rfocusMorph.test( bubbleType + type ) ) {[m
[32m+[m				[32mcur = cur.parentNode;[m
[32m+[m			[32m}[m
[32m+[m			[32mfor ( ; cur; cur = cur.parentNode ) {[m
[32m+[m				[32meventPath.push( cur );[m
[32m+[m				[32mtmp = cur;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Only add window if we got to document (e.g., not plain obj or detached DOM)[m
[32m+[m			[32mif ( tmp === ( elem.ownerDocument || document ) ) {[m
[32m+[m				[32meventPath.push( tmp.defaultView || tmp.parentWindow || window );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Fire handlers on the event path[m
[32m+[m		[32mi = 0;[m
[32m+[m		[32mwhile ( ( cur = eventPath[ i++ ] ) && !event.isPropagationStopped() ) {[m
[32m+[m
[32m+[m			[32mevent.type = i > 1 ?[m
[32m+[m				[32mbubbleType :[m
[32m+[m				[32mspecial.bindType || type;[m
[32m+[m
[32m+[m			[32m// jQuery handler[m
[32m+[m			[32mhandle = ( dataPriv.get( cur, "events" ) || {} )[ event.type ] &&[m
[32m+[m				[32mdataPriv.get( cur, "handle" );[m
[32m+[m			[32mif ( handle ) {[m
[32m+[m				[32mhandle.apply( cur, data );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Native handler[m
[32m+[m			[32mhandle = ontype && cur[ ontype ];[m
[32m+[m			[32mif ( handle && handle.apply && acceptData( cur ) ) {[m
[32m+[m				[32mevent.result = handle.apply( cur, data );[m
[32m+[m				[32mif ( event.result === false ) {[m
[32m+[m					[32mevent.preventDefault();[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m		[32mevent.type = type;[m
[32m+[m
[32m+[m		[32m// If nobody prevented the default action, do it now[m
[32m+[m		[32mif ( !onlyHandlers && !event.isDefaultPrevented() ) {[m
[32m+[m
[32m+[m			[32mif ( ( !special._default ||[m
[32m+[m				[32mspecial._default.apply( eventPath.pop(), data ) === false ) &&[m
[32m+[m				[32macceptData( elem ) ) {[m
[32m+[m
[32m+[m				[32m// Call a native DOM method on the target with the same name as the event.[m
[32m+[m				[32m// Don't do default actions on window, that's where global variables be (#6170)[m
[32m+[m				[32mif ( ontype && jQuery.isFunction( elem[ type ] ) && !jQuery.isWindow( elem ) ) {[m
[32m+[m
[32m+[m					[32m// Don't re-trigger an onFOO event when we call its FOO() method[m
[32m+[m					[32mtmp = elem[ ontype ];[m
[32m+[m
[32m+[m					[32mif ( tmp ) {[m
[32m+[m						[32melem[ ontype ] = null;[m
[32m+[m					[32m}[m
[32m+[m
[32m+[m					[32m// Prevent re-triggering of the same event, since we already bubbled it above[m
[32m+[m					[32mjQuery.event.triggered = type;[m
[32m+[m					[32melem[ type ]();[m
[32m+[m					[32mjQuery.event.triggered = undefined;[m
[32m+[m
[32m+[m					[32mif ( tmp ) {[m
[32m+[m						[32melem[ ontype ] = tmp;[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn event.result;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Piggyback on a donor event to simulate a different one[m
[32m+[m	[32m// Used only for `focus(in | out)` events[m
[32m+[m	[32msimulate: function( type, elem, event ) {[m
[32m+[m		[32mvar e = jQuery.extend([m
[32m+[m			[32mnew jQuery.Event(),[m
[32m+[m			[32mevent,[m
[32m+[m			[32m{[m
[32m+[m				[32mtype: type,[m
[32m+[m				[32misSimulated: true[m
[32m+[m			[32m}[m
[32m+[m		[32m);[m
[32m+[m
[32m+[m		[32mjQuery.event.trigger( e, null, elem );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m
[32m+[m	[32mtrigger: function( type, data ) {[m
[32m+[m		[32mreturn this.each( function() {[m
[32m+[m			[32mjQuery.event.trigger( type, data, this );[m
[32m+[m		[32m} );[m
[32m+[m	[32m},[m
[32m+[m	[32mtriggerHandler: function( type, data ) {[m
[32m+[m		[32mvar elem = this[ 0 ];[m
[32m+[m		[32mif ( elem ) {[m
[32m+[m			[32mreturn jQuery.event.trigger( type, data, elem, true );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m[32mjQuery.each( ( "blur focus focusin focusout resize scroll click dblclick " +[m
[32m+[m	[32m"mousedown mouseup mousemove mouseover mouseout mouseenter mouseleave " +[m
[32m+[m	[32m"change select submit keydown keypress keyup contextmenu" ).split( " " ),[m
[32m+[m	[32mfunction( i, name ) {[m
[32m+[m
[32m+[m	[32m// Handle event binding[m
[32m+[m	[32mjQuery.fn[ name ] = function( data, fn ) {[m
[32m+[m		[32mreturn arguments.length > 0 ?[m
[32m+[m			[32mthis.on( name, null, data, fn ) :[m
[32m+[m			[32mthis.trigger( name );[m
[32m+[m	[32m};[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m	[32mhover: function( fnOver, fnOut ) {[m
[32m+[m		[32mreturn this.mouseenter( fnOver ).mouseleave( fnOut || fnOver );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32msupport.focusin = "onfocusin" in window;[m
[32m+[m
[32m+[m
[32m+[m[32m// Support: Firefox <=44[m
[32m+[m[32m// Firefox doesn't have focus(in | out) events[m
[32m+[m[32m// Related ticket - https://bugzilla.mozilla.org/show_bug.cgi?id=687787[m
[32m+[m[32m//[m
[32m+[m[32m// Support: Chrome <=48 - 49, Safari <=9.0 - 9.1[m
[32m+[m[32m// focus(in | out) events fire after focus & blur events,[m
[32m+[m[32m// which is spec violation - http://www.w3.org/TR/DOM-Level-3-Events/#events-focusevent-event-order[m
[32m+[m[32m// Related ticket - https://bugs.chromium.org/p/chromium/issues/detail?id=449857[m
[32m+[m[32mif ( !support.focusin ) {[m
[32m+[m	[32mjQuery.each( { focus: "focusin", blur: "focusout" }, function( orig, fix ) {[m
[32m+[m
[32m+[m		[32m// Attach a single capturing handler on the document while someone wants focusin/focusout[m
[32m+[m		[32mvar handler = function( event ) {[m
[32m+[m			[32mjQuery.event.simulate( fix, event.target, jQuery.event.fix( event ) );[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m		[32mjQuery.event.special[ fix ] = {[m
[32m+[m			[32msetup: function() {[m
[32m+[m				[32mvar doc = this.ownerDocument || this,[m
[32m+[m					[32mattaches = dataPriv.access( doc, fix );[m
[32m+[m
[32m+[m				[32mif ( !attaches ) {[m
[32m+[m					[32mdoc.addEventListener( orig, handler, true );[m
[32m+[m				[32m}[m
[32m+[m				[32mdataPriv.access( doc, fix, ( attaches || 0 ) + 1 );[m
[32m+[m			[32m},[m
[32m+[m			[32mteardown: function() {[m
[32m+[m				[32mvar doc = this.ownerDocument || this,[m
[32m+[m					[32mattaches = dataPriv.access( doc, fix ) - 1;[m
[32m+[m
[32m+[m				[32mif ( !attaches ) {[m
[32m+[m					[32mdoc.removeEventListener( orig, handler, true );[m
[32m+[m					[32mdataPriv.remove( doc, fix );[m
[32m+[m
[32m+[m				[32m} else {[m
[32m+[m					[32mdataPriv.access( doc, fix, attaches );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m};[m
[32m+[m	[32m} );[m
[32m+[m[32m}[m
[32m+[m[32mvar location = window.location;[m
[32m+[m
[32m+[m[32mvar nonce = jQuery.now();[m
[32m+[m
[32m+[m[32mvar rquery = ( /\?/ );[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m// Cross-browser xml parsing[m
[32m+[m[32mjQuery.parseXML = function( data ) {[m
[32m+[m	[32mvar xml;[m
[32m+[m	[32mif ( !data || typeof data !== "string" ) {[m
[32m+[m		[32mreturn null;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Support: IE 9 - 11 only[m
[32m+[m	[32m// IE throws on parseFromString with invalid input.[m
[32m+[m	[32mtry {[m
[32m+[m		[32mxml = ( new window.DOMParser() ).parseFromString( data, "text/xml" );[m
[32m+[m	[32m} catch ( e ) {[m
[32m+[m		[32mxml = undefined;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( !xml || xml.getElementsByTagName( "parsererror" ).length ) {[m
[32m+[m		[32mjQuery.error( "Invalid XML: " + data );[m
[32m+[m	[32m}[m
[32m+[m	[32mreturn xml;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m[32mvar[m
[32m+[m	[32mrbracket = /\[\]$/,[m
[32m+[m	[32mrCRLF = /\r?\n/g,[m
[32m+[m	[32mrsubmitterTypes = /^(?:submit|button|image|reset|file)$/i,[m
[32m+[m	[32mrsubmittable = /^(?:input|select|textarea|keygen)/i;[m
[32m+[m
[32m+[m[32mfunction buildParams( prefix, obj, traditional, add ) {[m
[32m+[m	[32mvar name;[m
[32m+[m
[32m+[m	[32mif ( jQuery.isArray( obj ) ) {[m
[32m+[m
[32m+[m		[32m// Serialize array item.[m
[32m+[m		[32mjQuery.each( obj, function( i, v ) {[m
[32m+[m			[32mif ( traditional || rbracket.test( prefix ) ) {[m
[32m+[m
[32m+[m				[32m// Treat each array item as a scalar.[m
[32m+[m				[32madd( prefix, v );[m
[32m+[m
[32m+[m			[32m} else {[m
[32m+[m
[32m+[m				[32m// Item is non-scalar (array or object), encode its numeric index.[m
[32m+[m				[32mbuildParams([m
[32m+[m					[32mprefix + "[" + ( typeof v === "object" && v != null ? i : "" ) + "]",[m
[32m+[m					[32mv,[m
[32m+[m					[32mtraditional,[m
[32m+[m					[32madd[m
[32m+[m				[32m);[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m	[32m} else if ( !traditional && jQuery.type( obj ) === "object" ) {[m
[32m+[m
[32m+[m		[32m// Serialize object item.[m
[32m+[m		[32mfor ( name in obj ) {[m
[32m+[m			[32mbuildParams( prefix + "[" + name + "]", obj[ name ], traditional, add );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m	[32m} else {[m
[32m+[m
[32m+[m		[32m// Serialize scalar item.[m
[32m+[m		[32madd( prefix, obj );[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Serialize an array of form elements or a set of[m
[32m+[m[32m// key/values into a query string[m
[32m+[m[32mjQuery.param = function( a, traditional ) {[m
[32m+[m	[32mvar prefix,[m
[32m+[m		[32ms = [],[m
[32m+[m		[32madd = function( key, valueOrFunction ) {[m
[32m+[m
[32m+[m			[32m// If value is a function, invoke it and use its return value[m
[32m+[m			[32mvar value = jQuery.isFunction( valueOrFunction ) ?[m
[32m+[m				[32mvalueOrFunction() :[m
[32m+[m				[32mvalueOrFunction;[m
[32m+[m
[32m+[m			[32ms[ s.length ] = encodeURIComponent( key ) + "=" +[m
[32m+[m				[32mencodeURIComponent( value == null ? "" : value );[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m	[32m// If an array was passed in, assume that it is an array of form elements.[m
[32m+[m	[32mif ( jQuery.isArray( a ) || ( a.jquery && !jQuery.isPlainObject( a ) ) ) {[m
[32m+[m
[32m+[m		[32m// Serialize the form elements[m
[32m+[m		[32mjQuery.each( a, function() {[m
[32m+[m			[32madd( this.name, this.value );[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m	[32m} else {[m
[32m+[m
[32m+[m		[32m// If traditional, encode the "old" way (the way 1.3.2 or older[m
[32m+[m		[32m// did it), otherwise encode params recursively.[m
[32m+[m		[32mfor ( prefix in a ) {[m
[32m+[m			[32mbuildParams( prefix, a[ prefix ], traditional, add );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Return the resulting serialization[m
[32m+[m	[32mreturn s.join( "&" );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m	[32mserialize: function() {[m
[32m+[m		[32mreturn jQuery.param( this.serializeArray() );[m
[32m+[m	[32m},[m
[32m+[m	[32mserializeArray: function() {[m
[32m+[m		[32mreturn this.map( function() {[m
[32m+[m
[32m+[m			[32m// Can add propHook for "elements" to filter or add form elements[m
[32m+[m			[32mvar elements = jQuery.prop( this, "elements" );[m
[32m+[m			[32mreturn elements ? jQuery.makeArray( elements ) : this;[m
[32m+[m		[32m} )[m
[32m+[m		[32m.filter( function() {[m
[32m+[m			[32mvar type = this.type;[m
[32m+[m
[32m+[m			[32m// Use .is( ":disabled" ) so that fieldset[disabled] works[m
[32m+[m			[32mreturn this.name && !jQuery( this ).is( ":disabled" ) &&[m
[32m+[m				[32mrsubmittable.test( this.nodeName ) && !rsubmitterTypes.test( type ) &&[m
[32m+[m				[32m( this.checked || !rcheckableType.test( type ) );[m
[32m+[m		[32m} )[m
[32m+[m		[32m.map( function( i, elem ) {[m
[32m+[m			[32mvar val = jQuery( this ).val();[m
[32m+[m
[32m+[m			[32mreturn val == null ?[m
[32m+[m				[32mnull :[m
[32m+[m				[32mjQuery.isArray( val ) ?[m
[32m+[m					[32mjQuery.map( val, function( val ) {[m
[32m+[m						[32mreturn { name: elem.name, value: val.replace( rCRLF, "\r\n" ) };[m
[32m+[m					[32m} ) :[m
[32m+[m					[32m{ name: elem.name, value: val.replace( rCRLF, "\r\n" ) };[m
[32m+[m		[32m} ).get();[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m[32mvar[m
[32m+[m	[32mr20 = /%20/g,[m
[32m+[m	[32mrhash = /#.*$/,[m
[32m+[m	[32mrts = /([?&])_=[^&]*/,[m
[32m+[m	[32mrheaders = /^(.*?):[ \t]*([^\r\n]*)$/mg,[m
[32m+[m
[32m+[m	[32m// #7653, #8125, #8152: local protocol detection[m
[32m+[m	[32mrlocalProtocol = /^(?:about|app|app-storage|.+-extension|file|res|widget):$/,[m
[32m+[m	[32mrnoContent = /^(?:GET|HEAD)$/,[m
[32m+[m	[32mrprotocol = /^\/\//,[m
[32m+[m
[32m+[m	[32m/* Prefilters[m
[32m+[m	[32m * 1) They are useful to introduce custom dataTypes (see ajax/jsonp.js for an example)[m
[32m+[m	[32m * 2) These are called:[m
[32m+[m	[32m *    - BEFORE asking for a transport[m
[32m+[m	[32m *    - AFTER param serialization (s.data is a string if s.processData is true)[m
[32m+[m	[32m * 3) key is the dataType[m
[32m+[m	[32m * 4) the catchall symbol "*" can be used[m
[32m+[m	[32m * 5) execution will start with transport dataType and THEN continue down to "*" if needed[m
[32m+[m	[32m */[m
[32m+[m	[32mprefilters = {},[m
[32m+[m
[32m+[m	[32m/* Transports bindings[m
[32m+[m	[32m * 1) key is the dataType[m
[32m+[m	[32m * 2) the catchall symbol "*" can be used[m
[32m+[m	[32m * 3) selection will start with transport dataType and THEN go to "*" if needed[m
[32m+[m	[32m */[m
[32m+[m	[32mtransports = {},[m
[32m+[m
[32m+[m	[32m// Avoid comment-prolog char sequence (#10098); must appease lint and evade compression[m
[32m+[m	[32mallTypes = "*/".concat( "*" ),[m
[32m+[m
[32m+[m	[32m// Anchor tag for parsing the document origin[m
[32m+[m	[32moriginAnchor = document.createElement( "a" );[m
[32m+[m	[32moriginAnchor.href = location.href;[m
[32m+[m
[32m+[m[32m// Base "constructor" for jQuery.ajaxPrefilter and jQuery.ajaxTransport[m
[32m+[m[32mfunction addToPrefiltersOrTransports( structure ) {[m
[32m+[m
[32m+[m	[32m// dataTypeExpression is optional and defaults to "*"[m
[32m+[m	[32mreturn function( dataTypeExpression, func ) {[m
[32m+[m
[32m+[m		[32mif ( typeof dataTypeExpression !== "string" ) {[m
[32m+[m			[32mfunc = dataTypeExpression;[m
[32m+[m			[32mdataTypeExpression = "*";[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mvar dataType,[m
[32m+[m			[32mi = 0,[m
[32m+[m			[32mdataTypes = dataTypeExpression.toLowerCase().match( rnotwhite ) || [];[m
[32m+[m
[32m+[m		[32mif ( jQuery.isFunction( func ) ) {[m
[32m+[m
[32m+[m			[32m// For each dataType in the dataTypeExpression[m
[32m+[m			[32mwhile ( ( dataType = dataTypes[ i++ ] ) ) {[m
[32m+[m
[32m+[m				[32m// Prepend if requested[m
[32m+[m				[32mif ( dataType[ 0 ] === "+" ) {[m
[32m+[m					[32mdataType = dataType.slice( 1 ) || "*";[m
[32m+[m					[32m( structure[ dataType ] = structure[ dataType ] || [] ).unshift( func );[m
[32m+[m
[32m+[m				[32m// Otherwise append[m
[32m+[m				[32m} else {[m
[32m+[m					[32m( structure[ dataType ] = structure[ dataType ] || [] ).push( func );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m};[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// Base inspection function for prefilters and transports[m
[32m+[m[32mfunction inspectPrefiltersOrTransports( structure, options, originalOptions, jqXHR ) {[m
[32m+[m
[32m+[m	[32mvar inspected = {},[m
[32m+[m		[32mseekingTransport = ( structure === transports );[m
[32m+[m
[32m+[m	[32mfunction inspect( dataType ) {[m
[32m+[m		[32mvar selected;[m
[32m+[m		[32minspected[ dataType ] = true;[m
[32m+[m		[32mjQuery.each( structure[ dataType ] || [], function( _, prefilterOrFactory ) {[m
[32m+[m			[32mvar dataTypeOrTransport = prefilterOrFactory( options, originalOptions, jqXHR );[m
[32m+[m			[32mif ( typeof dataTypeOrTransport === "string" &&[m
[32m+[m				[32m!seekingTransport && !inspected[ dataTypeOrTransport ] ) {[m
[32m+[m
[32m+[m				[32moptions.dataTypes.unshift( dataTypeOrTransport );[m
[32m+[m				[32minspect( dataTypeOrTransport );[m
[32m+[m				[32mreturn false;[m
[32m+[m			[32m} else if ( seekingTransport ) {[m
[32m+[m				[32mreturn !( selected = dataTypeOrTransport );[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m		[32mreturn selected;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn inspect( options.dataTypes[ 0 ] ) || !inspected[ "*" ] && inspect( "*" );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m// A special extend for ajax options[m
[32m+[m[32m// that takes "flat" options (not to be deep extended)[m
[32m+[m[32m// Fixes #9887[m
[32m+[m[32mfunction ajaxExtend( target, src ) {[m
[32m+[m	[32mvar key, deep,[m
[32m+[m		[32mflatOptions = jQuery.ajaxSettings.flatOptions || {};[m
[32m+[m
[32m+[m	[32mfor ( key in src ) {[m
[32m+[m		[32mif ( src[ key ] !== undefined ) {[m
[32m+[m			[32m( flatOptions[ key ] ? target : ( deep || ( deep = {} ) ) )[ key ] = src[ key ];[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m	[32mif ( deep ) {[m
[32m+[m		[32mjQuery.extend( true, target, deep );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn target;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* Handles responses to an ajax request:[m
[32m+[m[32m * - finds the right dataType (mediates between content-type and expected dataType)[m
[32m+[m[32m * - returns the corresponding response[m
[32m+[m[32m */[m
[32m+[m[32mfunction ajaxHandleResponses( s, jqXHR, responses ) {[m
[32m+[m
[32m+[m	[32mvar ct, type, finalDataType, firstDataType,[m
[32m+[m		[32mcontents = s.contents,[m
[32m+[m		[32mdataTypes = s.dataTypes;[m
[32m+[m
[32m+[m	[32m// Remove auto dataType and get content-type in the process[m
[32m+[m	[32mwhile ( dataTypes[ 0 ] === "*" ) {[m
[32m+[m		[32mdataTypes.shift();[m
[32m+[m		[32mif ( ct === undefined ) {[m
[32m+[m			[32mct = s.mimeType || jqXHR.getResponseHeader( "Content-Type" );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Check if we're dealing with a known content-type[m
[32m+[m	[32mif ( ct ) {[m
[32m+[m		[32mfor ( type in contents ) {[m
[32m+[m			[32mif ( contents[ type ] && contents[ type ].test( ct ) ) {[m
[32m+[m				[32mdataTypes.unshift( type );[m
[32m+[m				[32mbreak;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// Check to see if we have a response for the expected dataType[m
[32m+[m	[32mif ( dataTypes[ 0 ] in responses ) {[m
[32m+[m		[32mfinalDataType = dataTypes[ 0 ];[m
[32m+[m	[32m} else {[m
[32m+[m
[32m+[m		[32m// Try convertible dataTypes[m
[32m+[m		[32mfor ( type in responses ) {[m
[32m+[m			[32mif ( !dataTypes[ 0 ] || s.converters[ type + " " + dataTypes[ 0 ] ] ) {[m
[32m+[m				[32mfinalDataType = type;[m
[32m+[m				[32mbreak;[m
[32m+[m			[32m}[m
[32m+[m			[32mif ( !firstDataType ) {[m
[32m+[m				[32mfirstDataType = type;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Or just use first one[m
[32m+[m		[32mfinalDataType = finalDataType || firstDataType;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// If we found a dataType[m
[32m+[m	[32m// We add the dataType to the list if needed[m
[32m+[m	[32m// and return the corresponding response[m
[32m+[m	[32mif ( finalDataType ) {[m
[32m+[m		[32mif ( finalDataType !== dataTypes[ 0 ] ) {[m
[32m+[m			[32mdataTypes.unshift( finalDataType );[m
[32m+[m		[32m}[m
[32m+[m		[32mreturn responses[ finalDataType ];[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/* Chain conversions given the request and the original response[m
[32m+[m[32m * Also sets the responseXXX fields on the jqXHR instance[m
[32m+[m[32m */[m
[32m+[m[32mfunction ajaxConvert( s, response, jqXHR, isSuccess ) {[m
[32m+[m	[32mvar conv2, current, conv, tmp, prev,[m
[32m+[m		[32mconverters = {},[m
[32m+[m
[32m+[m		[32m// Work with a copy of dataTypes in case we need to modify it for conversion[m
[32m+[m		[32mdataTypes = s.dataTypes.slice();[m
[32m+[m
[32m+[m	[32m// Create converters map with lowercased keys[m
[32m+[m	[32mif ( dataTypes[ 1 ] ) {[m
[32m+[m		[32mfor ( conv in s.converters ) {[m
[32m+[m			[32mconverters[ conv.toLowerCase() ] = s.converters[ conv ];[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mcurrent = dataTypes.shift();[m
[32m+[m
[32m+[m	[32m// Convert to each sequential dataType[m
[32m+[m	[32mwhile ( current ) {[m
[32m+[m
[32m+[m		[32mif ( s.responseFields[ current ] ) {[m
[32m+[m			[32mjqXHR[ s.responseFields[ current ] ] = response;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Apply the dataFilter if provided[m
[32m+[m		[32mif ( !prev && isSuccess && s.dataFilter ) {[m
[32m+[m			[32mresponse = s.dataFilter( response, s.dataType );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mprev = current;[m
[32m+[m		[32mcurrent = dataTypes.shift();[m
[32m+[m
[32m+[m		[32mif ( current ) {[m
[32m+[m
[32m+[m			[32m// There's only work to do if current dataType is non-auto[m
[32m+[m			[32mif ( current === "*" ) {[m
[32m+[m
[32m+[m				[32mcurrent = prev;[m
[32m+[m
[32m+[m			[32m// Convert response if prev dataType is non-auto and differs from current[m
[32m+[m			[32m} else if ( prev !== "*" && prev !== current ) {[m
[32m+[m
[32m+[m				[32m// Seek a direct converter[m
[32m+[m				[32mconv = converters[ prev + " " + current ] || converters[ "* " + current ];[m
[32m+[m
[32m+[m				[32m// If none found, seek a pair[m
[32m+[m				[32mif ( !conv ) {[m
[32m+[m					[32mfor ( conv2 in converters ) {[m
[32m+[m
[32m+[m						[32m// If conv2 outputs current[m
[32m+[m						[32mtmp = conv2.split( " " );[m
[32m+[m						[32mif ( tmp[ 1 ] === current ) {[m
[32m+[m
[32m+[m							[32m// If prev can be converted to accepted input[m
[32m+[m							[32mconv = converters[ prev + " " + tmp[ 0 ] ] ||[m
[32m+[m								[32mconverters[ "* " + tmp[ 0 ] ];[m
[32m+[m							[32mif ( conv ) {[m
[32m+[m
[32m+[m								[32m// Condense equivalence converters[m
[32m+[m								[32mif ( conv === true ) {[m
[32m+[m									[32mconv = converters[ conv2 ];[m
[32m+[m
[32m+[m								[32m// Otherwise, insert the intermediate dataType[m
[32m+[m								[32m} else if ( converters[ conv2 ] !== true ) {[m
[32m+[m									[32mcurrent = tmp[ 0 ];[m
[32m+[m									[32mdataTypes.unshift( tmp[ 1 ] );[m
[32m+[m								[32m}[m
[32m+[m								[32mbreak;[m
[32m+[m							[32m}[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Apply converter (if not an equivalence)[m
[32m+[m				[32mif ( conv !== true ) {[m
[32m+[m
[32m+[m					[32m// Unless errors are allowed to bubble, catch and return them[m
[32m+[m					[32mif ( conv && s.throws ) {[m
[32m+[m						[32mresponse = conv( response );[m
[32m+[m					[32m} else {[m
[32m+[m						[32mtry {[m
[32m+[m							[32mresponse = conv( response );[m
[32m+[m						[32m} catch ( e ) {[m
[32m+[m							[32mreturn {[m
[32m+[m								[32mstate: "parsererror",[m
[32m+[m								[32merror: conv ? e : "No conversion from " + prev + " to " + current[m
[32m+[m							[32m};[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn { state: "success", data: response };[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mjQuery.extend( {[m
[32m+[m
[32m+[m	[32m// Counter for holding the number of active queries[m
[32m+[m	[32mactive: 0,[m
[32m+[m
[32m+[m	[32m// Last-Modified header cache for next request[m
[32m+[m	[32mlastModified: {},[m
[32m+[m	[32metag: {},[m
[32m+[m
[32m+[m	[32majaxSettings: {[m
[32m+[m		[32murl: location.href,[m
[32m+[m		[32mtype: "GET",[m
[32m+[m		[32misLocal: rlocalProtocol.test( location.protocol ),[m
[32m+[m		[32mglobal: true,[m
[32m+[m		[32mprocessData: true,[m
[32m+[m		[32masync: true,[m
[32m+[m		[32mcontentType: "application/x-www-form-urlencoded; charset=UTF-8",[m
[32m+[m
[32m+[m		[32m/*[m
[32m+[m		[32mtimeout: 0,[m
[32m+[m		[32mdata: null,[m
[32m+[m		[32mdataType: null,[m
[32m+[m		[32musername: null,[m
[32m+[m		[32mpassword: null,[m
[32m+[m		[32mcache: null,[m
[32m+[m		[32mthrows: false,[m
[32m+[m		[32mtraditional: false,[m
[32m+[m		[32mheaders: {},[m
[32m+[m		[32m*/[m
[32m+[m
[32m+[m		[32maccepts: {[m
[32m+[m			[32m"*": allTypes,[m
[32m+[m			[32mtext: "text/plain",[m
[32m+[m			[32mhtml: "text/html",[m
[32m+[m			[32mxml: "application/xml, text/xml",[m
[32m+[m			[32mjson: "application/json, text/javascript"[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mcontents: {[m
[32m+[m			[32mxml: /\bxml\b/,[m
[32m+[m			[32mhtml: /\bhtml/,[m
[32m+[m			[32mjson: /\bjson\b/[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32mresponseFields: {[m
[32m+[m			[32mxml: "responseXML",[m
[32m+[m			[32mtext: "responseText",[m
[32m+[m			[32mjson: "responseJSON"[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// Data converters[m
[32m+[m		[32m// Keys separate source (or catchall "*") and destination types with a single space[m
[32m+[m		[32mconverters: {[m
[32m+[m
[32m+[m			[32m// Convert anything to text[m
[32m+[m			[32m"* text": String,[m
[32m+[m
[32m+[m			[32m// Text to html (true = no transformation)[m
[32m+[m			[32m"text html": true,[m
[32m+[m
[32m+[m			[32m// Evaluate text as a json expression[m
[32m+[m			[32m"text json": JSON.parse,[m
[32m+[m
[32m+[m			[32m// Parse text as xml[m
[32m+[m			[32m"text xml": jQuery.parseXML[m
[32m+[m		[32m},[m
[32m+[m
[32m+[m		[32m// For options that shouldn't be deep extended:[m
[32m+[m		[32m// you can add your own custom options here if[m
[32m+[m		[32m// and when you create one that shouldn't be[m
[32m+[m		[32m// deep extended (see ajaxExtend)[m
[32m+[m		[32mflatOptions: {[m
[32m+[m			[32murl: true,[m
[32m+[m			[32mcontext: true[m
[32m+[m		[32m}[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// Creates a full fledged settings object into target[m
[32m+[m	[32m// with both ajaxSettings and settings fields.[m
[32m+[m	[32m// If target is omitted, writes into ajaxSettings.[m
[32m+[m	[32majaxSetup: function( target, settings ) {[m
[32m+[m		[32mreturn settings ?[m
[32m+[m
[32m+[m			[32m// Building a settings object[m
[32m+[m			[32majaxExtend( ajaxExtend( target, jQuery.ajaxSettings ), settings ) :[m
[32m+[m
[32m+[m			[32m// Extending ajaxSettings[m
[32m+[m			[32majaxExtend( jQuery.ajaxSettings, target );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32majaxPrefilter: addToPrefiltersOrTransports( prefilters ),[m
[32m+[m	[32majaxTransport: addToPrefiltersOrTransports( transports ),[m
[32m+[m
[32m+[m	[32m// Main method[m
[32m+[m	[32majax: function( url, options ) {[m
[32m+[m
[32m+[m		[32m// If url is an object, simulate pre-1.5 signature[m
[32m+[m		[32mif ( typeof url === "object" ) {[m
[32m+[m			[32moptions = url;[m
[32m+[m			[32murl = undefined;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Force options to be an object[m
[32m+[m		[32moptions = options || {};[m
[32m+[m
[32m+[m		[32mvar transport,[m
[32m+[m
[32m+[m			[32m// URL without anti-cache param[m
[32m+[m			[32mcacheURL,[m
[32m+[m
[32m+[m			[32m// Response headers[m
[32m+[m			[32mresponseHeadersString,[m
[32m+[m			[32mresponseHeaders,[m
[32m+[m
[32m+[m			[32m// timeout handle[m
[32m+[m			[32mtimeoutTimer,[m
[32m+[m
[32m+[m			[32m// Url cleanup var[m
[32m+[m			[32murlAnchor,[m
[32m+[m
[32m+[m			[32m// Request state (becomes false upon send and true upon completion)[m
[32m+[m			[32mcompleted,[m
[32m+[m
[32m+[m			[32m// To know if global events are to be dispatched[m
[32m+[m			[32mfireGlobals,[m
[32m+[m
[32m+[m			[32m// Loop variable[m
[32m+[m			[32mi,[m
[32m+[m
[32m+[m			[32m// uncached part of the url[m
[32m+[m			[32muncached,[m
[32m+[m
[32m+[m			[32m// Create the final options object[m
[32m+[m			[32ms = jQuery.ajaxSetup( {}, options ),[m
[32m+[m
[32m+[m			[32m// Callbacks context[m
[32m+[m			[32mcallbackContext = s.context || s,[m
[32m+[m
[32m+[m			[32m// Context for global events is callbackContext if it is a DOM node or jQuery collection[m
[32m+[m			[32mglobalEventContext = s.context &&[m
[32m+[m				[32m( callbackContext.nodeType || callbackContext.jquery ) ?[m
[32m+[m					[32mjQuery( callbackContext ) :[m
[32m+[m					[32mjQuery.event,[m
[32m+[m
[32m+[m			[32m// Deferreds[m
[32m+[m			[32mdeferred = jQuery.Deferred(),[m
[32m+[m			[32mcompleteDeferred = jQuery.Callbacks( "once memory" ),[m
[32m+[m
[32m+[m			[32m// Status-dependent callbacks[m
[32m+[m			[32mstatusCode = s.statusCode || {},[m
[32m+[m
[32m+[m			[32m// Headers (they are sent all at once)[m
[32m+[m			[32mrequestHeaders = {},[m
[32m+[m			[32mrequestHeadersNames = {},[m
[32m+[m
[32m+[m			[32m// Default abort message[m
[32m+[m			[32mstrAbort = "canceled",[m
[32m+[m
[32m+[m			[32m// Fake xhr[m
[32m+[m			[32mjqXHR = {[m
[32m+[m				[32mreadyState: 0,[m
[32m+[m
[32m+[m				[32m// Builds headers hashtable if needed[m
[32m+[m				[32mgetResponseHeader: function( key ) {[m
[32m+[m					[32mvar match;[m
[32m+[m					[32mif ( completed ) {[m
[32m+[m						[32mif ( !responseHeaders ) {[m
[32m+[m							[32mresponseHeaders = {};[m
[32m+[m							[32mwhile ( ( match = rheaders.exec( responseHeadersString ) ) ) {[m
[32m+[m								[32mresponseHeaders[ match[ 1 ].toLowerCase() ] = match[ 2 ];[m
[32m+[m							[32m}[m
[32m+[m						[32m}[m
[32m+[m						[32mmatch = responseHeaders[ key.toLowerCase() ];[m
[32m+[m					[32m}[m
[32m+[m					[32mreturn match == null ? null : match;[m
[32m+[m				[32m},[m
[32m+[m
[32m+[m				[32m// Raw string[m
[32m+[m				[32mgetAllResponseHeaders: function() {[m
[32m+[m					[32mreturn completed ? responseHeadersString : null;[m
[32m+[m				[32m},[m
[32m+[m
[32m+[m				[32m// Caches the header[m
[32m+[m				[32msetRequestHeader: function( name, value ) {[m
[32m+[m					[32mif ( completed == null ) {[m
[32m+[m						[32mname = requestHeadersNames[ name.toLowerCase() ] =[m
[32m+[m							[32mrequestHeadersNames[ name.toLowerCase() ] || name;[m
[32m+[m						[32mrequestHeaders[ name ] = value;[m
[32m+[m					[32m}[m
[32m+[m					[32mreturn this;[m
[32m+[m				[32m},[m
[32m+[m
[32m+[m				[32m// Overrides response content-type header[m
[32m+[m				[32moverrideMimeType: function( type ) {[m
[32m+[m					[32mif ( completed == null ) {[m
[32m+[m						[32ms.mimeType = type;[m
[32m+[m					[32m}[m
[32m+[m					[32mreturn this;[m
[32m+[m				[32m},[m
[32m+[m
[32m+[m				[32m// Status-dependent callbacks[m
[32m+[m				[32mstatusCode: function( map ) {[m
[32m+[m					[32mvar code;[m
[32m+[m					[32mif ( map ) {[m
[32m+[m						[32mif ( completed ) {[m
[32m+[m
[32m+[m							[32m// Execute the appropriate callbacks[m
[32m+[m							[32mjqXHR.always( map[ jqXHR.status ] );[m
[32m+[m						[32m} else {[m
[32m+[m
[32m+[m							[32m// Lazy-add the new callbacks in a way that preserves old ones[m
[32m+[m							[32mfor ( code in map ) {[m
[32m+[m								[32mstatusCode[ code ] = [ statusCode[ code ], map[ code ] ];[m
[32m+[m							[32m}[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m					[32mreturn this;[m
[32m+[m				[32m},[m
[32m+[m
[32m+[m				[32m// Cancel the request[m
[32m+[m				[32mabort: function( statusText ) {[m
[32m+[m					[32mvar finalText = statusText || strAbort;[m
[32m+[m					[32mif ( transport ) {[m
[32m+[m						[32mtransport.abort( finalText );[m
[32m+[m					[32m}[m
[32m+[m					[32mdone( 0, finalText );[m
[32m+[m					[32mreturn this;[m
[32m+[m				[32m}[m
[32m+[m			[32m};[m
[32m+[m
[32m+[m		[32m// Attach deferreds[m
[32m+[m		[32mdeferred.promise( jqXHR );[m
[32m+[m
[32m+[m		[32m// Add protocol if not provided (prefilters might expect it)[m
[32m+[m		[32m// Handle falsy url in the settings object (#10093: consistency with old signature)[m
[32m+[m		[32m// We also use the url parameter if available[m
[32m+[m		[32ms.url = ( ( url || s.url || location.href ) + "" )[m
[32m+[m			[32m.replace( rprotocol, location.protocol + "//" );[m
[32m+[m
[32m+[m		[32m// Alias method option to type as per ticket #12004[m
[32m+[m		[32ms.type = options.method || options.type || s.method || s.type;[m
[32m+[m
[32m+[m		[32m// Extract dataTypes list[m
[32m+[m		[32ms.dataTypes = ( s.dataType || "*" ).toLowerCase().match( rnotwhite ) || [ "" ];[m
[32m+[m
[32m+[m		[32m// A cross-domain request is in order when the origin doesn't match the current origin.[m
[32m+[m		[32mif ( s.crossDomain == null ) {[m
[32m+[m			[32murlAnchor = document.createElement( "a" );[m
[32m+[m
[32m+[m			[32m// Support: IE <=8 - 11, Edge 12 - 13[m
[32m+[m			[32m// IE throws exception on accessing the href property if url is malformed,[m
[32m+[m			[32m// e.g. http://example.com:80x/[m
[32m+[m			[32mtry {[m
[32m+[m				[32murlAnchor.href = s.url;[m
[32m+[m
[32m+[m				[32m// Support: IE <=8 - 11 only[m
[32m+[m				[32m// Anchor's host property isn't correctly set when s.url is relative[m
[32m+[m				[32murlAnchor.href = urlAnchor.href;[m
[32m+[m				[32ms.crossDomain = originAnchor.protocol + "//" + originAnchor.host !==[m
[32m+[m					[32murlAnchor.protocol + "//" + urlAnchor.host;[m
[32m+[m			[32m} catch ( e ) {[m
[32m+[m
[32m+[m				[32m// If there is an error parsing the URL, assume it is crossDomain,[m
[32m+[m				[32m// it can be rejected by the transport if it is invalid[m
[32m+[m				[32ms.crossDomain = true;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Convert data if not already a string[m
[32m+[m		[32mif ( s.data && s.processData && typeof s.data !== "string" ) {[m
[32m+[m			[32ms.data = jQuery.param( s.data, s.traditional );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Apply prefilters[m
[32m+[m		[32minspectPrefiltersOrTransports( prefilters, s, options, jqXHR );[m
[32m+[m
[32m+[m		[32m// If request was aborted inside a prefilter, stop there[m
[32m+[m		[32mif ( completed ) {[m
[32m+[m			[32mreturn jqXHR;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// We can fire global events as of now if asked to[m
[32m+[m		[32m// Don't fire events if jQuery.event is undefined in an AMD-usage scenario (#15118)[m
[32m+[m		[32mfireGlobals = jQuery.event && s.global;[m
[32m+[m
[32m+[m		[32m// Watch for a new set of requests[m
[32m+[m		[32mif ( fireGlobals && jQuery.active++ === 0 ) {[m
[32m+[m			[32mjQuery.event.trigger( "ajaxStart" );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Uppercase the type[m
[32m+[m		[32ms.type = s.type.toUpperCase();[m
[32m+[m
[32m+[m		[32m// Determine if request has content[m
[32m+[m		[32ms.hasContent = !rnoContent.test( s.type );[m
[32m+[m
[32m+[m		[32m// Save the URL in case we're toying with the If-Modified-Since[m
[32m+[m		[32m// and/or If-None-Match header later on[m
[32m+[m		[32m// Remove hash to simplify url manipulation[m
[32m+[m		[32mcacheURL = s.url.replace( rhash, "" );[m
[32m+[m
[32m+[m		[32m// More options handling for requests with no content[m
[32m+[m		[32mif ( !s.hasContent ) {[m
[32m+[m
[32m+[m			[32m// Remember the hash so we can put it back[m
[32m+[m			[32muncached = s.url.slice( cacheURL.length );[m
[32m+[m
[32m+[m			[32m// If data is available, append data to url[m
[32m+[m			[32mif ( s.data ) {[m
[32m+[m				[32mcacheURL += ( rquery.test( cacheURL ) ? "&" : "?" ) + s.data;[m
[32m+[m
[32m+[m				[32m// #9682: remove data so that it's not used in an eventual retry[m
[32m+[m				[32mdelete s.data;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Add anti-cache in uncached url if needed[m
[32m+[m			[32mif ( s.cache === false ) {[m
[32m+[m				[32mcacheURL = cacheURL.replace( rts, "" );[m
[32m+[m				[32muncached = ( rquery.test( cacheURL ) ? "&" : "?" ) + "_=" + ( nonce++ ) + uncached;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Put hash and anti-cache on the URL that will be requested (gh-1732)[m
[32m+[m			[32ms.url = cacheURL + uncached;[m
[32m+[m
[32m+[m		[32m// Change '%20' to '+' if this is encoded form body content (gh-2658)[m
[32m+[m		[32m} else if ( s.data && s.processData &&[m
[32m+[m			[32m( s.contentType || "" ).indexOf( "application/x-www-form-urlencoded" ) === 0 ) {[m
[32m+[m			[32ms.data = s.data.replace( r20, "+" );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Set the If-Modified-Since and/or If-None-Match header, if in ifModified mode.[m
[32m+[m		[32mif ( s.ifModified ) {[m
[32m+[m			[32mif ( jQuery.lastModified[ cacheURL ] ) {[m
[32m+[m				[32mjqXHR.setRequestHeader( "If-Modified-Since", jQuery.lastModified[ cacheURL ] );[m
[32m+[m			[32m}[m
[32m+[m			[32mif ( jQuery.etag[ cacheURL ] ) {[m
[32m+[m				[32mjqXHR.setRequestHeader( "If-None-Match", jQuery.etag[ cacheURL ] );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Set the correct header, if data is being sent[m
[32m+[m		[32mif ( s.data && s.hasContent && s.contentType !== false || options.contentType ) {[m
[32m+[m			[32mjqXHR.setRequestHeader( "Content-Type", s.contentType );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Set the Accepts header for the server, depending on the dataType[m
[32m+[m		[32mjqXHR.setRequestHeader([m
[32m+[m			[32m"Accept",[m
[32m+[m			[32ms.dataTypes[ 0 ] && s.accepts[ s.dataTypes[ 0 ] ] ?[m
[32m+[m				[32ms.accepts[ s.dataTypes[ 0 ] ] +[m
[32m+[m					[32m( s.dataTypes[ 0 ] !== "*" ? ", " + allTypes + "; q=0.01" : "" ) :[m
[32m+[m				[32ms.accepts[ "*" ][m
[32m+[m		[32m);[m
[32m+[m
[32m+[m		[32m// Check for headers option[m
[32m+[m		[32mfor ( i in s.headers ) {[m
[32m+[m			[32mjqXHR.setRequestHeader( i, s.headers[ i ] );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Allow custom headers/mimetypes and early abort[m
[32m+[m		[32mif ( s.beforeSend &&[m
[32m+[m			[32m( s.beforeSend.call( callbackContext, jqXHR, s ) === false || completed ) ) {[m
[32m+[m
[32m+[m			[32m// Abort if not done already and return[m
[32m+[m			[32mreturn jqXHR.abort();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Aborting is no longer a cancellation[m
[32m+[m		[32mstrAbort = "abort";[m
[32m+[m
[32m+[m		[32m// Install callbacks on deferreds[m
[32m+[m		[32mcompleteDeferred.add( s.complete );[m
[32m+[m		[32mjqXHR.done( s.success );[m
[32m+[m		[32mjqXHR.fail( s.error );[m
[32m+[m
[32m+[m		[32m// Get transport[m
[32m+[m		[32mtransport = inspectPrefiltersOrTransports( transports, s, options, jqXHR );[m
[32m+[m
[32m+[m		[32m// If no transport, we auto-abort[m
[32m+[m		[32mif ( !transport ) {[m
[32m+[m			[32mdone( -1, "No Transport" );[m
[32m+[m		[32m} else {[m
[32m+[m			[32mjqXHR.readyState = 1;[m
[32m+[m
[32m+[m			[32m// Send global event[m
[32m+[m			[32mif ( fireGlobals ) {[m
[32m+[m				[32mglobalEventContext.trigger( "ajaxSend", [ jqXHR, s ] );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// If request was aborted inside ajaxSend, stop there[m
[32m+[m			[32mif ( completed ) {[m
[32m+[m				[32mreturn jqXHR;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Timeout[m
[32m+[m			[32mif ( s.async && s.timeout > 0 ) {[m
[32m+[m				[32mtimeoutTimer = window.setTimeout( function() {[m
[32m+[m					[32mjqXHR.abort( "timeout" );[m
[32m+[m				[32m}, s.timeout );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mtry {[m
[32m+[m				[32mcompleted = false;[m
[32m+[m				[32mtransport.send( requestHeaders, done );[m
[32m+[m			[32m} catch ( e ) {[m
[32m+[m
[32m+[m				[32m// Rethrow post-completion exceptions[m
[32m+[m				[32mif ( completed ) {[m
[32m+[m					[32mthrow e;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Propagate others as results[m
[32m+[m				[32mdone( -1, e );[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Callback for when everything is done[m
[32m+[m		[32mfunction done( status, nativeStatusText, responses, headers ) {[m
[32m+[m			[32mvar isSuccess, success, error, response, modified,[m
[32m+[m				[32mstatusText = nativeStatusText;[m
[32m+[m
[32m+[m			[32m// Ignore repeat invocations[m
[32m+[m			[32mif ( completed ) {[m
[32m+[m				[32mreturn;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mcompleted = true;[m
[32m+[m
[32m+[m			[32m// Clear timeout if it exists[m
[32m+[m			[32mif ( timeoutTimer ) {[m
[32m+[m				[32mwindow.clearTimeout( timeoutTimer );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Dereference transport for early garbage collection[m
[32m+[m			[32m// (no matter how long the jqXHR object will be used)[m
[32m+[m			[32mtransport = undefined;[m
[32m+[m
[32m+[m			[32m// Cache response headers[m
[32m+[m			[32mresponseHeadersString = headers || "";[m
[32m+[m
[32m+[m			[32m// Set readyState[m
[32m+[m			[32mjqXHR.readyState = status > 0 ? 4 : 0;[m
[32m+[m
[32m+[m			[32m// Determine if successful[m
[32m+[m			[32misSuccess = status >= 200 && status < 300 || status === 304;[m
[32m+[m
[32m+[m			[32m// Get response data[m
[32m+[m			[32mif ( responses ) {[m
[32m+[m				[32mresponse = ajaxHandleResponses( s, jqXHR, responses );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Convert no matter what (that way responseXXX fields are always set)[m
[32m+[m			[32mresponse = ajaxConvert( s, response, jqXHR, isSuccess );[m
[32m+[m
[32m+[m			[32m// If successful, handle type chaining[m
[32m+[m			[32mif ( isSuccess ) {[m
[32m+[m
[32m+[m				[32m// Set the If-Modified-Since and/or If-None-Match header, if in ifModified mode.[m
[32m+[m				[32mif ( s.ifModified ) {[m
[32m+[m					[32mmodified = jqXHR.getResponseHeader( "Last-Modified" );[m
[32m+[m					[32mif ( modified ) {[m
[32m+[m						[32mjQuery.lastModified[ cacheURL ] = modified;[m
[32m+[m					[32m}[m
[32m+[m					[32mmodified = jqXHR.getResponseHeader( "etag" );[m
[32m+[m					[32mif ( modified ) {[m
[32m+[m						[32mjQuery.etag[ cacheURL ] = modified;[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// if no content[m
[32m+[m				[32mif ( status === 204 || s.type === "HEAD" ) {[m
[32m+[m					[32mstatusText = "nocontent";[m
[32m+[m
[32m+[m				[32m// if not modified[m
[32m+[m				[32m} else if ( status === 304 ) {[m
[32m+[m					[32mstatusText = "notmodified";[m
[32m+[m
[32m+[m				[32m// If we have data, let's convert it[m
[32m+[m				[32m} else {[m
[32m+[m					[32mstatusText = response.state;[m
[32m+[m					[32msuccess = response.data;[m
[32m+[m					[32merror = response.error;[m
[32m+[m					[32misSuccess = !error;[m
[32m+[m				[32m}[m
[32m+[m			[32m} else {[m
[32m+[m
[32m+[m				[32m// Extract error from statusText and normalize for non-aborts[m
[32m+[m				[32merror = statusText;[m
[32m+[m				[32mif ( status || !statusText ) {[m
[32m+[m					[32mstatusText = "error";[m
[32m+[m					[32mif ( status < 0 ) {[m
[32m+[m						[32mstatus = 0;[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Set data for the fake xhr object[m
[32m+[m			[32mjqXHR.status = status;[m
[32m+[m			[32mjqXHR.statusText = ( nativeStatusText || statusText ) + "";[m
[32m+[m
[32m+[m			[32m// Success/Error[m
[32m+[m			[32mif ( isSuccess ) {[m
[32m+[m				[32mdeferred.resolveWith( callbackContext, [ success, statusText, jqXHR ] );[m
[32m+[m			[32m} else {[m
[32m+[m				[32mdeferred.rejectWith( callbackContext, [ jqXHR, statusText, error ] );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Status-dependent callbacks[m
[32m+[m			[32mjqXHR.statusCode( statusCode );[m
[32m+[m			[32mstatusCode = undefined;[m
[32m+[m
[32m+[m			[32mif ( fireGlobals ) {[m
[32m+[m				[32mglobalEventContext.trigger( isSuccess ? "ajaxSuccess" : "ajaxError",[m
[32m+[m					[32m[ jqXHR, s, isSuccess ? success : error ] );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Complete[m
[32m+[m			[32mcompleteDeferred.fireWith( callbackContext, [ jqXHR, statusText ] );[m
[32m+[m
[32m+[m			[32mif ( fireGlobals ) {[m
[32m+[m				[32mglobalEventContext.trigger( "ajaxComplete", [ jqXHR, s ] );[m
[32m+[m
[32m+[m				[32m// Handle the global AJAX counter[m
[32m+[m				[32mif ( !( --jQuery.active ) ) {[m
[32m+[m					[32mjQuery.event.trigger( "ajaxStop" );[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn jqXHR;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mgetJSON: function( url, data, callback ) {[m
[32m+[m		[32mreturn jQuery.get( url, data, callback, "json" );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mgetScript: function( url, callback ) {[m
[32m+[m		[32mreturn jQuery.get( url, undefined, callback, "script" );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.each( [ "get", "post" ], function( i, method ) {[m
[32m+[m	[32mjQuery[ method ] = function( url, data, callback, type ) {[m
[32m+[m
[32m+[m		[32m// Shift arguments if data argument was omitted[m
[32m+[m		[32mif ( jQuery.isFunction( data ) ) {[m
[32m+[m			[32mtype = type || callback;[m
[32m+[m			[32mcallback = data;[m
[32m+[m			[32mdata = undefined;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// The url can be an options object (which then must have .url)[m
[32m+[m		[32mreturn jQuery.ajax( jQuery.extend( {[m
[32m+[m			[32murl: url,[m
[32m+[m			[32mtype: method,[m
[32m+[m			[32mdataType: type,[m
[32m+[m			[32mdata: data,[m
[32m+[m			[32msuccess: callback[m
[32m+[m		[32m}, jQuery.isPlainObject( url ) && url ) );[m
[32m+[m	[32m};[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m[32mjQuery._evalUrl = function( url ) {[m
[32m+[m	[32mreturn jQuery.ajax( {[m
[32m+[m		[32murl: url,[m
[32m+[m
[32m+[m		[32m// Make this explicit, since user can override this through ajaxSetup (#11264)[m
[32m+[m		[32mtype: "GET",[m
[32m+[m		[32mdataType: "script",[m
[32m+[m		[32mcache: true,[m
[32m+[m		[32masync: false,[m
[32m+[m		[32mglobal: false,[m
[32m+[m		[32m"throws": true[m
[32m+[m	[32m} );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m	[32mwrapAll: function( html ) {[m
[32m+[m		[32mvar wrap;[m
[32m+[m
[32m+[m		[32mif ( this[ 0 ] ) {[m
[32m+[m			[32mif ( jQuery.isFunction( html ) ) {[m
[32m+[m				[32mhtml = html.call( this[ 0 ] );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// The elements to wrap the target around[m
[32m+[m			[32mwrap = jQuery( html, this[ 0 ].ownerDocument ).eq( 0 ).clone( true );[m
[32m+[m
[32m+[m			[32mif ( this[ 0 ].parentNode ) {[m
[32m+[m				[32mwrap.insertBefore( this[ 0 ] );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mwrap.map( function() {[m
[32m+[m				[32mvar elem = this;[m
[32m+[m
[32m+[m				[32mwhile ( elem.firstElementChild ) {[m
[32m+[m					[32melem = elem.firstElementChild;[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mreturn elem;[m
[32m+[m			[32m} ).append( this );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn this;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mwrapInner: function( html ) {[m
[32m+[m		[32mif ( jQuery.isFunction( html ) ) {[m
[32m+[m			[32mreturn this.each( function( i ) {[m
[32m+[m				[32mjQuery( this ).wrapInner( html.call( this, i ) );[m
[32m+[m			[32m} );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mreturn this.each( function() {[m
[32m+[m			[32mvar self = jQuery( this ),[m
[32m+[m				[32mcontents = self.contents();[m
[32m+[m
[32m+[m			[32mif ( contents.length ) {[m
[32m+[m				[32mcontents.wrapAll( html );[m
[32m+[m
[32m+[m			[32m} else {[m
[32m+[m				[32mself.append( html );[m
[32m+[m			[32m}[m
[32m+[m		[32m} );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mwrap: function( html ) {[m
[32m+[m		[32mvar isFunction = jQuery.isFunction( html );[m
[32m+[m
[32m+[m		[32mreturn this.each( function( i ) {[m
[32m+[m			[32mjQuery( this ).wrapAll( isFunction ? html.call( this, i ) : html );[m
[32m+[m		[32m} );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32munwrap: function( selector ) {[m
[32m+[m		[32mthis.parent( selector ).not( "body" ).each( function() {[m
[32m+[m			[32mjQuery( this ).replaceWith( this.childNodes );[m
[32m+[m		[32m} );[m
[32m+[m		[32mreturn this;[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m[32mjQuery.expr.pseudos.hidden = function( elem ) {[m
[32m+[m	[32mreturn !jQuery.expr.pseudos.visible( elem );[m
[32m+[m[32m};[m
[32m+[m[32mjQuery.expr.pseudos.visible = function( elem ) {[m
[32m+[m	[32mreturn !!( elem.offsetWidth || elem.offsetHeight || elem.getClientRects().length );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mjQuery.ajaxSettings.xhr = function() {[m
[32m+[m	[32mtry {[m
[32m+[m		[32mreturn new window.XMLHttpRequest();[m
[32m+[m	[32m} catch ( e ) {}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mvar xhrSuccessStatus = {[m
[32m+[m
[32m+[m		[32m// File protocol always yields status code 0, assume 200[m
[32m+[m		[32m0: 200,[m
[32m+[m
[32m+[m		[32m// Support: IE <=9 only[m
[32m+[m		[32m// #1450: sometimes IE returns 1223 when it should be 204[m
[32m+[m		[32m1223: 204[m
[32m+[m	[32m},[m
[32m+[m	[32mxhrSupported = jQuery.ajaxSettings.xhr();[m
[32m+[m
[32m+[m[32msupport.cors = !!xhrSupported && ( "withCredentials" in xhrSupported );[m
[32m+[m[32msupport.ajax = xhrSupported = !!xhrSupported;[m
[32m+[m
[32m+[m[32mjQuery.ajaxTransport( function( options ) {[m
[32m+[m	[32mvar callback, errorCallback;[m
[32m+[m
[32m+[m	[32m// Cross domain only allowed if supported through XMLHttpRequest[m
[32m+[m	[32mif ( support.cors || xhrSupported && !options.crossDomain ) {[m
[32m+[m		[32mreturn {[m
[32m+[m			[32msend: function( headers, complete ) {[m
[32m+[m				[32mvar i,[m
[32m+[m					[32mxhr = options.xhr();[m
[32m+[m
[32m+[m				[32mxhr.open([m
[32m+[m					[32moptions.type,[m
[32m+[m					[32moptions.url,[m
[32m+[m					[32moptions.async,[m
[32m+[m					[32moptions.username,[m
[32m+[m					[32moptions.password[m
[32m+[m				[32m);[m
[32m+[m
[32m+[m				[32m// Apply custom fields if provided[m
[32m+[m				[32mif ( options.xhrFields ) {[m
[32m+[m					[32mfor ( i in options.xhrFields ) {[m
[32m+[m						[32mxhr[ i ] = options.xhrFields[ i ];[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Override mime type if needed[m
[32m+[m				[32mif ( options.mimeType && xhr.overrideMimeType ) {[m
[32m+[m					[32mxhr.overrideMimeType( options.mimeType );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// X-Requested-With header[m
[32m+[m				[32m// For cross-domain requests, seeing as conditions for a preflight are[m
[32m+[m				[32m// akin to a jigsaw puzzle, we simply never set it to be sure.[m
[32m+[m				[32m// (it can always be set on a per-request basis or even using ajaxSetup)[m
[32m+[m				[32m// For same-domain requests, won't change header if already provided.[m
[32m+[m				[32mif ( !options.crossDomain && !headers[ "X-Requested-With" ] ) {[m
[32m+[m					[32mheaders[ "X-Requested-With" ] = "XMLHttpRequest";[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Set headers[m
[32m+[m				[32mfor ( i in headers ) {[m
[32m+[m					[32mxhr.setRequestHeader( i, headers[ i ] );[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Callback[m
[32m+[m				[32mcallback = function( type ) {[m
[32m+[m					[32mreturn function() {[m
[32m+[m						[32mif ( callback ) {[m
[32m+[m							[32mcallback = errorCallback = xhr.onload =[m
[32m+[m								[32mxhr.onerror = xhr.onabort = xhr.onreadystatechange = null;[m
[32m+[m
[32m+[m							[32mif ( type === "abort" ) {[m
[32m+[m								[32mxhr.abort();[m
[32m+[m							[32m} else if ( type === "error" ) {[m
[32m+[m
[32m+[m								[32m// Support: IE <=9 only[m
[32m+[m								[32m// On a manual native abort, IE9 throws[m
[32m+[m								[32m// errors on any property access that is not readyState[m
[32m+[m								[32mif ( typeof xhr.status !== "number" ) {[m
[32m+[m									[32mcomplete( 0, "error" );[m
[32m+[m								[32m} else {[m
[32m+[m									[32mcomplete([m
[32m+[m
[32m+[m										[32m// File: protocol always yields status 0; see #8605, #14207[m
[32m+[m										[32mxhr.status,[m
[32m+[m										[32mxhr.statusText[m
[32m+[m									[32m);[m
[32m+[m								[32m}[m
[32m+[m							[32m} else {[m
[32m+[m								[32mcomplete([m
[32m+[m									[32mxhrSuccessStatus[ xhr.status ] || xhr.status,[m
[32m+[m									[32mxhr.statusText,[m
[32m+[m
[32m+[m									[32m// Support: IE <=9 only[m
[32m+[m									[32m// IE9 has no XHR2 but throws on binary (trac-11426)[m
[32m+[m									[32m// For XHR2 non-text, let the caller handle it (gh-2498)[m
[32m+[m									[32m( xhr.responseType || "text" ) !== "text"  ||[m
[32m+[m									[32mtypeof xhr.responseText !== "string" ?[m
[32m+[m										[32m{ binary: xhr.response } :[m
[32m+[m										[32m{ text: xhr.responseText },[m
[32m+[m									[32mxhr.getAllResponseHeaders()[m
[32m+[m								[32m);[m
[32m+[m							[32m}[m
[32m+[m						[32m}[m
[32m+[m					[32m};[m
[32m+[m				[32m};[m
[32m+[m
[32m+[m				[32m// Listen to events[m
[32m+[m				[32mxhr.onload = callback();[m
[32m+[m				[32merrorCallback = xhr.onerror = callback( "error" );[m
[32m+[m
[32m+[m				[32m// Support: IE 9 only[m
[32m+[m				[32m// Use onreadystatechange to replace onabort[m
[32m+[m				[32m// to handle uncaught aborts[m
[32m+[m				[32mif ( xhr.onabort !== undefined ) {[m
[32m+[m					[32mxhr.onabort = errorCallback;[m
[32m+[m				[32m} else {[m
[32m+[m					[32mxhr.onreadystatechange = function() {[m
[32m+[m
[32m+[m						[32m// Check readyState before timeout as it changes[m
[32m+[m						[32mif ( xhr.readyState === 4 ) {[m
[32m+[m
[32m+[m							[32m// Allow onerror to be called first,[m
[32m+[m							[32m// but that will not handle a native abort[m
[32m+[m							[32m// Also, save errorCallback to a variable[m
[32m+[m							[32m// as xhr.onerror cannot be accessed[m
[32m+[m							[32mwindow.setTimeout( function() {[m
[32m+[m								[32mif ( callback ) {[m
[32m+[m									[32merrorCallback();[m
[32m+[m								[32m}[m
[32m+[m							[32m} );[m
[32m+[m						[32m}[m
[32m+[m					[32m};[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Create the abort callback[m
[32m+[m				[32mcallback = callback( "abort" );[m
[32m+[m
[32m+[m				[32mtry {[m
[32m+[m
[32m+[m					[32m// Do send the request (this may raise an exception)[m
[32m+[m					[32mxhr.send( options.hasContent && options.data || null );[m
[32m+[m				[32m} catch ( e ) {[m
[32m+[m
[32m+[m					[32m// #14683: Only rethrow if this hasn't been notified as an error yet[m
[32m+[m					[32mif ( callback ) {[m
[32m+[m						[32mthrow e;[m
[32m+[m					[32m}[m
[32m+[m				[32m}[m
[32m+[m			[32m},[m
[32m+[m
[32m+[m			[32mabort: function() {[m
[32m+[m				[32mif ( callback ) {[m
[32m+[m					[32mcallback();[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m};[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m// Prevent auto-execution of scripts when no explicit dataType was provided (See gh-2432)[m
[32m+[m[32mjQuery.ajaxPrefilter( function( s ) {[m
[32m+[m	[32mif ( s.crossDomain ) {[m
[32m+[m		[32ms.contents.script = false;[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32m// Install script dataType[m
[32m+[m[32mjQuery.ajaxSetup( {[m
[32m+[m	[32maccepts: {[m
[32m+[m		[32mscript: "text/javascript, application/javascript, " +[m
[32m+[m			[32m"application/ecmascript, application/x-ecmascript"[m
[32m+[m	[32m},[m
[32m+[m	[32mcontents: {[m
[32m+[m		[32mscript: /\b(?:java|ecma)script\b/[m
[32m+[m	[32m},[m
[32m+[m	[32mconverters: {[m
[32m+[m		[32m"text script": function( text ) {[m
[32m+[m			[32mjQuery.globalEval( text );[m
[32m+[m			[32mreturn text;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32m// Handle cache's special case and crossDomain[m
[32m+[m[32mjQuery.ajaxPrefilter( "script", function( s ) {[m
[32m+[m	[32mif ( s.cache === undefined ) {[m
[32m+[m		[32ms.cache = false;[m
[32m+[m	[32m}[m
[32m+[m	[32mif ( s.crossDomain ) {[m
[32m+[m		[32ms.type = "GET";[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32m// Bind script tag hack transport[m
[32m+[m[32mjQuery.ajaxTransport( "script", function( s ) {[m
[32m+[m
[32m+[m	[32m// This transport only deals with cross domain requests[m
[32m+[m	[32mif ( s.crossDomain ) {[m
[32m+[m		[32mvar script, callback;[m
[32m+[m		[32mreturn {[m
[32m+[m			[32msend: function( _, complete ) {[m
[32m+[m				[32mscript = jQuery( "<script>" ).prop( {[m
[32m+[m					[32mcharset: s.scriptCharset,[m
[32m+[m					[32msrc: s.url[m
[32m+[m				[32m} ).on([m
[32m+[m					[32m"load error",[m
[32m+[m					[32mcallback = function( evt ) {[m
[32m+[m						[32mscript.remove();[m
[32m+[m						[32mcallback = null;[m
[32m+[m						[32mif ( evt ) {[m
[32m+[m							[32mcomplete( evt.type === "error" ? 404 : 200, evt.type );[m
[32m+[m						[32m}[m
[32m+[m					[32m}[m
[32m+[m				[32m);[m
[32m+[m
[32m+[m				[32m// Use native DOM manipulation to avoid our domManip AJAX trickery[m
[32m+[m				[32mdocument.head.appendChild( script[ 0 ] );[m
[32m+[m			[32m},[m
[32m+[m			[32mabort: function() {[m
[32m+[m				[32mif ( callback ) {[m
[32m+[m					[32mcallback();[m
[32m+[m				[32m}[m
[32m+[m			[32m}[m
[32m+[m		[32m};[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mvar oldCallbacks = [],[m
[32m+[m	[32mrjsonp = /(=)\?(?=&|$)|\?\?/;[m
[32m+[m
[32m+[m[32m// Default jsonp settings[m
[32m+[m[32mjQuery.ajaxSetup( {[m
[32m+[m	[32mjsonp: "callback",[m
[32m+[m	[32mjsonpCallback: function() {[m
[32m+[m		[32mvar callback = oldCallbacks.pop() || ( jQuery.expando + "_" + ( nonce++ ) );[m
[32m+[m		[32mthis[ callback ] = true;[m
[32m+[m		[32mreturn callback;[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32m// Detect, normalize options and install callbacks for jsonp requests[m
[32m+[m[32mjQuery.ajaxPrefilter( "json jsonp", function( s, originalSettings, jqXHR ) {[m
[32m+[m
[32m+[m	[32mvar callbackName, overwritten, responseContainer,[m
[32m+[m		[32mjsonProp = s.jsonp !== false && ( rjsonp.test( s.url ) ?[m
[32m+[m			[32m"url" :[m
[32m+[m			[32mtypeof s.data === "string" &&[m
[32m+[m				[32m( s.contentType || "" )[m
[32m+[m					[32m.indexOf( "application/x-www-form-urlencoded" ) === 0 &&[m
[32m+[m				[32mrjsonp.test( s.data ) && "data"[m
[32m+[m		[32m);[m
[32m+[m
[32m+[m	[32m// Handle iff the expected data type is "jsonp" or we have a parameter to set[m
[32m+[m	[32mif ( jsonProp || s.dataTypes[ 0 ] === "jsonp" ) {[m
[32m+[m
[32m+[m		[32m// Get callback name, remembering preexisting value associated with it[m
[32m+[m		[32mcallbackName = s.jsonpCallback = jQuery.isFunction( s.jsonpCallback ) ?[m
[32m+[m			[32ms.jsonpCallback() :[m
[32m+[m			[32ms.jsonpCallback;[m
[32m+[m
[32m+[m		[32m// Insert callback into url or form data[m
[32m+[m		[32mif ( jsonProp ) {[m
[32m+[m			[32ms[ jsonProp ] = s[ jsonProp ].replace( rjsonp, "$1" + callbackName );[m
[32m+[m		[32m} else if ( s.jsonp !== false ) {[m
[32m+[m			[32ms.url += ( rquery.test( s.url ) ? "&" : "?" ) + s.jsonp + "=" + callbackName;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Use data converter to retrieve json after script execution[m
[32m+[m		[32ms.converters[ "script json" ] = function() {[m
[32m+[m			[32mif ( !responseContainer ) {[m
[32m+[m				[32mjQuery.error( callbackName + " was not called" );[m
[32m+[m			[32m}[m
[32m+[m			[32mreturn responseContainer[ 0 ];[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m		[32m// Force json dataType[m
[32m+[m		[32ms.dataTypes[ 0 ] = "json";[m
[32m+[m
[32m+[m		[32m// Install callback[m
[32m+[m		[32moverwritten = window[ callbackName ];[m
[32m+[m		[32mwindow[ callbackName ] = function() {[m
[32m+[m			[32mresponseContainer = arguments;[m
[32m+[m		[32m};[m
[32m+[m
[32m+[m		[32m// Clean-up function (fires after converters)[m
[32m+[m		[32mjqXHR.always( function() {[m
[32m+[m
[32m+[m			[32m// If previous value didn't exist - remove it[m
[32m+[m			[32mif ( overwritten === undefined ) {[m
[32m+[m				[32mjQuery( window ).removeProp( callbackName );[m
[32m+[m
[32m+[m			[32m// Otherwise restore preexisting value[m
[32m+[m			[32m} else {[m
[32m+[m				[32mwindow[ callbackName ] = overwritten;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Save back as free[m
[32m+[m			[32mif ( s[ callbackName ] ) {[m
[32m+[m
[32m+[m				[32m// Make sure that re-using the options doesn't screw things around[m
[32m+[m				[32ms.jsonpCallback = originalSettings.jsonpCallback;[m
[32m+[m
[32m+[m				[32m// Save the callback name for future use[m
[32m+[m				[32moldCallbacks.push( callbackName );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Call if it was a function and we have a response[m
[32m+[m			[32mif ( responseContainer && jQuery.isFunction( overwritten ) ) {[m
[32m+[m				[32moverwritten( responseContainer[ 0 ] );[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mresponseContainer = overwritten = undefined;[m
[32m+[m		[32m} );[m
[32m+[m
[32m+[m		[32m// Delegate to script[m
[32m+[m		[32mreturn "script";[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m// Support: Safari 8 only[m
[32m+[m[32m// In Safari 8 documents created via document.implementation.createHTMLDocument[m
[32m+[m[32m// collapse sibling forms: the second one becomes a child of the first one.[m
[32m+[m[32m// Because of that, this security measure has to be disabled in Safari 8.[m
[32m+[m[32m// https://bugs.webkit.org/show_bug.cgi?id=137337[m
[32m+[m[32msupport.createHTMLDocument = ( function() {[m
[32m+[m	[32mvar body = document.implementation.createHTMLDocument( "" ).body;[m
[32m+[m	[32mbody.innerHTML = "<form></form><form></form>";[m
[32m+[m	[32mreturn body.childNodes.length === 2;[m
[32m+[m[32m} )();[m
[32m+[m
[32m+[m
[32m+[m[32m// Argument "data" should be string of html[m
[32m+[m[32m// context (optional): If specified, the fragment will be created in this context,[m
[32m+[m[32m// defaults to document[m
[32m+[m[32m// keepScripts (optional): If true, will include scripts passed in the html string[m
[32m+[m[32mjQuery.parseHTML = function( data, context, keepScripts ) {[m
[32m+[m	[32mif ( typeof data !== "string" ) {[m
[32m+[m		[32mreturn [];[m
[32m+[m	[32m}[m
[32m+[m	[32mif ( typeof context === "boolean" ) {[m
[32m+[m		[32mkeepScripts = context;[m
[32m+[m		[32mcontext = false;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mvar base, parsed, scripts;[m
[32m+[m
[32m+[m	[32mif ( !context ) {[m
[32m+[m
[32m+[m		[32m// Stop scripts or inline event handlers from being executed immediately[m
[32m+[m		[32m// by using document.implementation[m
[32m+[m		[32mif ( support.createHTMLDocument ) {[m
[32m+[m			[32mcontext = document.implementation.createHTMLDocument( "" );[m
[32m+[m
[32m+[m			[32m// Set the base href for the created document[m
[32m+[m			[32m// so any parsed elements with URLs[m
[32m+[m			[32m// are based on the document's URL (gh-2965)[m
[32m+[m			[32mbase = context.createElement( "base" );[m
[32m+[m			[32mbase.href = document.location.href;[m
[32m+[m			[32mcontext.head.appendChild( base );[m
[32m+[m		[32m} else {[m
[32m+[m			[32mcontext = document;[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mparsed = rsingleTag.exec( data );[m
[32m+[m	[32mscripts = !keepScripts && [];[m
[32m+[m
[32m+[m	[32m// Single tag[m
[32m+[m	[32mif ( parsed ) {[m
[32m+[m		[32mreturn [ context.createElement( parsed[ 1 ] ) ];[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mparsed = buildFragment( [ data ], context, scripts );[m
[32m+[m
[32m+[m	[32mif ( scripts && scripts.length ) {[m
[32m+[m		[32mjQuery( scripts ).remove();[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn jQuery.merge( [], parsed.childNodes );[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Load a url into a page[m
[32m+[m[32m */[m
[32m+[m[32mjQuery.fn.load = function( url, params, callback ) {[m
[32m+[m	[32mvar selector, type, response,[m
[32m+[m		[32mself = this,[m
[32m+[m		[32moff = url.indexOf( " " );[m
[32m+[m
[32m+[m	[32mif ( off > -1 ) {[m
[32m+[m		[32mselector = jQuery.trim( url.slice( off ) );[m
[32m+[m		[32murl = url.slice( 0, off );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// If it's a function[m
[32m+[m	[32mif ( jQuery.isFunction( params ) ) {[m
[32m+[m
[32m+[m		[32m// We assume that it's the callback[m
[32m+[m		[32mcallback = params;[m
[32m+[m		[32mparams = undefined;[m
[32m+[m
[32m+[m	[32m// Otherwise, build a param string[m
[32m+[m	[32m} else if ( params && typeof params === "object" ) {[m
[32m+[m		[32mtype = "POST";[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m// If we have elements to modify, make the request[m
[32m+[m	[32mif ( self.length > 0 ) {[m
[32m+[m		[32mjQuery.ajax( {[m
[32m+[m			[32murl: url,[m
[32m+[m
[32m+[m			[32m// If "type" variable is undefined, then "GET" method will be used.[m
[32m+[m			[32m// Make value of this field explicit since[m
[32m+[m			[32m// user can override it through ajaxSetup method[m
[32m+[m			[32mtype: type || "GET",[m
[32m+[m			[32mdataType: "html",[m
[32m+[m			[32mdata: params[m
[32m+[m		[32m} ).done( function( responseText ) {[m
[32m+[m
[32m+[m			[32m// Save response for use in complete callback[m
[32m+[m			[32mresponse = arguments;[m
[32m+[m
[32m+[m			[32mself.html( selector ?[m
[32m+[m
[32m+[m				[32m// If a selector was specified, locate the right elements in a dummy div[m
[32m+[m				[32m// Exclude scripts to avoid IE 'Permission Denied' errors[m
[32m+[m				[32mjQuery( "<div>" ).append( jQuery.parseHTML( responseText ) ).find( selector ) :[m
[32m+[m
[32m+[m				[32m// Otherwise use the full result[m
[32m+[m				[32mresponseText );[m
[32m+[m
[32m+[m		[32m// If the request succeeds, this function gets "data", "status", "jqXHR"[m
[32m+[m		[32m// but they are ignored because response was set above.[m
[32m+[m		[32m// If it fails, this function gets "jqXHR", "status", "error"[m
[32m+[m		[32m} ).always( callback && function( jqXHR, status ) {[m
[32m+[m			[32mself.each( function() {[m
[32m+[m				[32mcallback.apply( this, response || [ jqXHR.responseText, status, jqXHR ] );[m
[32m+[m			[32m} );[m
[32m+[m		[32m} );[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn this;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m// Attach a bunch of functions for handling common AJAX events[m
[32m+[m[32mjQuery.each( [[m
[32m+[m	[32m"ajaxStart",[m
[32m+[m	[32m"ajaxStop",[m
[32m+[m	[32m"ajaxComplete",[m
[32m+[m	[32m"ajaxError",[m
[32m+[m	[32m"ajaxSuccess",[m
[32m+[m	[32m"ajaxSend"[m
[32m+[m[32m], function( i, type ) {[m
[32m+[m	[32mjQuery.fn[ type ] = function( fn ) {[m
[32m+[m		[32mreturn this.on( type, fn );[m
[32m+[m	[32m};[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mjQuery.expr.pseudos.animated = function( elem ) {[m
[32m+[m	[32mreturn jQuery.grep( jQuery.timers, function( fn ) {[m
[32m+[m		[32mreturn elem === fn.elem;[m
[32m+[m	[32m} ).length;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Gets a window from an element[m
[32m+[m[32m */[m
[32m+[m[32mfunction getWindow( elem ) {[m
[32m+[m	[32mreturn jQuery.isWindow( elem ) ? elem : elem.nodeType === 9 && elem.defaultView;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mjQuery.offset = {[m
[32m+[m	[32msetOffset: function( elem, options, i ) {[m
[32m+[m		[32mvar curPosition, curLeft, curCSSTop, curTop, curOffset, curCSSLeft, calculatePosition,[m
[32m+[m			[32mposition = jQuery.css( elem, "position" ),[m
[32m+[m			[32mcurElem = jQuery( elem ),[m
[32m+[m			[32mprops = {};[m
[32m+[m
[32m+[m		[32m// Set position first, in-case top/left are set even on static elem[m
[32m+[m		[32mif ( position === "static" ) {[m
[32m+[m			[32melem.style.position = "relative";[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mcurOffset = curElem.offset();[m
[32m+[m		[32mcurCSSTop = jQuery.css( elem, "top" );[m
[32m+[m		[32mcurCSSLeft = jQuery.css( elem, "left" );[m
[32m+[m		[32mcalculatePosition = ( position === "absolute" || position === "fixed" ) &&[m
[32m+[m			[32m( curCSSTop + curCSSLeft ).indexOf( "auto" ) > -1;[m
[32m+[m
[32m+[m		[32m// Need to be able to calculate position if either[m
[32m+[m		[32m// top or left is auto and position is either absolute or fixed[m
[32m+[m		[32mif ( calculatePosition ) {[m
[32m+[m			[32mcurPosition = curElem.position();[m
[32m+[m			[32mcurTop = curPosition.top;[m
[32m+[m			[32mcurLeft = curPosition.left;[m
[32m+[m
[32m+[m		[32m} else {[m
[32m+[m			[32mcurTop = parseFloat( curCSSTop ) || 0;[m
[32m+[m			[32mcurLeft = parseFloat( curCSSLeft ) || 0;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( jQuery.isFunction( options ) ) {[m
[32m+[m
[32m+[m			[32m// Use jQuery.extend here to allow modification of coordinates argument (gh-1848)[m
[32m+[m			[32moptions = options.call( elem, i, jQuery.extend( {}, curOffset ) );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( options.top != null ) {[m
[32m+[m			[32mprops.top = ( options.top - curOffset.top ) + curTop;[m
[32m+[m		[32m}[m
[32m+[m		[32mif ( options.left != null ) {[m
[32m+[m			[32mprops.left = ( options.left - curOffset.left ) + curLeft;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mif ( "using" in options ) {[m
[32m+[m			[32moptions.using.call( elem, props );[m
[32m+[m
[32m+[m		[32m} else {[m
[32m+[m			[32mcurElem.css( props );[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m	[32moffset: function( options ) {[m
[32m+[m
[32m+[m		[32m// Preserve chaining for setter[m
[32m+[m		[32mif ( arguments.length ) {[m
[32m+[m			[32mreturn options === undefined ?[m
[32m+[m				[32mthis :[m
[32m+[m				[32mthis.each( function( i ) {[m
[32m+[m					[32mjQuery.offset.setOffset( this, options, i );[m
[32m+[m				[32m} );[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mvar docElem, win, rect, doc,[m
[32m+[m			[32melem = this[ 0 ];[m
[32m+[m
[32m+[m		[32mif ( !elem ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Support: IE <=11 only[m
[32m+[m		[32m// Running getBoundingClientRect on a[m
[32m+[m		[32m// disconnected node in IE throws an error[m
[32m+[m		[32mif ( !elem.getClientRects().length ) {[m
[32m+[m			[32mreturn { top: 0, left: 0 };[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mrect = elem.getBoundingClientRect();[m
[32m+[m
[32m+[m		[32m// Make sure element is not hidden (display: none)[m
[32m+[m		[32mif ( rect.width || rect.height ) {[m
[32m+[m			[32mdoc = elem.ownerDocument;[m
[32m+[m			[32mwin = getWindow( doc );[m
[32m+[m			[32mdocElem = doc.documentElement;[m
[32m+[m
[32m+[m			[32mreturn {[m
[32m+[m				[32mtop: rect.top + win.pageYOffset - docElem.clientTop,[m
[32m+[m				[32mleft: rect.left + win.pageXOffset - docElem.clientLeft[m
[32m+[m			[32m};[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Return zeros for disconnected and hidden elements (gh-2310)[m
[32m+[m		[32mreturn rect;[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mposition: function() {[m
[32m+[m		[32mif ( !this[ 0 ] ) {[m
[32m+[m			[32mreturn;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mvar offsetParent, offset,[m
[32m+[m			[32melem = this[ 0 ],[m
[32m+[m			[32mparentOffset = { top: 0, left: 0 };[m
[32m+[m
[32m+[m		[32m// Fixed elements are offset from window (parentOffset = {top:0, left: 0},[m
[32m+[m		[32m// because it is its only offset parent[m
[32m+[m		[32mif ( jQuery.css( elem, "position" ) === "fixed" ) {[m
[32m+[m
[32m+[m			[32m// Assume getBoundingClientRect is there when computed position is fixed[m
[32m+[m			[32moffset = elem.getBoundingClientRect();[m
[32m+[m
[32m+[m		[32m} else {[m
[32m+[m
[32m+[m			[32m// Get *real* offsetParent[m
[32m+[m			[32moffsetParent = this.offsetParent();[m
[32m+[m
[32m+[m			[32m// Get correct offsets[m
[32m+[m			[32moffset = this.offset();[m
[32m+[m			[32mif ( !jQuery.nodeName( offsetParent[ 0 ], "html" ) ) {[m
[32m+[m				[32mparentOffset = offsetParent.offset();[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32m// Add offsetParent borders[m
[32m+[m			[32mparentOffset = {[m
[32m+[m				[32mtop: parentOffset.top + jQuery.css( offsetParent[ 0 ], "borderTopWidth", true ),[m
[32m+[m				[32mleft: parentOffset.left + jQuery.css( offsetParent[ 0 ], "borderLeftWidth", true )[m
[32m+[m			[32m};[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32m// Subtract parent offsets and element margins[m
[32m+[m		[32mreturn {[m
[32m+[m			[32mtop: offset.top - parentOffset.top - jQuery.css( elem, "marginTop", true ),[m
[32m+[m			[32mleft: offset.left - parentOffset.left - jQuery.css( elem, "marginLeft", true )[m
[32m+[m		[32m};[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32m// This method will return documentElement in the following cases:[m
[32m+[m	[32m// 1) For the element inside the iframe without offsetParent, this method will return[m
[32m+[m	[32m//    documentElement of the parent window[m
[32m+[m	[32m// 2) For the hidden or detached element[m
[32m+[m	[32m// 3) For body or html element, i.e. in case of the html node - it will return itself[m
[32m+[m	[32m//[m
[32m+[m	[32m// but those exceptions were never presented as a real life use-cases[m
[32m+[m	[32m// and might be considered as more preferable results.[m
[32m+[m	[32m//[m
[32m+[m	[32m// This logic, however, is not guaranteed and can change at any point in the future[m
[32m+[m	[32moffsetParent: function() {[m
[32m+[m		[32mreturn this.map( function() {[m
[32m+[m			[32mvar offsetParent = this.offsetParent;[m
[32m+[m
[32m+[m			[32mwhile ( offsetParent && jQuery.css( offsetParent, "position" ) === "static" ) {[m
[32m+[m				[32moffsetParent = offsetParent.offsetParent;[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mreturn offsetParent || documentElement;[m
[32m+[m		[32m} );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32m// Create scrollLeft and scrollTop methods[m
[32m+[m[32mjQuery.each( { scrollLeft: "pageXOffset", scrollTop: "pageYOffset" }, function( method, prop ) {[m
[32m+[m	[32mvar top = "pageYOffset" === prop;[m
[32m+[m
[32m+[m	[32mjQuery.fn[ method ] = function( val ) {[m
[32m+[m		[32mreturn access( this, function( elem, method, val ) {[m
[32m+[m			[32mvar win = getWindow( elem );[m
[32m+[m
[32m+[m			[32mif ( val === undefined ) {[m
[32m+[m				[32mreturn win ? win[ prop ] : elem[ method ];[m
[32m+[m			[32m}[m
[32m+[m
[32m+[m			[32mif ( win ) {[m
[32m+[m				[32mwin.scrollTo([m
[32m+[m					[32m!top ? val : win.pageXOffset,[m
[32m+[m					[32mtop ? val : win.pageYOffset[m
[32m+[m				[32m);[m
[32m+[m
[32m+[m			[32m} else {[m
[32m+[m				[32melem[ method ] = val;[m
[32m+[m			[32m}[m
[32m+[m		[32m}, method, val, arguments.length );[m
[32m+[m	[32m};[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32m// Support: Safari <=7 - 9.1, Chrome <=37 - 49[m
[32m+[m[32m// Add the top/left cssHooks using jQuery.fn.position[m
[32m+[m[32m// Webkit bug: https://bugs.webkit.org/show_bug.cgi?id=29084[m
[32m+[m[32m// Blink bug: https://bugs.chromium.org/p/chromium/issues/detail?id=589347[m
[32m+[m[32m// getComputedStyle returns percent when specified for top/left/bottom/right;[m
[32m+[m[32m// rather than make the css module depend on the offset module, just check for it here[m
[32m+[m[32mjQuery.each( [ "top", "left" ], function( i, prop ) {[m
[32m+[m	[32mjQuery.cssHooks[ prop ] = addGetHookIf( support.pixelPosition,[m
[32m+[m		[32mfunction( elem, computed ) {[m
[32m+[m			[32mif ( computed ) {[m
[32m+[m				[32mcomputed = curCSS( elem, prop );[m
[32m+[m
[32m+[m				[32m// If curCSS returns percentage, fallback to offset[m
[32m+[m				[32mreturn rnumnonpx.test( computed ) ?[m
[32m+[m					[32mjQuery( elem ).position()[ prop ] + "px" :[m
[32m+[m					[32mcomputed;[m
[32m+[m			[32m}[m
[32m+[m		[32m}[m
[32m+[m	[32m);[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m[32m// Create innerHeight, innerWidth, height, width, outerHeight and outerWidth methods[m
[32m+[m[32mjQuery.each( { Height: "height", Width: "width" }, function( name, type ) {[m
[32m+[m	[32mjQuery.each( { padding: "inner" + name, content: type, "": "outer" + name },[m
[32m+[m		[32mfunction( defaultExtra, funcName ) {[m
[32m+[m
[32m+[m		[32m// Margin is only for outerHeight, outerWidth[m
[32m+[m		[32mjQuery.fn[ funcName ] = function( margin, value ) {[m
[32m+[m			[32mvar chainable = arguments.length && ( defaultExtra || typeof margin !== "boolean" ),[m
[32m+[m				[32mextra = defaultExtra || ( margin === true || value === true ? "margin" : "border" );[m
[32m+[m
[32m+[m			[32mreturn access( this, function( elem, type, value ) {[m
[32m+[m				[32mvar doc;[m
[32m+[m
[32m+[m				[32mif ( jQuery.isWindow( elem ) ) {[m
[32m+[m
[32m+[m					[32m// $( window ).outerWidth/Height return w/h including scrollbars (gh-1729)[m
[32m+[m					[32mreturn funcName.indexOf( "outer" ) === 0 ?[m
[32m+[m						[32melem[ "inner" + name ] :[m
[32m+[m						[32melem.document.documentElement[ "client" + name ];[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32m// Get document width or height[m
[32m+[m				[32mif ( elem.nodeType === 9 ) {[m
[32m+[m					[32mdoc = elem.documentElement;[m
[32m+[m
[32m+[m					[32m// Either scroll[Width/Height] or offset[Width/Height] or client[Width/Height],[m
[32m+[m					[32m// whichever is greatest[m
[32m+[m					[32mreturn Math.max([m
[32m+[m						[32melem.body[ "scroll" + name ], doc[ "scroll" + name ],[m
[32m+[m						[32melem.body[ "offset" + name ], doc[ "offset" + name ],[m
[32m+[m						[32mdoc[ "client" + name ][m
[32m+[m					[32m);[m
[32m+[m				[32m}[m
[32m+[m
[32m+[m				[32mreturn value === undefined ?[m
[32m+[m
[32m+[m					[32m// Get width or height on the element, requesting but not forcing parseFloat[m
[32m+[m					[32mjQuery.css( elem, type, extra ) :[m
[32m+[m
[32m+[m					[32m// Set width or height on the element[m
[32m+[m					[32mjQuery.style( elem, type, value, extra );[m
[32m+[m			[32m}, type, chainable ? margin : undefined, chainable );[m
[32m+[m		[32m};[m
[32m+[m	[32m} );[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m
[32m+[m[32mjQuery.fn.extend( {[m
[32m+[m
[32m+[m	[32mbind: function( types, data, fn ) {[m
[32m+[m		[32mreturn this.on( types, null, data, fn );[m
[32m+[m	[32m},[m
[32m+[m	[32munbind: function( types, fn ) {[m
[32m+[m		[32mreturn this.off( types, null, fn );[m
[32m+[m	[32m},[m
[32m+[m
[32m+[m	[32mdelegate: function( selector, types, data, fn ) {[m
[32m+[m		[32mreturn this.on( types, selector, data, fn );[m
[32m+[m	[32m},[m
[32m+[m	[32mundelegate: function( selector, types, fn ) {[m
[32m+[m
[32m+[m		[32m// ( namespace ) or ( selector, types [, fn] )[m
[32m+[m		[32mreturn arguments.length === 1 ?[m
[32m+[m			[32mthis.off( selector, "**" ) :[m
[32m+[m			[32mthis.off( types, selector || "**", fn );[m
[32m+[m	[32m}[m
[32m+[m[32m} );[m
[32m+[m
[32m+[m[32mjQuery.parseJSON = JSON.parse;[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32m// Register as a named AMD module, since jQuery can be concatenated with other[m
[32m+[m[32m// files that may use define, but not via a proper concatenation script that[m
[32m+[m[32m// understands anonymous AMD modules. A named AMD is safest and most robust[m
[32m+[m[32m// way to register. Lowercase jquery is used because AMD module names are[m
[32m+[m[32m// derived from file names, and jQuery is normally delivered in a lowercase[m
[32m+[m[32m// file name. Do this after creating the global so that if an AMD module wants[m
[32m+[m[32m// to call noConflict to hide this version of jQuery, it will work.[m
[32m+[m
[32m+[m[32m// Note that for maximum portability, libraries that are not jQuery should[m
[32m+[m[32m// declare themselves as anonymous modules, and avoid setting a global if an[m
[32m+[m[32m// AMD loader is present. jQuery is a special case. For more information, see[m
[32m+[m[32m// https://github.com/jrburke/requirejs/wiki/Updating-existing-libraries#wiki-anon[m
[32m+[m
[32m+[m[32mif ( typeof define === "function" && define.amd ) {[m
[32m+[m	[32mdefine( "jquery", [], function() {[m
[32m+[m		[32mreturn jQuery;[m
[32m+[m	[32m} );[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m
[32m+[m[32mvar[m
[32m+[m
[32m+[m	[32m// Map over jQuery in case of overwrite[m
[32m+[m	[32m_jQuery = window.jQuery,[m
[32m+[m
[32m+[m	[32m// Map over the $ in case of overwrite[m
[32m+[m	[32m_$ = window.$;[m
[32m+[m
[32m+[m[32mjQuery.noConflict = function( deep ) {[m
[32m+[m	[32mif ( window.$ === jQuery ) {[m
[32m+[m		[32mwindow.$ = _$;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mif ( deep && window.jQuery === jQuery ) {[m
[32m+[m		[32mwindow.jQuery = _jQuery;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mreturn jQuery;[m
[32m+[m[32m};[m
[32m+[m
[32m+[m[32m// Expose jQuery and $ identifiers, even in AMD[m
[32m+[m[32m// (#7102#comment:10, https://github.com/jquery/jquery/pull/557)[m
[32m+[m[32m// and CommonJS for browser emulators (#13566)[m
[32m+[m[32mif ( !noGlobal ) {[m
[32m+[m	[32mwindow.jQuery = window.$ = jQuery;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mreturn jQuery;[m
[32m+[m[32m} );[m
