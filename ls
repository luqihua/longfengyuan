[32m+[m[32m/**[m
[32m+[m[32m * A simple, efficent mobile slider solution[m
[32m+[m[32m * @file iSlider.js[m
[32m+[m[32m * @author BE-FE Team[m
[32m+[m[32m *    qbaty qbaty.qi@gmail.com[m
[32m+[m[32m *    xieyu33333 xieyu33333@gmail.com[m
[32m+[m[32m *    shinate shine.wangrs@gmail.com[m
[32m+[m[32m *[m
[32m+[m[32m * @LICENSE https://github.com/BE-FE/iSlider/blob/master/LICENSE[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m(function (global) {[m
[32m+[m[32m    'use strict';[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * noop function[m
[32m+[m[32m     */[m
[32m+[m[32m    function noop() {[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Check in array[m
[32m+[m[32m     * @param {*} o[m
[32m+[m[32m     * @param {Array} arr[m
[32m+[m[32m     * @returns {Boolean}[m
[32m+[m[32m     */[m
[32m+[m[32m    function inArray(o, arr) {[m
[32m+[m[32m        return arr.indexOf(o) > -1;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Check is array[m
[32m+[m[32m     * @param {*} o[m
[32m+[m[32m     * @returns {Boolean}[m
[32m+[m[32m     */[m
[32m+[m[32m    function isArray(o) {[m
[32m+[m[32m        return Object.prototype.toString.call(o) === '[object Array]';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Check is object[m
[32m+[m[32m     * @param {*} o[m
[32m+[m[32m     * @returns {Boolean}[m
[32m+[m[32m     */[m
[32m+[m[32m    function isObject(o) {[m
[32m+[m[32m        return Object.prototype.toString.call(o) === '[object Object]';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @param {HTMLElement} el[m
[32m+[m[32m     * @param {String} cls[m
[32m+[m[32m     * @returns {Array|{index: number, input: string}}[m
[32m+[m[32m     */[m
[32m+[m[32m    function hasClass(el, cls) {[m
[32m+[m[32m        return el.className.match(new RegExp('(\\s|^)(' + cls + ')(\\s|$)'));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @param {HTMLElement} el[m
[32m+[m[32m     * @param {String} cls[m
[32m+[m[32m     */[m
[32m+[m[32m    function addClass(el, cls) {[m
[32m+[m[32m        if (!hasClass(el, cls)) {[m
[32m+[m[32m            el.className += ' ' + cls;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @param {HTMLElement} el[m
[32m+[m[32m     * @param {String} cls[m
[32m+[m[32m     */[m
[32m+[m[32m    function removeClass(el, cls) {[m
[32m+[m[32m        if (hasClass(el, cls)) {[m
[32m+[m[32m            el.className = el.className.replace(RegExp('(\\s|^)(' + cls + ')(\\s|$)'), '$3');[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Check is url[m
[32m+[m[32m     * @param {String} url[m
[32m+[m[32m     * @returns {Boolean}[m
[32m+[m[32m     */[m
[32m+[m[32m    function isUrl(url) {[m
[32m+[m[32m        if (/<\/?[^>]*>/.test(url))[m
[32m+[m[32m            return false;[m
[32m+[m[32m        return /^(?:(https|http|ftp|rtsp|mms):)?(\/\/)?(\w+:{0,1}\w*@)?([^\?#:\/]+\.[a-z]+|\d+\.\d+\.\d+\.\d+)?(:[0-9]+)?((?:\.?\/)?([^\?#]*)?(\?[^#]+)?(#.+)?)?$/.test(url);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Check is dom object[m
[32m+[m[32m     * @param {object} dom[m
[32m+[m[32m     * @returns {Boolean}[m
[32m+[m[32m     */[m
[32m+[m[32m    function isDom(obj) {[m
[32m+[m[32m        try {[m
[32m+[m[32m            return obj instanceof HTMLElement;[m
[32m+[m[32m        }[m
[32m+[m[32m        catch(e){[m
[32m+[m[32m            return (typeof obj==="object") &&[m
[32m+[m[32m              (obj.nodeType===1) && (typeof obj.style === "object") &&[m
[32m+[m[32m              (typeof obj.ownerDocument ==="object");[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Parse arguments to array[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param {Arguments} a[m
[32m+[m[32m     * @param {Number|null} start[m
[32m+[m[32m     * @param {Number|null} end[m
[32m+[m[32m     * @returns {Array}[m
[32m+[m[32m     */[m
[32m+[m[32m    function _A(a) {[m
[32m+[m[32m        return Array.prototype.slice.apply(a, Array.prototype.slice.call(arguments, 1));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function IU(word) {[m
[32m+[m[32m        return word.replace(/^[a-z]/, function (t) {[m
[32m+[m[32m            return t.toUpperCase();[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @constructor[m
[32m+[m[32m     *[m
[32m+[m[32m     * iSlider([[{HTMLElement} container,] {Array} datalist,] {Object} options)[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param {HTMLElement} container[m
[32m+[m[32m     * @param {Array} datalist[m
[32m+[m[32m     * @param {Object} options[m
[32m+[m[32m     *[m
[32m+[m[32m     * @description[m
[32m+[m[32m     *  options.dom > container[m
[32m+[m[32m     *  options.data > datalist[m
[32m+[m[32m     */[m
[32m+[m[32m    var iSlider = function () {[m
[32m+[m
[32m+[m[32m        var args = _A(arguments, 0, 3);[m
[32m+[m[32m        if (!args.length) {[m
[32m+[m[32m            throw new Error('Parameters required!');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var opts = isObject(args.slice(-1)[0]) ? args.pop() : {};[m
[32m+[m
[32m+[m[32m        switch (args.length) {[m
[32m+[m[32m            case 2:[m
[32m+[m[32m                opts.data = opts.data || args[1];[m
[32m+[m[32m            case 1:[m
[32m+[m[32m                opts.dom = opts.dom || args[0];[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (!opts.dom) {[m
[32m+[m[32m            throw new Error('Container can not be empty!');[m
[32m+[m[32m        }[m
[32m+[m[32m        else if (!isDom(opts.dom)){[m
[32m+[m[32m            throw new Error('Container must be a HTMLElement instance!');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (!opts.data || !opts.data.length) {[m
[32m+[m[32m            throw new Error('Data must be an array and must have more than one element!');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Options[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        this._opts = opts;[m
[32m+[m
[32m+[m[32m        opts = null, args = null;[m
[32m+[m
[32m+[m[32m        this._setting();[m
[32m+[m
[32m+[m[32m        this.fire('initialize');[m
[32m+[m
[32m+[m[32m        this._renderWrapper();[m
[32m+[m[32m        this._initPlugins();[m
[32m+[m[32m        this._bindHandler();[m
[32m+[m
[32m+[m[32m        this.fire('initialized');[m
[32m+[m
[32m+[m[32m        // Autoplay mode[m
[32m+[m[32m        this._autoPlay();[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * version[m
[32m+[m[32m     * @type {string}[m
[32m+[m[32m     */[m
[32m+[m[32m    iSlider.VERSION = '2.2.0';[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Event white list[m
[32m+[m[32m     * @type {Array}[m
[32m+[m[32m     * @protected[m
[32m+[m[32m     */[m
[32m+[m[32m    iSlider.EVENTS = [[m
[32m+[m[32m        'initialize',[m
[32m+[m[32m        'initialized',[m
[32m+[m[32m        'pluginInitialize',[m
[32m+[m[32m        'pluginInitialized',[m
[32m+[m[32m        'renderComplete',[m
[32m+[m[32m        'slide',[m
[32m+[m[32m        'slideStart',[m
[32m+[m[32m        'slideEnd',[m
[32m+[m[32m        'slideChange',[m
[32m+[m[32m        'slideChanged',[m
[32m+[m[32m        'slideRestore',[m
[32m+[m[32m        'slideRestored',[m
[32m+[m[32m        'loadData',[m
[32m+[m[32m        'reset',[m
[32m+[m[32m        'destroy'[m
[32m+[m[32m    ];[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Easing white list[m
[32m+[m[32m     * @type [Array, RegExp[]][m
[32m+[m[32m     * @protected[m
[32m+[m[32m     */[m
[32m+[m[32m    iSlider.EASING = [[m
[32m+[m[32m        ['linear', 'ease', 'ease-in', 'ease-out', 'ease-in-out'],[m
[32m+[m[32m        /cubic-bezier\(([^\d]*(\d+.?\d*)[^\,]*\,?){4}\)/[m
[32m+[m[32m    ];[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * TAGS whitelist on fixpage mode[m
[32m+[m[32m     * @type {Array}[m
[32m+[m[32m     * @protected[m
[32m+[m[32m     */[m
[32m+[m[32m    iSlider.FIX_PAGE_TAGS = ['SELECT', 'INPUT', 'TEXTAREA', 'BUTTON', 'LABEL'];[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Scene node types[m
[32m+[m[32m     * @type {Object}[m
[32m+[m[32m     * @protected[m
[32m+[m[32m     * TODO Prepare for the migration Symbol ES6.[m
[32m+[m[32m     */[m
[32m+[m[32m    iSlider.NODE_TYPE = {[m
[32m+[m[32m        unknown: 'unknown',[m
[32m+[m[32m        empty: 'empty',[m
[32m+[m[32m        pic: 'pic',[m
[32m+[m[32m        dom: 'dom',[m
[32m+[m[32m        html: 'html',[m
[32m+[m[32m        node: 'node',[m
[32m+[m[32m        element: 'element'[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @returns {String}[m
[32m+[m[32m     * @private[m
[32m+[m[32m     */[m
[32m+[m[32m    iSlider.TRANSITION_END_EVENT = null;[m
[32m+[m
[32m+[m[32m    iSlider.BROWSER_PREFIX = null;[m
[32m+[m
[32m+[m[32m    (function () {[m
[32m+[m[32m        var e = document.createElement('fakeElement');[m
[32m+[m[32m        [[m
[32m+[m[32m            ['WebkitTransition', 'webkitTransitionEnd', 'webkit'],[m
[32m+[m[32m            ['transition', 'transitionend', null],[m
[32m+[m[32m            ['MozTransition', 'transitionend', 'moz'],[m
[32m+[m[32m            ['OTransition', 'oTransitionEnd', 'o'][m
[32m+[m[32m        ].some(function (t) {[m
[32m+[m[32m            if (e.style[t[0]] !== undefined) {[m
[32m+[m[32m                iSlider.TRANSITION_END_EVENT = t[1];[m
[32m+[m[32m                iSlider.BROWSER_PREFIX = t[2];[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    })();[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Event match depending on the browser supported[m
[32m+[m[32m     * @type {{hasTouch, startEvt, moveEvt, endEvt, cancelEvt, resizeEvt}}[m
[32m+[m[32m     */[m
[32m+[m[32m    iSlider.DEVICE_EVENTS = (function () {[m
[32m+[m[32m        // IOS desktop has touch events, make them busting[m
[32m+[m[32m        var hasTouch = !!(('ontouchstart' in global && !/Mac OS X /.test(global.navigator.userAgent)) || global.DocumentTouch && document instanceof global.DocumentTouch);[m
[32m+[m[32m        return {[m
[32m+[m[32m            hasTouch: hasTouch,[m
[32m+[m[32m            startEvt: hasTouch ? 'touchstart' : 'mousedown',[m
[32m+[m[32m            moveEvt: hasTouch ? 'touchmove' : 'mousemove',[m
[32m+[m[32m            endEvt: hasTouch ? 'touchend' : 'mouseup',[m
[32m+[m[32m            cancelEvt: hasTouch ? 'touchcancel' : 'mouseout',[m
[32m+[m[32m            resizeEvt: 'onorientationchange' in global ? 'orientationchange' : 'resize'[m
[32m+[m[32m        };[m
[32m+[m[32m    })();[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Extend[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSlider.extend = function () {[m
[32m+[m[32m        var main, extend, args = arguments;[m
[32m+[m
[32m+[m[32m        switch (args.length) {[m
[32m+[m[32m            case 0:[m
[32m+[m[32m                return;[m
[32m+[m[32m            case 1:[m
[32m+[m[32m                main = iSlider.prototype;[m
[32m+[m[32m                extend = args[0];[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 2:[m
[32m+[m[32m                main = args[0];[m
[32m+[m[32m                extend = args[1];[m
[32m+[m[32m                break;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        for (var property in extend) {[m
[32m+[m[32m            if (extend.hasOwnProperty(property)) {[m
[32m+[m[32m                main[property] = extend[property];[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Plugins[m
[32m+[m[32m     * @type {{}}[m
[32m+[m[32m     * @protected[m
[32m+[m[32m     */[m
[32m+[m[32m    iSlider.plugins = {};[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @param name[m
[32m+[m[32m     * @param plugin[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSlider.regPlugin = function (name, plugin) {[m
[32m+[m[32m        iSlider.plugins[name] = iSlider.plugins[name] || plugin;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @param {String} prop[m
[32m+[m[32m     * @param {String} value[m
[32m+[m[32m     * @returns {String}[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSlider.styleProp = function (prop, isDP) {[m
[32m+[m[32m        if (iSlider.BROWSER_PREFIX) {[m
[32m+[m[32m            if (!!isDP) {[m
[32m+[m[32m                return iSlider.BROWSER_PREFIX + IU(prop);[m
[32m+[m[32m            } else {[m
[32m+[m[32m                return '-' + iSlider.BROWSER_PREFIX + '-' + prop;[m
[32m+[m[32m            }[m
[32m+[m[32m        } else {[m
[32m+[m[32m            return prop;[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @param {String} prop[m
[32m+[m[32m     * @param {HTMLElement} dom[m
[32m+[m[32m     * @param {String} value[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSlider.setStyle = function (dom, prop, value) {[m
[32m+[m[32m        dom.style[iSlider.styleProp(prop, 1)] = value;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @param {String} prop[m
[32m+[m[32m     * @param {HTMLElement} dom[m
[32m+[m[32m     * @param {String} value[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSlider.getStyle = function (dom, prop) {[m
[32m+[m[32m        return dom.style[iSlider.styleProp(prop, 1)];[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @type {Object}[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param {HTMLElement} dom The wrapper <li> element[m
[32m+[m[32m     * @param {String} axis Animate direction[m
[32m+[m[32m     * @param {Number} scale Outer wrapper[m
[32m+[m[32m     * @param {Number} i Wrapper's index[m
[32m+[m[32m     * @param {Number} offset Move distance[m
[32m+[m[32m     * @protected[m
[32m+[m[32m     */[m
[32m+[m[32m    iSlider._animateFuncs = {[m
[32m+[m[32m        normal: (function () {[m
[32m+[m[32m            function normal(dom, axis, scale, i, offset) {[m
[32m+[m[32m                iSlider.setStyle(dom, 'transform', 'translateZ(0) translate' + axis + '(' + (offset + scale * (i - 1)) + 'px)');[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            normal.effect = iSlider.styleProp('transform');[m
[32m+[m[32m            return normal;[m
[32m+[m[32m        })()[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * This is a alias, conducive to compression[m
[32m+[m[32m     * @type {Object}[m
[32m+[m[32m     */[m
[32m+[m[32m    var iSliderPrototype = iSlider.prototype;[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * & iSlider.extend[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.extend = iSlider.extend;[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * setting parameters for slider[m
[32m+[m[32m     * @private[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype._setting = function () {[m
[32m+[m
[32m+[m[32m        var self = this;[m
[32m+[m
[32m+[m[32m        // --------------------------------[m
[32m+[m[32m        // - Status[m
[32m+[m[32m        // --------------------------------[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * The plugins[m
[32m+[m[32m         * @type {Array|{}|*}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self._plugins = iSlider.plugins;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Extend animations[m
[32m+[m[32m         * @type {{default: Function}|*}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self._animateFuncs = iSlider._animateFuncs;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * @type {Boolean}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self._holding = false;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * @type {Boolean}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self._locking = false;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * @type {Array}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self._intermediateScene = null;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * @type {null}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self._transitionEndHandler = null;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * listener[m
[32m+[m[32m         * @type {{autoPlay: null, resize: null, transitionEnd: null}}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self._LSN = {[m
[32m+[m[32m            autoPlay: null,[m
[32m+[m[32m            resize: null,[m
[32m+[m[32m            transitionEnd: null[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * current scene element: this.els[1][m
[32m+[m[32m         * @type {null}[m
[32m+[m[32m         * @public[m
[32m+[m[32m         */[m
[32m+[m[32m        self.currentEl = null;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Event handle[m
[32m+[m[32m         * @type {Object}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self._EventHandle = {};[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * is on Moving[m
[32m+[m[32m         * @type {Boolean}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.onMoving = false;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * is on Sliding[m
[32m+[m[32m         * @type {Boolean}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.onSliding = false;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * animate direction[m
[32m+[m[32m         * @type {Number|null}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.direction = null;[m
[32m+[m
[32m+[m[32m        // --------------------------------[m
[32m+[m[32m        // - Set options[m
[32m+[m[32m        // --------------------------------[m
[32m+[m
[32m+[m[32m        var opts = this._opts;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * dom element wrapping content[m
[32m+[m[32m         * @type {HTMLElement}[m
[32m+[m[32m         * @public[m
[32m+[m[32m         */[m
[32m+[m[32m        self.wrap = opts.dom;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Data list[m
[32m+[m[32m         * @type {Array}[m
[32m+[m[32m         * @public[m
[32m+[m[32m         */[m
[32m+[m[32m        self.data = opts.data;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * default slide direction[m
[32m+[m[32m         * @type {Boolean}[m
[32m+[m[32m         * @public[m
[32m+[m[32m         */[m
[32m+[m[32m        self.isVertical = !!opts.isVertical;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Overspread mode[m
[32m+[m[32m         * @type {Boolean}[m
[32m+[m[32m         * @public[m
[32m+[m[32m         */[m
[32m+[m[32m        self.isOverspread = !!opts.isOverspread;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Play time gap[m
[32m+[m[32m         * @type {Number}[m
[32m+[m[32m         * @public[m
[32m+[m[32m         */[m
[32m+[m[32m        self.duration = opts.duration || 2000;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * start from initIndex or 0[m
[32m+[m[32m         * @type {Number}[m
[32m+[m[32m         * @public[m
[32m+[m[32m         */[m
[32m+[m[32m        self.initIndex = opts.initIndex > 0 && opts.initIndex <= opts.data.length - 1 ? opts.initIndex : 0;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * touchstart prevent default to fixPage[m
[32m+[m[32m         * @type {Boolean}[m
[32m+[m[32m         * @public[m
[32m+[m[32m         */[m
[32m+[m[32m        self.fixPage = (function () {[m
[32m+[m[32m            var fp = opts.fixPage;[m
[32m+[m[32m            if (fp === false || fp === 0) {[m
[32m+[m[32m                return false;[m
[32m+[m[32m            }[m
[32m+[m[32m            if (isArray(fp) && fp.length > 0 || typeof fp === 'string' && fp !== '') {[m
[32m+[m[32m                return [].concat(fp).toString();[m
[32m+[m[32m            }[m
[32m+[m[32m            return true;[m
[32m+[m[32m        })();[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Fill seam when render[m
[32m+[m[32m         * Default is false[m
[32m+[m[32m         * @type {Boolean}[m
[32m+[m[32m         * @public[m
[32m+[m[32m         */[m
[32m+[m[32m        self.fillSeam = !!opts.fillSeam;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * slideIndex[m
[32m+[m[32m         * @type {Number}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.slideIndex = self.slideIndex || self.initIndex || 0;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Axis[m
[32m+[m[32m         * @type {String}[m
[32m+[m[32m         * @public[m
[32m+[m[32m         */[m
[32m+[m[32m        self.axis = self.isVertical ? 'Y' : 'X';[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * reverseAxis[m
[32m+[m[32m         * @type {String}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.reverseAxis = self.axis === 'Y' ? 'X' : 'Y';[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Wrapper width[m
[32m+[m[32m         * @type {Number}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.width = typeof opts.width === 'number' ? opts.width : self.wrap.offsetWidth;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Wrapper height[m
[32m+[m[32m         * @type {Number}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.height = typeof opts.height === 'number' ? opts.height : self.wrap.offsetHeight;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Ratio height:width[m
[32m+[m[32m         * @type {Number}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.ratio = self.height / self.width;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Scale, size rule[m
[32m+[m[32m         * @type {Number}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.scale = self.isVertical ? self.height : self.width;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * On slide offset position[m
[32m+[m[32m         * @type {{X: number, Y: number}}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.offset = self.offset || {X: 0, Y: 0};[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Enable/disable touch events[m
[32m+[m[32m         * @type {Boolean}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.isTouchable = opts.isTouchable == null ? true : !!opts.isTouchable;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * looping logic adjust[m
[32m+[m[32m         * @type {Boolean}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.isLooping = opts.isLooping && self.data.length > 1 ? true : false;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Damping force[m
[32m+[m[32m         * Effect in non-looping mode[m
[32m+[m[32m         * Range 0 ~ 1[m
[32m+[m[32m         * @type {Number}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.dampingForce = Math.max(0, Math.min(1, parseFloat(opts.dampingForce) || 0));[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * AutoPlay waitting milsecond to start[m
[32m+[m[32m         * @type {Number}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.delay = opts.delay || 0;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * autoplay logic adjust[m
[32m+[m[32m         * @type {Boolean}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.isAutoplay = opts.isAutoplay && self.data.length > 1 ? true : false;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Animate type[m
[32m+[m[32m         * @type {String}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.animateType = opts.animateType in self._animateFuncs ? opts.animateType : 'normal';[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * @protected[m
[32m+[m[32m         */[m
[32m+[m[32m        self._animateFunc = self._animateFuncs[self.animateType];[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self._animateReverse = (function () {[m
[32m+[m[32m            var _ = [];[m
[32m+[m[32m            for (var type in self._animateFuncs) {[m
[32m+[m[32m                if (self._animateFuncs.hasOwnProperty(type) && self._animateFuncs[type].reverse) {[m
[32m+[m[32m                    _.push(type);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            return _;[m
[32m+[m[32m        })();[m
[32m+[m
[32m+[m[32m        // little trick set, when you chooce tear & vertical same time[m
[32m+[m[32m        // iSlider overspread mode will be set true autometicly[m
[32m+[m[32m        if (self.isVertical && self.animateType === 'card') {[m
[32m+[m[32m            self.isOverspread = true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Debug mode[m
[32m+[m[32m         * @type {Function}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.log = opts.isDebug ? function () {[m
[32m+[m[32m            global.console.log.apply(global.console, arguments);[m
[32m+[m[32m        } : noop;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Enable damping when slider meet the edge[m
[32m+[m[32m         * @param distance[m
[32m+[m[32m         * @returns {*}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self._damping = (function () {[m
[32m+[m[32m            return function (distance) {[m
[32m+[m[32m                return Math.atan(Math.abs(distance) / self.scale) * 0.62 * (1 - self.dampingForce) * self.scale * (distance > 0 ? 1 : -1);[m
[32m+[m[32m            }[m
[32m+[m[32m        })();[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * animate process time (ms), default: 300ms[m
[32m+[m[32m         * @type {Number}[m
[32m+[m[32m         * @public[m
[32m+[m[32m         */[m
[32m+[m[32m        self.animateTime = opts.animateTime != null && opts.animateTime > -1 ? opts.animateTime : 300;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * animate effects, default: ease[m
[32m+[m[32m         * @type {String}[m
[32m+[m[32m         * @public[m
[32m+[m[32m         */[m
[32m+[m[32m        self.animateEasing =[m
[32m+[m[32m            inArray(opts.animateEasing, iSlider.EASING[0])[m
[32m+[m[32m            || iSlider.EASING[1].test(opts.animateEasing)[m
[32m+[m[32m                ? opts.animateEasing[m
[32m+[m[32m                : 'ease';[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Fix touch/mouse events[m
[32m+[m[32m         * @type {{hasTouch, startEvt, moveEvt, endEvt}}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.deviceEvents = iSlider.DEVICE_EVENTS;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Finger recognition range, prevent inadvertently touch[m
[32m+[m[32m         * @type {Number}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.fingerRecognitionRange = opts.fingerRecognitionRange > -1 ? parseInt(opts.fingerRecognitionRange) : 10;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Init events[m
[32m+[m[32m         * @type {{}}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.events = {};[m
[32m+[m
[32m+[m[32m        // --------------------------------[m
[32m+[m[32m        // - Register events[m
[32m+[m[32m        // --------------------------------[m
[32m+[m
[32m+[m[32m        iSlider.EVENTS.forEach(function (eventName) {[m
[32m+[m[32m            // TODO callback name of All-Lower-Case will be discarded[m
[32m+[m[32m            var fn = opts['on' + eventName.replace(/^\w{1}/, function (m) {[m
[32m+[m[32m                    return m.toUpperCase();[m
[32m+[m[32m                })] || opts['on' + eventName.toLowerCase()];[m
[32m+[m[32m            typeof fn === 'function' && self.on(eventName, fn, 1);[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        // --------------------------------[m
[32m+[m[32m        // - Plugins[m
[32m+[m[32m        // --------------------------------[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * @type {Object}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        self.pluginConfig = (function () {[m
[32m+[m[32m            var config = {};[m
[32m+[m[32m            if (isArray(opts.plugins)) {[m
[32m+[m[32m                opts.plugins.forEach(function pluginConfigEach(plugin) {[m
[32m+[m[32m                    if (isArray(plugin)) {[m
[32m+[m[32m                        config[plugin[0]] = plugin.slice(1);[m
[32m+[m[32m                    } else if (typeof plugin === 'string') {[m
[32m+[m[32m                        config[plugin] = [];[m
[32m+[m[32m                    }[m
[32m+[m[32m                });[m
[32m+[m[32m            }[m
[32m+[m[32m            return config;[m
[32m+[m[32m        })();[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Init plugins[m
[32m+[m[32m     * @private[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype._initPlugins = function () {[m
[32m+[m[32m        var config = this.pluginConfig;[m
[32m+[m[32m        var plugins = this._plugins;[m
[32m+[m[32m        for (var i in config) {[m
[32m+[m[32m            if (config.hasOwnProperty(i) && plugins.hasOwnProperty(i)) {[m
[32m+[m[32m                this.log('[INIT PLUGIN]:', i, plugins[i]);[m
[32m+[m[32m                plugins[i][m
[32m+[m[32m                && typeof plugins[i] === 'function'[m
[32m+[m[32m                && typeof plugins[i].apply[m
[32m+[m[32m                && plugins[i].apply(this, config[i]);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        this.fire('pluginInitialized');[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Get item type[m
[32m+[m[32m     * @param {Number} index[m
[32m+[m[32m     * @returns {String}[m
[32m+[m[32m     * @private[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype._itemType = function (item) {[m
[32m+[m[32m        if (!isNaN(item)) {[m
[32m+[m[32m            item = this.data[item];[m
[32m+[m[32m        }[m
[32m+[m[32m        if (item.hasOwnProperty('type')) {[m
[32m+[m[32m            return item.type;[m
[32m+[m[32m        }[m
[32m+[m[32m        var content = item.content;[m
[32m+[m[32m        var _NT = iSlider.NODE_TYPE;[m
[32m+[m[32m        var type;[m
[32m+[m[32m        if (content == null) {[m
[32m+[m[32m            type = _NT.empty;[m
[32m+[m[32m        } else {[m
[32m+[m[32m            if (Boolean(content.nodeName) && Boolean(content.nodeType)) {[m
[32m+[m[32m                type = _NT.node;[m
[32m+[m[32m            } else if (typeof content === 'string') {[m
[32m+[m[32m                if (isUrl(content)) {[m
[32m+[m[32m                    type = _NT.pic;[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    type = _NT.html;[m
[32m+[m[32m                }[m
[32m+[m[32m            } else {[m
[32m+[m[32m                type = _NT.unknown;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        item.type = type;[m
[32m+[m
[32m+[m[32m        return type;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * render single item html by idx[m
[32m+[m[32m     * @param {HTMLElement} el ..[m
[32m+[m[32m     * @param {Number} dataIndex  ..[m
[32m+[m[32m     * @private[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype._renderItem = function (el, dataIndex) {[m
[32m+[m
[32m+[m[32m        var item,[m
[32m+[m[32m            self = this,[m
[32m+[m[32m            len = this.data.length;[m
[32m+[m
[32m+[m[32m        var insertImg = function renderItemInsertImg() {[m
[32m+[m[32m            var simg = ' src="' + item.content + '"';[m
[32m+[m[32m            // auto scale to full screen[m
[32m+[m[32m            if (item.height / item.width > self.ratio) {[m
[32m+[m[32m                simg += ' height="100%"';[m
[32m+[m[32m            } else {[m
[32m+[m[32m                simg += ' width="100%"';[m
[32m+[m[32m            }[m
[32m+[m[32m            if (self.isOverspread) {[m
[32m+[m[32m                el.style.cssText += 'background-image:url(' + item.content + ');background-repeat:no-repeat;background-position:50% 50%;background-size:cover';[m
[32m+[m[32m                simg += ' style="display:block;opacity:0;height:100%;width:100%;"';[m
[32m+[m[32m            }[m
[32m+[m[32m            // for right button, save picture[m
[32m+[m[32m            el.innerHTML = '<img' + simg + ' />';[m
[32m+[m[32m        };[m
[32m+[m
[32m+[m[32m        // clean scene[m
[32m+[m[32m        el.innerHTML = '';[m
[32m+[m[32m        el.style.background = '';[m
[32m+[m
[32m+[m[32m        // get the right item of data[m
[32m+[m[32m        if (!this.isLooping && this.data[dataIndex] == null) {[m
[32m+[m[32m            // Stop slide when item is empty[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[32m        else {[m
[32m+[m[32m            dataIndex = (len /* * Math.ceil(Math.abs(dataIndex / len))*/ + dataIndex) % len;[m
[32m+[m[32m            item = this.data[dataIndex];[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var type = this._itemType(item);[m
[32m+[m[32m        var _NT = iSlider.NODE_TYPE;[m
[32m+[m
[32m+[m[32m        this.log('[RENDER]:', type, dataIndex, item);[m
[32m+[m
[32m+[m[32m        el.className = 'islider-' + type;[m
[32m+[m
[32m+[m[32m        switch (type) {[m
[32m+[m[32m            case _NT.pic:[m
[32m+[m[32m                if (item.load === 2) {[m
[32m+[m[32m                    insertImg();[m
[32m+[m[32m                }[m
[32m+[m[32m                else {[m
[32m+[m[32m                    var currentImg = new Image();[m
[32m+[m[32m                    currentImg.src = item.content;[m
[32m+[m[32m                    currentImg.onload = function () {[m
[32m+[m[32m                        item.height = currentImg.height;[m
[32m+[m[32m                        item.width = currentImg.width;[m
[32m+[m[32m                        insertImg();[m
[32m+[m[32m                        item.load = 2;[m
[32m+[m[32m                    };[m
[32m+[m[32m                }[m
[32m+[m[32m                break;[m
[32m+[m[32m            case _NT.dom:[m
[32m+[m[32m            case _NT.html:[m
[32m+[m[32m                el.innerHTML = item.content;[m
[32m+[m[32m                break;[m
[32m+[m[32m            case _NT.node:[m
[32m+[m[32m            case _NT.element:[m
[32m+[m[32m                // fragment, create container[m
[32m+[m[32m                if (item.content.nodeType === 11) {[m
[32m+[m[32m                    var entity = document.createElement('div');[m
[32m+[m[32m                    entity.appendChild(item.content);[m
[32m+[m[32m                    item.content = entity;[m
[32m+[m[32m                }[m
[32m+[m[32m                el.appendChild(item.content);[m
[32m+[m[32m                break;[m
[32m+[m[32m            default:[m
[32m+[m[32m                // do nothing[m
[32m+[m[32m                break;[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Postponing the intermediate scene rendering[m
[32m+[m[32m     * until the target scene is completely rendered (render in event slideChanged)[m
[32m+[m[32m     * to avoid a jumpy feel when switching between scenes[m
[32m+[m[32m     * given that the distance of sliding is more than 1.[m
[32m+[m[32m     * e.g. ```this.slideTo(>+-1)```[m
[32m+[m[32m     *[m
[32m+[m[32m     * @private[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype._renderIntermediateScene = function () {[m
[32m+[m[32m        if (this._intermediateScene != null) {[m
[32m+[m[32m            this._renderItem.apply(this, this._intermediateScene);[m
[32m+[m[32m            this._intermediateScene = null;[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Apply styles on changed[m
[32m+[m[32m     * @private[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype._changedStyles = function () {[m
[32m+[m[32m        var slideStyles = ['islider-prev', 'islider-active', 'islider-next'];[m
[32m+[m[32m        this.els.forEach(function changeStypeEach(el, index) {[m
[32m+[m[32m            removeClass(el, slideStyles.join('|'));[m
[32m+[m[32m            addClass(el, slideStyles[index]);[m
[32m+[m[32m            this.fillSeam && this.originScale(el);[m
[32m+[m[32m        }.bind(this));[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * render list html[m
[32m+[m[32m     * @private[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype._renderWrapper = function () {[m
[32m+[m[32m        //this.wrap.style.overflow = 'hidden';[m
[32m+[m[32m        // initail outer element[m
[32m+[m[32m        var outer;[m
[32m+[m[32m        if (this.outer) {[m
[32m+[m[32m            outer = this.outer;[m
[32m+[m[32m            outer.innerHTML = '';[m
[32m+[m[32m        } else {[m
[32m+[m[32m            outer = document.createElement('ul');[m
[32m+[m[32m        }[m
[32m+[m[32m        outer.className = 'islider-outer';[m
[32m+[m
[32m+[m[32m        //outer.style.overflow = 'hidden';[m
[32m+[m[32m        // no need...[m
[32m+[m[32m        // outer.style.cssText += 'width:' + this.width + 'px;height:' + this.height + 'px';[m
[32m+[m
[32m+[m[32m        // storage li elements, only store 3 elements to reduce memory usage[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Slider elements x3[m
[32m+[m[32m         * @type {Array}[m
[32m+[m[32m         * @public[m
[32m+[m[32m         */[m
[32m+[m[32m        this.els = [];[m
[32m+[m
[32m+[m[32m        for (var i = 0; i < 3; i++) {[m
[32m+[m[32m            var li = document.createElement('li');[m
[32m+[m[32m            outer.appendChild(li);[m
[32m+[m[32m            this.els.push(li);[m
[32m+[m
[32m+[m[32m            // prepare style animation[m
[32m+[m[32m            this._animateFunc(li, this.axis, this.scale, i, 0);[m
[32m+[m
[32m+[m[32m            // auto overflow in none fixPage mode[m
[32m+[m[32m            if (!this.fixPage) {[m
[32m+[m[32m                li.style.overflow = 'auto';[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            this.isVertical && (this.animateType === 'rotate' || this.animateType === 'flip')[m
[32m+[m[32m                ? this._renderItem(li, 1 - i + this.slideIndex)[m
[32m+[m[32m                : this._renderItem(li, i - 1 + this.slideIndex);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this._changedStyles();[m
[32m+[m
[32m+[m[32m        if (this.fillSeam) {[m
[32m+[m[32m            this.els.forEach(function (el, i) {[m
[32m+[m[32m                addClass(el, 'islider-sliding' + (i === 1 ? '-focus' : ''));[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Preload picture [ may be pic :) ][m
[32m+[m[32m        global.setTimeout(function () {[m
[32m+[m[32m            this._preloadImg(this.slideIndex);[m
[32m+[m[32m        }.bind(this), 200);[m
[32m+[m
[32m+[m[32m        // append ul to div#canvas[m
[32m+[m[32m        if (!this.outer) {[m
[32m+[m[32m            /**[m
[32m+[m[32m             * @type {HTMLElement}[m
[32m+[m[32m             * @public[m
[32m+[m[32m             */[m
[32m+[m[32m            this.outer = outer;[m
[32m+[m[32m            this.wrap.appendChild(outer);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this.currentEl = this.els[1];[m
[32m+[m
[32m+[m[32m        this.fire('renderComplete', this.slideIndex, this.currentEl, this);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Preload img when slideChange[m
[32m+[m[32m     * From current index +2, -2 scene[m
[32m+[m[32m     * @param {Number} dataIndex means which image will be load[m
[32m+[m[32m     * @private[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype._preloadImg = function (dataIndex) {[m
[32m+[m[32m        if (this.data.length > 3) {[m
[32m+[m[32m            var data = this.data;[m
[32m+[m[32m            var len = data.length;[m
[32m+[m[32m            var self = this;[m
[32m+[m[32m            var loadImg = function preloadImgLoadingProcess(index) {[m
[32m+[m[32m                var item = data[index];[m
[32m+[m[32m                if (self._itemType(item) === 'pic' && !item.load) {[m
[32m+[m[32m                    var preloadImg = new Image();[m
[32m+[m[32m                    preloadImg.src = item.content;[m
[32m+[m[32m                    preloadImg.onload = function () {[m
[32m+[m[32m                        item.width = preloadImg.width;[m
[32m+[m[32m                        item.height = preloadImg.height;[m
[32m+[m[32m                        item.load = 2;[m
[32m+[m[32m                    };[m
[32m+[m[32m                    item.load = 1;[m
[32m+[m[32m                }[m
[32m+[m[32m            };[m
[32m+[m
[32m+[m[32m            loadImg((dataIndex + 2) % len);[m
[32m+[m[32m            loadImg((dataIndex - 2 + len) % len);[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * watch transition end[m
[32m+[m[32m     * @private[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype._watchTransitionEnd = function (squeezeTime, eventType) {[m
[32m+[m[32m        var cb = function () {[m
[32m+[m[32m            this._unWatchTransitionEnd();[m
[32m+[m[32m            if (eventType === 'slideChanged') {[m
[32m+[m[32m                this._changedStyles();[m
[32m+[m[32m            }[m
[32m+[m[32m            this.fire.call(this, eventType, this.slideIndex, this.currentEl, this);[m
[32m+[m[32m            this._renderIntermediateScene();[m
[32m+[m[32m            this.play();[m
[32m+[m[32m            this.onSliding = false;[m
[32m+[m[32m            this.direction = null;[m
[32m+[m[32m        }.bind(this);[m
[32m+[m
[32m+[m[32m        if (iSlider.TRANSITION_END_EVENT) {[m
[32m+[m[32m            this.currentEl.addEventListener(iSlider.TRANSITION_END_EVENT, cb);[m
[32m+[m[32m            // keep handler and element[m
[32m+[m[32m            this._transitionEndHandler = {el: this.currentEl, handler: cb};[m
[32m+[m[32m        }[m
[32m+[m[32m        this._LSN.transitionEnd = global.setTimeout(cb, squeezeTime);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * unwatch transition end[m
[32m+[m[32m     * @private[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype._unWatchTransitionEnd = function () {[m
[32m+[m[32m        if (this._LSN.transitionEnd) {[m
[32m+[m[32m            global.clearTimeout(this._LSN.transitionEnd);[m
[32m+[m[32m        }[m
[32m+[m[32m        if (this._transitionEndHandler !== null) {[m
[32m+[m[32m            this._transitionEndHandler.el.removeEventListener(iSlider.TRANSITION_END_EVENT, this._transitionEndHandler.handler);[m
[32m+[m[32m            this._transitionEndHandler = null;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        //this.onSliding = false;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * bind all event handler, when on PC, disable drag event[m
[32m+[m[32m     * @private[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype._bindHandler = function () {[m
[32m+[m[32m        var outer = this.outer;[m
[32m+[m[32m        var device = this.deviceEvents;[m
[32m+[m
[32m+[m[32m        if (this.isTouchable) {[m
[32m+[m[32m            if (!device.hasTouch) {[m
[32m+[m[32m                outer.style.cursor = 'pointer';[m
[32m+[m[32m                // disable drag[m
[32m+[m[32m                outer.ondragstart = function (evt) {[m
[32m+[m[32m                    if (evt) {[m
[32m+[m[32m                        return false;[m
[32m+[m[32m                    }[m
[32m+[m[32m                    return true;[m
[32m+[m[32m                };[m
[32m+[m[32m            }[m
[32m+[m[32m            outer.addEventListener(device.startEvt, this);[m
[32m+[m[32m            outer.addEventListener(device.moveEvt, this);[m
[32m+[m[32m            outer.addEventListener(device.endEvt, this);[m
[32m+[m
[32m+[m[32m            // Viscous drag adaptation[m
[32m+[m[32m            !device.hasTouch && outer.addEventListener('mouseout', this);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        global.addEventListener(device.resizeEvt, this);[m
[32m+[m
[32m+[m[32m        // Fix android device[m
[32m+[m[32m        global.addEventListener('focus', this, false);[m
[32m+[m[32m        global.addEventListener('blur', this, false);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     *  Uniformity admin event[m
[32m+[m[32m     *  Event router[m
[32m+[m[32m     *  @param {Object} evt event object[m
[32m+[m[32m     *  @protected[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.handleEvent = function (evt) {[m
[32m+[m[32m        var device = this.deviceEvents;[m
[32m+[m[32m        switch (evt.type) {[m
[32m+[m[32m            case 'mousedown':[m
[32m+[m[32m                // block mouse buttons except left[m
[32m+[m[32m                if (evt.button !== 0) break;[m
[32m+[m[32m            case 'touchstart':[m
[32m+[m[32m                this.startHandler(evt);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case device.moveEvt:[m
[32m+[m[32m                this.moveHandler(evt);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case device.endEvt:[m
[32m+[m[32m            case device.cancelEvt: // mouseout, touchcancel event, trigger endEvent[m
[32m+[m[32m                this.endHandler(evt);[m
[32m+[m[32m                break;[m
[32m+[m[32m            case device.resizeEvt:[m
[32m+[m[32m                this.resizeHandler();[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 'focus':[m
[32m+[m[32m                this.play();[m
[32m+[m[32m                break;[m
[32m+[m[32m            case 'blur':[m
[32m+[m[32m                this.pause();[m
[32m+[m[32m                break;[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     *  touchstart callback[m
[32m+[m[32m     *  @param {Object} evt event object[m
[32m+[m[32m     *  @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.startHandler = function (evt) {[m
[32m+[m[32m        if (this.fixPage && iSlider.FIX_PAGE_TAGS.indexOf(evt.target.tagName.toUpperCase()) < 0 && !this._isItself(evt.target)) {[m
[32m+[m[32m            evt.preventDefault();[m
[32m+[m[32m        }[m
[32m+[m[32m        if (this._holding || this._locking) {[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[32m        var device = this.deviceEvents;[m
[32m+[m[32m        this.onMoving = true;[m
[32m+[m[32m        this.pause();[m
[32m+[m
[32m+[m[32m        this.log('[EVENT]: start');[m
[32m+[m[32m        this.fire('slideStart', evt, this);[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * @type {Number}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        this.startTime = new Date().getTime();[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * @type {Number}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        this.startX = device.hasTouch ? evt.targetTouches[0].pageX : evt.pageX;[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * @type {Number}[m
[32m+[m[32m         * @private[m
[32m+[m[32m         */[m
[32m+[m[32m        this.startY = device.hasTouch ? evt.targetTouches[0].pageY : evt.pageY;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     *  touchmove callback[m
[32m+[m[32m     *  @param {Object} evt event object[m
[32m+[m[32m     *  @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.moveHandler = function (evt) {[m
[32m+[m[32m        if (!this.onMoving) {[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[32m        this.log('[EVENT]: moving');[m
[32m+[m[32m        var device = this.deviceEvents;[m
[32m+[m[32m        var len = this.data.length;[m
[32m+[m[32m        var axis = this.axis;[m
[32m+[m[32m        var reverseAxis = this.reverseAxis;[m
[32m+[m[32m        var offset = {};[m
[32m+[m
[32m+[m[32m        if (evt.hasOwnProperty('offsetRatio')) {[m
[32m+[m[32m            offset[axis] = evt.offsetRatio * this.scale;[m
[32m+[m[32m            offset[reverseAxis] = 0;[m
[32m+[m[32m        } else {[m
[32m+[m[32m            offset.X = device.hasTouch ? (evt.targetTouches[0].pageX - this.startX) : (evt.pageX - this.startX);[m
[32m+[m[32m            offset.Y = device.hasTouch ? (evt.targetTouches[0].pageY - this.startY) : (evt.pageY - this.startY);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this.offset = offset;[m
[32m+[m[32m        evt.offsetRatio = offset[axis] / this.scale;[m
[32m+[m
[32m+[m[32m        if (Math.abs(offset[axis]) - Math.abs(offset[reverseAxis]) > 10) {[m
[32m+[m
[32m+[m[32m            evt.preventDefault();[m
[32m+[m[32m            this._unWatchTransitionEnd();[m
[32m+[m
[32m+[m[32m            if (!this.isLooping) {[m
[32m+[m[32m                if (offset[axis] > 0 && this.slideIndex === 0 || offset[axis] < 0 && this.slideIndex === len - 1) {[m
[32m+[m[32m                    offset[axis] = this._damping(offset[axis]);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            this.els.forEach(function (item, i) {[m
[32m+[m[32m                item.style.visibility = 'visible';[m
[32m+[m[32m                iSlider.setStyle(item, 'transition', 'none');[m
[32m+[m[32m                this._animateFunc(item, axis, this.scale, i, offset[axis], offset[axis]);[m
[32m+[m[32m                this.fillSeam && this.seamScale(item);[m
[32m+[m[32m            }.bind(this));[m
[32m+[m
[32m+[m[32m            this.fire('slide', evt, this);[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     *  touchend callback[m
[32m+[m[32m     *  @param {Object} evt event object[m
[32m+[m[32m     *  @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.endHandler = function (evt) {[m
[32m+[m[32m        if (!this.onMoving) {[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[32m        this.log('[EVENT]: end');[m
[32m+[m[32m        this.onMoving = false;[m
[32m+[m[32m        var offset = this.offset;[m
[32m+[m[32m        var axis = this.axis;[m
[32m+[m[32m        var boundary = this.scale / 2;[m
[32m+[m[32m        var endTime = new Date().getTime();[m
[32m+[m[32m        var FRR = this.fingerRecognitionRange;[m
[32m+[m
[32m+[m[32m        // a quick slide time must under 300ms[m
[32m+[m[32m        // a quick slide should also slide at least 14 px[m
[32m+[m[32m        boundary = endTime - this.startTime > 300 ? boundary : 14;[m
[32m+[m
[32m+[m[32m        var absOffset = Math.abs(offset[axis]);[m
[32m+[m[32m        var absReverseOffset = Math.abs(offset[this.reverseAxis]);[m
[32m+[m
[32m+[m[32m        function dispatchLink(el) {[m
[32m+[m[32m            if (el != null) {[m
[32m+[m[32m                if (el.tagName === 'A') {[m
[32m+[m[32m                    if (el.href) {[m
[32m+[m[32m                        if (el.getAttribute('target') === '_blank') {[m
[32m+[m[32m                            global.open(el.href);[m
[32m+[m[32m                        } else {[m
[32m+[m[32m                            global.location.href = el.href;[m
[32m+[m[32m                        }[m
[32m+[m[32m                        evt.preventDefault();[m
[32m+[m[32m                        return false;[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                else if (el.tagName === 'LI' && el.className.search(/^islider\-/) > -1) {[m
[32m+[m[32m                    return false;[m
[32m+[m[32m                }[m
[32m+[m[32m                else {[m
[32m+[m[32m                    dispatchLink(el.parentNode);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this.fire('slideEnd', evt, this);[m
[32m+[m
[32m+[m[32m        if (offset[axis] >= boundary && absReverseOffset < absOffset) {[m
[32m+[m[32m            this.slideTo(this.slideIndex - 1);[m
[32m+[m[32m        }[m
[32m+[m[32m        else if (offset[axis] < -boundary && absReverseOffset < absOffset) {[m
[32m+[m[32m            this.slideTo(this.slideIndex + 1);[m
[32m+[m[32m        }[m
[32m+[m[32m        else {[m
[32m+[m[32m            if (Math.abs(this.offset[axis]) >= FRR) {[m
[32m+[m[32m                this.slideTo(this.slideIndex);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // create sim tap event if offset < this.fingerRecognitionRange[m
[32m+[m[32m        if (Math.abs(this.offset[axis]) < FRR && this.fixPage && evt.target) {[m
[32m+[m[32m            dispatchLink(evt.target);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this.offset.X = this.offset.Y = 0;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * resize callback[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.resizeHandler = function () {[m
[32m+[m[32m        var _L = this._LSN.resize;[m
[32m+[m[32m        var startTime = +new Date, _W, _H;[m
[32m+[m
[32m+[m[32m        if (this.deviceEvents.hasTouch) {[m
[32m+[m[32m            // Fuck Android[m
[32m+[m[32m            _L && global.clearInterval(_L);[m
[32m+[m[32m            _L = global.setInterval(function () {[m
[32m+[m[32m                if (this.height !== this.wrap.offsetHeight || this.width !== this.wrap.offsetWidth) {[m
[32m+[m[32m                    _L && global.clearInterval(_L);[m
[32m+[m[32m                    _L = global.setInterval(function () {[m
[32m+[m[32m                        if (_W === this.wrap.offsetWidth && _H === this.wrap.offsetHeight) {[m
[32m+[m[32m                            _L && global.clearInterval(_L);[m
[32m+[m[32m                            this.reset();[m
[32m+[m[32m                            this.log('[EVENT]: resize');[m
[32m+[m[32m                        } else {[m
[32m+[m[32m                            _W = this.wrap.offsetWidth, _H = this.wrap.offsetHeight;[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }.bind(this), 12);[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    if (+new Date - startTime >= 1000) {[m
[32m+[m[32m                        _L && global.clearInterval(_L);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m            }.bind(this), 12);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            _L && global.clearTimeout(_L);[m
[32m+[m[32m            _L = global.setTimeout(function () {[m
[32m+[m[32m                if (this.height !== this.wrap.offsetHeight || this.width !== this.wrap.offsetWidth) {[m
[32m+[m[32m                    _L && global.clearInterval(_L);[m
[32m+[m[32m                    this.reset();[m
[32m+[m[32m                    this.log('[EVENT]: resize');[m
[32m+[m[32m                }[m
[32m+[m[32m            }.bind(this), 200);[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     *  slide logical, goto data index[m
[32m+[m[32m     *  @param {Number} dataIndex the goto index[m
[32m+[m[32m     *  @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.slideTo = function (dataIndex, opts) {[m
[32m+[m[32m        // stop auto play[m
[32m+[m[32m        if (this.isAutoplay) {[m
[32m+[m[32m            this.pause();[m
[32m+[m[32m        }[m
[32m+[m[32m        if (this._locking) {[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[32m        this.unhold();[m
[32m+[m[32m        this.onSliding = true;[m
[32m+[m[32m        var animateTime = this.animateTime;[m
[32m+[m[32m        var animateType = this.animateType;[m
[32m+[m[32m        var animateFunc = this._animateFunc;[m
[32m+[m[32m        var data = this.data;[m
[32m+[m[32m        var els = this.els;[m
[32m+[m[32m        var axis = this.axis;[m
[32m+[m[32m        var idx = dataIndex;[m
[32m+[m[32m        var n = dataIndex - this.slideIndex;[m
[32m+[m[32m        var offset = this.offset;[m
[32m+[m[32m        var eventType;[m
[32m+[m[32m        var squeezeTime = 0;[m
[32m+[m
[32m+[m[32m        if (typeof opts === 'object') {[m
[32m+[m[32m            if (opts.animateTime > -1) {[m
[32m+[m[32m                animateTime = opts.animateTime;[m
[32m+[m[32m            }[m
[32m+[m[32m            if (typeof opts.animateType === 'string' && opts.animateType in this._animateFuncs) {[m
[32m+[m[32m                animateType = opts.animateType;[m
[32m+[m[32m                animateFunc = this._animateFuncs[animateType];[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // In the slide process, animate time is squeezed[m
[32m+[m[32m        if (offset[axis] !== 0) {[m
[32m+[m[32m            squeezeTime = Math.abs(offset[axis]) / this.scale * animateTime;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (Math.abs(n) > 1) {[m
[32m+[m[32m            this._renderItem(n > 0 ? this.els[2] : this.els[0], idx);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // preload when slide[m
[32m+[m[32m        this._preloadImg(idx);[m
[32m+[m
[32m+[m[32m        // get right item of data[m
[32m+[m[32m        if (data[idx]) {[m
[32m+[m[32m            this.slideIndex = idx;[m
[32m+[m[32m        }[m
[32m+[m[32m        else {[m
[32m+[m[32m            if (this.isLooping) {[m
[32m+[m[32m                this.slideIndex = n > 0 ? 0 : data.length - 1;[m
[32m+[m[32m            }[m
[32m+[m[32m            else {[m
[32m+[m[32m                n = 0;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this.log('[SLIDE TO]: ' + this.slideIndex);[m
[32m+[m
[32m+[m[32m        // keep the right order of items[m
[32m+[m[32m        var headEl, tailEl, direction;[m
[32m+[m
[32m+[m[32m        // slidechange should render new item[m
[32m+[m[32m        // and change new item style to fit animation[m
[32m+[m[32m        if (n === 0) {[m
[32m+[m[32m            // Restore to current scene[m
[32m+[m[32m            eventType = 'slideRestore';[m
[32m+[m[32m        } else {[m
[32m+[m
[32m+[m[32m            if ((this.isVertical && (inArray(animateType, this._animateReverse))) ^ (n > 0)) {[m
[32m+[m[32m                els.push(els.shift());[m
[32m+[m[32m                headEl = els[2];[m
[32m+[m[32m                tailEl = els[0];[m
[32m+[m[32m                direction = 1;[m
[32m+[m[32m            } else {[m
[32m+[m[32m                els.unshift(els.pop());[m
[32m+[m[32m                headEl = els[0];[m
[32m+[m[32m                tailEl = els[2];[m
[32m+[m[32m                direction = -1;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            this.currentEl = els[1];[m
[32m+[m
[32m+[m[32m            if (Math.abs(n) === 1) {[m
[32m+[m[32m                this._renderIntermediateScene();[m
[32m+[m[32m                this._renderItem(headEl, idx + n);[m
[32m+[m[32m            } else if (Math.abs(n) > 1) {[m
[32m+[m[32m                if ((this.isVertical && (inArray(animateType, this._animateReverse)))) {[m
[32m+[m[32m                    this._renderItem(tailEl, idx + direction);[m
[32m+[m[32m                    this._renderItem(els[1], idx);[m
[32m+[m[32m                    this._intermediateScene = [headEl, idx - direction];[m
[32m+[m[32m                }[m
[32m+[m[32m                else {[m
[32m+[m[32m                    this._renderItem(headEl, idx + direction);[m
[32m+[m[32m                    this._intermediateScene = [tailEl, idx - direction];[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            iSlider.setStyle(headEl, 'transition', 'none');[m
[32m+[m
[32m+[m[32m            // Minus squeeze time[m
[32m+[m[32m            squeezeTime = animateTime - squeezeTime;[m
[32m+[m
[32m+[m[32m            eventType = 'slideChange';[m
[32m+[m
[32m+[m[32m            // For seams[m
[32m+[m[32m            if (this.fillSeam) {[m
[32m+[m[32m                els.forEach(function (el) {[m
[32m+[m[32m                    removeClass(el, 'islider-sliding|islider-sliding-focus');[m
[32m+[m[32m                });[m
[32m+[m[32m                addClass(this.currentEl, 'islider-sliding-focus');[m
[32m+[m[32m                addClass(headEl, 'islider-sliding');[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            this.direction = direction;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // do the trick animation[m
[32m+[m[32m        for (var i = 0; i < 3; i++) {[m
[32m+[m[32m            if (els[i] !== headEl) {[m
[32m+[m[32m                // Only applies their effects[m
[32m+[m[32m                iSlider.setStyle(els[i], 'transition', (animateFunc.effect || 'all') + ' ' + squeezeTime + 'ms ' + this.animateEasing);[m
[32m+[m[32m            }[m
[32m+[m[32m            animateFunc.call(this, els[i], axis, this.scale, i, 0, direction);[m
[32m+[m[32m            this.fillSeam && this.seamScale(els[i]);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this._watchTransitionEnd(squeezeTime, eventType + 'd');[m
[32m+[m[32m        this.fire(eventType, this.slideIndex, this.currentEl, this);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Slide to next scene[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.slideNext = function () {[m
[32m+[m[32m        this.slideTo.apply(this, [this.slideIndex + 1].concat(_A(arguments)));[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Slide to previous scene[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.slidePrev = function () {[m
[32m+[m[32m        this.slideTo.apply(this, [this.slideIndex - 1].concat(_A(arguments)));[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Register plugin (run time mode)[m
[32m+[m[32m     * @param {String} name[m
[32m+[m[32m     * @param {Function} plugin[m
[32m+[m[32m     * @param {...}[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.regPlugin = function () {[m
[32m+[m[32m        var args = _A(arguments);[m
[32m+[m[32m        var name = args.shift(),[m
[32m+[m[32m            plugin = args[0];[m
[32m+[m
[32m+[m[32m        if (!this._plugins.hasOwnProperty(name) && typeof plugin !== 'function') {[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[32m        if (typeof plugin === 'function') {[m
[32m+[m[32m            this._plugins[name] = plugin;[m
[32m+[m[32m            args.shift();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Auto enable and init plugin when at run time[m
[32m+[m[32m        if (!inArray(name, this._opts.plugins)) {[m
[32m+[m[32m            this._opts.plugins.push(args.length ? [].concat([name], args) : name);[m
[32m+[m[32m            typeof this._plugins[name] === 'function' && this._plugins[name].apply(this, args);[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     *  simple event delegate method[m
[32m+[m[32m     *[m
[32m+[m[32m     *  @param {String} evtType event name[m
[32m+[m[32m     *  @param {String} selector the simple css selector like jQuery[m
[32m+[m[32m     *  @param {Function} callback event callback[m
[32m+[m[32m     *  @public[m
[32m+[m[32m     *[m
[32m+[m[32m     *  @alias iSliderPrototype.bind[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.bind = iSliderPrototype.delegate = function (evtType, selector, callback) {[m
[32m+[m
[32m+[m[32m        function delegatedEventCallbackHandle(e) {[m
[32m+[m[32m            var evt = global.event ? global.event : e;[m
[32m+[m[32m            var target = evt.target;[m
[32m+[m[32m            var eleArr = document.querySelectorAll(selector);[m
[32m+[m[32m            for (var i = 0; i < eleArr.length; i++) {[m
[32m+[m[32m                if (target === eleArr[i]) {[m
[32m+[m[32m                    callback.call(target);[m
[32m+[m[32m                    break;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this.wrap.addEventListener(evtType, delegatedEventCallbackHandle, false);[m
[32m+[m
[32m+[m[32m        var key = evtType + ';' + selector;[m
[32m+[m[32m        if (!this._EventHandle.hasOwnProperty(key)) {[m
[32m+[m[32m            this._EventHandle[key] = [[m
[32m+[m[32m                [callback],[m
[32m+[m[32m                [delegatedEventCallbackHandle][m
[32m+[m[32m            ];[m
[32m+[m[32m        } else {[m
[32m+[m[32m            this._EventHandle[key][0].push(callback);[m
[32m+[m[32m            this._EventHandle[key][1].push(delegatedEventCallbackHandle);[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * remove event delegate from wrap[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param {String} evtType event name[m
[32m+[m[32m     * @param {String} selector the simple css selector like jQuery[m
[32m+[m[32m     * @param {Function} callback event callback[m
[32m+[m[32m     * @public[m
[32m+[m[32m     *[m
[32m+[m[32m     * @alias iSliderPrototype.unbind[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.unbind = iSliderPrototype.unDelegate = function (evtType, selector, callback) {[m
[32m+[m[32m        var key = evtType + ';' + selector;[m
[32m+[m[32m        if (this._EventHandle.hasOwnProperty(key)) {[m
[32m+[m[32m            var i = this._EventHandle[key][0].indexOf(callback);[m
[32m+[m[32m            if (i > -1) {[m
[32m+[m[32m                this.wrap.removeEventListener(evtType, this._EventHandle[key][1][i]);[m
[32m+[m[32m                this._EventHandle[key][0][i] = this._EventHandle[key][1][i] = null;[m
[32m+[m[32m                // delete this._EventHandle[key][0][i];[m
[32m+[m[32m                // delete this._EventHandle[key][1][i];[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return false;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * removeEventListener to release the memory[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.destroy = function () {[m
[32m+[m[32m        var outer = this.outer;[m
[32m+[m[32m        var device = this.deviceEvents;[m
[32m+[m
[32m+[m[32m        this.fire('destroy');[m
[32m+[m
[32m+[m[32m        // Clear events[m
[32m+[m[32m        if (this.isTouchable) {[m
[32m+[m[32m            outer.removeEventListener(device.startEvt, this);[m
[32m+[m[32m            outer.removeEventListener(device.moveEvt, this);[m
[32m+[m[32m            outer.removeEventListener(device.endEvt, this);[m
[32m+[m
[32m+[m[32m            // Viscous drag unbind[m
[32m+[m[32m            !device.hasTouch && outer.removeEventListener('mouseout', this);[m
[32m+[m[32m        }[m
[32m+[m[32m        global.removeEventListener(device.resizeEvt, this);[m
[32m+[m[32m        global.removeEventListener('focus', this);[m
[32m+[m[32m        global.removeEventListener('blur', this);[m
[32m+[m
[32m+[m[32m        var n;[m
[32m+[m
[32m+[m[32m        // Clear delegate events[m
[32m+[m[32m        for (n in this._EventHandle) {[m
[32m+[m[32m            var handList = this._EventHandle[n][1];[m
[32m+[m[32m            for (var i = 0; i < handList.length; i++) {[m
[32m+[m[32m                if (typeof handList[i] === 'function') {[m
[32m+[m[32m                    this.wrap.removeEventListener(n.substr(0, n.indexOf(';')), handList[i]);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        this._EventHandle = null;[m
[32m+[m
[32m+[m[32m        // Clear timer[m
[32m+[m[32m        for (n in this._LSN) {[m
[32m+[m[32m            this._LSN.hasOwnProperty(n) && this._LSN[n] && global.clearTimeout(this._LSN[n]);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        this._LSN = null;[m
[32m+[m
[32m+[m[32m        this.wrap.innerHTML = '';[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Register event callback[m
[32m+[m[32m     * @param {String} eventName[m
[32m+[m[32m     * @param {Function} func[m
[32m+[m[32m     * @returns {Object} return this instance of iSlider[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.on = function (eventName, func, force) {[m
[32m+[m[32m        if (inArray(eventName, iSlider.EVENTS) && typeof func === 'function') {[m
[32m+[m[32m            !(eventName in this.events) && (this.events[eventName] = []);[m
[32m+[m[32m            if (!force) {[m
[32m+[m[32m                this.events[eventName].push(func);[m
[32m+[m[32m            } else {[m
[32m+[m[32m                this.events[eventName].unshift(func);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        return this;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Find callback function position[m
[32m+[m[32m     * @param {String} eventName[m
[32m+[m[32m     * @param {Function} func[m
[32m+[m[32m     * @returns {Boolean}[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.has = function (eventName, func) {[m
[32m+[m[32m        if (eventName in this.events) {[m
[32m+[m[32m            return -1 < this.events[eventName].indexOf(func);[m
[32m+[m[32m        }[m
[32m+[m[32m        return false;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Remove event callback[m
[32m+[m[32m     * @param {String} eventName[m
[32m+[m[32m     * @param {Function} func[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.off = function (eventName, func) {[m
[32m+[m[32m        if (eventName in this.events) {[m
[32m+[m[32m            var index = this.events[eventName].indexOf(func);[m
[32m+[m[32m            if (index > -1) {[m
[32m+[m[32m                delete this.events[eventName][index];[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Trigger event callbacks[m
[32m+[m[32m     * @param {String} eventName[m
[32m+[m[32m     * @param {*} args[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.fire = function (eventNames) {[m
[32m+[m[32m        var args = _A(arguments, 1);[m
[32m+[m[32m        eventNames.split(/\x20+/).forEach(function (eventName) {[m
[32m+[m[32m            this.log('[EVENT FIRE]:', eventName, args);[m
[32m+[m[32m            if (eventName in this.events) {[m
[32m+[m[32m                var funcs = this.events[eventName];[m
[32m+[m[32m                for (var i = 0; i < funcs.length; i++) {[m
[32m+[m[32m                    typeof funcs[i] === 'function'[m
[32m+[m[32m                    && funcs[i].apply[m
[32m+[m[32m                    && funcs[i].apply(this, args);[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }.bind(this));[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * reset & rerender[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.reset = function () {[m
[32m+[m[32m        this.pause();[m
[32m+[m[32m        this._unWatchTransitionEnd();[m
[32m+[m[32m        //this._setting();[m
[32m+[m[32m        this.width = typeof this._opts.width === 'number' ? this._opts.width : this.wrap.offsetWidth;[m
[32m+[m[32m        this.height = typeof this._opts.height === 'number' ? this._opts.height : this.wrap.offsetHeight;[m
[32m+[m[32m        this.ratio = this.height / this.width;[m
[32m+[m[32m        this.scale = this.isVertical ? this.height : this.width;[m
[32m+[m[32m        this._renderWrapper();[m
[32m+[m[32m        this._autoPlay();[m
[32m+[m[32m        this.fire('reset slideRestored', this.slideIndex, this.currentEl, this);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Reload Data & render[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param {Array} data[m
[32m+[m[32m     * @param {Number} initIndex[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.loadData = function (data, initIndex) {[m
[32m+[m[32m        this.pause();[m
[32m+[m[32m        this._unWatchTransitionEnd();[m
[32m+[m[32m        this.slideIndex = initIndex || 0;[m
[32m+[m[32m        this.data = data;[m
[32m+[m[32m        this._renderWrapper();[m
[32m+[m[32m        this._autoPlay();[m
[32m+[m[32m        this.fire('loadData slideChanged', this.slideIndex, this.currentEl, this);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Add scenes to the end of the data datasheets[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param {Object|Array} sceneData[m
[32m+[m[32m     * @description[m
[32m+[m[32m     *   Object:[m
[32m+[m[32m     *     {content:...}[m
[32m+[m[32m     *   Array:[m
[32m+[m[32m     *     [{content:...}, {content:...}, ...][m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.pushData = function (sceneData) {[m
[32m+[m[32m        if (sceneData == null) {[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[32m        var len = this.data.length;[m
[32m+[m[32m        this.data = this.data.concat(sceneData);[m
[32m+[m[32m        if (this.isLooping && this.slideIndex === 0) {[m
[32m+[m[32m            this._renderItem(this.els[0], this.data.length - 1);[m
[32m+[m[32m        } else if (len - 1 === this.slideIndex) {[m
[32m+[m[32m            this._renderItem(this.els[2], len);[m
[32m+[m[32m            this._autoPlay(); // restart[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Add scenes to the head of the data datasheets[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param {Object|Array} sceneData[m
[32m+[m[32m     * @description[m
[32m+[m[32m     *   Object:[m
[32m+[m[32m     *     {content:...}[m
[32m+[m[32m     *   Array:[m
[32m+[m[32m     *     [{content:...}, {content:...}, ...][m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.unshiftData = function (sceneData) {[m
[32m+[m[32m        if (sceneData == null) {[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (!isArray(sceneData)) {[m
[32m+[m[32m            sceneData = [sceneData];[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var n = sceneData.length;[m
[32m+[m[32m        this.data = sceneData.concat(this.data);[m
[32m+[m
[32m+[m[32m        if (this.slideIndex === 0) {[m
[32m+[m[32m            this._renderItem(this.els[0], n - 1);[m
[32m+[m[32m        }[m
[32m+[m[32m        this.slideIndex += n;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * auto check to play and bind events[m
[32m+[m[32m     * @private[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype._autoPlay = function () {[m
[32m+[m[32m        this.delay > 0 ? global.setTimeout(this.play.bind(this), this.delay) : this.play();[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Start autoplay[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.play = function () {[m
[32m+[m[32m        this.pause();[m
[32m+[m[32m        // If not looping, stop playing when meet the end of data[m
[32m+[m[32m        if (this.isAutoplay && (this.isLooping || this.slideIndex < this.data.length - 1)) {[m
[32m+[m[32m            this._LSN.autoPlay = global.setTimeout(this.slideNext.bind(this), this.duration);[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * pause autoplay[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.pause = function () {[m
[32m+[m[32m        this._LSN.autoPlay && global.clearTimeout(this._LSN.autoPlay);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Maintaining the current scene[m
[32m+[m[32m     * Disable touch events, except for the native method.[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.hold = function () {[m
[32m+[m[32m        this._holding = true;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Release current scene[m
[32m+[m[32m     * unlock at same time[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.unhold = function () {[m
[32m+[m[32m        this._holding = false;[m
[32m+[m[32m        this.unlock();[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * You can't do anything on this scene[m
[32m+[m[32m     * lock native method calls[m
[32m+[m[32m     * hold at same time[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.lock = function () {[m
[32m+[m[32m        this.hold();[m
[32m+[m[32m        this._locking = true;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * unlock native method calls[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.unlock = function () {[m
[32m+[m[32m        this._locking = false;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Fill the seam[m
[32m+[m[32m     * @param {HTMLElement} el[m
[32m+[m[32m     * @private[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.seamScale = function (el) {[m
[32m+[m[32m        var regex = /scale([XY]?)\(([^\)]+)\)/;[m
[32m+[m[32m        var transformStyle = iSlider.getStyle(el, 'transform');[m
[32m+[m[32m        if (regex.test(transformStyle)) {[m
[32m+[m[32m            iSlider.setStyle(el, 'transform', transformStyle.replace(regex, function (res, axis, scale) {[m
[32m+[m[32m                var sc = {};[m
[32m+[m[32m                if (axis) {[m
[32m+[m[32m                    sc[axis] = parseFloat(scale);[m
[32m+[m[32m                    return 'scale' + this.axis + '(' + (axis === this.axis ? 1.001 * sc[this.axis] : 1.001) + ')';[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    if (scale.indexOf(',') > -1) {[m
[32m+[m[32m                        scale = scale.split(',');[m
[32m+[m[32m                        sc.X = parseFloat(scale[0]);[m
[32m+[m[32m                        sc.Y = parseFloat(scale[1]);[m
[32m+[m[32m                    } else {[m
[32m+[m[32m                        sc.Y = sc.X = parseFloat(scale);[m
[32m+[m[32m                    }[m
[32m+[m[32m                    sc[this.axis] *= 1.001;[m
[32m+[m[32m                    return 'scale(' + sc.X + ', ' + sc.Y + ')';[m
[32m+[m[32m                }[m
[32m+[m[32m            }.bind(this)));[m
[32m+[m[32m        } else {[m
[32m+[m[32m            iSlider.setStyle(el, 'transform', transformStyle + ' scale' + this.axis + '(1.001)');[m
[32m+[m[32m        }[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @param {HTMLElement} el[m
[32m+[m[32m     * @private[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.originScale = function (el) {[m
[32m+[m[32m        var regex = /([\x20]?scale)([XY]?)\(([^\)]+)\)/;[m
[32m+[m[32m        iSlider.setStyle(el, 'transform', iSlider.getStyle(el, 'transform').replace(regex, function (sc, res, axis, scale) {[m
[32m+[m[32m            sc = {};[m
[32m+[m[32m            if (axis) {[m
[32m+[m[32m                if (scale === '1.001') {[m
[32m+[m[32m                    return '';[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    sc[axis] = parseFloat(scale);[m
[32m+[m[32m                    return 'scale' + this.axis + '(' + (axis === this.axis ? sc[this.axis] / 1.001 : 1) + ')';[m
[32m+[m[32m                }[m
[32m+[m[32m            } else {[m
[32m+[m[32m                if (scale.indexOf(',') > -1) {[m
[32m+[m[32m                    scale = scale.split(',');[m
[32m+[m[32m                    sc.X = parseFloat(scale[0]);[m
[32m+[m[32m                    sc.Y = parseFloat(scale[1]);[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    sc.Y = sc.X = parseFloat(scale);[m
[32m+[m[32m                }[m
[32m+[m[32m                sc[this.axis] /= 1.001;[m
[32m+[m[32m                return 'scale(' + sc.X + ', ' + sc.Y + ')';[m
[32m+[m[32m            }[m
[32m+[m[32m        }.bind(this)));[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Whether this node in rule[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param {HTMLElement} target[m
[32m+[m[32m     * @param {Array} rule[m
[32m+[m[32m     * @returns {boolean}[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype._isItself = function (target) {[m
[32m+[m[32m        var rule = this.fixPage;[m
[32m+[m[32m        if (typeof rule === 'string') {[m
[32m+[m[32m            //get dom path[m
[32m+[m[32m            var path = [], parent = target, selector;[m
[32m+[m[32m            while (!hasClass(parent, 'islider-outer') && parent !== this.wrap) {[m
[32m+[m[32m                path.push(parent);[m
[32m+[m[32m                parent = parent.parentNode;[m
[32m+[m[32m            }[m
[32m+[m[32m            parent = path.pop();[m
[32m+[m
[32m+[m[32m            if (path.length) {[m
[32m+[m[32m                try {[m
[32m+[m[32m                    selector = Array.prototype.slice.call(parent.querySelectorAll(rule));[m
[32m+[m[32m                    if (selector.length) {[m
[32m+[m[32m                        for (var i = 0; i < path.length; i++) {[m
[32m+[m[32m                            if (selector.indexOf(path[i]) > -1) {[m
[32m+[m[32m                                return true;[m
[32m+[m[32m                            }[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                } catch (e) {[m
[32m+[m[32m                    return false;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Let target islider controls this one[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param {iSlider} target[m
[32m+[m[32m     * @param {Object} how[m
[32m+[m[32m     * @public[m
[32m+[m[32m     */[m
[32m+[m[32m    iSliderPrototype.subjectTo = function (target, how) {[m
[32m+[m[32m        if (!target instanceof iSlider) {[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        var self = this;[m
[32m+[m
[32m+[m[32m        self.animateTime = target.animateTime;[m
[32m+[m[32m        self.isLooping = target.isLooping;[m
[32m+[m[32m        self.isAutoplay = false;[m
[32m+[m
[32m+[m[32m        target.on('slideStart', function (evt) {[m
[32m+[m[32m            self.startHandler(evt);[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        target.on('slide', function (evt) {[m
[32m+[m[32m            self.moveHandler(evt);[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        target.on('slideEnd', function (evt) {[m
[32m+[m[32m            self.endHandler(evt);[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        target.on('slideChange', function (i) {[m
[32m+[m[32m            var l = self.data.length;[m
[32m+[m[32m            var d = self.direction;[m
[32m+[m[32m            if (d > 0 && (i - self.slideIndex + l) % l === 1) {[m
[32m+[m[32m                self.slideNext();[m
[32m+[m[32m            } else if (d < 0 && (i - self.slideIndex - l) % l === -1) {[m
[32m+[m[32m                self.slidePrev();[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        target.on('slideRestore', function (i) {[m
[32m+[m[32m            if (self.slideIndex !== i) {[m
[32m+[m[32m                self.slideTo(i);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    /* CommonJS */[m
[32m+[m[32m    if (typeof require === 'function' && typeof module === 'object' && module && typeof exports === 'object' && exports)[m
[32m+[m[32m        module.exports = iSlider;[m
[32m+[m[32m    /* AMD */[m
[32m+[m[32m    else if (typeof define === 'function' && define['amd'])[m
[32m+[m[32m        define(function () {[m
[32m+[m[32m            return iSlider;[m
[32m+[m[32m        });[m
[32m+[m[32m    /* Global */[m
[32m+[m[32m    else[m
