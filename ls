[32m+[m[32m  background-repeat: repeat-x;[m
[32m+[m[32m  border-color: #dcdcdc;[m
[32m+[m[32m  -webkit-box-shadow: inset 0 1px 3px rgba(0, 0, 0, .05), 0 1px 0 rgba(255, 255, 255, .1);[m
