[32m+[m[32m<!DOCTYPE html>[m
[32m+[m[32m<html lang="en">[m
[32m+[m[32m<head>[m
[32m+[m[32m    <meta charset="UTF-8">[m
[32m+[m[32m    <title>jquery动画</title>[m
[32m+[m[32m    <script src="../lib/jquery/jquery-3.1.0.js"></script>[m
[32m+[m[32m    <script>[m
[32m+[m[32m        $(document).ready(function () {[m
[32m+[m[32m            $("#box").css({[m
[32m+[m[32m                "width": "200px",[m
[32m+[m[32m                "height": "200px",[m
[32m+[m[32m                "background": "red"[m
[32m+[m[32m            });[m
[32m+[m
[32m+[m[32m            $("button:nth-child(1)").click(function () {[m
[32m+[m[32m                $("#box").fadeOut(1000);[m
[32m+[m[32m            });[m
[32m+[m[32m            $("button:nth-child(2)").click(function () {[m
[32m+[m[32m                $("#box").fadeIn(1000);[m
[32m+[m[32m            });[m
[32m+[m[32m            $("button:nth-child(3)").click(function () {[m
[32m+[m[32m                $("#box").fadeToggle(1000);[m
[32m+[m[32m            });[m
[32m+[m[32m            $("button:nth-child(4)").click(function () {[m
[32m+[m[32m                $("#box").fadeTo(2000, 0.5);[m
[32m+[m[32m            });[m
[32m+[m
[32m+[m[32m            /**自定义动画**/[m
[32m+[m
[32m+[m[32m            $("button:nth-child(5)").click(function () {[m
[32m+[m[32m                $("#box").animate({[m
[32m+[m[32m                    "margin-left":""+$("#box").width(),[m
[32m+[m[32m                    "background-color":"gold"[m
[32m+[m[32m                },2000,"swing", function () {[m
[32m+[m[32m                    $("#box").animate({[m
[32m+[m[32m                        "margin-left":"0px",[m
[32m+[m[32m                        "background-color":"red"[m
[32m+[m[32m                    },2000,"swing",this);[m
[32m+[m[32m                });[m
[32m+[m[32m            });[m
[32m+[m[32m        });[m
[32m+[m[32m    </script>[m
[32m+[m
[32m+[m[32m</head>[m
[32m+[m[32m<body>[m
[32m+[m[32m<button>淡出</button>[m
