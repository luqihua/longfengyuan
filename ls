[32m+[m[32m    </div>[m
[32m+[m[32m    <span class="to-left" id="left"> < </span>[m
[32m+[m[32m    <span class="to-right" id="right"> > </span>[m
[32m+[m
[32m+[m[32m    <script>[m
[32m+[m[32m        var imgList = [[m
[32m+[m[32m            "img/a.jpg",[m
[32m+[m[32m            "img/b.jpg",[m
[32m+[m[32m            "img/c.jpg",[m
[32m+[m[32m            "img/d.jpg"[m
[32m+[m[32m        ];[m
[32m+[m[32m        var slider = document.getElementById("slider");[m
[32m+[m[32m        var itemWidth = slider.offsetWidth;[m
[32m+[m
[32m+[m[32m        var banner = slider.children[0];[m
[32m+[m[32m        var circle = slider.children[1];[m
[32m+[m
[32m+[m[32m        banner.style.width = imgList.length * itemWidth + "px";[m
[32m+[m[32m        for (var i = 0; i < imgList.length; i++) {[m
[32m+[m[32m            /*æ·»åŠ å›¾ç‰‡*/[m
[32m+[m[32m            var li = document.createElement("li");[m
[32m+[m[32m            var img = document.createElement("img");[m
[32m+[m[32m            img.src = imgList[i];[m
[32m+[m[32m            li.appendChild(img);[m
[32m+[m[32m            banner.appendChild(li);[m
[32m+[m[32m            /*æ·»åŠ åœ†ç‚¹*/[m
[32m+[m[32m            var span = document.createElement("span");[m
[32m+[m[32m            span.innerHTML = i + 1;[m
[32m+[m[32m            span.index = i;[m
[32m+[m[32m            if (i == 0)span.style.backgroundColor = "RGBA(255, 0, 0, 0.6)";[m
[32m+[m[32m            circle.appendChild(span);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        circle.style.left = itemWidth / 2 - circle.offsetWidth / 2+"px";[m
[32m+[m[32m        /*--å·¦å³æŒ‰é’®è®¾ç½®--*/[m
[32m+[m
[32m+[m[32m    </script>[m
[32m+[m[32m</div>[m
[32m+[m
[32m+[m[32m</body>[m
[32m+[m[32m</html>[m
