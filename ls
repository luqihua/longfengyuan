[1mdiff --git a/js/main.js b/js/main.js[m
[1mnew file mode 100644[m
[1mindex 0000000..292e2c6[m
[1m--- /dev/null[m
[1m+++ b/js/main.js[m
[36m@@ -0,0 +1,94 @@[m
[32m+[m[32m/**[m
[32m+[m[32m * Created by 82172 on 2016/10/30.[m
[32m+[m[32m */[m
[32m+[m[32m$(document).ready(function () {[m
[32m+[m
[32m+[m[32m    /*---样式常量--*/[m
[32m+[m[32m    var checkBgc = "RGBA(255, 0, 0, 0.6)";[m
[32m+[m[32m    var unCheckBgc = "RGBA(100, 100, 100, 0.6)";[m
[32m+[m[32m    var animatedTime = 1000;[m
[32m+[m[32m    var interval = 2000;[m
[32m+[m[32m    /*--dom元素--*/[m
[32m+[m[32m    var slider = $("#slider");[m
[32m+[m[32m    var banner = $("#banner");[m
[32m+[m[32m    var circle = $("#circle");[m
[32m+[m[32m    var leftBtn = $("#left");[m
[32m+[m[32m    var rightBtn = $("#right");[m
[32m+[m[32m    /*--全局标记--*/[m
[32m+[m[32m    var timer;[m
[32m+[m[32m    var itemWidth = slider.width();[m
[32m+[m[32m    console.log(itemWidth);[m
[32m+[m[32m    var currentPos = 0;[m
[32m+[m[32m    var isAnimated = false;[m
[32m+[m
[32m+[m[32m    /*---左右点击--*/[m
[32m+[m[32m    var itemHeight = slider.height();[m
[32m+[m[32m    slider.children("span").css("top", itemHeight / 2 - leftBtn.height() / 2 + "px");[m
[32m+[m[32m    leftBtn.click(function () {[m
[32m+[m[32m        turnPage(-1);[m
[32m+[m[32m    });[m
[32m+[m[32m    rightBtn.click(function () {[m
[32m+[m[32m        turnPage(1);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    /*播放*/[m
[32m+[m[32m    var dots = circle.children("span");[m
[32m+[m[32m    var num = dots.length;[m
[32m+[m[32m    for (var i = 0; i < num; i++) {[m
[32m+[m[32m        dots[i].onclick = dotClick;[m
[32m+[m[32m    }[m
[32m+[m[32m    /*-自动播放--*/[m
[32m+[m[32m    timer = setInterval(doAnim, interval);[m
[32m+[m
[32m+[m[32m    /*--函数---*/[m
[32m+[m[32m    function dotClick() {[m
[32m+[m[32m        clearInterval(timer);[m
[32m+[m[32m        var position = parseInt(this.index);[m
[32m+[m[32m        dots[currentPos].style.backgroundColor = unCheckBgc;[m
[32m+[m[32m        dots[position].style.backgroundColor = checkBgc;[m
[32m+[m[32m        currentPos = position;[m
[32m+[m[32m        banner.animate([m
[32m+[m[32m            {"left": -position * itemWidth + "px"}[m
[32m+[m[32m            , animatedTime[m
[32m+[m[32m            , "swing"[m
[32m+[m[32m            , function () {[m
[32m+[m[32m                timer = setInterval(doAnim, interval);[m
[32m+[m[32m            });[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function doAnim() {[m
[32m+[m[32m        dots[currentPos].style.backgroundColor = unCheckBgc;[m
[32m+[m[32m        currentPos++;[m
[32m+[m[32m        if (currentPos >= num) {[m
[32m+[m[32m            banner.animate({[m
[32m+[m[32m                "left": 0[m
[32m+[m[32m            }, animatedTime);[m
[32m+[m[32m            currentPos = 0;[m
[32m+[m[32m        }[m
[32m+[m[32m        dots[currentPos].style.backgroundColor = checkBgc;[m
[32m+[m[32m        banner.animate([m
[32m+[m[32m            {"left": -currentPos * itemWidth + "px"},[m
[32m+[m[32m            animatedTime);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function turnPage(type) {[m
[32m+[m[32m        if (isAnimated) return;[m
[32m+[m[32m        isAnimated = true;[m
[32m+[m[32m        clearInterval(timer);[m
[32m+[m[32m        var position = currentPos + type;[m
[32m+[m[32m        if (position < 0) position = num - 1;[m
[32m+[m[32m        if (position >= num)position = 0;[m
[32m+[m[32m        //console.log(currentPos + "====" + position);[m
[32m+[m[32m        dots[currentPos].style.backgroundColor = unCheckBgc;[m
[32m+[m[32m        dots[position].style.backgroundColor = checkBgc;[m
[32m+[m[32m        currentPos = position;[m
[32m+[m[32m        banner.animate([m
[32m+[m[32m            {"left": -position * itemWidth + "px"}[m
[32m+[m[32m            , animatedTime[m
[32m+[m[32m            , "swing"[m
[32m+[m[32m            , function () {[m
[32m+[m[32m                isAnimated = false;[m
[32m+[m[32m                timer = setInterval(doAnim, interval);[m
[32m+[m[32m            });[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
\ No newline at end of file[m
