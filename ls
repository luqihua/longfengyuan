[32m+[m[32m                if (i == 0)span.style.backgroundColor = "RGBA(255, 0, 0, 0.6)";[m
[32m+[m[32m                circle.appendChild(span);[m
[32m+[m[32m            }[m
[32m+[m[32m            banner.style.width = imgList.length * itemWidth + "px";[m
[32m+[m[32m            circle.style.left = itemWidth / 2 - circle.offsetWidth / 2 + "px";[m
[32m+[m
[32m+[m[32m        </script>[m
[32m+[m[32m    </div>[m
[32m+[m[32m    <div class="all">[m
[32m+[m[32m        <span class="glyphicon glyphicon-thumbs-up"></span>[m
[32m+[m[32m        查看全部商品[m
[32m+[m[32m        <span class="glyphicon glyphicon-chevron-right more"></span>[m
[32m+[m[32m    </div>[m
[32m+[m[32m    <div class="content" id="content">[m
[32m+[m
[32m+[m[32m        <div class="item">[m
[32m+[m[32m            <p>黄金</p>[m
[32m+[m[32m            <div><a href="#"><img src="img/a.jpg" alt=""></a></div>[m
[32m+[m[32m        </div>[m
[32m+[m[32m        <div class="item">[m
[32m+[m[32m            <p>白银</p>[m
[32m+[m[32m            <div><a href="#"><img src="img/a.jpg" alt=""></a></div>[m
[32m+[m[32m        </div>[m
[32m+[m[32m        <div class="item">[m
[32m+[m[32m            <p>钻石</p>[m
[32m+[m[32m            <div><a href="#"><img src="img/a.jpg" alt=""></a></div>[m
[32m+[m[32m        </div>[m
[32m+[m[32m        <div class="item">[m
[32m+[m[32m            <p>珠宝</p>[m
[32m+[m[32m            <div><a href="#"><img src="img/a.jpg" alt=""></a></div>[m
[32m+[m[32m        </div>[m
[32m+[m
[32m+[m[32m    </div>[m
[32m+[m[32m    <script>[m
[32m+[m[32m        var content = document.getElementById("content");[m
[32m+[m[32m        var dds = content.getElementsByTagName("img");[m
[32m+[m[32m        var width = dds[0].offsetWidth;[m
[32m+[m[32m        for (var i = 0; i < dds.length; i++) {[m
[32m+[m[32m            dds[i].style.height = width + "px";[m
[32m+[m[32m        }[m
[32m+[m[32m    </script>[m
[32m+[m[32m</section>[m
